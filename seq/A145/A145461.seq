%o A145461 i=1
%o A145461 while i<(10**10-1)/9:
%o A145461     i=10*i+1
%o A145461     for m in range(1,10):
%o A145461         q=i*m
%o A145461         q2=q
%o A145461         for b in range(2,10):
%o A145461             restes=[]
%o A145461             q=q2
%o A145461             while q>0:
%o A145461                 r=q%b
%o A145461                 q=q//b
%o A145461                 restes.append(r)
%o A145461             if restes==[restes[0]]*len(restes):
%o A145461                 print(q2,restes,"en base ",b)
