%p A145149 row:= proc(n) option remember; local f,i,x; f:= unapply (simplify (sum ('cat (a||i) *x^i', 'i'=0..n-1) ), x); unapply (subs (solve ({seq(f(i+1)= coeftayl (x/ (1-x-x^4)/ (1-x)^i, x=0, n), i=0..n-1)}, {seq (cat (a||i), i=0..n-1)}), sum ('cat (a||i) *x^i', 'i'=0..n-1) ), x); end: a:= n-> `if` (n=0, 0, coeftayl (row(n)(x), x=0, 7) *(n-1)!): seq (a(n), n=8..26);
%t A145149 row[n_] := row[n] = Module[{f, i, x, a}, f = Function[Sum[a[i]*#^i, {i, 0, n-1}]]; Function[x, Sum[a[i]*x^i, {i, 0, n-1}] /. First @ Solve[Table[f[i+1] == SeriesCoefficient[x/(1-x-x^4)/(1-x)^i, {x, 0, n}], {i, 0, n-1}]]]]; a[n_] := If[n == 0, 0, SeriesCoefficient[row[n][x], {x, 0, 7}]*(n-1)!]; Table[a[n], {n, 8, 26}] (* _Jean-Fran√ßois Alcover_, Feb 13 2014, after Maple *)
