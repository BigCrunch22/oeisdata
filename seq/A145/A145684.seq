%N A145684 Triangle T(n,k) = 2^(1 + floor((n-1)/2)) * A158893(n,k+1).
%C A145684 Row sums are 2, 18, 72, 112, 312, 408, 1024, 1248, 2976, 3488, ....
%D A145684 H. S. M. Coxeter, Regular Polytopes, 3rd ed., Dover, NY, 1973, pp. 159-162.
%e A145684 The triangle starts in row n=1 with columns 0 <= k < n as:
%e A145684      2;
%e A145684     16,   2;
%e A145684     60,   8,   4;
%e A145684     88,  12,   8,   4;
%e A145684    232,  32,  24,  16,   8;
%e A145684    288,  40,  32,  24,  16,   8;
%e A145684    688,  96,  80,  64,  48,  32,  16;
%e A145684    800, 112,  96,  80,  64,  48,  32, 16;
%e A145684   1824, 256, 224, 192, 160, 128,  96, 64, 32;
%e A145684   2048, 288, 256, 224, 192, 160, 128, 96, 64, 32;
%p A145684 A145684 := proc(n,k) if k = 0 then 7*n-6 ; else n-k; end if; 2^(1+floor((n-1)/2))*% ; end proc: # _R. J. Mathar_, Sep 02 2011
%t A145684 Clear[e, n, k];
%t A145684 e[n_, 0] := 7*n - 6;
%t A145684 e[n_, k_] := 0 /; k >= n;
%t A145684 e[n_, k_] := (e[n - 1, k]*e[n, k - 1] + 1)/e[n - 1, k - 1];
%t A145684 Table[Table[2^(Floor[(n - 1)/2] + 1)*e[n, k], {k, 0, n - 1}], {n, 1, 10}];
%t A145684 Flatten[%]
%K A145684 nonn,tabl
%O A145684 1,1
