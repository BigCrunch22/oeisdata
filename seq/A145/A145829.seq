%o A145829 (Python)
%o A145829 from itertools import count, islice
%o A145829 from sympy import integer_nthroot
%o A145829 def A145829gen(): # generator of terms
%o A145829     m = 0
%o A145829     for n in count(1):
%o A145829         m ^= n**2
%o A145829         a, b = integer_nthroot(m,2)
