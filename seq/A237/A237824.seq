%t A237824 z = 60; q[n_] := q[n] = IntegerPartitions[n];
%t A237824 Table[Count[q[n], p_ /; 2 Min[p] < Max[p]], {n, z}]  (* A237820 *)
%t A237824 Table[Count[q[n], p_ /; 2 Min[p] <= Max[p]], {n, z}] (* A237821 *)
