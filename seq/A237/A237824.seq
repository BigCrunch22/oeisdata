%o A237824 (PARI)
%o A237824 N=60; x='x+O('x^N);
%o A237824 gf = sum(m=1, N, (x^m)/(1-x^m)) + sum(i=1, N, sum(j=1, i, x^((2*i)+j)/prod(k=0, j, 1 - x^(k+i))));
