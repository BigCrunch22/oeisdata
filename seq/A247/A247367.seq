%t A247367 sQ[n_] := sQ[n] = IntegerQ[Sqrt[n]];
%t A247367 a[n_] := Sum[Boole[sQ[k] && !sQ[n-k] || !sQ[k] && sQ[n-k]], {k, 0, Quotient[n, 2]}];
%t A247367 Table[a[n], {n, 0, 100}] (* _Jean-Fran√ßois Alcover_, Aug 10 2022 *)
