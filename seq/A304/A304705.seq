%p A304705 b:= proc(n, r, i, t) option remember; `if`(n=0, 1, `if`(i>n, 0,
%p A304705       b(n, r, i+1, t)+`if`(i/t>r, 0, b(n-i, i/t, i, t+1))))
%p A304705     end:
%p A304705 a:= n-> b(n$2, 1$2):
%p A304705 seq(a(n), n=0..80);  # _Alois P. Heinz_, May 17 2018
%t A304705 b[n_, r_, i_, t_] := b[n, r, i, t] = If[n == 0, 1, If[i > n, 0, b[n, r, i + 1, t] + If[i/t > r, 0, b[n - i, i/t, i, t + 1]]]];
%t A304705 a[n_] := b[n, n, 1, 1];
%t A304705 a /@ Range[0, 80] (* _Jean-Fran√ßois Alcover_, Nov 23 2020, after _Alois P. Heinz_ *)
