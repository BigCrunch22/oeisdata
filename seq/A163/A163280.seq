%p A163280 A163280 := proc(n,k) local r,T ; r := 0 ; for T from k^2 by k do if A033676(T) = k then r := r+1 ; if r = n then RETURN(T) ; fi; fi; od: end: # _R. J. Mathar_, Aug 09 2009
%t A163280 nmax = 12;
%t A163280 pm = Prime[nmax];
%t A163280 sDiv[n_] := Select[Divisors[n], #^2 <= n&][[-1]];
%t A163280 Clear[col]; col[k_] := col[k] = Select[Range[k pm], sDiv[#] == k&];
%t A163280 T[n_, k_ /; 1 <= k <= Length[col[k]]] := col[k][[n]];
%t A163280 Table[T[n-k+1, k], {n, 1, nmax}, {k, 1, n}] // Flatten (* _Jean-François Alcover_, Dec 15 2019 *)
