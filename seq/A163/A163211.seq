%p A163211 A163211 := n -> select(isprime,A163210(n));
%t A163211 sf[n_] := n!/Quotient[n, 2]!^2; a[n_] := (p = Prime[n]; (sf[p - 1] + (-1)^Floor[(p + 2)/2])/p); Select[PrimeQ][Table[a[n], {n, 1, 100}]] (* _G. C. Greubel_, Dec 10 2016 *)
%o A163211 (PARI) sf(n)=n!/(n\2)!^2
