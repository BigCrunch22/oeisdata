%o A333462 (PARI) c=vectorsmall(2000000);for(x=1,1000,my(x2=x*x);c[x2]++;c[x2+x2]++;for(y=1,x-1,my(y2=y*y);c[x2+y2]+=2));
%o A333462 a(n)=sum(k=ceil((1+(n-1)^2)/4),floor(n^2/4),c[k]);
