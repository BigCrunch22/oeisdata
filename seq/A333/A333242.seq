%p A333242 b:= proc(n) option remember;
%p A333242        `if`(isprime(n), 1+b(numtheory[pi](n)), 0)
%p A333242     end:
%p A333242 a:= proc(n) option remember; local p; p:= a(n-1);
%p A333242       do p:= nextprime(p);
%p A333242          if b(p)::odd then break fi
%p A333242       od; p
%p A333242     end: a(1):=2:
