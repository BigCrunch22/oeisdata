%e A100064 such that for each row n, Sum_{k=0..n} T(n,k)/k! = [exp(n)]:
%e A100064 [exp(0)] = 1 = 1
%e A100064 [exp(1)] = 1+1 = 2
%e A100064 [exp(2)] = 1+2+8/2! = 7
%e A100064 [exp(3)] = 1+3+15/2!+51/3! = 20
%e A100064 [exp(4)] = 1+4+24/2!+120/3!+408/4! = 54
%e A100064 [exp(5)] = 1+5+35/2!+225/3!+1215/4!+4365/5! = 148
%o A100064 (PARI) {T(n,k)=if(n==0,1,if(k==0,1,if(k==n,n!*(floor(exp(n))-sum(j=0,n-1,T(n,j)/j!)), k!*polcoeff((Ser(vector(n,i,T(n-1,i-1)/(i-1)!),x)+x*O(x^k))^(n/(n-1)),k,x))))}
