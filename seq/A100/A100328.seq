%o A100328 (PARI) {a(n)=if(n==0,1,sum(j=0,n, if(j==0,1,sum(k=0,j,2*binomial(j,k)*binomial(2*j+k,k-1)/j))* if(n-j==0,1,sum(k=0,n-j,2*binomial(n-j,k)*binomial(2*n-2*j+k,k-1)/(n-j)))))}
