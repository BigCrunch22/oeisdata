%S A110849 30,42,30,84,516,768,804,4806,2838,174,23418,22770
%N A110849 Consider the sequence b(0)=127 and for n>0, b(n) is the least prime of the form k * b(n-1)^2 - 1 where k is a multiple of 6. This sequence gives the values of k.
%e A110849 127,
%e A110849 30*127^2-1,
%e A110849 42*(30*127^2-1)^2-1,
%e A110849 30*(42*(30*127^2-1)^2-1)^2-1,
%e A110849 84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1,
%e A110849 516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 804*(768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 4806*(804*(768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 2838*(4806*(804*(768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 174*(2838*(4806*(804*(768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 23418*(174*(2838*(4806*(804*(768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 and 22770*(23418*(174*(2838*(4806*(804*(768*(516*(84*(30*(42*(30*127^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1)^2-1,
%e A110849 are primes.
%t A110849 lpf[{n_,t_}]:=Module[{a=t^2,k=6},While[!PrimeQ[k*a-1],k=k+6];{k,k*a-1}]; Rest[ NestList[lpf,{127,127},12][[All,1]]] (* _Harvey P. Dale_, Apr 25 2017 *)
