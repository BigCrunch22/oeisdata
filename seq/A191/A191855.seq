%t A191855 r[b_, c_] := (red = Reduce[x > 0 && y > 0 && b*x^2 + 1 == c*y^2, {x, y}, Integers] /. C[1] -> 1 // Simplify; If[Head[red] === Or, First[red], red]); f[128] = {}(* to speed up *); f[n_] := f[n] = If[IntegerQ[Sqrt[n]], {}, Do[c = n/b; If[(r0 = r[b, c]) =!= False, {x0, y0} = {x, y} /. ToRules[r0]; Return[{b, c, x0, y0}]], {b, Divisors[n] // Most}]]; A191855 = Reap[Table[Print[n, " ", f[n]];    If[f[n] != {} && f[n] =!= Null, Sow[f[n][[2]]]], {n, 1, 130}]][[2, 1]] (* _Jean-Fran√ßois Alcover_, Sep 18 2015 *)
