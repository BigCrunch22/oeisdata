%t A191490 f[n_, k_] := f[n, k] = f[n - 1, k] + (n - 1)f[n - 1, k - 1] + If[n == k, 1, 0]
%t A191490 f[_, 0] = 1;
%t A191490 f[0, _] = 0;
%t A191490 Flatten[Table[f[n, k], {n, 0, 100}, {k, 0, n}]]
%o A191490 (Maxima) P[0]:1$
%o A191490 P[n]:=(1+(n-1)*q)*P[n-1]+q^n$
%o A191490 create_list(coeff(expand(P[n]),q^k),n,0,12,k,0,n);
