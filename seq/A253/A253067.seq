%p A253067 OddCA2:=proc(f,M) local n,a,i,f2,g,p;
%p A253067    f2:=simplify(expand(f)) mod 2;
%p A253067    p:=1; g:=f2;
%p A253067    for n from 1 to M do p:=expand(p*g) mod 2; print(n,nops(p)); g:=expand(g^2) mod 2; od:
%p A253067    return;
%p A253067 end;
%p A253067 f24:=1/x+1+x+x/y+x*y;
