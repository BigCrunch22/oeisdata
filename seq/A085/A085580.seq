%o A085580 (PARI) superdiagonal(n,n1) = { default(realprecision,n); b= ""; j=1; for(k=1,n1, v = ""; for(m=1,n, s=.1; for(x=1,n, s=k/(s+m); ); a = Vec(Str(s)); v = concat(v,a[m+2]); ); a2 = Vec(Str(v)); print1(a2[k]","); ) }
