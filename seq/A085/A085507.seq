%p A085507 seq(a(n), n=0..25);  # _Alois P. Heinz_, Aug 06 2021
%t A085507 a[n_] := Sum[ StirlingS2[n, k]*Boole[PrimeQ[k]], {k, 0, n}]; Table[a[n], {n, 0, 23}] (* _Jean-Fran√ßois Alcover_, Oct 29 2012 *)
