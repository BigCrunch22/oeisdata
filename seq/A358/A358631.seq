%o A358631 b2(n)=if(n>0, my(A=b1(3*2^logint(n, 2) - n - 1)); n + if(A>0, 2^(A-1)))
%o A358631 P(n,k)=if(n==0 || k==1, (n==0 && k<3) + (k==1 && n>0), my(L=logint(n, 2), A=n - 2^L); (hammingweight(A) + 2)*P(A, k-1)*(L - b1(n) + 1) + P(b2(A), k))
