%p A358205 N:= 18: # for a(0)..a(N)
%p A358205 V:= Array(0..N): count:= 0:
%p A358205 for k from 0 while count < N+1 do
%p A358205   v:= numtheory:-bigomega(1+2*k+3*k^2);
%p A358205 if v <= N and V[v] = 0 then
%p A358205     count:= count+1; V[v]:= k
%p A358205 fi
%p A358205 od:
%p A358205 convert(V,list);
%t A358205 a[n_] := Module[{i = 0},While[! PrimeOmega[1 + 2 i + 3 i^2] == n, i += 1]; i]
%t A358205 Table[a[n], {n, 0, 14}] (* _Gerry Martens_, Nov 05 2022 *)
