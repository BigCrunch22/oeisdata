%o A358258 (Python)
%o A358258 from itertools import count
%o A358258 def A358258(n):
%o A358258     b, bdict, k = 0, {0:(1,)},1<<n-1 if n > 1 else 0
%o A358258     for m in count(2):
%o A358258         if b >= k:
%o A358258             return b
%o A358258         if len(l := bdict[b]) > 1:
