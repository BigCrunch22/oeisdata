%t A187770 digits = 87; max = 250; s[n_, k_] := s[n, k] = a[n+1-k] + If[n < 2*k, 0, s[n-k, k]]; a[1] = 1; a[n_] := a[n] = Sum[a[k]*s[n-1, k]*k, {k, 1, n-1}]/(n-1); A[x_] := Sum[a[k]*x^k, {k, 0, max}]; APrime[x_] := Sum[k*a[k]*x^(k-1), {k, 0, max}]; eq = Log[c] == 1 + Sum[A[c^(-k)]/k, {k, 2, max}]; alpha = c /. FindRoot[eq, {c, 3}, WorkingPrecision -> digits+5]; b = Sqrt[(1 + Sum[APrime[alpha^-k]/alpha^k, {k, 2, max}])/(2*Pi)]; RealDigits[b, 10, digits] // First (* _Jean-Fran√ßois Alcover_, Sep 24 2014 *)
