%o A187093 (Python)
%o A187093 print(0, end=',')       # a(-1)=0
%o A187093 prpr = prev = 1         # a(0)=a(1)=1
%o A187093 for n in range(2, 77):
%o A187093     print(prpr, end=',')
%o A187093     curr = n*n - prpr   # a(n) = n^2 - a(n-2)
%o A187093     prpr = prev
%o A187093     prev = curr
