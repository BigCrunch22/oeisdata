%N A187755 E.g.f.: Sum_{n>=0} (1 - exp(-n^2*x))^n.
%H A187755 Seiichi Manyama, <a href="/A187755/b187755.txt">Table of n, a(n) for n = 0..162</a>
%F A187755 O.g.f.: Sum_{n>=0} n^(2*n) * n! * x^n / Product_{k=1..n} (1 + n^2*k*x).
%F A187755 a(n) = Sum_{k=0..n} (-1)^(n-k) * k^(2*n) * k! * Stirling2(n,k).
%F A187755 a(n) == 1 (mod 10) for n>=0.
%F A187755 a(n) == 31 (mod 60) for n>=2.
%F A187755 a(n) ~ c * d^n * (n!)^3 / n, where d = 6.8312860494079582446988970296645779575650627187418208311407895492635... and c = 0.192038502554748256318271067254582378566365276592... . - _Vaclav Kotesovec_, May 08 2014
%e A187755 O.g.f.: F(x) = 1 + x + 31*x^2 + 3991*x^3 + 1340251*x^4 + 929043391*x^5 +...
%e A187755 where
%e A187755 F(x) = 1 + x/(1+x) + 2^4*2!*x^2/((1+2^2*1*x)*(1+2^2*2*x)) + 3^6*3!*x^3/((1+3^2*1*x)*(1+3^2*2*x)*(1+3^2*3*x)) + 4^8*4!*x^4/((1+4^2*1*x)*(1+4^2*2*x)*(1+4^2*3*x)*(1+4^2*4*x)) +...
%e A187755 ...
%e A187755 E.g.f.: A(x) = 1 + x + 31*x^2/2! + 3991*x^3/3! + 1340251*x^4/4! +...
%e A187755 where
%e A187755 A(x) = 1 + (1-exp(-x)) + (1-exp(-2^2*x))^2 + (1-exp(-3^2*x))^3 + (1-exp(-4^2*x))^4 + (1-exp(-5^2*x))^5 + (1-exp(-6^2*x))^6 +...
%t A187755 Flatten[{1,Table[Sum[(-1)^(n-k) * k^(2*n) * k! * StirlingS2[n,k], {k,0,n}], {n,1,20}]}] (* _Vaclav Kotesovec_, May 08 2014 *)
%o A187755 (PARI) {a(n)=n!*polcoeff(sum(k=0, n, (1-exp(-k^2*x+x*O(x^n)))^k), n)}
%o A187755 for(n=0, 20, print1(a(n), ", "))
%o A187755 (PARI) {a(n)=polcoeff(sum(m=0, n, m^(2*m)*m!*x^m/prod(k=1, m, 1+m^2*k*x+x*O(x^n))), n)}
%o A187755 for(n=0, 20, print1(a(n), ", "))
%o A187755 (PARI) {a(n) = sum(k=0, n, (-1)^(n-k)*k^(2*n)*k!*stirling(n, k, 2))}
