%e A243882 Here 1=Up=(1,1), 0=Down=(1,-1).
%p A243882 b:= proc(x, y, t, s) option remember; `if`(y<0 or y>x, 0,
%p A243882       `if`(x=0, `if`(s={}, 1, 0), `if`(nops(s)>x, 0, add(
%p A243882       b(x-1, y-1+2*j, irem(2*t+j, 4), s minus {2*t+j}), j=0..1))))
%p A243882     end:
%p A243882 a:= n-> add(b(2*n-2, l[], {$0..7}), l=[[0, 2], [2, 3]]):
%p A243882 seq(a(n), n=5..35);
%t A243882 b[x_, y_, t_, s_List] := b[x, y, t, s] = If[y<0 || y>x, 0, If[x == 0, If[s == {}, 1, 0], If[Length[s]>x, 0, Sum[b[x-1, y-1 + 2*j, Mod[2*t+j, 4], s ~Complement~ {2*t + j}], {j, 0, 1}]]]]; a[n_] :=  Sum[b[2*n-2, Sequence @@ l, Range[0, 7]], {l, {{0, 2}, {2, 3}}}]; Table[a[n], {n, 5, 35}] (* _Jean-Fran√ßois Alcover_, Feb 05 2015, after _Alois P. Heinz_ *)
