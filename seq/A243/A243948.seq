%N A243948 G.f.: Sum_{n>=0} x^n / (1-x)^(2*n+1) * [Sum_{k=0..n} C(n,k)^2 * x^k] * [Sum_{k=0..n} C(n,k)^2 * 2^k * x^k].
%H A243948 Paul D. Hanna, <a href="/A243948/b243948.txt">Table of n, a(n) for n = 0..400</a>
%F A243948 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k)^2 * Sum_{j=0..k} C(k,j)^2 * 2^j * x^j.
%F A243948 a(n) = Sum_{k=0..[n/2]} Sum_{j=0..n-2*k} C(n-k, k+j)^2 * C(k+j, k)^2 * 2^k.
%F A243948 Recurrence: (n-3)*(n-2)*n^2*a(n) = 2*(n-3)*(4*n^3 - 12*n^2 + 10*n - 3)*a(n-1) - 2*(n-1)*(5*n^3 - 30*n^2 + 58*n - 38)*a(n-2) + 8*(n-2)*a(n-3) + 4*(n-3)*(5*n^3 - 30*n^2 + 58*n - 34)*a(n-4) - 8*(n-1)*(4*n^3 - 36*n^2 + 106*n - 101)*a(n-5) + 8*(n-4)^2*(n-2)*(n-1)*a(n-6). - _Vaclav Kotesovec_, Aug 17 2014
%F A243948 a(n) ~ sqrt(c) * d^n / (2^(3/2)*Pi*n), where d = 2 + sqrt(2) + 2*sqrt(1+sqrt(2)) = 6.52176151043316966349... is the root of the equation 4 - 16*d + 12*d^2 - 8*d^3 + d^4 = 0, and c = 4 + 5/sqrt(2) + 2*sqrt(7+5*sqrt(2)) = 15.0378183078640521... is the root of the equation 1 - 32*c + 60*c^2 - 64*c^3 + 4*c^4 = 0. - _Vaclav Kotesovec_, Aug 17 2014
%e A243948 G.f.: A(x) = 1 + 2*x + 8*x^2 + 36*x^3 + 182*x^4 + 964*x^5 + 5296*x^6 +...
%e A243948 where the g.f. is given by the binomial series:
%e A243948 A(x) = 1/(1-x) + x/(1-x)^3*(1+x) * (1+2*x)
%e A243948 + x^2/(1-x)^5*(1 + 2^2*x + x^2) * (1 + 2^2*2*x + 4*x^2)
%e A243948 + x^3/(1-x)^7*(1 + 3^2*x + 3^2*x^2 + x^3) * (1 + 3^2*2*x + 3^2*4*x^2 + 8*x^3)
%e A243948 + x^4/(1-x)^9*(1 + 4^2*x + 6^2*x^2 + 4^2*x^3 + x^4) * (1 + 4^2*2*x + 6^2*4*x^2 + 4^2*8*x^3 + 16*x^4)
%e A243948 + x^5/(1-x)^11*(1 + 5^2*x + 10^2*x^2 + 10^2*x^3 + 5^2*x^4 + x^5) * (1 + 5^2*2*x + 10^2*4*x^2 + 10^2*8*x^3 + 5^2*16*x^4 + 32*x^5) +...
%e A243948 We can also express the g.f. by the binomial series identity:
%e A243948 A(x) = 1 + x*(1 + (1+2*x)) + x^2*(1 + 2^2*(1+2*x) + (1+2^2*2*x+4*x^2))
%e A243948 + x^3*(1 + 3^2*(1+2*x) + 3^2*(1+2^2*2*x+4*x^2) + (1+3^2*2*x+3^2*4*x^2+8*x^3))
%e A243948 + x^4*(1 + 4^2*(1+2*x) + 6^2*(1+2^2*2*x+4*x^2) + 4^2*(1+3^2*2*x+3^2*4*x^2+8*x^3) + (1+4^2*2*x+6^2*4*x^2+4^2*8*x^3+16*x^4))
%e A243948 + x^5*(1 + 5^2*(1+2*x) + 10^2*(1+2^2*2*x+4*x^2) + 10^2*(1+3^2*2*x+3^2*4*x^2+8*x^3) + 5^2*(1+4^2*2*x+6^2*4*x^2+4^2*8*x^3+16*x^4) + (1+5^2*2*x+10^2*4*x^2+10^2*8*x^3+5^2*16*x^4+32*x^5)) +...
%o A243948 (PARI) /* By definition: */
%o A243948 {a(n)=local(A=1); A=sum(m=0, n, x^m/(1-x)^(2*m+1) * sum(k=0, m, binomial(m, k)^2*x^k) * sum(k=0, m, binomial(m, k)^2*2^k*x^k) +x*O(x^n)); polcoeff(A, n)}
%o A243948 for(n=0, 30, print1(a(n), ", "))
%o A243948 (PARI) /* By a binomial identity: */
%o A243948 {a(n)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2*sum(j=0, k, binomial(k, j)^2*2^j*x^j)+x*O(x^n))), n)}
%o A243948 for(n=0, 30, print1(a(n), ", "))
%o A243948 (PARI) /* Formula for a(n): */
%o A243948 {a(n)=sum(k=0, n\2, sum(j=0, n-2*k, binomial(n-k, k+j)^2*binomial(k+j, k)^2*2^k))}
%o A243948 for(n=0, 30, print1(a(n), ", "))
