%p A243737 seq(a(n), n=2..40);
%t A243737 b[n_, p_, i_] := b[n, p, i] = If[n == 0, p!, If[i<1, 0, Sum[b[n-i*j, p+j, i-1]/j!, {j, 0, n/i}]]]; a[n_] := (k=2; Sum[b[n-i*k, k, i-1]/k!, {i, 1, n/k}]) ; Table[ a[n], {n, 2, 40}] (* _Jean-FranÃ§ois Alcover_, Feb 10 2015, after Maple *)
