%p A243744 b:= proc(n, p, i) option remember; `if`(n=0, p!,
%p A243744       `if`(i<1, 0, add(b(n-i*j, p+j, i-1)/j!, j=0..n/i)))
%p A243744     end:
%p A243744 a:= proc(n) local k; k:=9;
