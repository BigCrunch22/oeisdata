%U A243918 1280109151917797032199865564812,2672783800502564772495577135824089014,22366199286781599568269093307412768076442280
%N A243918 a(n) = Sum_{k=0..n} binomial(n,k) * (1 + 2^k)^k.
%F A243918 a(n) = Sum_{k=0..n} binomial(n,k) * (1 + 2^k)^(n-k) * 2^(k^2).
%F A243918 O.g.f.: Sum_{n>=0} (1 + 2^n)^n * x^n / (1-x)^(n+1).
%F A243918 O.g.f.: Sum_{n>=0} 2^(n^2) * x^n / (1 - (1+2^n)*x)^(n+1).
%F A243918 E.g.f.: exp(x) * Sum_{n>=0} (1 + 2^n)^n * x^n / n!.
%F A243918 a(n) ~ 2^(n^2). - _Vaclav Kotesovec_, Jun 18 2014
%e A243918 O.g.f.: A(x) = 1 + 4*x + 32*x^2 + 814*x^3 + 86600*x^4 + 39560554*x^5 +...
%e A243918 where the g.f. may be expressed by the series identity:
%e A243918 A(x) = 1/(1-x) + 3*x/(1-x)^2 + 5^2*x^2/(1-x)^3 + 9^3*x^3/(1-x)^4 + 17^4*x^4/(1-x)^5 + 33^5*x^5/(1-x)^6 + 65^6*x^6/(1-x)^7 +...
%e A243918 A(x) = 1/(1-2*x) + 2*x/(1-3*x)^2 + 2^4*x^2/(1-5*x)^3 + 2^9*x^3/(1-9*x)^4 + 2^16*x^4/(1-17*x)^5 + 2^25*x^5/(1-33*x)^6 + 2^36*x^6/(1-65*x)^7 +...
%e A243918 Illustration of initial terms:
%e A243918 a(0) = 1;
%e A243918 a(1) = 1 + (1+2);
%e A243918 a(2) = 1 + 2*(1+2) + (1+2^2)^2;
%e A243918 a(3) = 1 + 3*(1+2) + 3*(1+2^2)^2 + (1+2^3)^3;
%e A243918 a(4) = 1 + 4*(1+2) + 6*(1+2^2)^2 + 4*(1+2^3)^3 + (1+2^4)^4;
%e A243918 a(5) = 1 + 5*(1+2) + 10*(1+2^2)^2 + 10*(1+2^3)^3 + 5*(1+2^4)^4 + (1+2^5)^5; ...
%e A243918 Also, by a binomial identity we have
%e A243918 a(0) = 1;
%e A243918 a(1) = 2 + 2;
%e A243918 a(2) = 2^2 + 2*(1+2)*2 + 2^4;
%e A243918 a(3) = 2^3 + 3*(1+2)^2*2 + 3*(1+2^2)*2^4 + 2^9;
%e A243918 a(4) = 2^4 + 4*(1+2)^3*2 + 6*(1+2^2)^2*2^4 + 4*(1+2^3)*2^9 + 2^16;
%e A243918 a(5) = 2^5 + 5*(1+2)^4*2 + 10*(1+2^2)^3*2^4 + 10*(1+2^3)^2*2^9 + 5*(1+2^4)*2^16 + 2^25; ...
%t A243918 Table[Sum[Binomial[n,k]*(1+2^k)^k,{k,0,n}],{n,0,20}] (* _Vaclav Kotesovec_, Jun 18 2014 *)
%o A243918 (PARI) {a(n)=sum(k=0, n, binomial(n, k)*(1+2^k)^k)}
%o A243918 for(n=0, 20, print1(a(n), ", "))
%o A243918 (PARI) {a(n)=sum(k=0, n, binomial(n, k)*(1+2^k)^(n-k)*2^(k^2))}
%o A243918 for(n=0, 20, print1(a(n), ", "))
