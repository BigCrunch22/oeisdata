%p A243743     end:
%p A243743 seq(a(n), n=8..50);
%t A243743 b[n_, p_, i_] := b[n, p, i] = If[n == 0, p!, If[i<1, 0, Sum[b[n-i*j, p+j, i-1]/j!, {j, 0, n/i}]]]; a[n_] := (k=8; Sum[b[n-i*k, k, i-1]/k!, {i, 1, n/k}]); Table[a[n], {n, 8, 50}] (* _Jean-FranÃ§ois Alcover_, Feb 10 2015, after Maple *)
