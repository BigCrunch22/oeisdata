%p A243743 b:= proc(n, p, i) option remember; `if`(n=0, p!,
%p A243743       `if`(i<1, 0, add(b(n-i*j, p+j, i-1)/j!, j=0..n/i)))
%p A243743     end:
%p A243743 a:= proc(n) local k; k:=8;
