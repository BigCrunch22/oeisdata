%F A066353 a(n) = 1 if n = 0, otherwise a(n) = A112873(n) = Sum_{j=1..n} A032378(j).
%t A066353 A032378:= A032378= Table[k*j, {k,15}, {j,k^2+1, k^2+3*k+3}]//Flatten;
%t A066353 A066353[n_]:= A066353[n]= 1 +Sum[A032378[[j+1]], {j,0,n-1}];
