%t A066364 fQ[p_] := Block[{fi = First@# & /@ FactorInteger[ MultiplicativeOrder[ 10, p]]}, Union[ MemberQ[ lst, #] & /@ fi] == {True}]; k = 4; lst = {3}; While[k < 180000, If[ p = Prime@ k; fQ@ p, AppendTo[ lst, p]; Print@ p]; k++]; lst (* _Robert G. Wilson v_, Nov 30 2013 *)
