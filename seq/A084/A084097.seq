%t A084097 T[j_, k_] := Expand[((1 + Sqrt[j])^k + (1 - Sqrt[j])^k)/2]; T[1, 0] = 1; Table[ T[j - k, k], {j, 0, 11}, {k, 0, j}] // Flatten (* _Robert G. Wilson v_, Jan 02 2013 *)
%o A084097 (Magma)
%o A084097 function A084097(n,k)
%o A084097   if k eq 0 then return 1;
%o A084097   else return k*2^(k-1)*(&+[ Binomial(k-j,j)*((n-k-1)/4)^j/(k-j): j in [0..Floor(k/2)]]);
%o A084097   end if; return A084097; end function;
%o A084097 [A084097(n,k): k in [0..n], n in [0..12]]; // _G. C. Greubel_, Oct 15 2022
%o A084097 (SageMath)
%o A084097 def A084097(n,k):
%o A084097     if (k==0): return 1
%o A084097     else: return k*2^(k-1)*sum( binomial(k-j,j)*((n-k-1)/4)^j/(k-j) for j in range( (k+2)//2 ) )
