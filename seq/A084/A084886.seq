%t A084886 ader[n_Integer] := ader[n] = Switch[n, 0|1, 0, _, If[PrimeQ[n], 1, Sum[Module[{p, e}, {p, e} = pe; n e/p], {pe, FactorInteger[n]}]]];
%t A084886 ader[Rational[n_, k_]] := (ader[n] k - ader[k] n)/k^2;
%t A084886 T[n_, k_] := ader[k/n] // Numerator;
