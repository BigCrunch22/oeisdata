%o A084600 instance Num a => Num [a] where
%o A084600    fromInteger k = [fromInteger k]
%o A084600    (p:ps) + (q:qs) = p + q : ps + qs
%o A084600    ps + qs         = ps ++ qs
%o A084600    (p:ps) * qs'@(q:qs) = p * q : ps * qs' + [p] * qs
