%o A020717 (PARI) pisotE(nmax, a1, a2) = {
%o A020717   a=vector(nmax); a[1]=a1; a[2]=a2;
%o A020717   for(n=3, nmax, a[n] = floor(a[n-1]^2/a[n-2]+1/2));
