%o A378290 (PARI) T(n, k, t=3, u=1) = if(k==0, 0^n, k*sum(r=0, n, binomial(t*r+u*(n-r)+k, r)*binomial(r, n-r)/(t*r+u*(n-r)+k)));
%o A378290 matrix(7, 7, n, k, T(n-1, k-1))
