%o A378237 (PARI) T(n, k, t=1, u=3) = if(k==0, 0^n, k*sum(r=0, n, binomial(n, r)*binomial(t*n+u*r+k, n)/(t*n+u*r+k)));
%o A378237 matrix(7, 7, n, k, T(n-1, k-1))
