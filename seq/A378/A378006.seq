%e A378006 See A378007 for more details.
%o A378006 (PARI) A378006(n,k) = {
%o A378006 my(f = factor(n), res = 1); for(i=1, #f~, if(k % f[i,1] == 0, return(0));
%o A378006 my(d = znorder(Mod(f[i,1],k))); if(f[i,2] % d != 0, return(0), my(m = f[i,2]/d, r = eulerphi(k)/d); res *= binomial(m+r-1,r-1)));
%o A378006 res;}
