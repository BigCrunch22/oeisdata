%p A276719 b:= proc(n, m, l) option remember; `if`(n=0, 1,
%p A276719       add(b(n-1, max(m, j), [subsop(1=NULL, l)[],
%p A276719       `if`(j<=m, 0, j)]), j={l[], m+1} minus {0}))
%p A276719     end:
%p A276719 A:= (n, k)-> `if`(n=0, 1, `if`(k<2, k, b(n, 0, [0$(k-1)]))):
%p A276719 seq(seq(A(n, d-n), n=0..d), d=0..14);
%t A276719 b[n_, m_, l_List] := b[n, m, l] = If[n == 0, 1, Sum[b[n - 1, Max[m, j], Append[ReplacePart[l, 1 -> Nothing], If[j <= m, 0, j]]], {j, Append[l, m + 1] ~Complement~ {0}}]]; A[n_, k_] := If[n == 0, 1, If[k < 2, k, b[n, 0, Array[0&, k-1]]]]; Table[A[n, d-n], {d, 0, 14}, {n, 0, d}] // Flatten (* _Jean-FranÃ§ois Alcover_, Jan 06 2017, after _Alois P. Heinz_ *)
