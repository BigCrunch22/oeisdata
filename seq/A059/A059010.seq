%o A059010 (R)
%o A059010 maxrow <- 4 # by choice
%o A059010 onezeros <- 1
%o A059010 for(m in 1:(maxrow+1)){
%o A059010   row <- onezeros[2^(m-1):(2^m-1)]
%o A059010   onezeros <- c(onezeros, c(1-row, row) )
%o A059010 }
%o A059010 a <- which(onezeros == 1)
%o A059010 a
