%t A152112 For [kk=1,kk<=Length[A],kk++,
%t A152112 AAA=Union[AAA,{A[[ii]]+A[[jj]]+A[[kk]]}]]]];
%t A152112 For[ii=1,ii<=Length[AAA],ii++,
%t A152112 If[ii==Length[AAA],max=ii-1];
%t A152112 If[AAA[[ii]]>ii-1,max=ii-2;Break[]]]);
