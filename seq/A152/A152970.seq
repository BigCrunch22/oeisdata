%t A152970 Clear[v, n, row, f]; row[n_] = (n+1);
%t A152970 f[n_, m_] = Floor[(m/n)*row[n]/2]; v[0] = {1}; v[1] = {1, 1};
%t A152970 v[n_] := v[n] = If[Mod[n, 2] == 0, Join[{1}, Table[ f[n, m], {m, 1, Floor[ n/2] - 1}], {row[n] - 2*Sum[ f[n, m], {m, 1, Floor[n/2] - 1}] - 2}, Table[ f[n, m], {m, Floor[n/ 2] - 1, 1, -1}], { 1}],
%t A152970 Join[{1}, Table[ f[n, m], {m, 1, Floor[n/2] - 1}], {row[n]/2 - Sum[ f[n, m], { m, 1, Floor[n/2] - 1}] - 1, row[n]/ 2 - Sum[ f[n, m], {m, 1, Floor[ n/2] - 1}] - 1}, Table[ f[n, m], {m, Floor[n/ 2] - 1, 1, -1}], {1}]];
%t A152970 Table[v[n], {n, 0, 10}]; Flatten[%]
