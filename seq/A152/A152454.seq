%p A152454 seq(B[s],s=2..N); # _Robert Israel_, Nov 11 2014
%t A152454 nn=100; s=Table[{},{nn}]; Do[k=DivisorSigma[1,n]-n; If[1<k<=nn, AppendTo[s[[k]],n]], {n,nn^2}]; Flatten[s]
