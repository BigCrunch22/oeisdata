%o A332662 function a_list(N)
%o A332662     a = Int[]
%o A332662     for n in 1:N
%o A332662         i = 0
%o A332662         for j in ((k:-1:1) for k in 1:n)
%o A332662             t = n - j[1]
%o A332662             for m in j
%o A332662                 push!(a, i, t)
%o A332662                 i += 1
%o A332662 end end end; a end
