%t A025134 ESym[u_] := Module[{v, t}, v = Table[0, {Length[u]+1}]; v[[1]] = 1; For[i = 1, i <= Length[u], i++, t = u[[i]]; For[j = i, j >= 1, j--, v[[j+1]] += v[[j]]*t]]; v];
%t A025134 a[n_] := ESym[Table[Binomial[n, k], {k, 0, n}]][[n+1]];
