%p A025512 N:= 10^3: # to get all terms <= N
%p A025512 B:= Vector(N):
%p A025512 B[1..4]:= <1,1,2,1>:
%p A025512 m:= 4: t:= 2:
%p A025512 for n from 1 while m < N do
%p A025512   t:= 3-t;
