%p A112354 seq(a(n), n = 1..22); # _Peter Luschny_, Nov 21 2022
%t A112354 ff = Range[n = 22]!; s = {}; For[i = 1, i <= n, i++, AppendTo[s, i*ff[[i]] - Sum[s[[d]]*ff[[i-d]], {d, i-1}]]]; Table[Sum[If[Divisible[i, d], MoebiusMu[i/d], 0]*s[[d]], {d, 1, i}]/i, {i, n}] (* _Jean-FranÃ§ois Alcover_, Apr 15 2016 *)
