%p A007616 S[1]:= [0,1,3,8]
%p A007616 for i from 2 to 5 do
%p A007616   S[i]:= map(t -> seq(j*(10^(2*i-2)+1)+10*t, j=[0,1,3,8]), S[i-1])
%p A007616 od:
%p A007616 sort(select(isprime, [11,seq(op(S[i]),i=1..5)])); # _Robert Israel_, Mar 16 2017
