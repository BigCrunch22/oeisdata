%o A325153 Cx = 1 + intformal( Sx*Cy, x);
%o A325153 Sy = intformal( Cy*Cx +y*O(y^(2*k)), y);
%o A325153 Cy = 1 + intformal( Sy*Cx, y));
%o A325153 (2*n+1-2*k)!*(2*k)! *polcoeff(polcoeff(Sx, 2*n+1-2*k, x), 2*k, y)}
