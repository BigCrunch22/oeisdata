%t A325201 e[k_][x_] := Sum[x^j/j!, {j, 0, k}];
%t A325201 A[0, _] = A[_, 0] = 0; A[n_, k_] := (n-1)! Coefficient[e[k][x]^n, x, n-1];
%t A325201 Table[A[n-k, k], {n, 0, 11}, {k, n, 0, -1}] (* _Jean-FranÃ§ois Alcover_, Jul 06 2019 *)
%o A325201 (Python)
%o A325201 # print first num_entries entries in column k
%o A325201 import math, sympy; x=sympy.symbols('x')
%o A325201 k=5; num_entries = 64
%o A325201 P=range(k+1); eP=sum([x**d/math.factorial(d) for d in P]); r = [0,1]; curr_pow = eP
%o A325201 for term in range(1, num_entries-1):
%o A325201     curr_pow=(curr_pow*eP).expand()
%o A325201     r.append(curr_pow.coeff(x**term)*math.factorial(term))
%o A325201 print(r)
