%p A221857 A:= proc(n, k) option remember; local m, r; if n<2 or k=1 then 1
%p A221857       elif k=0 then 0 else r:= iquo(n-1, k, 'm');
%p A221857       binomial(k, m)*A(r+1, k)^m*A(r, k)^(k-m) fi
%p A221857     end:
%p A221857 seq(seq(A(n, d-n), n=0..d), d=0..12);
%t A221857 a[n_, k_] := a[n, k] = Module[{m, r}, If[n < 2 || k == 1, 1, If[k == 0, 0, {r, m} = QuotientRemainder[n-1, k]; Binomial[k, m]*a[r+1, k]^m*a[r, k]^(k-m)]]]; Table[a[n, d-n], {d, 0, 12}, {n, 0, d}] // Flatten (* _Jean-FranÃ§ois Alcover_, Apr 17 2013, translated from Maple *)
