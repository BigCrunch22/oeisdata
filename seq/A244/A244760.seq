%N A244760 a(n) = Sum_{k=0..n} C(n,k) * (1 + 3^k)^(n-k) * 2^k.
%H A244760 G. C. Greubel, <a href="/A244760/b244760.txt">Table of n, a(n) for n = 0..89</a>
%F A244760 E.g.f.: Sum_{n>=0} exp((1+3^n)*x) * (2*x)^n/n!.
%F A244760 O.g.f.: Sum_{n>=0} (2*x)^n/(1 - (1+3^n)*x)^(n+1).
%F A244760 a(n) ~ c * 3^(n^2/4) * 2^((3*n+1)/2) / sqrt(Pi*n), where c = sum_{k=-inf..+inf} 1/(3^(k^2) * 2^k) = 1.88621563508001862566062... if n is even, and c = sum_{k=-inf..+inf} 1/(3^((k+1/2)^2) * 2^(k+1/2)) = 1.88659407336643412717014... if n is odd. - _Vaclav Kotesovec_, Jan 25 2015
%e A244760 E.g.f.: A(x) = 1 + 4*x + 24*x^2/2! + 232*x^3/3! + 3840*x^4/4! + 111904*x^5/5! +...
%e A244760 ILLUSTRATION OF INITIAL TERMS:
%e A244760 a(1) = (1+3^0)^1 + (1+3^1)^0*2 = 4;
%e A244760 a(2) = (1+3^0)^2 + 2*(1+3^1)^1*2 + (1+3^2)^0*2^2 = 24;
%e A244760 a(3) = (1+3^0)^3 + 3*(1+3^1)^2*2 + 3*(1+3^2)^1*2^2 + (1+3^3)^0*2^3 = 232;
%e A244760 a(4) = (1+3^0)^4 + 4*(1+3^1)^3*2 + 6*(1+3^2)^2*2^2 + 4*(1+3^3)^1*2^3 + (1+3^4)^0*2^4 = 3480; ...
%t A244760 Table[Sum[Binomial[n,k] * (1 + 3^k)^(n-k) * 2^k,{k,0,n}],{n,0,20}] (* _Vaclav Kotesovec_, Jan 25 2015 *)
%o A244760 (PARI) {a(n) = sum(k=0,n,binomial(n,k) * (1 + 3^k)^(n-k)*2^k )}
%o A244760 for(n=0,25,print1(a(n),", "))
%o A244760 (PARI) /* E.g.f. Sum_{n>=0} exp((1+3^n)*x)*(2*x)^n/n! */
%o A244760 {a(n)=n!*polcoeff(sum(k=0, n, exp((1+3^k)*x +x*O(x^n))*(2*x)^k/k!), n)}
%o A244760 for(n=0,25,print1(a(n),", "))
%o A244760 (PARI) /* O.g.f. Sum_{n>=0} (2*x)^n/(1 - (1+3^n)*x)^(n+1): */
%o A244760 {a(n)=polcoeff(sum(k=0, n, (2*x)^k/(1-(1+3^k)*x +x*O(x^n))^(k+1)), n)}
%o A244760 for(n=0,25,print1(a(n),", "))
