%N A244689 a(n) = Sum_{k=0..n} C(n,k) * (n + 3*2^k)^(n-k) * 2^(k^2).
%F A244689 a(n) = Sum_{k=0..n} C(n,k) * n^(n-k) * (3 + 2^k)^k.
%F A244689 a(n) ~ 2^(n^2). - _Vaclav Kotesovec_, Jul 11 2014
%e A244689 E.g.f.: A(x) = 1 + 6*x + 73*x^2/2! + 1934*x^3/3! + 157857*x^4/4! + 56192650*x^5/5! +...
%e A244689 ILLUSTRATION OF INITIAL TERMS:
%e A244689 a(1) = (1+3*2^0)^1*2^0 + (1+3*2^1)^0*2^1 = 6;
%e A244689 a(2) = (2+3*2^0)^2*2^0 + 2*(2+3*2^1)^1*2^1 + (2+3*2^2)^0*2^4 = 73;
%e A244689 a(3) = (3+3*2^0)^3*2^0 + 3*(3+3*2^1)^2*2^1 + 3*(3+3*2^2)^1*2^4 + (3+3*2^3)^0*2^9 = 1934;
%e A244689 a(4) = (4+3*2^0)^4*2^0 + 4*(4+3*2^1)^3*2^1 + 6*(4+3*2^2)^2*2^4 + 4*(4+3*2^3)^1*2^9 + (4+3*2^4)^0*2^16 = 157857; ...
%e A244689 where we have the binomial identity:
%e A244689 a(1) = 1^1*(3+2^0)^0 + 1^1*(3+2^1)^1 = 6;
%e A244689 a(2) = 2^2*(3+2^0)^0 + 2*2^1*(3+2^1)^1 + 2^0*(3+2^2)^2 = 73;
%e A244689 a(3) = 3^3*(3+2^0)^0 + 3*3^2*(3+2^1)^1 + 3*3^1*(3+2^2)^2 + 3^0*(3+2^3)^3 = 1934;
%e A244689 a(4) = 4^4*(3+2^0)^0 + 4*4^3*(3+2^1)^1 + 6*4^2*(3+2^2)^2 + 4*4^1*(3+2^3)^3 + 4^0*(3+2^4)^4 = 157857; ...
%t A244689 Table[Sum[Binomial[n,k] * (n + 3*2^k)^(n-k) * 2^(k^2),{k,0,n}],{n,0,20}] (* _Vaclav Kotesovec_, Jul 11 2014 *)
%o A244689 (PARI) {a(n) = sum(k=0,n,binomial(n,k) * (n + 3*2^k)^(n-k) * 2^(k^2) )}
%o A244689 for(n=0,15,print1(a(n),", "))
