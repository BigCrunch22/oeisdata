%t A244392 selQ[p_] := (id = IntegerDigits[p]; id2 = IntegerDigits[p + FromDigits[Reverse[id]]]; id2 == Reverse[id2]); Select[Array[Prime, 200], selQ] (* _Jean-Fran√ßois Alcover_, Jul 05 2014 *)
