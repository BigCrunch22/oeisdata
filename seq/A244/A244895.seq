%t A244895 a[ n_] := Sign[ Mod[ n, 5, -2]]; (* _Michael Somos_, Jan 08 2015 *)
%t A244895 PadRight[{},120,{0,1,1,-1,-1}] (* _Harvey P. Dale_, Nov 11 2020 *)
