%N A244751 a(n) = Sum_{k=0..n} C(n,k) * (n + 2^k)^(n-k) * 2^(k^2).
%F A244751 a(n) = Sum_{k=0..n} C(n,k) * n^(n-k) * (1 + 2^k)^k.
%e A244751 E.g.f.: A(x) = 1 + 4*x + 41*x^2/2! + 1062*x^3/3! + 98609*x^4/4! + 41449418*x^5/5! +...
%e A244751 ILLUSTRATION OF INITIAL TERMS:
%e A244751 a(1) = (1+2^0)^1*2^0 + (1+2^1)^0*2^1 = 4;
%e A244751 a(2) = (2+2^0)^2*2^0 + 2*(2+2^1)^1*2^1 + (2+2^2)^0*2^4 = 41;
%e A244751 a(3) = (3+2^0)^3*2^0 + 3*(3+2^1)^2*2^1 + 3*(3+2^2)^1*2^4 + (3+2^3)^0*2^9 = 1062;
%e A244751 a(4) = (4+2^0)^4*2^0 + 4*(4+2^1)^3*2^1 + 6*(4+2^2)^2*2^4 + 4*(4+2^3)^1*2^9 + (4+2^4)^0*2^16 = 98609; ...
%e A244751 where we have the binomial identity:
%e A244751 a(1) = 1^1*(1+2^0)^0 + 1^1*(1+2^1)^1 = 4;
%e A244751 a(2) = 2^2*(1+2^0)^0 + 2*2^1*(1+2^1)^1 + 2^0*(1+2^2)^2 = 41;
%e A244751 a(3) = 3^3*(1+2^0)^0 + 3*3^2*(1+2^1)^1 + 3*3^1*(1+2^2)^2 + 3^0*(1+2^3)^3 = 1062;
%e A244751 a(4) = 4^4*(1+2^0)^0 + 4*4^3*(1+2^1)^1 + 6*4^2*(1+2^2)^2 + 4*4^1*(1+2^3)^3 + 4^0*(1+2^4)^4 = 98609; ...
%o A244751 (PARI) {a(n) = sum(k=0,n,binomial(n,k) * (n + 2^k)^(n-k) * 2^(k^2) )}
%o A244751 for(n=0,15,print1(a(n),", "))
