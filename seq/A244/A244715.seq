%p A244715 seq(a(n), n=6..60);
%t A244715 b[n_, v_] := b[n, v] = If[n == 0, 1, Expand[Sum[b[n - i, i]*
%t A244715      If[v == 0 || v == i, 1, x], {i, n}]]];
%t A244715 a[n_] := Coefficient[b[n, 0], x, 3];
