%N A244755 a(n) = Sum_{k=0..n} C(n,k) * (1 + 3^k)^(n-k).
%H A244755 G. C. Greubel, <a href="/A244755/b244755.txt">Table of n, a(n) for n = 0..90</a>
%F A244755 E.g.f.: Sum_{n>=0} exp((1+3^n)*x) * x^n/n!.
%F A244755 O.g.f.: Sum_{n>=0} x^n/(1 - (1+3^n)*x)^(n+1).
%F A244755 a(n) ~ c * 3^(n^2/4) * 2^(n+1/2) / sqrt(Pi*n), where c = JacobiTheta3(0,1/3) = EllipticTheta[3, 0, 1/3] = 1.69145968168171534134842... if n is even, and c = JacobiTheta2(0,1/3) = EllipticTheta[2, 0, 1/3] = 1.69061120307521423305296... if n is odd. - _Vaclav Kotesovec_, Jan 25 2015
%e A244755 E.g.f.: A(x) = 1 + 3*x + 13*x^2/2! + 87*x^3/3! + 985*x^4/4! + 19563*x^5/5! +...
%e A244755 ILLUSTRATION OF INITIAL TERMS:
%e A244755 a(1) = (1+3^0)^1 + (1+3^1)^0 = 3;
%e A244755 a(2) = (1+3^0)^2 + 2*(1+3^1)^1 + (1+3^2)^0 = 13;
%e A244755 a(3) = (1+3^0)^3 + 3*(1+3^1)^2 + 3*(1+3^2)^1 + (1+3^3)^0 = 87;
%e A244755 a(4) = (1+3^0)^4 + 4*(1+3^1)^3 + 6*(1+3^2)^2 + 4*(1+3^3)^1 + (1+3^4)^0 = 985; ...
%t A244755 Table[Sum[Binomial[n,k] * (1 + 3^k)^(n-k),{k,0,n}],{n,0,20}] (* _Vaclav Kotesovec_, Jan 25 2015 *)
%o A244755 (PARI) {a(n) = sum(k=0,n,binomial(n,k) * (1 + 3^k)^(n-k) )}
%o A244755 for(n=0,25,print1(a(n),", "))
%o A244755 (PARI) /* E.g.f. Sum_{n>=0} exp((1+3^n)*x)*x^n/n!" */
%o A244755 {a(n)=n!*polcoeff(sum(k=0, n, exp((1+3^k)*x +x*O(x^n))*x^k/k!), n)}
%o A244755 for(n=0,25,print1(a(n),", "))
%o A244755 (PARI) /* O.g.f. Sum_{n>=0} x^n/(1 - (1+3^n)*x)^(n+1): */
%o A244755 {a(n)=polcoeff(sum(k=0, n, x^k/(1-(1+3^k)*x +x*O(x^n))^(k+1)), n)}
%o A244755 for(n=0,25,print1(a(n),", "))
