%N A244820 E.g.f.: Sum_{n>=0} exp(n*2^n*x) * x^n/n!.
%H A244820 Vaclav Kotesovec, <a href="/A244820/a244820.pdf">Asymptotic of sequences A244820, A244821 and A244822</a>
%F A244820 O.g.f.: Sum_{n>=0} x^n/(1 - n*2^n*x)^(n+1).
%F A244820 a(n) = Sum_{k=0..n} C(n,k) * k^(n-k) * 2^(k*(n-k)).
%e A244820 E.g.f.: A(x) = 1 + x + 5*x^2/2! + 37*x^3/3! + 513*x^4/4! + 11281*x^5/5! +...
%e A244820 where
%e A244820 A(x) = 1 + exp(2*x)*x + exp(2^2*x)^2*x^2/2! + exp(2^3*x)^3*x^3/3! + exp(2^4*x)^4*x^4/4! + exp(2^5*x)^5*x^5/5! + exp(2^6*x)^6*x^6/6! +...
%t A244820 Flatten[{1, Table[Sum[Binomial[n, k]*k^(n-k)*2^(k*(n-k)), {k, 0, n}], {n, 1, 20}]}] (* _Vaclav Kotesovec_, Jul 11 2014 *)
%o A244820 (PARI) {a(n) = sum(k=0, n, binomial(n, k) * k^(n-k) * 2^(k*(n-k)) )}
%o A244820 for(n=0, 25, print1(a(n), ", "))
%o A244820 (PARI) {a(n)=n!*polcoeff(sum(k=0, n, exp(k*2^k*x +x*O(x^n))*x^k/k!), n)}
%o A244820 for(n=0, 25, print1(a(n), ", "))
%o A244820 (PARI) {a(n)=polcoeff(sum(k=0, n, x^k/(1-k*2^k*x +x*O(x^n))^(k+1)), n)}
%o A244820 for(n=0, 25, print1(a(n), ", "))
