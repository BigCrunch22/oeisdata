%C A157896 {1, 2, 8, 32, 50, 128, 200, 242, 392, 512, 648,...}.
%F A157896 p(x,n)=Sum[x^i, {i, 0, (Prime[n] - 1)/2.2}];
%F A157896 q(n,n)=Sum[(-1)^i*x^i, {i, 0, (Prime[n] - 1)/2,2}];
%F A157896 t(x,n)=If[n == 0, 1, If[n == 1, x + 1, (x + 1)*p[x, n]*q[x, n]]];
