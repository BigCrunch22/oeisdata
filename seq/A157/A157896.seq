%t A157896 Clear[p, q, t, x, n];
%t A157896 p[x_, n_] := Sum[x^i, {i, 0, (Prime[n] - 1)/2, 2}];
%t A157896 q[x_, n_] := Sum[(-1)^i*x^i, {i, 0, (Prime[n] - 1)/2, 2}];
%t A157896 t[x_, n_] := If[n == 0, 1, If[n == 1, x + 1, (x + 1)*p[x, n]*q[x, n]]];
