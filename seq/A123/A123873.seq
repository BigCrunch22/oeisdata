%N A123873 Similar to A123872 but with a(0)=4 as seed.
%D A123873 G. Balzarotti and P. P. Lava, Le sequenze di numeri interi, Hoepli, 2008, p. 99.
%e A123873 a(0)=4;
%e A123873 a(1)=6 because 4*6 = 24;
%e A123873 a(2)=26 because 24*26 = 624;
%e A123873 a(3)=126 because 624*126 = 78624;
%e A123873 a(4)=3126 because 78624*3126 = 245778624;
%e A123873 a(5)=15625001 because 245778624*15625001 = 3840291245778624.
%p A123873 P:=proc(q,h) local a,b,k,n; a:=h; b:=ilog10(a)+1; print(h);
%p A123873 for k from 1 to 10 do for n from 2 to q do
%p A123873 if ((a*n) mod 10^b)=a then print(n); a:=a*n; b:=ilog10(a)+1;
%p A123873 break; fi; od; od; end: P(10^9,4);
