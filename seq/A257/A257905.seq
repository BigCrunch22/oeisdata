%t A257905 {a, f} = {{0}, {0}}; Do[tmp = {#, # - Last[a]} &[Min[Complement[#, Intersection[a, #]]&[Last[a] + Complement[#, Intersection[f, #]] &[Range[2 - Last[a], -1]]]]];
%t A257905 If[! IntegerQ[tmp[[1]]], tmp = {Last[a] + #, #} &[NestWhile[# + 1 &, 1, ! (! MemberQ[f, #] && ! MemberQ[a, Last[a] - #]) &]]]; AppendTo[a, tmp[[1]]]; AppendTo[f, tmp[[2]]], {120}]; {a, f} (* _Peter J. C. Moses_, May 14 2015 *)
%o A257905 (Haskell)
%o A257905 import Data.List ((\\))
%o A257905 a257905 n = a257905_list !! (n-1)
%o A257905 a257905_list = 0 : f [0] [0] where
%o A257905    f xs@(x:_) ds = g [2 - x .. -1] where
%o A257905      g [] = y : f (y:xs) (h:ds) where
%o A257905                   y = x + h
%o A257905                   (h:_) = [z | z <- [1..] \\ ds, x - z `notElem` xs]
%o A257905      g (h:hs) | h `notElem` ds && y `notElem` xs = y : f (y:xs) (h:ds)
%o A257905               | otherwise = g hs
%o A257905               where y = x + h
