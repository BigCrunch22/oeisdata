%o A367736 (Python)
%o A367736 from itertools import islice
%o A367736 from collections import Counter
%o A367736 def A367736_gen(): # generator of terms
%o A367736     c, b = {0:1}, 1
%o A367736     while True:
%o A367736         yield b
%o A367736         d = Counter(c)
%o A367736         for k in c:
%o A367736             e = c[k]
