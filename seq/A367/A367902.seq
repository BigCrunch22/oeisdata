%e A367902   {{1,2}}
%e A367902   {{1},{2}}
%e A367902   {{1},{1,2}}
%e A367902   {{2},{1,2}}
%t A367902 Table[Length[Select[Subsets[Subsets[Range[n]]], Select[Tuples[#],UnsameQ@@#&]!={}&]],{n,0,3}]
