%t A367352 b = 10; m = b - 1; a[1] = 8; a[n_] := a[n] = For[r = Mod[a[n - 1], b]; y = 0, y <= m, y++, If[y == IntegerDigits[#, b][[1]], Return[#]] &[a[n - 1] + b r + y]]; Array[a, 45] (* _Michael De Vlieger_, Nov 18 2023, after _Jean-FranÃ§ois Alcover_ at A121805 *)
%o A367352 (Python)
%o A367352 from itertools import islice
%o A367352 def agen(start=8): # generator of terms
%o A367352     an, y = start, 1
%o A367352     while y < 10:
%o A367352         yield an
%o A367352         an, y = an + 10*(an%10), 1
%o A367352         while y < 10:
%o A367352             if str(an+y)[0] == str(y):
%o A367352                 an += y
%o A367352                 break
%o A367352             y += 1
%o A367352 print(list(islice(agen(), 50))) # _Michael S. Branicky_, Nov 18 2023
