%o A367609 from sympy.ntheory.factor_ import digits
%o A367609 def a(n):
%o A367609     b = n + 3*(n%3)
%o A367609     return next((int("".join(map(str, digits(b+y-n, 3)[1:]))) for y in [1, 2] if digits(b+y, 3)[1] == y), -1)
