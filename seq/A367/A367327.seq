%p A367327 divides := (k, n) -> k = n or (k > 0 and irem(n, k) = 0):
%p A367327 A367327 := n -> local k; add(`if`(divides(n - k, n), 0, k^2), k = 0..n - 1):
%p A367327 seq(A367327(n), n = 0..61);
%t A367327 a[n_] := Sum[If[Divisible[n, n - k], 0, k^2], {k, 0, n - 1}]
