%t A367269 T[n_, k_]:= (n+1) * (n+2) / 2 + n * Mod[n,2] - 2 * k + Mod[k,2]; Table[T[n,k],{n,0,11},{k,0,n}]//Flatten (* _Stefano Spezia_, Dec 06 2023 *)
%o A367269 (PARI) T(n, k) = (n+1)*(n+2)/2+n*(n%2)-2*k+(k%2)
