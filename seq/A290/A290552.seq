%S A290552 1,1,1,1,1,1,11,1,1,1001,1111,1011,1,111,11011,11111,11101,1001,
%T A290552 10011001,10001001,10000001,11100111,10001101,1001000101,1000100111,
%U A290552 1000000111,11,111000011,111000011,1000111011001,11000010010011,1010010011,1001001001
%N A290552 Binary representation of the diagonal from the corner to the origin of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 846", based on the 5-celled von Neumann neighborhood.
%C A290552 Initialized with a single black (ON) cell at stage zero.
%D A290552 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A290552 Robert Price, <a href="/A290552/b290552.txt">Table of n, a(n) for n = 0..126</a>
%H A290552 Robert Price, <a href="/A290552/a290552.tmp.txt">Diagrams of first 20 stages</a>
%H A290552 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A290552 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A290552 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A290552 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A290552 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A290552 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A290552 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A290552 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A290552 code = 846; stages = 128;
%t A290552 rule = IntegerDigits[code, 2, 10];
%t A290552 g = 2 * stages + 1; (* Maximum size of grid *)
%t A290552 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A290552 ca = a;
%t A290552 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A290552 PrependTo[ca, a];
%t A290552 (* Trim full grid to reflect growth by one cell at each stage *)
%t A290552 k = (Length[ca[[1]]] + 1)/2;
%t A290552 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A290552 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
