%p A001272 with(numtheory); f := proc(n) local i; add((-1)^(n-i)*i!,i=1..n); end; isprime(f(15));
%t A001272 (* This program is not convenient for more than 15 terms *) Reap[For[n = 1, n <= 1000, n++, If[PrimeQ[Sum[(-1)^(n - k) * k!, {k, n}]], Print[n]; Sow[n]]]][[2, 1]] (* _Jean-François Alcover_, Sep 05 2013 *)
%t A001272 Position[AlternatingFactorial[Range[200]], _?PrimeQ] // Flatten (* _Eric W. Weisstein_, Sep 19 2017 *)
