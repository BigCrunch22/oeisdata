%p A001464 map(f, [$0..100]); # _Robert Israel_, Apr 27 2017
%p A001464 a := n -> (-1)^n*2^((n-1)/2)*KummerU((1-n)/2, 3/2, 1/2): seq(simplify(a(n)), n=0..28); # _Peter Luschny_, Apr 30 2017
