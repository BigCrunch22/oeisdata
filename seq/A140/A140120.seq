%N A140120 Primes of the form prime(x)*prime(x+1) - (prime(x+1)-prime(x)).
%e A140120 2*3 - (3-2) = 2*3 - 1 = 5;
%e A140120 3*5 - (5-3) = 3*5 - 2 = 13;
%e A140120 7*11 - (11-4) = 7*11 - 4 = 73.
%t A140120 a = {}; Do[p1 = Prime[n]; p2 = Prime[n + 1]; p = p1*p2 - (p2 - p1); If[PrimeQ[p], AppendTo[a, p]], {n, 10^2}]; a
%t A140120 Select[#[[1]]#[[2]]-(#[[2]]-#[[1]])&/@Partition[Prime[Range[250]],2,1],PrimeQ]  (* _Harvey P. Dale_, Feb 01 2011 *)
