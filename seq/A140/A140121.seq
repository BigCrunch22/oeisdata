%N A140121 Primes of the form prime(x)*prime(x+1) + (prime(x+1)-prime(x)).
%e A140121 2*3+(3-2) = 2*3+1 = 7, 23*29+(29-23) = 23*29+6 = 673.
%t A140121 a = {}; Do[p1 = Prime[n]; p2 = Prime[n + 1]; p = p1*p2 + (p2 - p1); p3 = p2 - p1; If[PrimeQ[p], AppendTo[a, p]], {n, 10^2}]; a
