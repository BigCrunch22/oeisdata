%t A318307 f[m_] := Module[{a = 1, b = 0, n = m}, While[n > 0, If[OddQ[n], b += a, a += b]; n = Floor[n/2]]; b]; Array[Times @@ Map[2^f@ # &, FactorInteger[#][[All, -1]] ] - Boole[# == 1] &, 105] (* after _Jean-FranÃ§ois Alcover_ at A002487 *)
%o A318307 (PARI)
%o A318307 A002487(n) = { my(a=1, b=0); while(n>0, if(bitand(n, 1), b+=a, a+=b); n>>=1); (b); }; \\ From A002487
%o A318307 A318307(n) = factorback(apply(e -> 2^A002487(e),factor(n)[,2]));
