%o A138107 permcount(v) = {my(m=1,s=0,k=0,t); for(i=1,#v,t=v[i]; k=if(i>1&&t==v[i-1],k+1,1); m*=t*k;s+=t); s!/m}
%o A138107 edges(v,t) = {prod(i=2, #v, prod(j=1, i-1, my(g=gcd(v[i],v[j])); t(v[i]*v[j]/g)^(2*g))) * prod(i=1, #v, t(v[i])^v[i])}
%o A138107 G(n, x)={my(s=0); forpart(p=n, s+=permcount(p)/edges(p,i->1-x^i)); s/n!}
%o A138107 T(n)={Mat(vector(n+1, k, Col(O(y*y^n) + G(k-1, y + O(y*y^n)))))}
