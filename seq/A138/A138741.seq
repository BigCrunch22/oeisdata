%o A138741 (PARI) {a(n) = my(A, p, e); if( n<0, 0, n = 2*n + 1; A = factor(n); prod(k=1, matsize(A)[1], [p, e] = A[k, ]; if( p==2, 0, p==3, 2 - (-1)^e, p%12<6, e+1, 1-e%2 )))};
