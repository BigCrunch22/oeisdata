%t A194629 b[n_, r_, k_] := b[n, r, k] = If[n < r, 0, If[r == 0, If[n == 0, 1, 0], Sum[b[n - j, k (r - j), k], {j, 0, Min[n, r]}]]];
%t A194629 a[n_] := b[5n-4, 1, 6];
