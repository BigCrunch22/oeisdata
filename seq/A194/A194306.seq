%t A194306 r = Pi;
%t A194306 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194306 g[n_, k_] := Sum[f[n, k, i], {i, 1, n}]
%t A194306 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
