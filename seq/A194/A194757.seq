%N A194757 Number of k such that {-k*Pi} > {-n*Pi}, where { } = fractional part.
%t A194757 r = -Pi; p[x_] := FractionalPart[x];
%t A194757 u[n_, k_] := If[p[k*r] <= p[n*r], 1, 0]
%t A194757 v[n_, k_] := If[p[k*r] > p[n*r], 1, 0]
%t A194757 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194757 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194757 Table[s[n], {n, 1, 100}]   (* A194756 *)
%t A194757 Table[t[n], {n, 1, 100}]   (* A194757 *)
