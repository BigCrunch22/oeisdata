%e A194341 0..1..2..1..1..1..1..1
%e A194341 0..1..2..0..1..2..1..0..2..1
%t A194341 r = 3-E;
%t A194341 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194341 g[n_, k_] := Sum[f[n, k, i], {i, 1, n}]
%t A194341 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
