%t A194331 r = 2-Sqrt[3];
%t A194331 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194331 g[n_, k_] := Sum[f[n, k, i], {i, 1, n^2}]
%t A194331 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
