%N A194817 Number of integers k in [1,n] such that {n*r+k*r} < {n*r-k*r}, where { } = fractional part and r=sqrt(3).
%t A194817 r = Sqrt[3]; p[x_] := FractionalPart[x];
%t A194817 u[n_, k_] := If[p[n*r + k*r] <= p[n*r - k*r], 1, 0]
%t A194817 v[n_, k_] := If[p[n*r + k*r] > p[n*r - k*r], 1, 0]
%t A194817 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194817 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194817 Table[s[n], {n, 1, 100}]   (* A194817 *)
%t A194817 Table[t[n], {n, 1, 100}]   (* A194818 *)
