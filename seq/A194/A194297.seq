%t A194297 r = (1+Sqrt[3])/2;
%t A194297 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194297 g[n_, k_] := Sum[f[n, k, i], {i, 1, n}]
%t A194297 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
