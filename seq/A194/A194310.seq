%e A194310 2..2..2..3..2..2..2..1
%e A194310 2..3..1..2..2..2..3..1..2
%t A194310 r = E;
%t A194310 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194310 g[n_, k_] := Sum[f[n, k, i], {i, 1, 2n}]
%t A194310 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
