%N A194816 Number of integers k in [1,n] such that {n*r+k*r} > {n*r-k*r}, where { } = fractional part and r=sqrt(2).
%t A194816 r = Sqrt[2]; p[x_] := FractionalPart[x];
%t A194816 u[n_, k_] := If[p[n*r + k*r] <= p[n*r - k*r], 1, 0]
%t A194816 v[n_, k_] := If[p[n*r + k*r] > p[n*r - k*r], 1, 0]
%t A194816 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194816 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194816 Table[s[n], {n, 1, 100}]   (* A194815 *)
%t A194816 Table[t[n], {n, 1, 100}]   (* A194816 *)
%Y A194816 Cf. A194815, A194813.
%K A194816 nonn
