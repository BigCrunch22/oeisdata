%N A194746 Number of k such that {k*sqrt(6)} < {n*sqrt(6)}, where { } = fractional part.
%t A194746 r = Sqrt[6]; p[x_] := FractionalPart[x];
%t A194746 u[n_, k_] := If[p[k*r] <= p[n*r], 1, 0]
%t A194746 v[n_, k_] := If[p[k*r] > p[n*r], 1, 0]
%t A194746 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194746 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194746 Table[s[n], {n, 1, 100}]   (* A194746 *)
%t A194746 Table[t[n], {n, 1, 100}]   (* A194747 *)
