%N A194742 Number of k such that {k*sqrt(5)} < {n*sqrt(5)}, where { } = fractional part.
%t A194742 r = Sqrt[5]; p[x_] := FractionalPart[x];
%t A194742 u[n_, k_] := If[p[k*r] <= p[n*r], 1, 0]
%t A194742 v[n_, k_] := If[p[k*r] > p[n*r], 1, 0]
%t A194742 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194742 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194742 Table[s[n], {n, 1, 100}]   (* A194742 *)
%t A194742 Table[t[n], {n, 1, 100}]   (* A194743 *)
