%N A194758 Number of k such that {k*log(2)} < {n*log(2)}, where { } = fractional part.
%t A194758 r = Log[2]; p[x_] := FractionalPart[x];
%t A194758 u[n_, k_] := If[p[k*r] <= p[n*r], 1, 0]
%t A194758 v[n_, k_] := If[p[k*r] > p[n*r], 1, 0]
%t A194758 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194758 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194758 Table[s[n], {n, 1, 100}]   (* A194758 *)
%t A194758 Table[t[n], {n, 1, 100}]   (* A194759 *)
