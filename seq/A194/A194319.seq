%e A194319 8...9...10..7...10..10..8...10...9
%t A194319 r = Sqrt[8];
%t A194319 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194319 g[n_, k_] := Sum[f[n, k, i], {i, 1, n^2}]
%t A194319 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
