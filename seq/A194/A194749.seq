%N A194749 Number of k such that {-k*sqrt(6)} > {-n*sqrt(6)}, where { } = fractional part.
%t A194749 r = -Sqrt[6]; p[x_] := FractionalPart[x];
%t A194749 u[n_, k_] := If[p[k*r] <= p[n*r], 1, 0]
%t A194749 v[n_, k_] := If[p[k*r] > p[n*r], 1, 0]
%t A194749 s[n_] := Sum[u[n, k], {k, 1, n}]
%t A194749 t[n_] := Sum[v[n, k], {k, 1, n}]
%t A194749 Table[s[n], {n, 1, 100}]   (* A194748 *)
%t A194749 Table[t[n], {n, 1, 100}]   (* A194749 *)
