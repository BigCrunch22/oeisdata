%p A023163 fpp:= n -> mpow(n-1,n)[2,2]:
%p A023163 M:= <<0,1>|<1,1>>:
%p A023163 mpow:= proc(n,p)
%p A023163   if n = 0 then <<1,0>|<0,1>>
