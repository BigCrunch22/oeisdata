%t A341525 f[p_, e_] := (p^(e + 1) - 1)/((p - 1)*p^e); g[p_, e_] := f[NextPrime[p], e]; a[1] = 1; a[n_] := Numerator[Times @@ g @@@ FactorInteger[n]]; Array[a, 100] (* _Amiram Eldar_, Feb 17 2021 *)
