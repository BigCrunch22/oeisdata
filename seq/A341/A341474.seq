%e A341474 - so a(0) = 0,
%e A341474      a(1) = a(2) = a(3) = a(4) = a(6) = a(7) = a(11) = 1,
%e A341474      a(5) = a(10) = a(18) = 2,
%e A341474      a(9) = a(17) = 3,
%e A341474      a(8) = a(12) = a(14) = 4.
