%t A351429 T[n_, 0] := (-1)^n*n!; T[n_, k_] := T[n, k] = Sum[StirlingS2[n, j]*T[j, k - 1], {j, 0, n}]; Table[T[k, n - k], {n, 0, 10}, {k, 0, n}] // Flatten (* _Amiram Eldar_, Feb 11 2022 *)
