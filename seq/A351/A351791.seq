%e A351791    24, -4,  40,  132,   248,   364, ...
%e A351791   120, 25, 120, -375, -2120, -5655, ...
%t A351791 T[n_, k_] := n!*(1 + Sum[(-k*(n - j))^j/j!, {j, 1, n}]); Table[T[k, n - k], {n, 0, 10}, {k, 0, n}] // Flatten (* _Amiram Eldar_, Feb 19 2022 *)
%o A351791 (PARI) T(n, k) = n!*sum(j=0, n, (-k*(n-j))^j/j!);
%o A351791 (PARI) T(n, k) = if(n==0, 1, n*sum(j=0, n-1, (-k)^(n-1-j)*binomial(n-1, j)*T(j, k)));
