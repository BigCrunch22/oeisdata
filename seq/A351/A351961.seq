%o A351961 (PARI)
%o A351961 up_to = 104; \\ 10439 = binomial(144+1,2)-1
%o A351961 A005940(n) = { my(p=2, t=1); n--; until(!n\=2, if((n%2), (t*=p), p=nextprime(p+1))); (t); };
%o A351961 A156552(n) = { my(f = factor(n), p, p2 = 1, res = 0); for(i = 1, #f~, p = 1 << (primepi(f[i, 1]) - 1); res += (p * p2 * (2^(f[i, 2]) - 1)); p2 <<= f[i, 2]); res };
%o A351961 A351961sq(n,k) = A156552(gcd(A005940(1+n),A005940(1+k)));
%o A351961 A351961list(up_to) = { my(v = vector(1+up_to), i=0); for(a=0,oo, for(col=0,a, i++; if(i > #v, return(v)); v[i] = A351961sq(col,(a-(col))))); (v); };
%o A351961 v351961 = A351961list(up_to);
%o A351961 A351961(n) = v351961[1+n];
