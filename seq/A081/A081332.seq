%t A081332 nmax = 1000;
%t A081332 S = Select[Range[nmax], Max[FactorInteger[#][[All, 1]]] <= 3 &];
%t A081332 P[n_] := IntegerPartitions[n, {2}, TakeWhile[S, # < n &]];
%t A081332 selQ[n_] := Length[P[n]] == 1;
