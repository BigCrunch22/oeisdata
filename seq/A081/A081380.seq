%t A081380 ffi[x_] := Flatten[FactorInteger[x]]; lf[x_] := Length[FactorInteger[x]]; ba[x_] := Table[Part[ffi[x], 2*w-1], {w, 1, lf[x]}]; Do[s=ba[DivisorSigma[1, n]]; s5=ba[DivisorSigma[2, n]]; If[Equal[s, s5], Print[n]], {n, 1, 1000000}]
