%t A205337 a[n_] := a[n] = If[n == 0, 1, Sum[Sum[Binomial[i, l] Sum[(-1)^j Binomial[i - l, j] Binomial[-l + 4(-l - 2j + i) - j + i - 1, 4(-l - 2j + i) - j], {j, 0, (4(i - l))/9}] (-1)^l, {l, 0, i}] a[n - i], {i, 1, n}]/n];
