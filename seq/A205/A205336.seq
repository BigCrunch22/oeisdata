%t A205336 a[n_] := a[n] = If[n == 0, 1, Sum[(Sum[Binomial[i, l] (Sum[(-1)^j Binomial[i - l, j] Binomial[-l + 3(-l - 2j + i) - j + i - 1, 3(-l - 2j + i) - j], {j, 0, (3(i - l))/7}]) (-1)^l, {l, 0, i}]) a[n - i], {i, 1, n}]/n];
