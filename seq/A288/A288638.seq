%p A288638 b:= proc(n, k, s) option remember;
%p A288638       `if`(n=0, `if`(s={}, 0, 1), add(b(n-1, k, select(y->
%p A288638        y<=(n-1)*k, map(x-> [abs(x-i), x+i][], s))), i=0..k))
%p A288638     end:
%p A288638 A:= (n, k)-> b(n, k, {0}):
%p A288638 seq(seq(A(n, d-n), n=0..d), d=0..12);
%t A288638 b[n_, k_, s_] := b[n, k, s] = If[n == 0, If[s == {}, 0, 1], Sum[b[n-1, k, Select[Flatten[{Abs[#-i], #+i}& /@ s], # <= (n-1)*k&]], {i, 0, k}]];
%t A288638 A[n_, k_] := b[n, k, {0}];
