%t A162895 oppQ[n_]:=Module[{r1=100n+1,r2=100n+100,prs},prs=Select[Range[r1,r2],PrimeQ];Length[prs] == 2&&Sort[IntegerDigits[prs[[1]]]]==Sort[IntegerDigits[prs[[2]]]]]; Select[Range[170000],oppQ] (* _Harvey P. Dale_, Jul 18 2023 *)
