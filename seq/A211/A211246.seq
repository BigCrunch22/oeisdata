%t A211246 c[m_, i_] := Binomial[m-1-i, i] + 4 Binomial[m-i, i-1];
%t A211246 d[m_, i_] := Binomial[m-i, i] m/(m -i);
%t A211246 a[m_, i_] := If[EvenQ[m], c[m, i], d[m, i]];
