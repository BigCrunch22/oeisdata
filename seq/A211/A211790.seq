%t A211790 Table[t[3, n], {n, 1, z}]  (* A211650 *)
%t A211790 TableForm[Table[t[k, n], {k, 1, 12}, {n, 1, 16}]]
%t A211790 Flatten[Table[t[k, n - k + 1], {n, 1, 12}, {k, 1, n}]] (* A211790 *)
