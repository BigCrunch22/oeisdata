%t A208884 a[1]=1; a[n_] := a[n] = #/2^IntegerExponent[#, 2] &@ (n + a[n-1]); Array[a, 70] (* _Giovanni Resta_, Jun 25 2020 *)
%o A208884 (PARI) {a(n)=if(n==1, 1, (a(n-1)+n)/2^valuation(a(n-1)+n,2))}
%o A208884 (PARI) {A=vector(1024); a(n)=A[n]=if(n==1, 1, (A[n-1]+n)/2^valuation(A[n-1]+n,2))}
