%t A293171 T[n_, k_] := T[n, k] = Module[{b=1, e=2, c=4}, Which[k<0 || k>n, 0, k==n, 1, k == 0, b*T[n-1, 0] + 2*c*T[n-1, 1], True, T[n-1, k-1] + b*T[n-1, k] + c*T[n-1, k+1]]];
