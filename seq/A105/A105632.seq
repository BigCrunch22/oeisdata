%p A105632 A105632 := proc(n,k)
%p A105632     (1-x-sqrt((1-x)^2-4*x^2/(1-x*y)))/2/x^2 ;
%p A105632     coeftayl(%,x=0,n) ;
%p A105632     coeftayl(%,y=0,k) ;
