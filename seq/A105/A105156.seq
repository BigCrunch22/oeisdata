%N A105156 The sum of a triangular array made from a negative 6-fold permutation product.
%e A105156 Triangle begins:
%e A105156    10;
%e A105156     6,   9;
%e A105156    -4,  -6, -10;
%e A105156   -14, -21, -35, -49;
%e A105156    22,  33,  55,  77, 121;
%e A105156    26,  39,  65,  91, 143, 169;
%e A105156   ...
%t A105156 a[n_] := -Prime[n] /; 10 - Mod[Prime[n], 10] == 1;
%t A105156 a[n_] := -Prime[n] /; 10 - Mod[Prime[n], 10] == 3;
%t A105156 a[n_] := Prime[ n] /; (10 - Mod[Prime[n], 10] == 7) || (10 - Mod[Prime[n], 10] == 9);
%t A105156 a[1] = 5; a[3] = -2;
%t A105156 digits = 200
%t A105156 aa = Table[Abs[Apply[Plus, Table[Prime[n]*a[m], {n, 1.m}]]], {m, 1, digits}]
