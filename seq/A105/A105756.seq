%t A105756 a = {1, 0, 0, 0, 0}; lst = {}; Do[s = Plus @@ a; a = RotateLeft[a];
%t A105756   a[[-1]] = s; If[PrimeQ[s], AppendTo[lst, n]], {n, 44000}]; lst
