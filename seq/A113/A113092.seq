%o A113092 (PARI) /* Generalized Cook-Kleber Recurrence */
%o A113092 {T(n,k,q=4)=if(n==0,1,if(n<0||k<=0,0,if(n==1,k, if(n>=k,sum(j=1,k,T(n-1,k+(q-1)*j)), sum(j=1,n+1,(-1)^(j-1)*binomial(n+1,j)*T(n,k-j))))))}
%o A113092 for(n=0,10,for(k=0,10,print1(T(n,k),", "));print(""))
%o A113092 (PARI) /* Matrix Power Recurrence (Paul D. Hanna) */
%o A113092 {T(n,k,q=4)=local(M=matrix(n+1,n+1));for(r=1,n+1, for(c=1,r, M[r,c]=if(r==c,1,if(c>1,(M^q)[r-1,c-1])+(M^q)[r-1,c]))); return((M^k)[n+1,1])}
%o A113092 for(n=0,10,for(k=0,10,print1(T(n,k),", "));print(""))
