%p A113243             elif A000078(j) - A000078(i) < n then
%p A113243                 break ;
%p A113243             end if;
%p A113243         end do:
%p A113243         if A000078(j) - A000078(j-1) > n then
%p A113243             return false;
%p A113243         end if;
%p A113243     end do:
%p A113243 end proc:
%p A113243 for n from 0 to 10000 do
