%e A319145 RELATED SERIES.
%e A319145 cn(A(x,m) + x, m) = 1 - 4*x^2/2! - 24*x^3/3! + (64*m - 224)*x^4/4! + (1920*m - 2880)*x^5/5! + (-1024*m^2 + 50944*m - 47104)*x^6/6! + (-129024*m^2 + 1405824*m - 948864)*x^7/7! + (16384*m^3 - 8798208*m^2 + 42037248*m - 22820864)*x^8/8! + (8355840*m^3 - 491212800*m^2 + 1381570560*m - 638699520)*x^9/9! + (-262144*m^4 + 1367932928*m^3 - 25781010432*m^2 + 50035417088*m - 20383842304)*x^10 + ...
%e A319145 sn(A(x,m) - x, m) = 1 - cn(A(x,m) + x, m) = 4*x^2/2! + 24*x^3/3! + (-64*m + 224)*x^4/4! + (-1920*m + 2880)*x^5/5! + (1024*m^2 - 50944*m + 47104)*x^6/6! + (129024*m^2 - 1405824*m + 948864)*x^7/7! + ...
%e A319145 Related Jacobi elliptic functions with parameter m begin:
%e A319145 sn(x,m) = x + (-m - 1)*x^3/3! + (m^2 + 14*m + 1)*x^5/5! + (-m^3 - 135*m^2 - 135*m - 1)*x^7/7! + (m^4 + 1228*m^3 + 5478*m^2 + 1228*m + 1)*x^9/9! + (-m^5 - 11069*m^4 - 165826*m^3 - 165826*m^2 - 11069*m - 1)*x^11/11! + ...
%e A319145 cn(x,m) = 1 - x^2/2! + (4*m + 1)*x^4/4! + (-16*m^2 - 44*m - 1)*x^6/6! + (64*m^3 + 912*m^2 + 408*m + 1)*x^8/8! + (-256*m^4 - 15808*m^3 - 30768*m^2 - 3688*m - 1)*x^10/10! + (1024*m^5 + 259328*m^4 + 1538560*m^3 + 870640*m^2 + 33212*m + 1)*x^12/12! + ...
%e A319145 dn(x,m) = 1 - m*x^2/2! + (m^2 + 4*m)*x^4/4! + (-m^3 - 44*m^2 - 16*m)*x^6/6! + (m^4 + 408*m^3 + 912*m^2 + 64*m)*x^8/8! + (-m^5 - 3688*m^4 - 30768*m^3 - 15808*m^2 - 256*m)*x^10/10! + (m^6 + 33212*m^5 + 870640*m^4 + 1538560*m^3 + 259328*m^2 + 1024*m)*x^12/12! + ...
%o A319145 (PARI) {T(n,k) = my(A=[1],S=x,C=1,D=1); for(i=0, n,
%o A319145 S = intformal(C*D +x*O(x^n));
%o A319145 C = 1 - intformal(S*D) ;
%o A319145 D = 1 - m*intformal(S*C); );
%o A319145 for(i=1,n, A=concat(A,0);
%o A319145 A[#A] = -Vec( subst(C,x,x*Ser(A) + x) + subst(S,x,x*Ser(A) - x) )[#A+1] );
%o A319145 n!*polcoeff(polcoeff(A,n,x),k,m)}
%o A319145 /* Print as a triangle: */
%o A319145 for(n=1,12,for(k=0,(n-1)\2,print1(T(n,k),", "));print(""))
