%o A319699 (PARI)
%o A319699 A001065(n) = (sigma(n)-n);
%o A319699 A064989(n) = {my(f); f = factor(n); if((n>1 && f[1,1]==2), f[1,2] = 0); for (i=1, #f~, f[i,1] = precprime(f[i,1]-1)); factorback(f)};
