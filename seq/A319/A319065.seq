%o A319065 (PARI) printrow(n, terms) = my(c=0); for(b=2, oo, my(j=0); for(i=0, 7, my(p=prime(n+i)); if(Mod(b, p^2)^(p-1)==1, j++)); if(j==8, print1(b, ", "); c++); if(c==terms, break))
%o A319065 array(rows, cols) = for(x=1, rows, printrow(x, cols); print(""))
