%t A319059 rows = 10; t = 1;
%t A319059 T = Table[lst = {}; b = 2;
%t A319059    While[Length[lst] < rows,
%t A319059     p = Prime[n + Range[0, t]];
%t A319059     If[AllTrue[PowerMod[b, (p - 1), p^2], # == 1 &], AppendTo[lst, b]]; b++];
