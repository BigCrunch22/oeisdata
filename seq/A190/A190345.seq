%S A190345 1,3,5,7,10,11,13,16,17,20,21,23,26,27,30,32,33,36,38,40,42,43,46,48,
%T A190345 50,52,54,56,58,60,62,64,66,68,71,72,74,77,78,81,82,84,87,88,91,93,94,
%U A190345 97,98,101,103,104,107,109,111,113,115,117,119,121,123,125,127,129,132,133,135,138,139,142,143,145,148,149,152
%N A190345 a(n) = n + [n*r/s] + [n*t/s]; r=1, s=sqrt(5/2), t=sqrt(2/5).
%C A190345 See A190344.
%H A190345 G. C. Greubel, <a href="/A190345/b190345.txt">Table of n, a(n) for n = 1..10000</a>
%F A190345 A190344:  f(n) = n + [n*sqrt(5/2)] + [n*sqrt(2/5)].
%F A190345 A190345:  g(n) = n + [n*sqrt(2/5)] + [2*n/5].
%F A190345 A190346:  h(n) = 3*n + [n*sqrt(5/2)] + [n/2].
%t A190345 r=1; s=(5/2)^(1/2); t=1/s;
%t A190345 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A190345 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A190345 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A190345 Table[f[n], {n, 1, 120}]  (* A190344 *)
%t A190345 Table[g[n], {n, 1, 120}]  (* A190345 *)
%t A190345 Table[h[n], {n, 1, 120}]  (* A190346 *)
%o A190345 (PARI) for(n=1,100, print1(n + floor(n*sqrt(2/5)) + floor(2*n/5), ", ")) \\ _G. C. Greubel_, Apr 05 2018
%o A190345 (Magma) [n + Floor(n*Sqrt(2/5)) + Floor(2*n/5): n in [1..100]]; // _G. C. Greubel_, Apr 05 2018
