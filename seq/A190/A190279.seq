%N A190279 a(n) = n + [nr/s] + [nt/s];  r=1/2, s=sinh(1), t=cosh(1).
%C A190279 See A190265.
%H A190279 G. C. Greubel, <a href="/A190279/b190279.txt">Table of n, a(n) for n = 1..10000</a>
%F A190279 A190265:  a(n)=n+[2n*sinh(1)]+[2n*cosh(1)].
%F A190279 A190279:  b(n)=n+[(n/2)*csch(1)]+[n*coth(1)].
%F A190279 A190280:  c(n)=n+[(n/2)*sech(1)]+[n*tanh(1)].
%t A190279 r=1/2; s=Sinh[1]; t=Cosh[1];
%t A190279 a[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A190279 b[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A190279 c[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A190279 Table[a[n], {n, 1, 120}]  (*A190265*)
%t A190279 Table[b[n], {n, 1, 120}]  (*A190279*)
%t A190279 Table[c[n], {n, 1, 120}]  (*A190280*)
%Y A190279 Cf. A190265, A190280.
