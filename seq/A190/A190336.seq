%T A190336 72,75,78,81,84,87,89,93,95,99,101,104,107,110,113,116,118,122,124,
%U A190336 128,130,134,136,139,142,145,148,151,153,157,159,163,165,169,171,174,177,180,183,186,188,192,194,198,200,203,206,209,212,215
%N A190336 a(n) = n + [n*r/s] + [n*t/s];  r=2, s=sqrt(2), t=1/s.
%C A190336 See A190335.
%H A190336 G. C. Greubel, <a href="/A190336/b190336.txt">Table of n, a(n) for n = 1..10000</a>
%F A190336 A190335:  f(n) = n + [n*sqrt(2)] + [n/sqrt(8)].
%F A190336 A190336:  g(n) = n + [n/sqrt(2)] + [n/2].
%F A190336 A190337:  h(n) = 3*n + [n*sqrt(8)].
%t A190336 r=2; s=2^(1/2); t=1/s;
%t A190336 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A190336 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A190336 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A190336 Table[f[n], {n, 1, 120}]  (*A190335*)
%t A190336 Table[g[n], {n, 1, 120}]  (*A190336*)
%t A190336 Table[h[n], {n, 1, 120}]  (*A190337*)
%o A190336 (PARI) for(n=1,100, print1(n + floor(n/sqrt(2)) + floor(n/2), ", ")) \\ _G. C. Greubel_, Apr 04 2018
%o A190336 (Magma) R:=RealField(); [n + Floor(n/Sqrt(2)) + Floor(n/2): n in [1..100]]; // _G. C. Greubel_, Apr 04 2018
