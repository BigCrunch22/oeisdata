%U A190086 196,201,205,210,215,220,225,231,235,240,245,250,255,260,264,269,274,279,284,289,294,299,304,309,314,318,323,328,333,338,344,348
%N A190086 a(n) = n + [n*r/s] + [n*t/s];  r=1, s=sin(1/2), t=cos(1/2).
%C A190086 See A190085.
%H A190086 G. C. Greubel, <a href="/A190086/b190086.txt">Table of n, a(n) for n = 1..10000</a>
%F A190086 A190085:  f(n) = n + [n*sin(1/2)] + [n*cos(1/2)].
%F A190086 A190086:  g(n) = n + [n*csc(1/2)] + [n*cot(1/2)], this sequence.
%F A190086 A190087:  h(n) = n + [n*sec(1/2)] + [n*tan(1/2)].
%t A190086 r=1; s=Sin[1/2]; t=Cos[1/2];
%t A190086 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A190086 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A190086 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A190086 Table[f[n], {n, 1, 120}]  (* A190085 *)
%t A190086 Table[g[n], {n, 1, 120}]  (* A190086 *)
%t A190086 Table[h[n], {n, 1, 120}]  (* A190087 *)
%o A190086 (PARI) for(n=1,100, print1(n + floor(n/sin(1/2)) + floor(n/tan(1/2)), ", ")) \\ _G. C. Greubel_, Mar 04 2018
%o A190086 (Magma) [n + Floor(n/Sin(1/2)) + Floor(n/Tan(1/2)): n in [1..100]]; // _G. C. Greubel_, Mar 04 2018
%Y A190086 Cf. A190085, A190087.
%K A190086 nonn
