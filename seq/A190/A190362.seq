%S A190362 1,4,7,10,13,15,18,21,24,26,28,31,34,37,40,42,45,48,50,53,55,58,61,64,
%T A190362 67,69,72,75,77,80,82,85,88,91,94,96,99,101,104,107,109,112,115,118,
%U A190362 121,123,126,128,131,134,136,139,142,145,148,150,152,155,158,161,163,166,169,172,175,177,179,182,185,188,190,193,196,199,202
%N A190362 a(n) = n + [n*r/s] + [n*t/s];  r=1, s=sqrt(5/4), t=sqrt(4/5).
%C A190362 See A190361.
%H A190362 G. C. Greubel, <a href="/A190362/b190362.txt">Table of n, a(n) for n = 1..10000</a>
%F A190362 A190361:  f(n) = n + [n*sqrt(5/4)] + [n*sqrt(4/5)].
%F A190362 A190362:  g(n) = n + [n*sqrt(4/5)] + [4*n/5].
%F A190362 A190363:  h(n) = 2*n + [n*sqrt(5/4)] + [n/4].
%t A190362 r=1; s=(5/4)^(1/2); t=1/s;
%t A190362 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A190362 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A190362 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A190362 Table[f[n], {n, 1, 120}]  (* A190361 *)
%t A190362 Table[g[n], {n, 1, 120}]  (* A190362 *)
%t A190362 Table[h[n], {n, 1, 120}]  (* A190363 *)
%o A190362 (PARI) for(n=1,100, print1(n + floor(n*sqrt(4/5)) + floor(4*n/5), ", ")) \\ _G. C. Greubel_, Apr 05 2018
%o A190362 (Magma) [n + Floor(n*Sqrt(4/5)) + Floor(4*n/5): n in [1..100]]; // _G. C. Greubel_, Apr 05 2018
