%U A190348 108,111,113,115,118,120,123,125,127,130,132,135,136,139,142,144,147,148,151,154,156,158,160,163,166,167,170,172,175
%N A190348 a(n) = n + [n*r/s] + [n*t/s]; r=1, s=sqrt(5/3), t=sqrt(3/5).
%C A190348 See A190347.
%H A190348 G. C. Greubel, <a href="/A190348/b190348.txt">Table of n, a(n) for n = 1..10000</a>
%F A190348 A190347:  f(n) = n + [n*sqrt(5/3)] + [n*sqrt(3/5)].
%F A190348 A190348:  g(n) = n + [n*sqrt(3/5)] + [3*n/5].
%F A190348 A190349:  h(n) = 2*n + [n*sqrt(5/3)] + [n/3].
%t A190348 r=1; s=(5/3)^(1/2); t=1/s;
%t A190348 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A190348 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A190348 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A190348 Table[f[n], {n, 1, 120}]  (* A190347 *)
%t A190348 Table[g[n], {n, 1, 120}]  (* A190348 *)
%t A190348 Table[h[n], {n, 1, 120}]  (* A190349 *)
%o A190348 (PARI) for(n=1,100, print1(n + floor(n*sqrt(3/5)) + floor(3*n/5), ", ")) \\ _G. C. Greubel_, Apr 05 2018
%o A190348 (Magma) [n + Floor(n*Sqrt(3/5)) + Floor(3*n/5): n in [1..100]]; // _G. C. Greubel_, Apr 05 2018
