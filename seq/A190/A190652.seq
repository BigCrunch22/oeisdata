%o A190652 from datetime import date
%o A190652 def ok(n): return sum(date.isoweekday(date(n, m, 13)) == 5 for m in range(1, 13)) == 2
%o A190652 print(list(filter(ok, range(1901, 2000)))) # _Michael S. Branicky_, Sep 12 2021
