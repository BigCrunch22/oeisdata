%e A255283 145;
%e A255283 601;
%e A255283 ............................................
%e A255283 7,     49,   49,  217,  49,  343, 217, 1015;
%e A255283 31,   217,  217,  961;
%e A255283 145, 1015;
%e A255283 601;
%e A255283 2551;
%e A255283 .......................................................................................
%e A255283 7,     49,   49,  217,  49,  343, 217, 1015, 49, 343, 343, 1519, 217, 1519, 1015, 4207;
%e A255283 31,   217,  217,  961, 217, 1519, 961, 4495;
