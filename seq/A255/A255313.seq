%t A255313 A[n_, 1] := n;
%t A255313 A[n_, k_] := A[n, k] = For[m = n-1, m >= 1, m--, If[PrimeQ[m + A[n, k-1]] && FreeQ[Table[A[n, j], {j, 1, k-1}], m], Return[m]]];
