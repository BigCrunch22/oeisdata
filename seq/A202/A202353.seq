%t A202353 Plot[{f[x], g[x]}, {x, -1, 1}, {AxesOrigin -> {0, 0}}]
%t A202353 r = x /. FindRoot[f[x] == g[x], {x, -.4, -.3}, WorkingPrecision -> 110]
%t A202353 RealDigits[r] (* A202353 *)
