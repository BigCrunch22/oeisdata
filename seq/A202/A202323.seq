%e A202323 x=-0.7920599684306770014183958778854220...
%t A202323 u = 3; v = 0;
%t A202323 f[x_] := u*x + v; g[x_] := E^x
%t A202323 Plot[{f[x], g[x]}, {x, -1, 2}, {AxesOrigin -> {0, 0}}]
