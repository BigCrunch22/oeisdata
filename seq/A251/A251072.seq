%p A251072 b:= proc(n, l) option remember; local d, k; d:= nops(l)/3;
%p A251072       if n=0 then 1
%p A251072     elif min(l[])>0 then (m->b(n-m, map(x->x-m, l)))(min(l[]))
%p A251072     else for k while l[k]>0 do od;
%p A251072          `if`(n<d, 0, b(n, subsop(k=d, l)))+
%p A251072          `if`(d=1 or k>2*d+1 or max(l[k..k+d-1][])>0, 0,
%p A251072           b(n, [l[1..k-1][], 1$d, l[k+d..3*d][]]))
%p A251072       fi
%p A251072     end:
%p A251072 A:= (n, k)-> `if`(k=0, 1, b(n, [0$3*k])):
%p A251072 seq(seq(A(n, d-n), n=0..d), d=0..12);
%t A251072 b[n_, l_List] := b[n, l] = Module[{d = Length[l]/3, k}, Which[n == 0, 1,  Min[l] > 0, Function[{m}, b[n-m, l-m]][Min[l]], True, For[k=1, l[[k]] > 0 , k++]; If[n<d, 0, b[n, ReplacePart[l, k -> d]]] + If[d == 1 || k > 2d + 1 || Max[l[[k ;; k + d - 1]]] > 0,  0,  b[n, Join[l[[1 ;; k-1]], Array[1&, d],  l[[k+d ;; 3*d]]]]]]]; A[n_, k_] := If[k == 0, 1, b[n, Array[0&, 3k]]]; Table[Table[A[n, d-n], {n, 0, d}], {d, 0, 12}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 30 2015, after _Alois P. Heinz_ *)
