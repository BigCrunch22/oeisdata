%o A192540 (PARI) {a(n)=local(A=x+x^2);for(i=1,n,A=x*prod(m=1,n\2,(1 + A^(2*m-1))/(1 - A^(2*m)+x*O(x^n))));polcoeff(A,n)}
%o A192540 (PARI) {a(n)=local(A=x+x^2);for(i=1,n,A=x*prod(m=1,n,(1 - A^(4*m-2))/(1 - A^m+x*O(x^n))));polcoeff(A,n)}
