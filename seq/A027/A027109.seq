%N A027109 a(n) = Sum_{k=0..n} T(n,k) * T(n,n+k), with T given by A027082.
%t A027109 T[_, 0] = T[_, 1] = T[_, 2] = 1;
%t A027109 T[n_, k_] := T[n, k] = If[k < 2n, T[n-1, k-3] + T[n-1, k-2] + T[n-1, k-1], T[n-1, 2n-3] + T[n-1, 2n-2]];
%t A027109 a[n_] := Sum[T[n, k] T[n, n+k], {k, 0, n}];
