%N A153738 Adjusted recursive triangular sequence with row sums 2*(n+5)!/6!: A(n,k)= A(n - 1, k - 1) + A(n - 1, k) + (n + 4)*(n + 3)*A(n - 2, k - 1).
%C A153738 Row sums are:
%C A153738 {2, 14, 112, 1008, 10080, 110880, 1330560, 17297280, 242161920, 3632428800,...}.
%C A153738 The division by 6! and changing the first element gives a nicer looking result.
%F A153738 A(n,k)= A(n - 1, k - 1) + A(n - 1, k) + (n + 4)*(n + 3)*A(n - 2, k - 1).
%e A153738 {2},
%e A153738 {7, 7},
%e A153738 {1, 110, 1},
%e A153738 {1, 503, 503, 1},
%e A153738 {1, 576, 8926, 576, 1},
%e A153738 {1, 667, 54772, 54772, 667, 1},
%e A153738 {1, 778, 118799, 1091404, 118799, 778, 1},
%e A153738 {1, 911, 207621, 8440107, 8440107, 207621, 911, 1},
%e A153738 {1, 1068, 329900, 27180372, 187139238, 27180372, 329900, 1068, 1},
%e A153738 {1, 1251, 496770, 65297294, 1750419084, 1750419084, 65297294, 496770, 1251, 1}
%t A153738 Clear[A] A[1, 1] = 2*6!/720; A[2, 1] := A[2, 2] = 7!/720;
%t A153738 A[3, 2] = (2*8! - 2*6!)/720; A[4, 2] = A[4, 3] = ( 9! - 6!)/720;
%t A153738 A[n_, 1] := 6!/720; A[n_, n_] := 6!/720;
%t A153738 A[n_, k_] := A[n - 1, k - 1] + A[n - 1, k] + (n + 4)*(n + 3)*A[n - 2, k - 1];
%t A153738 a = Table[A[n, k], {n, 10}, {k, n}];
%t A153738 Flatten[a]
%t A153738 Table[Apply[Plus, a[[n]]], {n, 1, 10}];
%t A153738 Table[Apply[Plus, 720*a[[n]]]/(2*(n + 5)!), {n, 1, 10}];
