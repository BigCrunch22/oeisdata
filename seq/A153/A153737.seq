%N A153737 A recursive triangular sequence with row sums (2*(n + 5)!): A(n,k)=A(n - 1, k - 1) + A(n - 1, k) + (n + 4)*(n + 3)*A(n - 2, k - 1).
%C A153737 Row sums are (2*(n + 5)!) except for n=1:
%C A153737 {2, 10080, 80640, 725760, 7257600, 79833600, 958003200, 12454041600,
%C A153737 174356582400, 2615348736000}.
%F A153737 A(n,k)=A(n - 1, k - 1) + A(n - 1, k) + (n + 4)*(n + 3)*A(n - 2, k - 1).
%e A153737 {2},
%e A153737 {5040, 5040},
%e A153737 {2, 80636, 2},
%e A153737 {2, 362878, 362878, 2},
%e A153737 {2, 363024, 6531548, 363024, 2},
%e A153737 {2, 363206, 39553592, 39553592, 363206, 2},
%e A153737 {2, 363428, 79849438, 797577464, 79849438, 363428, 2},
%e A153737 {2, 363694, 128156058, 6098501046, 6098501046, 128156058, 363694, 2},
%e A153737 {2, 364008, 185214520, 18683169432, 136619086476, 18683169432, 185214520, 364008, 2},
%e A153737 {2, 364374, 251770836, 42192786508, 1265229446280, 1265229446280, 42192786508, 251770836, 364374, 2}
%t A153737 Clear[A]; A[2, 1] := A[2, 2] = 7!;
%t A153737 A[3, 2] = 2*8! - 4; A[4, 2] = A[4, 3] = 9! - 2;
%t A153737 A[n_, 1] := 2; A[n_, n_] := 2;
%t A153737 A[n_, k_] := A[n - 1, k - 1] + A[n - 1, k] + (n + 4)*(n + 3)*A[n - 2, k - 1];
%t A153737 a = Table[A[n, k], {n, 10}, {k, n}];
%t A153737 Flatten[a]
%t A153737 Table[Apply[Plus, a[[n]]], {n, 1, 10}];|Q Table[Apply[Plus, a[[n]]]/(2*(n + 5)!), {n, 1, 10}];
