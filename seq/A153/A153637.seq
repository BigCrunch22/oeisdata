%t A153637 Flatten[a] Table[Apply[Plus, a[[n]]], {n, 1, Length[a]}];
%t A153637 Table[Apply[Plus, a[[n]]]/(3^(n - 1)*(n + 1)!), {n, 1, Length[a]}];
%t A153637 Clear[A, b]; Table[b[n] = (39 n + 9 n^2)/(n + 1), {n, 1, 4}];
%t A153637 b[5] = 8; b[6] = 57/7; b[7] = 33/4; b[8] = 25/3; b[9] = 42/5;
%t A153637 b[10] = 93/11; b[11] = 17/2; b[12] = 111/13;
%t A153637 A[2, 1] := A[2, 2] = 9; A[3, 2] = 212;
%t A153637 A[4, 2] = 1618; A[4, 3] = 1618;
%t A153637 A[n_, 1] := 2; A[n_, n_] := 2;
%t A153637 A[n_, k_] := A[n - 1, k - 1] + A[n - 1, k] + b[n]*n*(n + 1)*A[n - 2, k - 1];
%t A153637 a = Table[A[n, k], {n, 12}, {k, n}];
%t A153637 Flatten[a]
%t A153637 Table[Apply[Plus, a[[n]]], {n, 1, 12}];
