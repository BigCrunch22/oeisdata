%t A153811 Clear[A]; A[1, 1] = 2; A[2, 1] := A[2, 2] = 5* 60/12; A[3, 2] = (18000 - 48)/12; A[4, 2] = (630000/2 - 24)/12; A[4, 3] = (630000/2 - 24)/12; A[n_, 1] := 2; A[n_, n_] := 2;
