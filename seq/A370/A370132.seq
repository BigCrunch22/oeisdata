%t A370132 q[n_] := Module[{k = n, p = 2, s = {}, r}, While[{k, r} = QuotientRemainder[k, p]; k != 0 || r != 0, AppendTo[s, r]; p = NextPrime[p]]; Count[s, _?(# > 2 &)] == 0]; Select[Range[0, 250], q] (* _Amiram Eldar_, Mar 06 2024 *)
