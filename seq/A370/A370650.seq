%t A370650 s2[0] = 0; s2[n_] := s2[n] = s2[Floor[n/4]] + If[Mod[n, 4] > 1, 1, 0]; f2[p_, e_] := s2[e]; a2[1] = 0; a2[n_] := Plus @@ f2 @@@ FactorInteger[n];
