%o A370173 from functools import cache
%o A370173 @cache
%o A370173 def T(n, k):
%o A370173     if k > n: return 0
%o A370173     if n == 0: return 1
%o A370173     if k == 0: return -1 if n == 1 or n == 2 else 0
%o A370173     return T(n-1, k-1) + T(n-2, k-1)
