%e A370676   80 * 86 = 6880
%o A370676 (Python)
%o A370676 def a(n):
%o A370676     count = 0
%o A370676     for i in range(1, 10**(n-1)):
%o A370676         for j in range(i, 10**n//i+1):
%o A370676             if len(str(i*j)) == n and sorted(str(i)+str(j)) == sorted(str(i*j)):
%o A370676                 count += 1
%o A370676     print(n, count)
