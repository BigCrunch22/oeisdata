%p A370946 h:= proc(n) option remember; `if`(n=0, 1,
%p A370946       add(h(n-j)*binomial(n-1, j-1), j=2..n))
%p A370946     end:
%p A370946 b:= proc(n, i, m) option remember; `if`(n>i*(i+1)/2, 0,
%p A370946      `if`(n=0, h(m), b(n, i-1, m)+b(n-i, min(n-i, i-1), m+1)))
%p A370946     end:
%p A370946 a:= n-> b(n$2, 0):
%p A370946 seq(a(n), n=0..48);
%t A370946 h[n_] := h[n] = If[n == 0, 1, Sum[h[n-j]*Binomial[n-1, j-1], {j, 2, n}]];
%t A370946 b[n_, i_, m_] := b[n, i, m] = If[n > i*(i + 1)/2, 0, If[n == 0, h[m], b[n, i - 1, m] + b[n - i, Min[n - i, i - 1], m + 1]]];
%t A370946 a[n_] := b[n, n, 0];
%t A370946 Table[a[n], {n, 0, 48}] (* _Jean-Fran√ßois Alcover_, Mar 08 2024, after _Alois P. Heinz_ *)
