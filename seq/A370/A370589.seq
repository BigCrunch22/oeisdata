%t A370589 bpe[n_]:=Join@@Position[Reverse[IntegerDigits[n,2]],1];
%t A370589 Table[Length[Select[Subsets[Range[n]],MemberQ[#,n] && Select[Tuples[bpe/@#],UnsameQ@@#&]=={}&]],{n,0,10}]
