%t A370219 zlist[m_] := With[{r = 2*Range[m]}, Reverse[Select[Subsets[Range[2*m-1], {m}, Binomial[2*(m-1), m-1]], Min[r-#] > 0 &]]];
%t A370219 dlist[m_] := Map[Append[#, m-Total[#]] &, Map[Differences, zlist[m]]-1];
%t A370219 Array[Delete[dlist[#], 0] &, 5]
