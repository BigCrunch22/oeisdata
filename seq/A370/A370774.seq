%p A370774 seq(A370774(n),n=1..20) ;
%t A370774 Table[-EulerGamma + HarmonicNumber[1 + n, 2] + n*HarmonicNumber[1 + n, 2] - PolyGamma[0, 2 + n], {n, 1, 20}] // Denominator (* _Vaclav Kotesovec_, May 02 2024 *)
