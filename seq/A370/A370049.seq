%o A370049 (PARI) bits(n) = { my (b=vector(hammingweight(n))); for (k=1, #b, n-=2^b[k]=valuation(n,2)); return (b); }
%o A370049 A(n, k) = { my (bn = bits(2*n), bk = bits(2*k), v = 0, e); for (i = 1, #bn, for (j = 1, #bk, e = bn[i] * bk[j] - 1; v = bitxor(v, 2^e););); return (v); }
