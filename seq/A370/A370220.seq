%t A370220 zlist[m_] := With[{r = 2*Range[m]}, Reverse[Select[Subsets[Range[2*m-1], {m}, Binomial[2*(m-1), m-1]], Min[r-#] > 0 &]]];
%t A370220 Array[Delete[zlist[#], 0] &, 5]
