%t A185788 f[n_,k_]:=n+(n+k-2)(n+k-1)/2;
%t A185788 s[k_]:=Sum[f[n,k],{n,1,k-1}];
%t A185788 Factor[s[k]]
%t A185788 Table[s[k],{k,1,70}]
