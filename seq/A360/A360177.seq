%p A360177 A360177 := (n, k) -> if n = k then 1 else
%p A360177 add((-1)^(u-k-1)*(n+u+1)^(n-1)/(u!*(k-1-u)!), u = 0.. k-1) fi:
%p A360177 for n from 0 to 8 do seq(A360177(n, k), k = 0..n) od;
