%t A200024 a = 1; b = -2; c = 4;
%t A200024 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c*Sin[x]
%t A200024 Plot[{f[x], g[x]}, {x, -1, 2}, {AxesOrigin -> {0, 0}}]
%t A200024 r = x /. FindRoot[f[x] == g[x], {x, -.43, -.42}, WorkingPrecision -> 110]
%t A200024 RealDigits[r]  (* A200024 *)
