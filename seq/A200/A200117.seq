%t A200117 a = 2; b = -2; c = 1;
%t A200117 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c*Sin[x]
%t A200117 Plot[{f[x], g[x]}, {x, -3, 3}, {AxesOrigin -> {0, 0}}]
%t A200117 r = x /. FindRoot[f[x] == g[x], {x, -.69, -.68}, WorkingPrecision -> 110]
%t A200117 RealDigits[r]  (* A200116 *)
