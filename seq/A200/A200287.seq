%t A200287 a = 4; b = -1; c = 2;
%t A200287 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c*Sin[x]
%t A200287 Plot[{f[x], g[x]}, {x, -1, 1}, {AxesOrigin -> {0, 0}}]
%t A200287 r = x /. FindRoot[f[x] == g[x], {x, -.31, -.30}, WorkingPrecision -> 110]
%t A200287 RealDigits[r]    (* A200287 *)
