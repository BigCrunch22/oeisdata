%t A200223 a = 3; b = -1; c = 2;
%t A200223 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c*Sin[x]
%t A200223 Plot[{f[x], g[x]}, {x, -3, 3}, {AxesOrigin -> {0, 0}}]
