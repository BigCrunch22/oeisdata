%t A291532      b[i - 1, i - 1, 0], j]*x^(t*j)*b[n - i*j, i - 1, t], {j, 1, n/i}]]];
%t A291532 a[n_] := Function[p, Sum[i*Coefficient[p, x, i], {i, 1, Exponent[p, x]}]][
