%o A334143 (Python)
%o A334143 def NORprime(n):
%o A334143     s = str(bin(primes[n]))[2:]
%o A334143     t = str(bin(primes[n-1]))[2:]
%o A334143     k = (len(s) -  len(t))
%o A334143     t = k*'0' + t
%o A334143     r = ''
%o A334143     for i in range(len(s)):
%o A334143         if s[i] == t[i] and s[i] == '0':
%o A334143             r += '1'
%o A334143         else:
%o A334143             r += '0'
%o A334143     return int(r,2)
