%t A334318 b[n_, s_, w_] := b[n, s, w] = If[s == {}, 0, With[{k = 1+n-Length[s]}, Sum[With[{t = w*n + j}, If[t == 0, x, If[Mod[t, k] == 0, b[n, s ~Complement~ {j}, t] + x^k, 0]]], {j, s}]]];
%t A334318 T[n_] := PadRight[CoefficientList[b[n, Range[0, n-1], 0]/x, x], n];
