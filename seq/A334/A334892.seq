%t A334892 b[n_, s_] := b[n, s] = If[n == 0, x, b[n-1, s] + If[
%t A334892      AnyTrue[s, MemberQ[s, 2#-n]&], 0, Expand[x*b[n-1, s ~Union~ {n}]]]];
%t A334892 T[n_] := If[n == 0, {1}, CoefficientList[b[n-1, {n}], x]];
