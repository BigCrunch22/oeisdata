%F A334281 a(n) = n*(n - 1)*(n - 2)*(n - 3)*(465 - 392n + 125n^2 - 18n^3 + n^4).
%F A334281 a(n) = -2790n + 7467n^2 - 7852n^3 + 4300n^4 - 1346n^5 + 244n^6 - 24n^7 + n^8.
