%N A324618 G.f. A(x) = Sum_{n>=0} x^n*(A(x)^n - 1)^n/(1 - x*A(x)^n)^(n+1).
%H A324618 Paul D. Hanna, <a href="/A324618/b324618.txt">Table of n, a(n) for n = 0..200</a>
%F A324618 G.f. A(x) satisfies:
%F A324618 (1) A(x) = Sum_{n>=0} x^n*(A(x)^n - 1)^n / (1 - x*A(x)^n)^(n+1).
%F A324618 (2) A(x) = Sum_{n>=0} x^n*(A(x)^n + 1)^n / (1 + x*A(x)^n)^(n+1).
%F A324618 (3) A(x) = Sum_{n>=0} x^n*Sum_{k=0..n} binomial(n,k) * (A(x)^n - A(x)^k)^(n-k).
%F A324618 (4) A(x) = Sum_{n>=0} x^n*Sum_{k=0..n} (-1)^k * binomial(n,k) * (A(x)^n + A(x)^k)^(n-k).
%e A324618 G.f.: A(x) = 1 + x + 2*x^2 + 5*x^3 + 19*x^4 + 86*x^5 + 436*x^6 + 2378*x^7 + 13731*x^8 + 83077*x^9 + 523275*x^10 + 3416329*x^11 + 23051600*x^12 + ...
%e A324618 such that
%e A324618 A(x) = 1/(1 - x) + x*(A(x) - 1)/(1 - x*A(x))^2 + x^2*(A(x)^2 - 1)^2/(1 - x*A(x)^2)^3 + x^3*(A(x)^3 - 1)^3/(1 - x*A(x)^3)^4 + x^4*(A(x)^4 - 1)^4/(1 - x*A(x)^4)^5 + x^5*(A(x)^5 - 1)^5/(1 - x*A(x)^5)^6 + ...
%e A324618 also
%e A324618 A(x) = 1/(1 + x) + x*(A(x) + 1)/(1 + x*A(x))^2 + x^2*(A(x)^2 + 1)^2/(1 + x*A(x)^2)^3 + x^3*(A(x)^3 + 1)^3/(1 + x*A(x)^3)^4 + x^4*(A(x)^4 + 1)^4/(1 + x*A(x)^4)^5 + x^5*(A(x)^5 + 1)^5/(1 + x*A(x)^5)^6 + ...
%t A324618 m = 35; A[_] = 0; Unprotect[Power]; 0^0 = 1; Protect[Power];
%t A324618 Do[A[x_] = Sum[ x^n (A[x]^n - 1)^n/(1 - x A[x]^n)^(n + 1), {n, 0, k}] + O[x]^k, {k, m}];
%t A324618 CoefficientList[A[x], x] (* _Jean-Fran√ßois Alcover_, Oct 21 2019 *)
%o A324618 (PARI) {a(n) = my(A=[1,1]); for(i=0,n, A = concat(A,0);
%o A324618 A[#A] = polcoeff( sum(n=0,#A+1, x^n*(Ser(A)^n - 1)^n/(1 - x*Ser(A)^n)^(n+1) ),#A-1));
%o A324618 polcoeff(Ser(A),n)}
%o A324618 for(n=0,40,print1(a(n),", "))
