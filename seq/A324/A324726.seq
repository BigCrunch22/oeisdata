%F A324726 {k such that 2*k = A318466(k)}.
%t A324726 Select[Range[1000], 2*# == BitOr[2*#, DivisorSigma[1, #]] &] (* _Paolo Xausa_, Mar 11 2024 *)
%o A324726 (PARI) for(n=1, oo, if(((2*n)==bitor(2*n, sigma(n))), print1(n, ", ")));
