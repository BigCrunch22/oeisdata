%N A324308 G.f.: Sum_{n>=0} (2^n + i)^n * x^n / (1 + 2^n*i*x)^(n+1), where i^2 = -1.
%C A324308 Notice that the imaginary components in the generating function vanish when expanded as a power series expansion in x.
%F A324308 G.f.: Sum_{n>=0} (2^n + i)^n * x^n / (1 + 2^n*i*x)^(n+1).
%F A324308 G.f.: Sum_{n>=0} (2^n - i)^n * x^n / (1 - 2^n*i*x)^(n+1).
%F A324308 a(n) = Sum_{k=0..n} i^k * binomial(n,k) * (2^n - 2^k*i)^(n-k).
%F A324308 a(n) = Sum_{k=0..n} (-i)^k * binomial(n,k) * (2^n + 2^k*i)^(n-k).
%e A324308 G.f.: A(x) = 1 + 2*x + 18*x^2 + 560*x^3 + 68642*x^4 + 34215232*x^5 + 69224885118*x^6 + 564395560679680*x^7 + 18462516899827251202*x^8 + ...
%e A324308 such that
%e A324308 A(x) = 1/(1+i*x) + (2 + i)*x/(1 + 2*i*x)^2 + (2^2 + i)^2*x^2/(1 + 2^2*i*x)^3 + (2^3 + i)^3*x^3/(1 + 2^3*i*x)^4 + (2^4 + i)^4*x^4/(1 + 2^4*i*x)^5 + (2^5 + i)^5*x^5/(1 + 2^5*i*x)^6 + (2^6 + i)^6*x^6/(1 + 2^6*i*x)^7 + ...
%e A324308 Also,
%e A324308 A(x) = 1/(1-i*x) + (2 - i)*x/(1 - 2*i*x)^2 + (2^2 - i)^2*x^2/(1 - 2^2*i*x)^3 + (2^3 - i)^3*x^3/(1 - 2^3*i*x)^4 + (2^4 - i)^4*x^4/(1 - 2^4*i*x)^5 + (2^5 - i)^5*x^5/(1 - 2^5*i*x)^6 + (2^6 - i)^6*x^6/(1 - 2^6*i*x)^7 + ...
%o A324308 (PARI) {a(n) = my(A = sum(m=0, n+1, (2^m + I)^m*x^m/(1 + I*2^m*x +x*O(x^n) )^(m+1) )); polcoeff(A, n)}
%o A324308 for(n=0, 20, print1(a(n), ", "))
%o A324308 (PARI) {a(n) = my(A = sum(m=0, n+1, (2^m - I)^m*x^m/(1 - I*2^m*x +x*O(x^n) )^(m+1) )); polcoeff(A, n)}
%o A324308 for(n=0, 20, print1(a(n), ", "))
%o A324308 (PARI) {a(n) = sum(k=0, n, I^k * binomial(n, k) * (2^n - 2^k*I)^(n-k) )}
%o A324308 for(n=0, 20, print1(a(n), ", "))
%o A324308 (PARI) {a(n) = sum(k=0, n, (-I)^k * binomial(n, k) * (2^n + 2^k*I)^(n-k) )}
%o A324308 for(n=0, 20, print1(a(n), ", "))
