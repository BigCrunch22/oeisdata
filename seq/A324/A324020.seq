%o A324020 (Ruby)
%o A324020 def A(n)
%o A324020   d = 0
%o A324020   a = (1..n - 1).map{|i| [i]}
%o A324020   cnt = n - 1
%o A324020   while d < n - 2
%o A324020     d += 1
%o A324020     b = []
%o A324020     a.each{|i|
%o A324020       (1..n - 1).each{|j|
%o A324020         m = i.clone + [j]
%o A324020         if (0..d).inject(0){|s, k| s + m[k] * n ** (d - k)} % (d + 1) == 0
%o A324020           b << m
%o A324020           cnt += 1
%o A324020         end
%o A324020       }
%o A324020     }
%o A324020     a = b
%o A324020   end
%o A324020   cnt
%o A324020 end
%o A324020 def A324020(n)
%o A324020   (2..n).map{|i| A(i)}
%o A324020 end
