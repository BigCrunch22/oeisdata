%t A324718 Select[Range[1, 10^4, 2], Block[{s = DivisorSigma[1, #]}, BitAnd[2*#, s] == 2* BitAnd[#, s-#]] &] (* _Paolo Xausa_, Mar 11 2024 *)
%o A324718 (PARI) for(n=1,oo,if((n%2) && (bitand(2*n,sigma(n)) == 2*bitand(n,sigma(n)-n)),print1(n, ", ")));
