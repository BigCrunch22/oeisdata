%N A324535 An analog of sigma(n)-n (A001065) for nonstandard factorization based on the sieve of Eratosthenes (A083221).
%H A324535 Antti Karttunen, <a href="/A324535/b324535.txt">Table of n, a(n) for n = 1..16384</a>
%H A324535 Antti Karttunen, <a href="/A324535/a324535.txt">Data supplement: n, a(n) computed for n = 1..65537</a>
%H A324535 <a href="/index/Si#sieve">Index entries for sequences generated by sieves</a>
%H A324535 <a href="/index/Si#SIGMAN">Index entries for sequences related to sigma(n)</a>
%F A324535 a(n) = A001065(A250246(n)) = A324545(n) - A250246(n).
%F A324535 a(n) = A250246(n) - A324546(n).
%o A324535 (PARI)
%o A324535 up_to = 65537;
%o A324535 ordinal_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), pt); for(i=1, length(invec), if(mapisdefined(om,invec[i]), pt = mapget(om, invec[i]), pt = 0); outvec[i] = (1+pt); mapput(om,invec[i],(1+pt))); outvec; };
%o A324535 A020639(n) = if(n>1, if(n>n=factor(n, 0)[1, 1], n, factor(n)[1, 1]), 1); \\ From A020639
%o A324535 A055396(n) = if(1==n,0,primepi(A020639(n)));
%o A324535 v078898 = ordinal_transform(vector(up_to,n,A020639(n)));
%o A324535 A078898(n) = v078898[n];
%o A324535 A003961(n) = my(f = factor(n)); for (i=1, #f~, f[i, 1] = nextprime(f[i, 1]+1)); factorback(f); \\ From A003961
%o A324535 A250246(n) = if(1==n,n,my(k = 2*A250246(A078898(n)), r = A055396(n)); if(1==r, k, while(r>1, k = A003961(k); r--); (k)));
%o A324535 A001065(n) = (sigma(n)-n);
%o A324535 A324535(n) = A001065(A250246(n));
