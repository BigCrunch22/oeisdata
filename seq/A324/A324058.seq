%o A324058 (PARI)
%o A324058 A005940(n) = { my(p=2, t=1); n--; until(!n\=2, if((n%2), (t*=p), p=nextprime(p+1))); t }; \\ From A005940
%o A324058 A106737(n) = sum(k=0, n, (binomial(n+k, n-k)*binomial(n, k)) % 2);
%o A324058 A324054(n) = { my(p=2,mp=p*p,m=1); while(n, if(!(n%2), p=nextprime(1+p); mp = p*p, if(3==(n%4),mp *= p,m *= (mp-1)/(p-1))); n>>=1); (m); };
%o A324058 A324058(n) = gcd(A324054(n), A005940(1+n)*A106737(n));
%o A324058 \\ Alternatively as:
%o A324058 A324121(n) = gcd(sigma(n),n*numdiv(n));
%o A324058 A324058(n) = A324121(A005940(1+n));
