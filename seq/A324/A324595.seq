%t A324595 b[n_, i_, k_] := b[n, i, k] = If[n == 0, 1, If[i < 1, 0, Sum[Function[t, b[t, Min[t, i - 1], k] Binomial[k, j]][n - i j], {j, 0, Min[k, n/i]}]]];
%t A324595 a[n_] := Sum[b[2n, 2n, n - i] (-1)^i Binomial[n, i], {i, 0, n}];
