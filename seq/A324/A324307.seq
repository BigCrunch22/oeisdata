%N A324307 G.f.: Sum_{n>=0} (3^n + 1)^n * x^n / (1 + 3^n*x)^(n+1).
%F A324307 G.f.: Sum_{n>=0} (3^n + 1)^n * x^n / (1 + 3^n*x)^(n+1).
%F A324307 G.f.: Sum_{n>=0} (3^n - 1)^n * x^n / (1 - 3^n*x)^(n+1).
%F A324307 a(n) = Sum_{k=0..n} binomial(n,k) * (3^n - 3^k)^(n-k).
%F A324307 a(n) = Sum_{k=0..n} (-1)^k * binomial(n,k) * (3^n + 3^k)^(n-k).
%e A324307 G.f.: A(x) = 1 + 3*x + 77*x^2 + 19359*x^3 + 42889529*x^4 + 846714983643*x^5 + 150077691692889797*x^6 + 239295126680408718226359*x^7 + ...
%e A324307 such that
%e A324307 A(x) = 1/(1+x) + (3 + 1)*x/(1 + 3*x)^2 + (3^2 + 1)^2*x^2/(1 + 3^2*x)^3 + (3^3 + 1)^3*x^3/(1 + 3^3*x)^4 + (3^4 + 1)^4*x^4/(1 + 3^4*x)^5 + (3^5 + 1)^5*x^5/(1 + 3^5*x)^6 + (3^6 + 1)^6*x^6/(1 + 3^6*x)^7 + ...
%e A324307 Also,
%e A324307 A(x) = 1/(1-x) + (3 - 1)*x/(1 - 3*x)^2 + (3^2 - 1)^2*x^2/(1 - 3^2*x)^3 + (3^3 - 1)^3*x^3/(1 - 3^3*x)^4 + (3^4 - 1)^4*x^4/(1 - 3^4*x)^5 + (3^5 - 1)^5*x^5/(1 - 3^5*x)^6 + (3^6 - 1)^6*x^6/(1 - 3^6*x)^7 + ...
%o A324307 (PARI) {a(n) = my(A = sum(m=0, n+1, (3^m + 1)^m*x^m/(1 + 3^m*x +x*O(x^n) )^(m+1) )); polcoeff(A, n)}
%o A324307 for(n=0, 20, print1(a(n), ", "))
%o A324307 (PARI) {a(n) = my(A = sum(m=0, n+1, (3^m - 1)^m*x^m/(1 - 3^m*x +x*O(x^n) )^(m+1) )); polcoeff(A, n)}
%o A324307 for(n=0, 20, print1(a(n), ", "))
