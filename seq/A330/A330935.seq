%t A330935 facs[n_]:=If[n<=1,{{}},Join@@Table[Map[Prepend[#,d]&,Select[facs[n/d],Min@@#>=d&]],{d,Rest[Divisors[n]]}]];
%t A330935 upfacs[q_]:=Union[Sort/@Join@@@Tuples[facs/@q]];
%t A330935 paths[eds_,start_,end_]:=If[start==end,Prepend[#,{}],#]&[Join@@Table[Prepend[#,e]&/@paths[eds,Last[e],end],{e,Select[eds,First[#]==start&]}]];
%t A330935 Table[Length[Select[paths[Join@@Table[{y,#}&/@DeleteCases[upfacs[y],y],{y,facs[n]}],{n},First[facs[n]]],Length[#]==k-1&]],{n,100},{k,PrimeOmega[n]}]
