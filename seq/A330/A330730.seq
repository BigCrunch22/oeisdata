%p A330730 A330730 := proc(n) (-1-n)^(2*n^3-4*n^2+2*n) * (n^2-2*n-1)^(n^2-2*n+1) * (n^2-n-1)^(2*n^2-2*n) * (2*n^2-2*n-1)^(2*n-2) * (3*n^2-2*n-1); end;
