%p A184296 F:= proc(n, t) option remember;
%p A184296    local k, r;
%p A184296    if t < 0 or t > (n+1)^2*n^2 then return 0 fi;
%p A184296    if n = 1 then if member(t,[0,1,4]) then return 1 else return 0 fi fi;
%p A184296    add(procname(n-1, t - n^3*k^2),k=0..2);
