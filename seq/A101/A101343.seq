%t A101343 row[n_] := CoefficientList[ Derivative[n][Tan][z] /. Tan -> t /. Sec -> (Sqrt[1+t[#]^2]&), t[z]] // DeleteCases[#, 0]& // Reverse; Table[row[n], {n, 0, 10}] // Flatten (* _Jean-François Alcover_, Feb 26 2013 *)
%o A101343 (Maxima)
%o A101343 T(n,k):=if k=0 then Tr(n,k) else if 2*k-1=n then Tr(n,k-1) else Tr(n,k)+Tr(n,k-1);
%o A101343 Tr(n,i):=((sum(binomial(j+n-2*i-1,n-2*i-1)*(j+n-2*i)!*2^(2*i-j)*(-1)^(j-i)*stirling2(n,j+n-2*i),j,0,2*i))); /* _Vladimir Kruchinin_, May 27 2011 */
