%t A101477 nmax = 11;
%t A101477 b[x_] = Sum[2^(n - 1)*(2*n - 2)!/(n - 1)!/n! x^n, {n, 1, nmax}];
%t A101477 c[x_] = 0; Do[c[x_] = x*(1 + c[x])^4/(1 + c[x]^2) + O[x]^nmax, {nmax}];
%t A101477 a[n_, t_] := a[n, t] = b[t]*(1 - c[t]^(n + 1))*(1 - c[t]^(n + 5))/((1 - c[t]^(n + 2))*(1 - c[t]^(n + 4)));
%t A101477 T[n_, k_] := SeriesCoefficient[a[n, t], {t, 0, k}];
%t A101477 Table[T[n - k, k], {n, 1, nmax}, {k, 1, n}] // Flatten (* _Jean-François Alcover_, Jul 25 2018 *)
