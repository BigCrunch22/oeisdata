%t A372850 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A372850 Select[Range[100],PrimeQ[Total[2^(Union[prix[#]]-1)]]&]
