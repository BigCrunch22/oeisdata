%t A372427 prix[n_]:=If[n==1,{},Flatten[Cases[FactorInteger[n],{p_,k_}:>Table[PrimePi[p],{k}]]]];
%t A372427 bix[n_]:=Join@@Position[Reverse[IntegerDigits[n,2]],1];
%t A372427 Select[Range[100],Total[prix[#]]==Total[bix[#]]&]
