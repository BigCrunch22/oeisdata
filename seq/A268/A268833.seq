%o A268833 (PARI) b(n) = if(n<1, 0, b(n\2) + n%2);
%o A268833 A101080(n, k) = b(bitxor(n, k));
%o A268833 A003188(n) = bitxor(n, n\2);
%o A268833 A006068(n) = if(n<2, n, {my(m = A006068(n\2)); 2*m + (n%2 + m%2)%2});
