%p A268698 g:= proc(l) option remember; (n-> f(l[1..nops(l)-1])*
%p A268698       binomial(n-1, l[-1]-1)+ add(f(sort(subsop(j=l[j]
%p A268698       -1, l))), j=1..nops(l)-1))(add(i, i=l))
%p A268698     end:
%p A268698 f:= l-> (n-> `if`(n<2 or l[-1]=1, 1, `if`(l[1]=0, 0, `if`(
%p A268698          n=2, binomial(l[1]+l[2], l[1])-1, g(l)))))(nops(l)):
%p A268698 h:= (n, i, l)-> `if`(n=0 or i=1, f([1$n, l[]]), h(n, i-1, l)+
%p A268698                 `if`(i>n, 0, h(n-i, i, [i, l[]]))):
%p A268698 a:= n-> h(n$2, []):
%p A268698 seq(a(n), n=0..25);
%t A268698 g[l_] := g[l] = Function[n, f[Most[l]]*Binomial[n-1, l[[-1]]-1] + Sum[f[ Sort[ ReplacePart[l, j -> l[[j]]-1]]], {j, 1, Length[l]-1}]][Total[l]]; f[l_] := Function[n, If[n<2 || l[[-1]] == 1, 1, If[l[[1]] == 0, 0, If[n == 2, Binomial[l[[1]] + l[[2]], l[[1]]]-1, g[l]]]]][Length[l]]; h[n_, i_, l_] := h[n, i, l] = If[n == 0 || i == 1, f[Join[Array[1&, n], l]], h[n, i - 1, l] + If[i>n, 0, h[n-i, i, Prepend[l, i]]]]; a[n_] := h[n, n, {}]; Table[a[n], {n, 0, 25}] (* _Jean-Fran√ßois Alcover_, Feb 02 2017, translated from Maple *)
