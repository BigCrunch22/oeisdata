%t A294840 a[n_] := a[n] = If[n == 0, 1, Sum[Sum[(-1)^(k/d + 1) d (5 d (d + 1)/8 + (-1)^d (2 d + 1)/16 - 1/16), {d, Divisors[k]}] a[n - k], {k, 1, n}]/n]; Table[a[n], {n, 0, 34}]
