%t A294220 b[n_, i_, t_, p_, k_] := b[n, i, t, p, k] = If[n == 0, 1, Sum[ If[ Coefficient[p, x, j] == k, 0, b[n-1, j, t + If[j>i, 1, 0], p + x^j, k]], {j, 1, t+1}]];
%t A294220 A[n_, k_] := b[n, 0, 0, 0, Min[n, k]];
