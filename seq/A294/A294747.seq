%N A294747 Number of compositions (ordered partitions) of 1 into exactly n^2+1 powers of 1/(n+1).
%H A294747 Alois P. Heinz, <a href="/A294747/b294747.txt">Table of n, a(n) for n = 0..26</a>
%F A294747 a(n) = [x^((n+1)^n)] (Sum_{j=0..n^2+1} x^((n+1)^j))^(n^2+1) for n>0, a(0) = 1.
%F A294747 a(n) = A294746(n,n).
%F A294747 a(n) ~ exp(-1/12) * n^(n^2 - n/2 + 2) / (2*Pi)^((n-1)/2). - _Vaclav Kotesovec_, Sep 20 2019
%e A294747 a(0) = 1: [1].
%e A294747 a(1) = 1: [1/2,1/2].
%e A294747 a(2) = 10 = binomial(5,2): [1/3,1/3,1/9,1/9,1/9], [1/3,1/9,1/3,1/9,1/9], [1/3,1/9,1/9,1/3,1/9], [1/3,1/9,1/9,1/9,1/3], [1/9,1/3,1/3,1/9,1/9], [1/9,1/3,1/9,1/3,1/9], [1/9,1/3,1/9,1/9,1/3], [1/9,1/9,1/3,1/3,1/9], [1/9,1/9,1/3,1/9,1/3], [1/9,1/9,1/9,1/3,1/3].
%p A294747 b:= proc(n, r, p, k) option remember;
%p A294747       `if`(n<r, 0, `if`(r=0, `if`(n=0, p!, 0), add(
%p A294747        b(n-j, k*(r-j), p+j, k)/j!, j=0..min(n, r))))
%p A294747     end:
%p A294747 a:= n-> `if`(n=0, 1, b(n^2+1, 1, 0, n+1)):
%p A294747 seq(a(n), n=0..10);
%t A294747 b[n_, r_, p_, k_] := b[n, r, p, k] = If[n < r, 0, If[r == 0, If[n == 0, p!, 0], Sum[b[n - j, k*(r - j), p + j, k]/j!, {j, 0, Min[n, r]}]]];
%t A294747 a[n_] := If[n == 0, 1, b[n^2 + 1, 1, 0, n + 1]];
%t A294747 Table[a[n], {n, 0, 10}] (* _Jean-FranÃ§ois Alcover_, May 21 2018, translated from Maple *)
