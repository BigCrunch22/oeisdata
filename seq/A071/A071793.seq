%t A071793 k = 4; f[x_] := Floor[10*FractionalPart[x]]; Clear[xx]; xx[n_] := xx[n] = Catch[For[x = xx[n - 1], True, x += 10^(-n), If[f[n*x] == f[10^(n - 1)*x], Throw[x]]]]; xx[1] = k/10; Scan[xx, Range[100]]; RealDigits[xx[100]][[1]] (* _Jean-Fran√ßois Alcover_, Dec 06 2012 *)
