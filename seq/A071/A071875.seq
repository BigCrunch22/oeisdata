%e A071875 wherein the tenths place of n*x yields the n-th digit of x.
%t A071875 k = 7; f[x_] := Floor[10*FractionalPart[x]]; Clear[xx]; xx[n_] := xx[n] = Catch[ For[x = xx[n - 1], True, x += 10^(-n), If[f[n*x] == f[10^(n - 1)*x], Throw[x]]]]; xx[1] = k/10; Scan[xx, Range[100]]; RealDigits[xx[100]][[1]] (* _Jean-François Alcover_, Dec 06 2012 *)
%t A071875 Clear[a]; a[1] = 7; a[2] = 4; a[n0=3] = 2; a[_] = 0; digits = 10^(n0-1); Do[a[n] = Mod[Floor[10*n*Sum[a[k]/10^k, {k, 1, n}]], 10], {n, n0+1, digits}]; Table[a[n], {n, 1, digits}] (* _Jean-François Alcover_, May 12 2015 *)
