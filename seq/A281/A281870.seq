%e A281870 a(11) = 1: {1,3,4,5,6,7,8,9,10,11}.
%e A281870 a(12) = 5: {1,2,3,4,5,7,9,10,11,12}, {1,2,3,4,6,7,8,10,11,12}, {1,2,3,5,6,7,8,9,11,12}, {1,2,4,5,6,7,8,9,10,12}, {1,3,4,5,6,7,8,9,10,11}.
%p A281870 b:= proc(n, i, t) option remember;
%p A281870       `if`(i<t, 0, `if`(n=0, `if`(t=0, 1, 0),
%p A281870       `if`(t<1 or n<t*(t+1)/2 or n>(t+1)*(2*i-t)/2, 0,
%p A281870       `if`(i>n, 0, b(n-i, i-1, t-1))+b(n, i-1, t))))
%p A281870     end:
%p A281870 a:= proc(n) option remember; `if`(n<0, 0, a(n-1)+add(
%p A281870        b(j^2-n, n-1, 9), j=isqrt(n-45)..isqrt(10*n-45)))
%p A281870     end:
%p A281870 seq(a(n), n=10..60);
