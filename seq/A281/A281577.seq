%o A281577 (PARI) a152155(n) = centerlift(Mod(3, 2^(2^n)+1)^(2^(2^n-1)))
%o A281577 row(n) = my(i=0, k=1); while(1, if(a152155(k)!=-1, i++); if(i==n, forprime(p=1, sqrtint(2^(2^k)+1), if(Mod(2, p)^(2^k)==-1, print1(lift(Mod(2, p^2)^(2^k))+1, ", ")))); k++)
