%N A136454 Triangle of coefficients of the Pollaczek polynomials with a=1, b=1 multiplied by n! to make then integers.
%H A136454 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/PollaczekPolynomial.html">Pollaczek Polynomial</a>.
%e A136454 {1},
%e A136454 {2, 3},
%e A136454 {2, 16, 15},
%e A136454 {-20, 10, 142, 105},
%e A136454 {-112, -736, -166, 1488, 945},
%e A136454 {1376, -3504, -19788, -7250,18258, 10395},
%e A136454 {19552, 121280, -60228, -494944, -199484, 258144, 135135},
%e A136454 {-307648, 1418848, 6685320, -66308, -12424144, -5095512, 4142430, 2027025},...
%t A136454 a = 1; b = 1;
%t A136454 P[x, 0] = 1;
%t A136454 P[x, 1] = (2*a + 1)*x + 2*b;
%t A136454 P[x_, n_] := P[x, n] = (1/n)*((2*n - 1 + 2*a)*x + 2*b)*P[x, n - 1] - (n - 1)*P[x, n - 2];
%t A136454 a0 = Table[CoefficientList[n!*P[x, n], x], {n, 0, 10}];
%t A136454 Flatten[a0]
%K A136454 uned,tabl,sign,changed
%O A136454 1,2
