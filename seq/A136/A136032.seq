%t A136032 a = {}; Do[If[PrimeQ[n] && !PrimeQ[2^n - 1], w = 2^n - 1; c = FactorInteger[w]; d = Length[c]; b = 0; Do[b = b + c[[k]][[2]], {k, 1, d}]; AppendTo[a, b]], {n, 2, 150}]; a
