%t A091292 sg[n_] := DivisorSigma[1, n]; g[x_, k_] := Apply[Plus, Table[sg[x + j], {j, 0, k - 1}]] / sg[Apply[Plus, Table[x + j, {j, 0, k - 1}]]]; Do[s = g[n, 3]; If[IntegerQ[s], Print[n]], {n, 1, 10000000}]
