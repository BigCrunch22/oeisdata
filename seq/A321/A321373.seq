%C A321373 T(n+1,k) = (-1)^k*A140966(k) + (n+1)*A001045(k).
%C A321373 Every row T(n+1,k) has the signature (1,2).
%C A321373 T(0,k) = 2, -2, 2, -2, ... = (-1)^n*2.
%C A321373 T(n+1,k) - T(0,k) = (n+1)*A001045(n).
%C A321373 5*A001045(n) is not in the OEIS.
%e A321373 Triangle a(n):
%e A321373   2;
%e A321373   2, -1;
%e A321373   2,  0,  3;
%e A321373   2,  1,  4,  1;
%e A321373   2,  2,  5,  4,  7;
%e A321373   2,  3,  6,  7, 12,  9;
%e A321373   2,  4,  7, 10, 17, 20, 23;
%e A321373   etc.
%e A321373 Row sums: 2, 1, 5, 8, 20, 39, 83, 166, 338, 677, 1361, 2724, ... = b(n+2).
%e A321373 With b(0) = 2 and b(1) = 0, b(n) = b(n-1) + 2*b(n-2)  + n - 4, n > 1.
%e A321373 b(n) = A001045(n) - A097065(n-1).
%e A321373 b(n) = b(n-2) + A000225(n-2).
%t A321373 T[_, 0] = 2;
%t A321373 T[0, k_] := (2^k + 5(-1)^k)/3;
%t A321373 T[n_ /; n>0, k_ /; k>0] := T[n, k] = T[n-1, k] + (2^k + (-1)^(k+1))/3;
%t A321373 T[_, _] = 0;
