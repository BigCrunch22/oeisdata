%e A321601 ...
%e A321601 where the g.f. of column k is (1+x)/( (1 - (-1)^k*x) * (1-x)^k ) for k >= 0.
%e A321601 Thus, for column k > 0,
%e A321601 if k odd: T(n,k) = binomial(n+k-1,k-1),
%e A321601 if k even: T(n,k) = binomial(n+k-1,k-1)*(2*n+k)/k.
%e A321601 ...
%o A321601 (PARI) /* Table of coefficients in Sum_{n=-oo...+oo} (x^n + y)^n */
%o A321601 ROWS=12
%o A321601 {Q(n, k) = polcoeff(polcoeff( sum(m=-n-k, n+k, (x^m + y +O(x^(n+1)))^m ), n, x) +O(y^(k+1)), k, y)}
%o A321601 {T(n,k) = polcoeff( sum(j=0, k, Q(n^2 + n*j,j)*z^j +O(z^(k+1))), k, z)}
%o A321601 for(n=0, ROWS, for(k=0, ROWS, print1(T(n, k), ", ")); print(""))
%o A321601 (PARI) /* Using binomial formula for terms T(n,k) */
%o A321601 ROWS=12
%o A321601 {T(n,k) = if(k==0, if(n==0,1,2),
%o A321601 if(k%2==1, binomial(n+k-1,k-1), binomial(n+k-1,k-1)*(2*n+k)/k))}
%o A321601 for(n=0, ROWS, for(k=0, ROWS, print1(T(n, k), ", ")); print(""))
