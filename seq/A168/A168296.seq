%t A168296 (* Worpitzky form polynomials for A142462 *)
%t A168296 m = 7;
%t A168296 A[n_, 1] := 1 A[n_, n_] := 1
%t A168296 A[n_, k_] := (m*n - m*k + 1)A[n - 1, k - 1] + (m*k - (m - 1))A[n - 1, k];
