%I A270318 #8 Jul 26 2024 21:16:33
%S A270318 1,8,44,209,892,3749,15160,61205,245509,985328,3942373,15778036,
%T A270318 63142628,252596776,1010495385,4042114757
%N A270318 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 149", based on the 5-celled von Neumann neighborhood.
%C A270318 Initialized with a single black (ON) cell at stage zero.
%D A270318 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270318 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270318 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270318 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270318 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270318 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270318 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270318 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270318 code=149; stages=128;
%t A270318 rule=IntegerDigits[code,2,10];
%t A270318 g=2*stages+1; (* Maximum size of grid *)
%t A270318 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270318 ca=a;
%t A270318 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270318 PrependTo[ca,a];
%t A270318 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270318 k=(Length[ca[[1]]]+1)/2;
%t A270318 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270318 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270318 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270318 Cf. A270317.
%K A270318 nonn,more,changed
%O A270318 0,2
