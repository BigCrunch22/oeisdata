%I A270178 #9 Jul 26 2024 21:16:33
%S A270178 1,4,32,184,845,3445,14061,56820,229473,920797,3692437,14771668,
%T A270178 59109548,236490292,945977529,3784346768
%N A270178 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 113", based on the 5-celled von Neumann neighborhood.
%C A270178 Initialized with a single black (ON) cell at stage zero.
%D A270178 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270178 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270178 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270178 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270178 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270178 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270178 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270178 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270178 code=113; stages=128;
%t A270178 rule=IntegerDigits[code,2,10];
%t A270178 g=2*stages+1; (* Maximum size of grid *)
%t A270178 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270178 ca=a;
%t A270178 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270178 PrependTo[ca,a];
%t A270178 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270178 k=(Length[ca[[1]]]+1)/2;
%t A270178 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270178 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270178 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270178 Cf. A270177.
%K A270178 nonn,more,changed
%O A270178 0,2
