%S A270094 4,3,12,0,12,16,8,32,-32,40,-12,56,-12,52,92,-12,-84,136,-72,88,-48,
%T A270094 128,-8,152,-244,228,-60,272,-188,288,-64,280,-296,324,-264,216,-116,
%U A270094 140,128,-96,24,208,76,252,-188,88,156,464,-496,372,-76,280,-228,416
%N A270094 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 78", based on the 5-celled von Neumann neighborhood.
%C A270094 Initialized with a single black (ON) cell at stage zero.
%D A270094 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270094 Robert Price, <a href="/A270094/b270094.txt">Table of n, a(n) for n = 0..127</a>
%H A270094 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270094 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270094 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270094 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270094 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270094 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270094 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270094 code=78; stages=128;
%t A270094 rule=IntegerDigits[code,2,10];
%t A270094 g=2*stages+1; (* Maximum size of grid *)
%t A270094 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270094 ca=a;
%t A270094 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270094 PrependTo[ca,a];
%t A270094 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270094 k=(Length[ca[[1]]]+1)/2;
%t A270094 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270094 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270094 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A270094 Cf. A270091.
