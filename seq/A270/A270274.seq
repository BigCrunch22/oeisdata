%U A270274 3484,625,4032,657,4500,737,5024,789,5612,793,6132,929,6508,1029,7072,1141
%N A270274 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 137", based on the 5-celled von Neumann neighborhood.
%C A270274 Initialized with a single black (ON) cell at stage zero.
%D A270274 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270274 Robert Price, <a href="/A270274/b270274.txt">Table of n, a(n) for n = 0..128</a>
%H A270274 Robert Price, <a href="/A270274/a270274.tmp.txt">Diagrams of the first 20 stages.</a>
%H A270274 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270274 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270274 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270274 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270274 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270274 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270274 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270274 code=137; stages=128;
%t A270274 rule=IntegerDigits[code,2,10];
%t A270274 g=2*stages+1; (* Maximum size of grid *)
%t A270274 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270274 ca=a;
%t A270274 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270274 PrependTo[ca,a];
%t A270274 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270274 k=(Length[ca[[1]]]+1)/2;
%t A270274 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270274 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
