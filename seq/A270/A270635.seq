%I A270635 #9 Feb 16 2025 08:33:31
%S A270635 1,4,32,176,801,3320,13352,53896,215808,864260,3460608,13830736,
%T A270635 55378196,221272552,885845744,3542787408
%N A270635 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 185", based on the 5-celled von Neumann neighborhood.
%C A270635 Initialized with a single black (ON) cell at stage zero.
%D A270635 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270635 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270635 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270635 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270635 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270635 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270635 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270635 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270635 code=185; stages=128;
%t A270635 rule=IntegerDigits[code,2,10];
%t A270635 g=2*stages+1; (* Maximum size of grid *)
%t A270635 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270635 ca=a;
%t A270635 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270635 PrependTo[ca,a];
%t A270635 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270635 k=(Length[ca[[1]]]+1)/2;
%t A270635 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270635 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270635 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270635 Cf. A270634.
%K A270635 nonn,more,changed
%O A270635 0,2
