%I A270717 #9 Feb 16 2025 08:33:32
%S A270717 1,8,40,184,821,3476,13729,55756,224073,899188,3598033,14415009,
%T A270717 57653973,230694612,922702724,3690959937
%N A270717 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 197", based on the 5-celled von Neumann neighborhood.
%C A270717 Initialized with a single black (ON) cell at stage zero.
%D A270717 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270717 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270717 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270717 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270717 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270717 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270717 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270717 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270717 code=197; stages=128;
%t A270717 rule=IntegerDigits[code,2,10];
%t A270717 g=2*stages+1; (* Maximum size of grid *)
%t A270717 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270717 ca=a;
%t A270717 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270717 PrependTo[ca,a];
%t A270717 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270717 k=(Length[ca[[1]]]+1)/2;
%t A270717 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270717 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270717 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270717 Cf. A270716.
%K A270717 nonn,more
%O A270717 0,2
