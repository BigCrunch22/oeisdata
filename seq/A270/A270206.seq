%S A270206 1,4,9,32,17,100,40,164,41,300,89,440,76,636,133,800,173,1088,193,
%T A270206 1288,265,1600,348,1880,373,2324,344,2649,564,2985,564,3505,640,3953,
%U A270206 756,4349,716,5053,716,5497,808,6149,980,6645,1112,7305,1300,7885,1416,8489
%N A270206 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 121", based on the 5-celled von Neumann neighborhood.
%C A270206 Initialized with a single black (ON) cell at stage zero.
%D A270206 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270206 Robert Price, <a href="/A270206/b270206.txt">Table of n, a(n) for n = 0..128</a>
%H A270206 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270206 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270206 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270206 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270206 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270206 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%H A270206 Robert Price, <a href="/A270206/a270206.tmp.txt">Diagrams of the first 20 stages.</a>
%t A270206 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270206 code=121; stages=128;
%t A270206 rule=IntegerDigits[code,2,10];
%t A270206 g=2*stages+1; (* Maximum size of grid *)
%t A270206 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270206 ca=a;
%t A270206 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270206 PrependTo[ca,a];
%t A270206 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270206 k=(Length[ca[[1]]]+1)/2;
%t A270206 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270206 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A270206 nonn,easy,changed
%O A270206 0,2
