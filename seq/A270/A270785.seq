%o A270785 (PARI)
%o A270785 A269750_seq(N, t='t) = {
%o A270785   my(a=vector(N), c(k)=binomial(2*k, k)/(k+1)); a[1]=1; a[2]=t;
%o A270785   for (n = 2, N-1,
%o A270785     a[n+1] = t*a[n] + sum(k = 2, n, (c(k-1)*t+1)*a[n+1-k]));
%o A270785   return(a);
%o A270785 };
