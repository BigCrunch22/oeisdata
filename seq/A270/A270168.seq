%S A270168 1,5,8,20,20,32,48,68,76,80,96,116,140,176,216,268,268,288,304,292,
%T A270168 332,352,408,444,476,512,600,588,724,792,920,1020,1004,1104,1096,1068,
%U A270168 1156,1136,1208,1244,1268,1328,1416,1364,1500,1608,1704,1796,1852,1936
%N A270168 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 110", based on the 5-celled von Neumann neighborhood.
%C A270168 Initialized with a single black (ON) cell at stage zero.
%D A270168 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270168 Robert Price, <a href="/A270168/b270168.txt">Table of n, a(n) for n = 0..128</a>
%H A270168 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270168 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270168 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270168 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270168 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270168 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%H A270168 Robert Price, <a href="/A270168/a270168.tmp.txt">Diagrams of the first 20 stages.</a>
%t A270168 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270168 code=110; stages=128;
%t A270168 rule=IntegerDigits[code,2,10];
%t A270168 g=2*stages+1; (* Maximum size of grid *)
%t A270168 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270168 ca=a;
%t A270168 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270168 PrependTo[ca,a];
%t A270168 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270168 k=(Length[ca[[1]]]+1)/2;
%t A270168 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270168 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A270168 nonn,easy
%O A270168 0,2
