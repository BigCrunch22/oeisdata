%t A270493 Select[Range[1, 10000], Divisible[Fibonacci[#, 2], Prime[#]] &] (* _Vaclav Kotesovec_, Mar 18 2016 *)
%o A270493 (PARI) a000129(n) = ([2, 1; 1, 0]^n)[2, 1];
%o A270493 for(n=1, 1e10, if(lift(Mod(a000129(n), prime(n))) == 0, print1(n, ", ")));
