%S A270931 1,5,37,185,817,3473,14437,59629,239405,962201,3852577,15427713,
%T A270931 61743929,247013121,988192245,3952817017
%N A270931 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 219", based on the 5-celled von Neumann neighborhood.
%C A270931 Initialized with a single black (ON) cell at stage zero.
%D A270931 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A270931 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A270931 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A270931 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A270931 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A270931 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A270931 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A270931 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A270931 code=219; stages=128;
%t A270931 rule=IntegerDigits[code,2,10];
%t A270931 g=2*stages+1; (* Maximum size of grid *)
%t A270931 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A270931 ca=a;
%t A270931 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A270931 PrependTo[ca,a];
%t A270931 (* Trim full grid to reflect growth by one cell at each stage *)
%t A270931 k=(Length[ca[[1]]]+1)/2;
%t A270931 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A270931 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A270931 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A270931 Cf. A270930.
%K A270931 nonn,more,changed
%O A270931 0,2
