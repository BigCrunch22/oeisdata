%o A166267 (Python)
%o A166267 def A000129(n):
%o A166267     if n <=1:
%o A166267         return n
%o A166267     else:
%o A166267         return 2*A000129(n-1)+A000129(n-2)
%o A166267 def A000120(n):
%o A166267     a=0
%o A166267     sh=n
%o A166267     while sh > 0:
%o A166267         a += sh & 1
%o A166267         sh >>= 1
%o A166267     return a
%o A166267 def A166267(n):
%o A166267     return A000120(A000129(n))
