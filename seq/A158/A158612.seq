%t A158612 Clear[HadamardMatrix, c, x, y, z, m, I0, J, K];
%t A158612 MatrixJoinH[A_, B_] := Transpose[Join[Transpose[A], Transpose[B]]];
%t A158612 KroneckerProduct[M_, N_] := Module[{M1, N1, LM, LN, N2},
%t A158612 M1 = M;
%t A158612 N1 = N;
%t A158612 LM = Length[M1];
%t A158612 LN = Length[N1];
%t A158612 Do[M1[[i, j]] = M1[[i, j]]N1, {i, 1, LM}, {j, 1, LM}];
%t A158612 Do[M1[[i, 1]] = MatrixJoinH[M1[[i, 1]], M1[[i, j]]], {j, 2, LM}, {i, 1, LM}];
%t A158612 N2 = {};
%t A158612 Do[AppendTo[N2, M1[[i, 1]]], {i, 1, LM}];
%t A158612 N2 = Flatten[N2];
%t A158612 Partition[N2, LM*LN, LM*LN]]
%t A158612 HadamardMatrix[2] := {{1, 1}, {1, -1}};
%t A158612 HadamardMatrix[4] := {{I0, I0, J, J}, {I0, -1, J, -1}, {K, K, -1, -1}, {K, -1, -1, 1}};
%t A158612 HadamardMatrix[n_] := Module[{m}, m = If[n == 8, {{I0, J}, {K, -1}}, {{1, 1, 1, 1}, {1, -I0, 1, -J}, {1, 1, -1, -1}, {1, -K, -1, 1}}]; KroneckerProduct[m, HadamardMatrix[n/2]]];
%t A158612 c = Table[HadamardMatrix[2^n], {n, 1, 5}] /. K -> 0 /. J -> 0 /. I0 -> 0;
%t A158612 Table[CoefficientList[CharacteristicPolynomial[ c[[n]], z], z], {n, 1, Length[c]}];
%t A158612 Flatten[%]
%t A158612 Table[Apply[Plus, CoefficientList[CharacteristicPolynomial[c[[n]], z], z]], {n, 1, Length[c]}];
