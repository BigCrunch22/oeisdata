%t A158417 Clear[c, b, a, An];
%t A158417 c[i_, k_] := Floor[Mod[i/2^k, 2]];
%t A158417 An[d_] := Table[If[Sum[c[n, k]*c[m, k], {k, 0, d - 1}] == 0, 1, If[Sum[c[n, k]*c[m, k], {k, 0, d - 1}] == 1, -1, 0]], {n, 0, d - 1}, {m, 0, d - 1}];
%t A158417 Table[An[n], {n, 1, 10}];
%t A158417 a = Join[{{1}}, Table[CoefficientList[CharacteristicPolynomial[An[ d], x], x], {d, 1, 10}]] ;
%t A158417 Flatten[a]
%t A158417 RowSum = Table[Apply[Plus, Abs[a[[n]]]], {n, 1, Length[a]}];
