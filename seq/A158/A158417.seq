%N A158417 A triangle sequence from matrix polynomials of a three symbol type {0, 1, -1}: c(i,k)= Floor[Mod[i/2^k, 2]]; M(d)=Table[If[Sum[c(n, k)*c(m, k), {k, 0, d - 1}] == 0, 1, If[Sum[c(n, k)*c(m, k), {k, 0, d - 1}] == 1, -1, 0]], {n, 0, d - 1}, {m, 0, d - 1}].
%C A158417 Row sums are:
%C A158417 {1, 2, 3, 10, 25, 71, 226, 910, 2146, 7264, 21842,...}.
%C A158417 Example matrix:
%C A158417 M(4)={{1, 1, 1, 1},
%C A158417 {1, -1, 1, -1},
%C A158417 {1, 1, -1, -1},
%C A158417 {1, -1, -1, 0}}.
%F A158417 c(i,k)= Floor[Mod[i/2^k, 2]];
%F A158417 m(d)=Table[If[Sum[c(n, k)*c(m, k), {k, 0, d - 1}] == 0, 1, If[Sum[c(n, k)*c(m, k), {k, 0, d - 1}] == 1, -1, 0]], {n, 0, d - 1}, {m, 0, d - 1}];
%F A158417 out_(n,m)=coefficient(characteristicpolynomial(M(n),x),x)
%e A158417 {1},
%e A158417 {1, -1},
%e A158417 {-2, 0, 1},
