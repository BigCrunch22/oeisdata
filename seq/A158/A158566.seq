%t A158566 Clear[HadamardMatrix];
%t A158566 MatrixJoinH[A_, B_] := Transpose[Join[Transpose[A], Transpose[B]]];
%t A158566 KroneckerProduct[M_, N_] := Module[{M1, N1, LM, LN, N2},
%t A158566 M1 = M; N1 = N; LM = Length[M1]; LN = Length[N1];
%t A158566 Do[M1[[i, j]] = M1[[i, j]]N1, {i, 1, LM}, {j, 1, LM}];
%t A158566 Do[M1[[i, 1]] = MatrixJoinH[M1[[i, 1]], M1[[i, j]]], {j, 2, LM}, {i, 1, LM}];
%t A158566 N2 = {}; Do[AppendTo[N2, M1[[i, 1]]], {i, 1, LM}];
%t A158566 N2 = Flatten[N2];
%t A158566 Partition[N2, LM*LN, LM*LN]]
%t A158566 HadamardMatrix[2] := {{1, 1}, {1, I}};
%t A158566 HadamardMatrix[n_] := Module[{m}, m = {{1, 1}, {1, I}}; KroneckerProduct[m, HadamardMatrix[n/2]]];
%t A158566 Table[Im[HadamardMatrix[2^n]], {n, 1, 4}];
%t A158566 Join[{{1}}, Table[CoefficientList[CharacteristicPolynomial[ Im[HadamardMatrix[2^n]], x], x], {n, 1, 6}]];
%t A158566 Flatten[%]
%t A158566 Join[{1}, Table[Apply[Plus, CoefficientList[CharacteristicPolynomial[Im[ HadamardMatrix[2^n]], x], x]], {n, 1, 6}]];
