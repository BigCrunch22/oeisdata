%t A031557 cfep59Q[n_]:=Module[{s=Sqrt[n],cf,len},cf=If[IntegerQ[s],{1}, ContinuedFraction[ s][[2]]];len=Length[cf];EvenQ[len]&&cf[[len/2]] == 59]; Select[Range[15000],cfep59Q] (* _Harvey P. Dale_, Feb 18 2016 *)
