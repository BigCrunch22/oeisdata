%p A254083 with(numtheory):
%p A254083 b:= proc(n, k) option remember; `if`(n=0, 1,
%p A254083       add(mobius(n/d)*k^d, d=divisors(n))/n)
%p A254083     end:
%p A254083 a:= n-> add(b(2*n, n-j)*binomial(n, j)*(-1)^j, j=0..n):
%p A254083 seq(a(n), n=0..20);
