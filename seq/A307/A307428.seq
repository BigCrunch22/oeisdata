%t A307428 nmax = 100; A307423 = Table[DivisorSum[n, Boole[Max[FactorInteger[#][[All, 2]]] < 3] * LiouvilleLambda[n/#]&], {n, 1, nmax}]; Table[DivisorSum[n, MoebiusMu[#] * A307423[[n/#]] &], {n, 1, nmax}]
