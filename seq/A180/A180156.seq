%t A180156 Floor[1/2+Total[#]/Total[Flatten[IntegerDigits/@#]]]&/@ Select[Partition[Prime[Range[500]],2,1], Last[#]-First[#]==2&]  (* _Harvey P. Dale_, Mar 23 2011 *)
