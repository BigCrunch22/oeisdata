%t A030150 altQ[n_] := n < 10 || Union[Total /@ Partition[ Mod[ IntegerDigits@ n, 2], 2, 1]] == {1}; palQ[n_] := Block[{d = IntegerDigits[n]}, d == Reverse@ d]; Select[ Prime@ Range@ 10000, palQ[#] && altQ[#] &] (* _Giovanni Resta_, Aug 16 2018 *)
