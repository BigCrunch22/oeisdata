%p A182804 with(numtheory): A:= proc(n, t) option remember; local d, j; `if`(n=0, 1, add(add(`if`(t=0 or irem(d, t)=0, d-d*t, d), d=divisors(j)) *A(n-j, t), j=1..n)/n) end: seq(A(n,9), n=0..50);
