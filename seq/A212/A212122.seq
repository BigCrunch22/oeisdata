%e A212122 8;
%e A212122 4,   6,  2, 10;
%e A212122 8,   6,  4,  2, 12;
%e A212122 10,  8,  6,  4,  2, 14;
%e A212122 10,  6, 12,  8,  2,  4, 16;
%e A212122 ...
