%o A026551 (Magma) [(3/5)*(-1 + 6^(1+Floor(n/2))*((n+1) mod 2) + 16*6^(Floor((n-1)/2))*(n mod 2)): n in [0..40]]; // _G. C. Greubel_, Apr 12 2022
%o A026551 (SageMath) [(3/5)*(-1 + 6*6^(n/2)*((n+1)%2)  + 16*6^((n-1)/2)*(n%2)) for n in (0..40)] # _G. C. Greubel_, Apr 12 2022
