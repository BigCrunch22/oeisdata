%N A026127 a(n) = T(2n-1,n-1), where T is the array in A026120.
%o A026127 (PARI)
%o A026127 A026300(n,k)={ if(n<0 || k < 0, return(0) ;) ; if(n<=1, 1, if(k==0, 1, sum(i=0,k/2, binomial(n,2*i+n-k)*(binomial(2*i+n-k,i)-binomial(2*i+n-k,i-1))) ;) ;) ; }
%o A026127 A026105(n,k)={ if(k==0 || n<=1, 1, A026300(n,k)-A026300(n-1,k-1) ;) ; }
%o A026127 A026120(n,k)={ if(n<=1, if(k== -1 || k < n, 1, 0), A026105(n+1,k+1)-A026105(n,k) ;) ; }
%o A026127 A026127(n)={ A026120(2*n-1,n-1) ; }
%o A026127 { vector(20, n, A026127(n)) } \\ _R. J. Mathar_, Dec 06 2006
