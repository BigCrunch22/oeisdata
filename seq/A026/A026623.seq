%N A026623 a(n) = Sum_{k=0..floor(n/2)} A026615(n, k).
%H A026623 G. C. Greubel, <a href="/A026623/b026623.txt">Table of n, a(n) for n = 0..1000</a>
%F A026623 a(n) = ( 2*(7*n-24)*(7*n-29)*(n-1)*a(n-1) + 4*(7*n-8)*(7*n-31)*(n-3)*a(n-2) - 8*(7*n-15)*(7*n-24)*(n-4)*a(n-3) - (147*n^3 - 1603*n^2 + 5896*n - 7152))/(n*(7*n-31)*(7*n-22)), for n > 3, with a(0) = a(1) = 1, a(2) = 4, and a(3) = 6. - _G. C. Greubel_, Jun 15 2024
%t A026623 T[n_, k_]:= T[n, k]= If[k==0 || k==n, 1, If[k==1 || k==n-1, 2*n-1, T[n -1, k-1] + T[n-1,k]]]; (* T = A026615 *)
%t A026623 A026623[n_]:= Sum[T[n, k], {k, 0, Floor[n/2]}];
%t A026623 Table[A026623[n], {n,0,40}] (* _G. C. Greubel_, Jun 15 2024 *)
%o A026623 (Magma)
%o A026623 I:=[1,4,6]; [1] cat [n le 3 select I[n] else ( 2*(7*n-24)*(7*n-29)*(n-1)*Self(n-1) + 4*(7*n-8)*(7*n-31)*(n-3)*Self(n-2) - 8*(7*n-15)*(7*n-24)*(n-4)*Self(n-3) - (147*n^3 - 1603*n^2 + 5896*n - 7152))/(n*(7*n-31)*(7*n-22)): n in [1..41]]; // _G. C. Greubel_, Jun 15 2024
%o A026623 (SageMath)
%o A026623 @CachedFunction
%o A026623 def T(n, k): # T = A026615
%o A026623     if k==0 or k==n: return 1
%o A026623     elif k==1 or k==n-1: return 2*n-1
%o A026623     else: return T(n-1, k-1) + T(n-1, k)
%o A026623 def A026623(n): return sum(T(n,k) for k in range((n//2)+1))
%o A026623 [A026623(n) for n in range(41)] # _G. C. Greubel_, Jun 15 2024
