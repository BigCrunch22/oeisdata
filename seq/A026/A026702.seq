%t A026702 T[_, 0] = 1; T[n_, n_] = 1; T[n_, k_] /; EvenQ[k] && 1 <= k/2 <= Floor[n/4] || EvenQ[n - k] && 1 <= (n - k)/2 <= Floor[n/4] := T[n, k] = T[n - 1, k - 1] + T[n - 2, k - 1] + T[n - 1, k]; T[n_, k_] := T[n, k] = T[n - 1, k - 1] + T[n - 1, k];
%t A026702 a[n_] := Sum[T[n - k, k], {k, 0, Floor[n/2]}];
