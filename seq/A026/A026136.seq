%t A026136 Block[{a, nn = 123}, a[1] = 1; Do[If[! IntegerQ[a[#1]], Set[a[#1], i], Set[a[#2], i]] & @@ {i - #, i + #} &@ Floor[i/2], {i, nn}]; TakeWhile[Array[a[#] &, nn], IntegerQ]] (* _Michael De Vlieger_, Apr 16 2020 *)
%o A026136 (Python)
%o A026136 import math
%o A026136 A026136 ={1:1}
%o A026136 for n in range(2,3000):
%o A026136     h=math.floor(n/2)
%o A026136     L=n-h
%o A026136     R=n+h
%o A026136     if not L in A026136 :
%o A026136         A026136[L]=n
%o A026136     else :
%o A026136         A026136[R]=n
%o A026136 for n in range(1,2000):
%o A026136     if n in A026136:
%o A026136        print(str(n) + " "+ str(A026136[n]))
%o A026136     else:
%o A026136        break # _R. J. Mathar_, Aug 26 2019
