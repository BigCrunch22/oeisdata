%p A175721 A175721 := proc(m,k) 1/(-x^m + 1 - x^(1 + m) + x + 3*x^(2 + m) - 2* x^2 - x^(3 + m)) ; coeftayl(%,x=0,k) ; end proc: # _R. J. Mathar_, Dec 22 2010
%t A175721 f[x_, m_] = ExpandAll[(x - x^(m + 1))*(1 - x - x^2) - (1 - 2*x + x^(m + 1))];
%t A175721 g[x_, n_] = ExpandAll[x^(m + 3)*f[1/x, m]];
%t A175721 a = Table[Table[SeriesCoefficient[
%t A175721       Series[1/g[x, m], {x, 0, 10}], n], {n, 0, 10}], {m, 1, 10}];
%t A175721 Table[Table[a[[m, n - m + 1]], {m, 1, n - 1}], {n, 1, 10}]
