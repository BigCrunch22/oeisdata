%e A013927 a(3) = 5:
%e A013927 Start:  {1,1} {2,2} {3,3}
%e A013927 Step 1: {2,1} {1,2} {3,3}
%e A013927 Step 2: {2,1} {3,2} {1,3}
%e A013927 Step 3: {2,3} {1,2} {1,3}
%e A013927 Step 4: {2,3} {3,2} {1,1}
%e A013927 Step 5: {3,3} {2,2} {1,1}
%e A013927 (End)
