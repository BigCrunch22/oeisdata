%o A258878 {a(n)=local(S=x,C=1+x);for(i=1,n+1,S=intformal(C +x*O(x^(3*n)));C=1-intformal(S^2/C +x*O(x^(3*n+1)));); (3*n+1)!*polcoeff(S,3*n+1)}
%o A258878 for(n=0,15,print1(a(n),", "))
