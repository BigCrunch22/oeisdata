%p A258450 seq(a(n), n=0..30);
%t A258450 A[n_, k_] := A[n, k] = If[n==0, 1, Sum[A[n-j, k]*DivisorSum[j, #*Binomial[# +k-1, k-1]&], {j, 1, n}]/n];
%t A258450 T[n_, k_] := Sum[A[n, k-i]*(-1)^i*Binomial[k, i], {i, 0, k}];
%t A258450 a[n_] := Sum[T[n-i, i], {i, 0, n/2}];
