%t A093172 Do[If[PrimeQ[10^n - 3], Print[10^n - 3]], {n, 100}] (* _Farideh Firoozbakht_, Nov 27 2013 *)
%t A093172 Select[Table[FromDigits[PadLeft[{7},n,9]],{n,25}],PrimeQ] (* _Harvey P. Dale_, Dec 12 2020 *)
