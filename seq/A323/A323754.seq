%t A323754 rad[n_] := Times @@ First /@ FactorInteger[n]; pf[n_] := Denominator[n/rad[n]^2]; esigma[n_] := Times @@ (Sum[First[#]^d, {d, Divisors[Last[#]]}] &) /@ FactorInteger[n]; es[n_] := esigma[n] - n; s = {}; Do[m = es[n]; If[m > n && es[m] == n && CoprimeQ[pf[n], pf[m]], AppendTo[s, m]], {n, 1, 10^7}]; s (* after _Jean-Fran√ßois Alcover_ at A055231 and A051377 *)
