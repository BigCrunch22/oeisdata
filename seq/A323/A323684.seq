%N A323684 G.f.: Sum_{n>=0} x^n * ((1+x)^n + 2*i)^n / (1 + 2*i*x*(1+x)^n)^(n+1), where i^2 = -1.
%C A323684 Note that the generating function expands into a power series in x with only real coefficients.
%H A323684 Paul D. Hanna, <a href="/A323684/b323684.txt">Table of n, a(n) for n = 0..300</a>
%F A323684 G.f.: Sum_{n>=0} x^n * ((1+x)^n + 2*i)^n / (1 + 2*i*x*(1+x)^n)^(n+1).
%F A323684 G.f.: Sum_{n>=0} x^n * ((1+x)^n - 2*i)^n / (1 - 2*i*x*(1+x)^n)^(n+1).
%e A323684 G.f.: A(x) = 1 + x + 2*x^2 + 13*x^3 + 40*x^4 + 177*x^5 + 959*x^6 + 4927*x^7 + 28118*x^8 + 173747*x^9 + 1116769*x^10 + 7572422*x^11 + ...
%e A323684 Let r = 2*i, so that r^2 = -4, then
%e A323684 A(x) = 1/(1 + r*x) + x*((1+x) + r)/(1 + r*x*(1+x))^2 + x^2*((1+x)^2 + r)^2/(1 + r*x*(1+x)^2)^3 + x^3*((1+x)^3 + r)^3/(1 + r*x*(1+x)^3)^4 + x^4*((1+x)^4 + r)^4/(1 + r*x*(1+x)^4)^5 + x^5*((1+x)^5 + r)^5/(1 + r*x*(1+x)^5)^6 + ...
%e A323684 also,
%e A323684 A(x) = 1/(1 - r*x) + x*((1+x) - r)/(1 - r*x*(1+x))^2 + x^2*((1+x)^2 - r)^2/(1 - r*x*(1+x)^2)^3 + x^3*((1+x)^3 - r)^3/(1 - r*x*(1+x)^3)^4 + x^4*((1+x)^4 - r)^4/(1 - r*x*(1+x)^4)^5 + x^5*((1+x)^5 - r)^5/(1 - r*x*(1+x)^5)^6 + ...
%o A323684 (PARI) {a(n) = my(r = 2*I, A = sum(m=0,n+1, x^m*((1+x +x*O(x^n))^m + r)^m/(1 + r*x*(1+x +x*O(x^n))^m)^(m+1) )); polcoeff(A,n)}
%o A323684 for(n=0,30,print1(a(n),", "))
%o A323684 (PARI) {a(n) = my(r = 2*I, A = sum(m=0,n+1, x^m*((1+x +x*O(x^n))^m - r)^m/(1 - r*x*(1+x +x*O(x^n))^m)^(m+1) )); polcoeff(A,n)}
%o A323684 for(n=0,30,print1(a(n),", "))
