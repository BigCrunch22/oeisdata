%N A323571 G.f.: Sum_{n>=0} 2^n * ((1+x)^n + i)^n / (3 + 2*i*(1+x)^n)^(n+1), where i^2 = -1.
%C A323571 It is remarkable that the generating function results in a power series in x with only real coefficients.
%F A323571 G.f.: Sum_{n>=0} 2^n * ((1+x)^n + i)^n / (3 + 2*i*(1+x)^n)^(n+1).
%F A323571 G.f.: Sum_{n>=0} 2^n * ((1+x)^n - i)^n / (3 - 2*i*(1+x)^n)^(n+1).
%F A323571 G.f.: Sum_{n>=0} 2^n * ((1+x)^n + i)^n * (3 - 2*i*(1+x)^n)^(n+1) / (9 + 4*(1+x)^(2*n))^(n+1).
%F A323571 G.f.: Sum_{n>=0} 2^n * ((1+x)^n - i)^n * (3 + 2*i*(1+x)^n)^(n+1) / (9 + 4*(1+x)^(2*n))^(n+1).
%e A323571 G.f.: A(x) = 1 + 18*x + 1080*x^2 + 109024*x^3 + 15425932*x^4 + 2807373808*x^5 + 624567442464*x^6 + 164229954020384*x^7 + 49830967023579400*x^8 + ...
%e A323571 such that
%e A323571 A(x) = 1/(3+2*i) + 2*((1+x) + i)/(3 + 2*i*(1+x))^2 + 2^2*((1+x)^2 + i)^2/(3 + 2*i*(1+x)^2)^3 + 2^3*((1+x)^3 + i)^3/(3 + 2*i*(1+x)^3)^4 + 2^4*((1+x)^4 + i)^4/(3 + 2*i*(1+x)^4)^5 + 2^5*((1+x)^5 + i)^5/(3 + 2*i*(1+x)^5)^6 + 2^6*((1+x)^6 + i)^6/(3 + 2*i*(1+x)^6)^7 + ...
%e A323571 also
%e A323571 A(x) = 1/(3-2*i) + 2*((1+x) - i)/(3 - 2*i*(1+x))^2 + 2^2*((1+x)^2 - i)^2/(3 - 2*i*(1+x)^2)^3 + 2^3*((1+x)^3 - i)^3/(3 - 2*i*(1+x)^3)^4 + 2^4*((1+x)^4 - i)^4/(3 - 2*i*(1+x)^4)^5 + 2^5*((1+x)^5 - i)^5/(3 - 2*i*(1+x)^5)^6 + 2^6*((1+x)^6 - i)^6/(3 - 2*i*(1+x)^6)^7 + ...
%e A323571 RELATED INFINITE SERIES.
%e A323571 At x = -1/3, the g.f. as a power series in x diverges, but the related series converges:
%e A323571 S = Sum_{n>=0} 2^n * ((2/3)^n + i)^n / (3 + 2*i*(2/3)^n)^(n+1).
%e A323571 Equivalently,
%e A323571 S = Sum_{n>=0} 6^n * (2^n + 3^n*i)^n / (3^(n+1) + 2^(n+1)*i)^(n+1) ;
%e A323571 written explicitly,
%e A323571 S = 1/(3+2*i) + 6*(2+3*i)/(3^2+2^2*i)^2 + 6^2*(2^2+3^2*i)^2/(3^3+2^3*i)^3
%e A323571 + 6^3*(2^3+3^3*i)^3/(3^4+2^4*i)^4 + 6^4*(2^4+3^4*i)^4/(3^5+2^5*i)^5
%e A323571 + 6^5*(2^5+3^5*i)^5/(3^6+2^6*i)^6 + 6^6*(2^6+3^6*i)^6/(3^7+2^7*i)^7 + ...
%e A323571 which equals the real number
%e A323571 S = 0.398373937215203455798934836530015104419943236574659953950499...
%o A323571 (PARI) {a(n) = my(A = sum(m=0, n*40 + 400, 2^m*((1+x +x*O(x^n))^m + I)^m/(3 + 2*I*(1+x +x*O(x^n))^m)^(m+1)*1. )); round(polcoeff(A, n))}
%o A323571 for(n=0, 20, print1(a(n), ", "))
%o A323571 (PARI) {a(n) = my(A = sum(m=0, n*40 + 400, 2^m*((1+x +x*O(x^n))^m - I)^m/(3 - 2*I*(1+x +x*O(x^n))^m)^(m+1)*1. )); round(polcoeff(A, n))}
%o A323571 for(n=0, 20, print1(a(n), ", "))
