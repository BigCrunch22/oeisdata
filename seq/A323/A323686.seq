%F A323686 G.f.: Sum_{n>=0} x^n * ((1+x)^n + (-1)^n)^n / (1 - (-1)^n*x*(1+x)^n)^(n+1).
%F A323686 G.f.: Sum_{n>=0} x^n * ((1+x)^n - (-1)^n)^n / (1 + (-1)^n*x*(1+x)^n)^(n+1).
%e A323686 G.f.: A(x) = 1 + x + 6*x^2 + 19*x^3 + 98*x^4 + 483*x^5 + 2713*x^6 + 16183*x^7 + 102982*x^8 + 694083*x^9 + 4922791*x^10 + 36609764*x^11 + ...
%e A323686 such that
%e A323686 A(x) = 1/(1 - x) + x*((1+x) - 1)/(1 + x*(1+x))^2 + x^2*((1+x)^2 + 1)^2/(1 - x*(1+x)^2)^3 + x^3*((1+x)^3 - 1)^3/(1 + x*(1+x)^3)^4 + x^4*((1+x)^4 + 1)^4/(1 - x*(1+x)^4)^5 + x^5*((1+x)^5 - 1)^5/(1 + x*(1+x)^5)^6 + ...
%e A323686 also,
%e A323686 A(x) = 1/(1 + x) + x*((1+x) + 1)/(1 - x*(1+x))^2 + x^2*((1+x)^2 - 1)^2/(1 + x*(1+x)^2)^3 + x^3*((1+x)^3 + 1)^3/(1 - x*(1+x)^3)^4 + x^4*((1+x)^4 - 1)^4/(1 + x*(1+x)^4)^5 + x^5*((1+x)^5 + 1)^5/(1 - x*(1+x)^5)^6 + ...
%o A323686 (PARI) {a(n) = my(A = sum(m=0,n+1, x^m*((1+x +x*O(x^n))^m + (-1)^m)^m/(1 - (-1)^m*x*(1+x +x*O(x^n))^m)^(m+1) )); round(polcoeff(A,n))}
%o A323686 for(n=0,30,print1(a(n),", "))
