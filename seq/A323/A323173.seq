%o A323173 A064989(n) = {my(f); f = factor(n); if((n>1 && f[1,1]==2), f[1,2] = 0); for (i=1, #f~, f[i,1] = precprime(f[i,1]-1)); factorback(f)};
%o A323173 A122111(n) = if(1==n,n,prime(bigomega(n))*A122111(A064989(n)));
