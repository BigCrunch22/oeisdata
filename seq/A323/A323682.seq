%N A323682 G.f.: Sum_{n>=0} x^n * ((1+x)^n + sqrt(2)*i)^n / (1 + sqrt(2)*i*x*(1+x)^n)^(n+1), where i^2 = -1.
%C A323682 Note that the generating function expands into a power series in x with only real integer coefficients.
%H A323682 Paul D. Hanna, <a href="/A323682/b323682.txt">Table of n, a(n) for n = 0..300</a>
%F A323682 G.f.: Sum_{n>=0} x^n * ((1+x)^n + sqrt(2)*i)^n / (1 + sqrt(2)*i*x*(1+x)^n)^(n+1).
%F A323682 G.f.: Sum_{n>=0} x^n * ((1+x)^n - sqrt(2)*i)^n / (1 - sqrt(2)*i*x*(1+x)^n)^(n+1).
%e A323682 G.f.: A(x) = 1 + x + 2*x^2 + 9*x^3 + 28*x^4 + 117*x^5 + 547*x^6 + 2671*x^7 + 14258*x^8 + 81335*x^9 + 490525*x^10 + 3124166*x^11 + 20903467*x^12 + ...
%e A323682 Let r = sqrt(2)*i, so that r^2 = -2, then
%e A323682 A(x) = 1/(1 + r*x) + x*((1+x) + r)/(1 + r*x*(1+x))^2 + x^2*((1+x)^2 + r)^2/(1 + r*x*(1+x)^2)^3 + x^3*((1+x)^3 + r)^3/(1 + r*x*(1+x)^3)^4 + x^4*((1+x)^4 + r)^4/(1 + r*x*(1+x)^4)^5 + x^5*((1+x)^5 + r)^5/(1 + r*x*(1+x)^5)^6 + ...
%e A323682 also,
%e A323682 A(x) = 1/(1 - r*x) + x*((1+x) - r)/(1 - r*x*(1+x))^2 + x^2*((1+x)^2 - r)^2/(1 - r*x*(1+x)^2)^3 + x^3*((1+x)^3 - r)^3/(1 - r*x*(1+x)^3)^4 + x^4*((1+x)^4 - r)^4/(1 - r*x*(1+x)^4)^5 + x^5*((1+x)^5 - r)^5/(1 - r*x*(1+x)^5)^6 + ...
%o A323682 (PARI) {a(n) = my(r = sqrt(2)*I, A = sum(m=0,n+1, x^m*((1+x +x*O(x^n))^m + r)^m/(1 + r*x*(1+x +x*O(x^n))^m)^(m+1) )); round(polcoeff(A,n))}
%o A323682 for(n=0,30,print1(a(n),", "))
%o A323682 (PARI) {a(n) = my(r = sqrt(2)*I, A = sum(m=0,n+1, x^m*((1+x +x*O(x^n))^m - r)^m/(1 - r*x*(1+x +x*O(x^n))^m)^(m+1) )); round(polcoeff(A,n))}
%o A323682 for(n=0,30,print1(a(n),", "))
