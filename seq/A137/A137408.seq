%t A137408 Clear[p, x, a] p[x, -1] = 0; p[x, 0] = 1; p[x, 1] = 2*x; p[x_, n_] := p[x, n] = If[Mod[n, 2] == 1, 2*x*p[x, n - 1] - p[x, n - 2], (1 - 2*x)*p[x, n - 1] - p[x, n - 2]]; Table[ExpandAll[p[x, n]], {n, 0, 10}]; a = Table[CoefficientList[p[x, n], x], {n, 0, 10}]; Flatten[a]
