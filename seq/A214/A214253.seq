%p A214253 b:= proc(n, i) option remember; `if`(n<1 or i<1, 0,
%p A214253       `if`(n=i, 1, add(b(n-i, i+j), j=[-2, 0, 2])))
%p A214253     end:
%p A214253 a:= n-> `if`(n=0, 1, add(b(n, j), j=1..n)):
%p A214253 seq(a(n), n=0..60);
%t A214253 b[n_, i_] := b[n, i] = If[n < 1 || i < 1, 0, If[n == i, 1, Sum[b[n-i, i+j], {j, {-2, 0, 2}}]]]; a[n_] := If[n == 0, 1, Sum[b[n, j], {j, 1, n}]]; Table[a[n], {n, 0, 60}] (* _Jean-FranÃ§ois Alcover_, Nov 06 2014, after _Alois P. Heinz_ *)
