%p A308412 SP:= proc(n) option remember; local k;
%p A308412 k:=floor(sqrt(4*n-7)) mod 4;
%p A308412 procname(n-1) -I*exp(I*k*Pi/2)
%p A308412 end proc:
%p A308412 SP(1):= 0:
%p A308412 select(i -> GaussInt:-GIprime(SP(i)), [$1..1000]); # _Robert Israel_, May 20 2024
