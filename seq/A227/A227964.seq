%e A227964 1, -6, 9, 16, -60, 24, 116, -144, -66, 220, -66, -144, 116, 24, -60, 16, 9, -6, 1;
%e A227964 1, -7, 14, 14, -91, 77, 168, -344, -14, 546, -364, -364, 546, -14, -344, 168, 77, -91, 14, 14, -7, 1; ...
%o A227964 (PARI) {T(n,k)=polcoeff((1-x-x^2+x^3 +x*O(x^k))^n,k)}
