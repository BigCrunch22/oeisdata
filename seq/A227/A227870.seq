%o A227870 def aupto(nn):
%o A227870   alst, an = [None], 0
%o A227870   for n in range(1, nn+1):
%o A227870     while len(alst) < nn+1:
%o A227870       if ok(an): alst.append(an)
%o A227870       an += 1
%o A227870   return alst[1:] # use alst[n] for a(n)
