%t A227579 b[l_] := b[l] = If[l[[-1]] == 0, 1, Sum[Sum[b[ReplacePart[l, i -> j]], {j, If[i == 1, 0, l[[i - 1]]], l[[i]] - 1}], {i, 1, Length[l]}]];
%t A227579 a[n_] := If[n == 0, 1, b[Table[n, {n}]]];
