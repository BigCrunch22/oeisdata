%e A227236 G.f.: A(x) = 1 + x + 2*x^2 + 4*x^3 + 8*x^4 + 15*x^5 + 32*x^6 + 64*x^7 +...
%e A227236 where
%e A227236 A(x) = 1 + x*(1+x) + x^2*(1+x)^3 + x^3*(1+x)^4 + x^4*(1+x)^7 + x^5*(1+x)^6 + x^6*(1+x)^12 + x^7*(1+x)^8 + x^8*(1+x)^15 + x^9*(1+x)^13 + x^10*(1+x)^18 +...
%o A227236 (PARI) {a(n)=polcoeff(1+sum(m=1,n,x^m*(1+x+x*O(x^n))^sigma(m)),n)}
%o A227236 for(n=0,40,print1(a(n),", "))
