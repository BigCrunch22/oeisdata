%t A107646 tw = Select[ Prime@ Range@100000, PrimeQ[ # + 2] &]; tw = Flatten /@ IntegerDigits@ Transpose[{tw, tw + 2}]; t = {}; Do[p = FromDigits@ Flatten@ Table[ tw[[i]], {i, n}]; If[ PrimeQ[p], Print[p]], {n, 1090}] (* _Robert G. Wilson v_ *)
