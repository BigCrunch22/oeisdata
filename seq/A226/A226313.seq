%t A226313 b[n_] := b[n] = DivisorSum[n, #*DivisorSigma[1, #]&];
%t A226313 a[n_] := a[n] = If[n == 0, 1, Sum[DivisorSum[j, #*b[#]&]*a[n-j], {j, 1, n}] /n];
