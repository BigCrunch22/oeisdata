%p A226545 b:= proc(n, l) option remember; local i, k, s, t;
%p A226545       if max(l[])>n then [0,0] elif n=0 or l=[] then [1,0]
%p A226545     elif min(l[])>0 then t:=min(l[]); b(n-t, map(h->h-t, l))
%p A226545     else for k do if l[k]=0 then break fi od; s:=[0$2];
%p A226545          for i from k to nops(l) while l[i]=0 do s:=s+(h->h+[0, h[1]])
%p A226545            (b(n, [l[j]$j=1..k-1, 1+i-k$j=k..i, l[j]$j=i+1..nops(l)]))
%p A226545          od; s
%p A226545       fi
%p A226545     end:
%p A226545 A:= (n, k)-> `if`(n>=k, b(n, [0$k]), b(k, [0$n]))[2]:
%p A226545 seq(seq(A(n, d-n), n=0..d), d=0..14);
%t A226545 b[n_, l_List] := b[n, l] = Module[{i, k, s, t}, Which[Max[l] > n, {0, 0}, n == 0 || l == {}, {1, 0}, Min[l] > 0, t=Min[l]; b[n-t, l-t], True, k = Position[l, 0, 1][[1, 1]]; s={0, 0}; For[i=k, i <= Length[l] && l[[i]] == 0, i++, s = s + Function[h, h+{0, h[[1]]}][b[n, Join[l[[1 ;; k-1]], Table[1+i-k, {j, k, i}], l[[i+1 ;; -1]]]]] ]; s]]; a[n_, k_] := If[n >= k, b[n, Array[0&, k]], b[k, Array[0&, n]]][[2]]; Table[Table[a[n, d-n], {n, 0, d}], {d, 0, 14}] // Flatten (* _Jean-FranÃ§ois Alcover_, Dec 13 2013, translated from Maple *)
