%t A120095      If[n == 1, j*x^#, b[n - 1, #]]&[Max[m, j]], {j, 1, m + 1}]];
%t A120095 T[n_] := Table[Coefficient[#, x, i], {i, 1, n}]&[b[n, 0]];
