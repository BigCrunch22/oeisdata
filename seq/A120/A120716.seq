%t A120716 A120716[n_] := Module[{x},
%t A120716    If[n == 1, Return[1]];
%t A120716    If[PrimeQ[n], Return[n]];
%t A120716    x = FromDigits[Flatten[IntegerDigits[Rest[Most[Divisors[n]]]]]];
%t A120716    If[x > 10^50, Return[">10^50"], A120716[x]]];
