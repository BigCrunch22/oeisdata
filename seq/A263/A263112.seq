%F A263112 a(n) = A007570(n) mod n.
%p A263112 F:= n-> (<<0|1>, <1|1>>^n)[1, 2]:
%p A263112 p:= (M, n, k)-> map(x-> x mod k, `if`(n=0, <<1|0>, <0|1>>,
%p A263112           `if`(n::even, p(M, n/2, k)^2, p(M, n-1, k).M))):
%p A263112 a:= n-> p(<<0|1>, <1|1>>, F(n), n)[1, 2]:
%p A263112 seq(a(n), n=1..80);
%t A263112 F[n_] := MatrixPower[{{0, 1}, {1, 1}}, n][[1, 2]];
%t A263112 p[M_, n_, k_] := Mod[#, k]& /@ If[n == 0, {{1, 0}, {0, 1}}, If[EvenQ[n], MatrixPower[p[M, n/2, k], 2], p[M, n - 1, k].M]];
%t A263112 a[n_] := p[{{0, 1}, {1, 1}}, F[n], n][[1, 2]];
%t A263112 Table[a[n], {n, 1, 80}] (* _Jean-Fran√ßois Alcover_, Oct 29 2024, after _Alois P. Heinz_ *)
