%t A263756 b[s_] := b[s] = With[{n = Length[s]}, If[n == 0, 1, Expand[       Sum[b[s~Complement~{j}]*If[j < n, x^j, 1], {j, s}]]]];
%t A263756 T[n_] := CoefficientList[b[Range[n]], x];
