%p A189073 T:= proc(n, k) option remember;
%p A189073       if k=2 then floor((n-1)/2)
%p A189073     elif k>=n then 0
%p A189073     else T(n-1, k) +k/(k-2) *T(n-1, k-1)
%p A189073       fi
%p A189073     end:
%p A189073 seq(seq(T(n, k), k=2..n-1), n=3..13);  # _Alois P. Heinz_, Apr 17 2011
%t A189073 T[n_, k_] := T[n, k] = Which[k == 2, Floor[(n-1)/2], k >= n, 0, True, T[n-1, k] + k/(k-2)*T[n-1, k-1]]; Table[Table[T[n, k], {k, 2, n-1}], {n, 3, 13}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 14 2014, after _Alois P. Heinz_ *)
