%N A189382 a(n) = n + [n*r/t] + [n*s/t]; r=1, s=-1+sqrt(2), t=1+sqrt(2).
%C A189382 See A189380.
%H A189382 G. C. Greubel, <a href="/A189382/b189382.txt">Table of n, a(n) for n = 1..10000</a>
%t A189382 r=1; s=-1+2^(1/2); t=1+2^(1/2);
%t A189382 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A189382 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A189382 h[n_] := n + Floor[n*r/t] + Floor[n*s/t]
%t A189382 Table[f[n], {n, 1, 120}]  (* A189380 *)
%t A189382 Table[g[n], {n, 1, 120}]  (* A189381 *)
%t A189382 Table[h[n], {n, 1, 120}]  (* A189382 *)
%o A189382 (PARI) for(n=1, 100, print1(n + floor(n/(sqrt(2)+1)) + floor(n*(sqrt(2)-1)/(sqrt(2)+1)), ", ")) \\ _G. C. Greubel_, Apr 20 2018
%o A189382 (Magma) [n + Floor(n/(Sqrt(2)+1)) + Floor(n*(Sqrt(2)-1)/(Sqrt(2)+1)): n in [1..100]]; // _G. C. Greubel_, Apr 20 2018
