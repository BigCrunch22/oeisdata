%N A189525 a(n) = n + [n*r/s] + [n*t/s]; r=2, s=1+sqrt(2), t=1+sqrt(3).
%C A189525 See A189524.
%H A189525 G. C. Greubel, <a href="/A189525/b189525.txt">Table of n, a(n) for n = 1..10000</a>
%t A189525 r=2; s=1+2^(1/2); t=1+3^(1/2);
%t A189525 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A189525 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A189525 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A189525 Table[f[n], {n, 1, 120}]  (* A189524 *)
%t A189525 Table[g[n], {n, 1, 120}]  (* A189525 *)
%t A189525 Table[h[n], {n, 1, 120}]  (* A189526 *)
%o A189525 (PARI) for(n=1,100, print1(n + floor(2*n/(sqrt(2)+1)) + floor(n*(1+sqrt(3))/(1+sqrt(2))), ", ")) \\ _G. C. Greubel_, Apr 20 2018
%o A189525 (Magma) [n + Floor(2*n/(Sqrt(2)+1)) + Floor(n*(1+Sqrt(3))/(1+Sqrt(2))): n in [1..100]]; // _G. C. Greubel_, Apr 20 2018
