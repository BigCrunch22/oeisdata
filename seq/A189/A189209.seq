%N A189209 Zero-one sequence based on the sequence of odd primes:  a(A065091(k))=a(k); a(v(k))=1-a(k), where v=A065090 and a(1)=0, a(2)=1.
%t A189209 u[n_] := Prime[n+1];  (*A065091*)
%t A189209 a[1] = 0; a[2]=1; h = 128;
%t A189209 c = (u[#1] &) /@ Range[2h];
%t A189209 d = (Complement[Range[Max[#1]], #1] &)[c];  (*A065090*)
%t A189209 Table[a[d[[n]]] = 1 - a[n], {n, 1, h - 1}]; (*A189209*)
%t A189209 Table[a[c[[n]]] = a[n], {n, 1, h}]   (*A189209*)
%t A189209 Flatten[Position[%, 0]]  (*A189210*)
