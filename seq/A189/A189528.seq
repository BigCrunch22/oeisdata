%N A189528 a(n) = n + [n*r/s] + [n*t/s]; r=1, s=sqrt(3)-sqrt(2), t=sqrt(3)+sqrt(2).
%C A189528 See A189527.
%H A189528 G. C. Greubel, <a href="/A189528/b189528.txt">Table of n, a(n) for n = 1..10000</a>
%t A189528 r=1; s=3^(1/2)-2^(1/2); t=3^(1/2)+2^(1/2);
%t A189528 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A189528 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A189528 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A189528 Table[f[n], {n, 1, 120}]  (* A189527 *)
%t A189528 Table[g[n], {n, 1, 120}]  (* A189528 *)
%t A189528 Table[h[n], {n, 1, 120}]  (* A189529 *)
%o A189528 (PARI) for(n=1,100, print1(n + floor(n/(sqrt(3) - sqrt(2))) + floor(n*(sqrt(3)+sqrt(2))/(sqrt(3)-sqrt(2))), ", ")) \\ _G. C. Greubel_, Apr 20 2018
