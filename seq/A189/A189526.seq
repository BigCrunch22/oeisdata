%N A189526 a(n) = n + [n*r/t] + [n*s/t]; r=2, s=1+sqrt(2), t=1+sqrt(3).
%C A189526 See A189524.
%H A189526 G. C. Greubel, <a href="/A189526/b189526.txt">Table of n, a(n) for n = 1..10000</a>
%t A189526 r=2; s=1+2^(1/2); t=1+3^(1/2);
%t A189526 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A189526 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A189526 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A189526 Table[f[n], {n, 1, 120}]  (* A189524 *)
%t A189526 Table[g[n], {n, 1, 120}]  (* A189525 *)
%t A189526 Table[h[n], {n, 1, 120}]  (* A189526 *)
%o A189526 (PARI) for(n=1,100, print1(n + floor(2*n/(sqrt(3)+1)) + floor(n*(sqrt(2)+1)/(1+sqrt(3))), ", ")) \\ _G. C. Greubel_, Apr 20 2018
%o A189526 (Magma) [n + Floor(2*n/(Sqrt(3)+1)) + Floor(n*(1+Sqrt(2))/(1+Sqrt(3))): n in [1..100]]; // _G. C. Greubel_, Apr 20 2018
