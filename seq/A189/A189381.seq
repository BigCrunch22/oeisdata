%N A189381 a(n) = n + [n*r/s] + [n*t/s]; r=1, s=-1+sqrt(2), t=1+sqrt(2).
%C A189381 (See A189380.)
%H A189381 G. C. Greubel, <a href="/A189381/b189381.txt">Table of n, a(n) for n = 1..10000</a>
%t A189381 r=1; s=-1+2^(1/2); t=1+2^(1/2);
%t A189381 f[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A189381 g[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A189381 h[n_] := n + Floor[n*r/t] + Floor[n*s/t];
%t A189381 Table[f[n], {n, 1, 120}]  (* A189380 *)
