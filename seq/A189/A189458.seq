%t A189458 r=2; s=2^(1/2); t=1+2^(1/2);
%t A189458 a[n_] := n + Floor[n*s/r] + Floor[n*t/r];
%t A189458 b[n_] := n + Floor[n*r/s] + Floor[n*t/s];
%t A189458 c[n_] := n + Floor[n*r/t] + Floor[n*s/t];
