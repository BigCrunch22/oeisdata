%p A054947 A054947 := proc(n)
%p A054947     option remember;
%p A054947     if n = 1 then
%p A054947         1;
%p A054947     else
%p A054947         2^(n*(n-1))-add(binomial(n,t)*2^((n-1)*(n-t))*procname(t),t=1..n-1) ;
