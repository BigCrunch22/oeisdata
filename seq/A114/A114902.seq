%p A114902 a:= n-> b(n, 0):
%p A114902 seq(a(n), n=0..25);  # _Alois P. Heinz_, Sep 04 2015
%t A114902 b[n_, i_] := b[n, i] = If[n==0, 1, Sum[If[i==j, 0, b[n-j, If[j>n-j, 0, j]]* Binomial[n, j]], {j, 1, n}]]; a[n_] := b[n, 0]; Table[a[n], {n, 0, 25}] (* _Jean-Fran√ßois Alcover_, Feb 20 2017, after _Alois P. Heinz_ *)
