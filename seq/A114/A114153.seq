%e A114153 Compare to R^2 (A113392):
%e A114153 1;
%e A114153 6,1;
%e A114153 48,12,1;
%e A114153 605,186,18,1;
%e A114153 11196,3892,414,24,1;
%e A114153 280440,106089,12021,732,30,1; ...
%e A114153 Thus R^-1*P^3 equals R^2 shift right one column.
%o A114153 (PARI) T(n,k)=local(P,Q,R,W);P=Mat(1);for(m=2,n+1,W=matrix(m,m); for(i=1,m, for(j=1,i,if(i<3 || j==i || j>m-1,W[i,j]=1,if(j==1, W[i,1]=1,W[i,j]=(P^(3*j-2))[i-j+1,1]));));P=W); Q=matrix(#P,#P,r,c,if(r>=c,(P^(3*c-1))[r-c+1,1])); R=matrix(#P,#P,r,c,if(r>=c,(P^(3*c))[r-c+1,1])); (R^-1*P^3)[n+1,k+1]
