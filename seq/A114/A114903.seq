%p A114903 b:= proc(n, i) option remember;
%p A114903       `if`(n=0, `if`(i=0, 1, 2), add(`if`(i=j, 0, b(n-j,
%p A114903       `if`(j>n-j, -1, j))*binomial(n, j)), j=0..n))
%p A114903     end:
%p A114903 a:= n-> b(n, -1):
%p A114903 seq(a(n), n=0..25);  # _Alois P. Heinz_, Sep 04 2015
%t A114903 b[n_, i_] := b[n, i] = If[n == 0, If[i == 0, 1, 2], Sum[If[i == j, 0, b[n - j, If[j > n - j, -1, j]]*Binomial[n, j]], {j, 0, n}]]; a[n_] := b[n, -1]; Table[a[n], {n, 0, 25}] (* _Jean-Fran√ßois Alcover_, Feb 25 2017, after _Alois P. Heinz_ *)
