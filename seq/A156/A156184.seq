%e A156184 {1, 46, 4270, 89023, 480380, 825380, 480380, 89023, 4270, 46, 1}
%t A156184 m = 1; e[n_, 0, m_] := 1;
%t A156184 e[n_, k_, m_] := 0 /; k >= n;
%t A156184 e[n_, k_, 1] := 1 /; k >= n;
%t A156184 e[n_, k_, m_] := (k + m - 1)e[n - 1, k, m] + (m*n - k + 1 - m)e[n - 1, k - 1, m];
