%t A156142 p[n_, u_] := D[Tan[x], {x, n}] /. Tan[x] -> u /. Sec[x] -> Sqrt[1+u^2] // Expand; p[-1, u_] = 1; t[n_, k_] := t[n, k] = k*t[n-1, k-1]+(k+1)*t[n-1, k+1]; t[0, 0] = 1; t[0, _] = 0; t[-1, _] = 0; q[n_, u_] := Sum[t[n, k]*u^k, {k, 0, n}]; a[n_] := p[n, 1]+q[n, 1]; Table[a[n], {n, -1, 21}] (* _Jean-Fran√ßois Alcover_, Feb 05 2014 *)
