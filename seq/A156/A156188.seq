%t A156188 m = 5; e[n_, 0, m_] := 1;
%t A156188 e[n_, k_, m_] := 0 /; k >= n;
%t A156188 e[n_, k_, 1] := 1 /; k >= n;
%t A156188 e[n_, k_, m_] := (k + m - 1)e[n - 1, k, m] + (m*n - k + 1 - m)e[n - 1, k - 1, m];
