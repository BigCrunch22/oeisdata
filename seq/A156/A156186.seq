%t A156186 m = 3; e[n_, 0, m_] := 1;
%t A156186 e[n_, k_, m_] := 0 /; k >= n;
%t A156186 e[n_, k_, 1] := 1 /; k >= n;
%t A156186 e[n_, k_, m_] := (k + m - 1)e[n - 1, k, m] + (m*n - k + 1 - m)e[n - 1, k - 1, m];
