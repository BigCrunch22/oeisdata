%t A156825 t[n_, m_] = If[m == 0, n!, Product[Sum[(m + 1)^i, {i, 0, k - 1}], {k, 1, n}]];
%t A156825 b[n_, k_, m_] = If[n == 0, 1, t[n, m]/(t[k, m]*t[n - k, m])];
%t A156825 t1[n_, k_, q_] = (1/(q - 1)^k)*Sum[(-1)^(k - j)* Binomial[k + n, k - j]*b[j + n, j, q - 1], {j, 0, k}];
%t A156825 Table[Flatten[Table[Table[t1[n, k, m + 1], {k, 0, n}], {n, 0, 10}]], {m, 1, 15}]
