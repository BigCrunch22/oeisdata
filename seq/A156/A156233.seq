%e A156233 {1, 656682190, 9977604269, 46096675274, 96031672538, 117399194772, 96031672538, 46096675274, 9977604269, 656682190, 1}
%t A156233 Clear[e, n, k, m]; m = 4; e[n_, 0, m_] := 1;
%t A156233 e[n_, k_, m_] := 0 /; k >= n; e[n_, k_, 1] := 1 /; k >= n;
%t A156233 e[n_, k_, m_] := (2*k + m - 1)e[n - 1, k, m] + (m*n - 2*k + 1 - m)e[n - 1, k - 1, m];
%t A156233 Table[Table[e[n, k, m], {k, 0, n - 1}], {n, 1, 10}];
%t A156233 Flatten[%];
