%t A156824 t[n_, m_] = If[m == 0, n!, Product[Sum[(m + 1)^i, {i, 0, k - 1}], {k, 1, n}]];
%t A156824 b[n_, k_, m_] = If[n == 0, 1, t[n, m]/(t[k, m]*t[n - k, m])];
%t A156824 t1[n_, k_, q_] = (1/(q - 1)^k)*Sum[(-1)^(k - j)* Binomial[k + n, k - j]*b[j + n, j, q - 1], {j, 0, k}];
%t A156824 Table[Flatten[Table[Table[t1[n, k, m + 1], {k, 0, n}], {n, 0, 10}]], {m, 1, 15}]
%t A156824 (* Second program: *)
%t A156824 (* S stands for qStirling2 *)
%t A156824 S[n_, k_, q_] /; 1 <= k <= n := S[n-1, k-1, q] + Sum[q^j, {j, 0, k-1}]* S[n-1, k, q];
%t A156824 S[n_, 0, _] := KroneckerDelta[n, 0];
%t A156824 S[0, k_, _] := KroneckerDelta[0, k];
%t A156824 S[_, _, _] = 0;
%t A156824 Table[S[n+k, n, 3], {n, 0, 7}, {k, 0, n}] // Flatten (* _Jean-FranÃ§ois Alcover_, Mar 09 2020 *)
