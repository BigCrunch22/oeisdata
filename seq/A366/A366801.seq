%o A366801 (PARI)
%o A366801 A003415(n) = if(n<=1, 0, my(f=factor(n)); n*sum(i=1, #f~, f[i, 2]/f[i, 1]));
%o A366801 A003557(n) = (n/factorback(factorint(n)[, 1]));
%o A366801 A005940(n) = { my(p=2, t=1); n--; until(!n\=2, if((n%2), (t*=p), p=nextprime(p+1))); (t); };
