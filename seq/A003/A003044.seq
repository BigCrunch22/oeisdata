%t A003044 a[n_ /; n <= 4] = n; a[n_] := a[n] = Catch[ For[an = a[n-1] + 1, True, an++, cnt = 0; Do[If[an == a[i] + a[j], cnt++], {i, 1, n-1}, {j, i+1, n-1}]; If[cnt == 2, Throw[an]]]]; Table[a[n], {n, 1, 56}](* _Jean-FranÃ§ois Alcover_, Apr 30 2012 *)
