%e A338817   0 :       1
%e A338817   1 :      -1      1
%e A338817   2 :      -1      0      1
%e A338817   3 :      -3      1      1      1
%e A338817   4 :     -12      4      5      2     1
%e A338817   5 :     -60     20     25     11     3     1
%e A338817   6 :    -360    120    150     66    19     4    1
%e A338817   7 :   -2520    840   1050    462   133    29    5   1
%e A338817   8 :  -20160   6720   8400   3696  1064   232   41   6  1
%e A338817   9 : -181440  60480  75600  33264  9576  2088  369  55  7  1
%e A338817 etc.
%o A338817 (PARI) for(n=0,10,for(k=0,n,if(k==n,print(" 1"),if(k==n-1,print1(n-2,", "),print1((k^2+k-1)*n!/(k+2)!,", ")))))
