%e A338179 G.f.: A(x) = 1 + 2*x + 5*x^2 + 18*x^3 + 78*x^4 + 389*x^5 + 2171*x^6 + 13293*x^7 + 88234*x^8 + 628645*x^9 + 4772159*x^10 + 38367795*x^11 + 325108942*x^12 + ...
%e A338179 where
%e A338179 A(x) = 1 + x*(1 + (1+x)) + x^2*(1 + (1+x)^2)*(1 + (1+x)^3) + x^3*(1 + (1+x)^3)*(1 + (1+x)^4)*(1 + (1+x)^5) + x^4*(1 + (1+x)^4)*(1 + (1+x)^5)*(1 + (1+x)^6)*(1 + (1+x)^7) + x^5*(1 + (1+x)^5)*(1 + (1+x)^6)*(1 + (1+x)^7)*(1 + (1+x)^8)*(1 + (1+x)^9) + ... + x^n*Product_{k=0..n-1} (1 + (1+x)^(n+k)) + ...
%e A338179 Also
%e A338179 A(x) = 1/(1 - x)  +  x*(1+x)/((1 - x*(1+x))*(1 - x*(1+x)^2))  +  x^2*(1+x)^5/((1 - x*(1+x)^2)*(1 - x*(1+x)^3)*(1 - x*(1+x)^4))  +  x^3*(1+x)^12/((1 - x*(1+x)^3)*(1 - x*(1+x)^4)*(1 - x*(1+x)^5)*(1 - x*(1+x)^6))  +  x^4*(1+x)^22/((1 - x*(1+x)^4)*(1 - x*(1+x)^5)*(1 - x*(1+x)^6)*(1 - x*(1+x)^7)*(1 - x*(1+x)^8)) + ... + x^n*(1+x)^(n*(3*n-1)/2)/(Product_{k=0..n} 1 - x*(1+x)^(n+k)) + ...
%t A338179 nmax = 25; CoefficientList[Series[Sum[x^n * Product[1 + (1+x)^(n+k), {k,0,n-1}], {n, 0, nmax-1}], {x, 0, nmax}], x] (* _Vaclav Kotesovec_, Oct 15 2020 *)
