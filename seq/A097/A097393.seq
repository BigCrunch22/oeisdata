%p A097393     if R <> n then
%p A097393         if numtheory[bigomega](R) = 2 and numtheory[bigomega](n) = 2 then
%p A097393             return true;
%p A097393         else
%p A097393             false;
%p A097393         end if;
%p A097393       else
%p A097393         false;
%p A097393     end if;
%p A097393 end proc:
