%p A215521 b:= proc(n, i) option remember; `if`(n=0, {1}, `if`(i<1, {},
%p A215521       {b(n, i-1)[], seq(map(x-> x*i!^j, b(n-i*j, i-1))[], j=1..n/i)}))
%p A215521     end:
%p A215521 T:= (n, k)-> nops(b(n-k, min(k, n-k))):
%p A215521 seq(seq(T(n, k), k=1..n), n=1..15);
%t A215521 b[n_, i_] := b[n, i] = If[n == 0, {1}, If[i < 1, {}, Join[b[n, i - 1], Table[ b[n - i*j, i - 1] *i!^j, {j, 1, n/i}] // Flatten]] // Union]; T[n_, k_] := Length[b[n, k]]; Table[Table[T[n - k, Min[k, n - k]], {k, 1, n}], {n, 1, 15}] // Flatten (* _Jean-FranÃ§ois Alcover_, Jan 21 2015, after _Alois P. Heinz_ *)
