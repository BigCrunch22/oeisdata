%p A215561 b:= proc(l) option remember; local m, n, g;
%p A215561       m, n:= nops(l), add(i, i=l);
%p A215561       g:= add(i*l[i], i=1..m)-(m+1)/2*(n-1);
%p A215561      `if`(n<2, 1, add(`if`(l[i]>0 and i<=g,
%p A215561         b(subsop(i=l[i]-1, l)), 0), i=1..m))
%p A215561     end:
%p A215561 A:= (n, k)-> b([k$n]):
%p A215561 seq(seq(A(n, d-n), n=0..d), d=0..10);
%t A215561 b[l_] := b[l] = Module[{m, n, g}, {m, n} = {Length[l], Total[l]}; g = Sum[i*l[[i]], {i, 1, m}] - (m+1)/2*(n-1); If[n < 2, 1, Sum[If[l[[i]] > 0 && i <= g, b[ReplacePart[l, i -> l[[i]] - 1]], 0], {i, 1, m}]]]; a[n_, k_] := b[Array[k&, n]]; Table [Table [a[n, d-n], {n, 0, d}], {d, 0, 9}] // Flatten (* _Jean-Fran√ßois Alcover_, Dec 06 2013, translated from Maple *)
