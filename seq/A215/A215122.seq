%p A215122 b:= proc(x, y, z) option remember; `if`(z<y, b(x, z, y),
%p A215122       `if`({x, y, z}={0}, 1, `if`(x>y and x>z, b(x-1, y, z), 0)+
%p A215122       `if`(y>0, b(x, y-1, z), 0)+ `if`(z>0, b(x, y, z-1), 0)))
%p A215122     end:
%p A215122 T:= (n, k)-> `if`(k=0 xor k=n, 0, b((n-k)*k, k, n-k)):
%p A215122 seq(seq(T(n, k), k=0..n), n=0..10);
%t A215122 b[x_, y_, z_] := b[x, y, z] = If[z<y, b[x, z, y], If[Union[{x, y, z}] == {0}, 1, If[x>y && x>z, b[x-1, y, z], 0] + If[y>0, b[x, y-1, z], 0] + If[z>0, b[x, y, z-1], 0]]]; T[n_, k_] := If[k == 0 || k == n, 0, b[(n-k)*k, k, n-k]]; T[0, 0] = 1; Table[Table[T[n, k], {k, 0, n}], {n, 0, 10}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 19 2015, after _Alois P. Heinz_ *)
