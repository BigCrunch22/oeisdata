%p A309055 S1:= 0: S2:= 0: Recv:= NULL: count:= 0:
%p A309055 maxv:= 0:
%p A309055 for n from 1 to 200000 do
%p A309055   p:= ithprime(n);
%p A309055   S1:= S1 + p;
%p A309055   S2:= S2 + n*p;
%p A309055   v:= igcd(S1, S2);
%p A309055   if v > maxv then
%p A309055     maxv:= v;
%p A309055     count:= count+1;
%p A309055     Recv:= Recv,v;
%p A309055   fi
%p A309055 od:
%p A309055 Recv;
