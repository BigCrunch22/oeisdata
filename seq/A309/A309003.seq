%o A309003 (PARI) sopfr(f) = f[, 1]~*f[, 2];
%o A309003 isCarmichael(n, f)= bittest(n, 0) && !for(i=1, #f~, (f[i, 2]==1 && n%(f[i, 1]-1)==1)||return) && (#f~>1);
