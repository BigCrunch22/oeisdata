%t A309996 b[n_, k_] := b[n, k] = If[n < 2, n, Sum[b[n - j, k]*Sum[b[d, k]*k*d*(-1)^(j/d+1), {d, Divisors[j]}], {j, 1, n-1}]/(n-1)];
%t A309996 a[n_] := Sum[b[2*n+1, n-i]*(-1)^i*Binomial[n, i], {i, 0, n}];
