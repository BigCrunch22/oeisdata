%e A103373 a(22) = 9 because a(22) = a(22-5) + a(22-6) = a(17) + a(16) = 5 + 4 = 9.
%t A103373 k = 5; Do[a[n] = 1, {n, k + 1}]; a[n_] := a[n] = a[n - k] + a[n - k - 1]; Array[a, 65]
%t A103373 RecurrenceTable[{a[n] == a[n - 5] + a[n - 6], a[1] == a[2] == a[3] == a[4] == a[5] == a[6] == 1}, a, {n, 65}] (* or *)
