%t A369557 nmax = 100; CoefficientList[Series[-1 + 2*Sum[x^(k^2) * Product[1 + x^(2*j), {j, 1, k}]^2, {k, 0, Sqrt[nmax]}] + Sum[x^((k-1)*k) * Product[1 + x^(2*j-1), {j, 1, k}]^2, {k, 0, Sqrt[nmax] + 1}], {x, 0, nmax}], x] (* _Vaclav Kotesovec_, Sep 27 2024 *)
