%e A369647            k   factorization           max.exp         k'    A049345(k')
%e A369647            1                              0,           0,            0
%e A369647            2 = 2^1,                       1,           1,            1
%e A369647            9 = 3^2,                       2,           6,          100
%e A369647           16 = 2^4,                       4,          32,         1010
%e A369647          108 = 2^2 * 3^3,                 3,         216,        10100
%e A369647         9024 = 2^6 * 3 * 47,              6,       30272,      1011010
