%p A078649     option remember;
%p A078649     if n = 1 then
%p A078649         2;
%p A078649     else
%p A078649         for a from procname(n-1)+1 do
%p A078649             if isA078649(a) then
%p A078649                 return a;
%p A078649             end if;
%p A078649         end do:
%p A078649     end if;
%p A078649 end proc:
