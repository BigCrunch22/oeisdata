%o A099997 def A099997(n):
%o A099997     def f(x): return int((n<<1)-3+x+sum(mobius(k)*(integer_nthroot(x,k)[0]-1) for k in range(2,x.bit_length())))
%o A099997     kmin, kmax = 1,2
