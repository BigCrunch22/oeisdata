%t A099042 b[n_] := b[n] = If[n == 1, 0, Product[If[n > d^2, 1, 1 - b[d]], {d, Select[Range[n - 1], Mod[n, #] == 0&]}]]; (* b is A099104 *)
%t A099042 a[n_] := Sum[b[d], {d, Divisors[n]}];
%t A099042 Array[a, 105] (* _Jean-Fran√ßois Alcover_, Dec 06 2021 *)
