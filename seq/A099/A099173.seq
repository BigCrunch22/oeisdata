%t A099173 A[k_, n_]:= Which[k==0, n, n==0, 0, True, ((1+Sqrt[k])^n - (1-Sqrt[k])^n)/(2 Sqrt[k])]; Table[A[k-n, n]//Simplify, {k, 0, 12}, {n, 0, k}]//Flatten (* _Jean-François Alcover_, Jan 21 2019 *)
%o A099173 (PARI) A(k,n)=sum(i=0,n\2,k^i*binomial(n,2*i+1))
%o A099173 (Magma)
%o A099173 A099173:= func< n,k | (&+[n^j*Binomial(k,2*j+1): j in [0..Floor(k/2)]]) >;
%o A099173 [A099173(n,k): k in [0..n], n in [0..12]]; // _G. C. Greubel_, Feb 17 2023
%o A099173 (SageMath)
%o A099173 def A099173(n,k): return sum( n^j*binomial(k, 2*j+1) for j in range((k//2)+1) )
