%e A374277   [3, 2, 1, 1, 1]
%e A374277   [4, 3, 2, 1, 1]
%t A374277 a[0]=1; a[n_]:=Min[Table[Permanent[ToeplitzMatrix[Join[{1}, Part[Permutations[Range[n - 1]], i]]]], {i, (n-1)!}]]; Array[a, 11, 0]
