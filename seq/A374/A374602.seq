%o A374602 (PARI) row(n, c)=my(v=List(), d=n+floor(sqrt(n)+1/2) /* d=A000037(n) */, t=ceil(sqrt(d))); while(#v<c, my(b=floor(sqrt(t^2/d))); if ((t^2-d*b^2)%(b*2+1)==0, listput(v, t)); t++); concat(v)
%o A374602 for(n=1, 20,  print(n, " ", row(n, 10)))
