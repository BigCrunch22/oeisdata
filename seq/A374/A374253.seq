%t A374253 stc[n_]:=Differences[Prepend[Join @@ Position[Reverse[IntegerDigits[n,2]],1],0]]//Reverse;
%t A374253 Select[Range[0,100],!UnsameQ@@First/@Split[stc[#]]&]
