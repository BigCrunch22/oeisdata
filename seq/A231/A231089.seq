%t A231089 AbundantQ[n_] := DivisorSigma[1, n] > 2n; m = 0; a = {}; Do[If[AbundantQ[n], m = m + 1; If[m > 3, AppendTo[a, n - 6]], m = 0], {n, 2, 1000000, 2}];a
