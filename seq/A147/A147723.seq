%t A147723 T[0,0] = T[1,1]= T[2,2] = 1; T[1,0] = T[2,0] = 0; T[2,1] = 3; T[n_, k_]:= If[k<0 || k>n, 0, 7 T[n - 1, k] + T[n - 1, k - 1] - 3 T[n - 2, k] - 4 T[n - 2, k - 1]]; Flatten[Table[ T[n, k], {n, 0, 8}, {k, 0, n}]] (* _Indranil Ghosh_, Mar 10 2017, after _Philippe Deléham_ *)
%o A147723 (PARI) T(n, k)= if(n==k, 1, if(k==0, 0, if(n==2 && k==1, 3, if(k<0 || k>n, 0, 7*T(n - 1, k) + T(n - 1, k - 1) - 3*T(n - 2, k) - 4*T(n - 2, k - 1)))));
%o A147723 {for(n=0, 8, for(k=0, n, print1(T(n,k),", ");); print();); } \\ _Indranil Ghosh_, Mar 10 2017
