%p A340987       (q-> add(b(j, q)*b(n-j, k-q), j=0..n))(iquo(k, 2)))
%p A340987     end:
%p A340987 a:= n-> b(n$2):
%p A340987 seq(a(n), n=0..25);
%t A340987 b[n_, k_] := b[n, k] = If[k<2, PartitionsP[n+1], With[{q = Quotient[k, 2]}, Sum[b[j, q] b[n-j, k-q], {j, 0, n}]]];
%t A340987 a[n_] := b[n, n];
