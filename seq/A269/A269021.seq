%p A269021 h:= proc(l) (n-> add(i, i=l)!/mul(mul(1+l[i]-j+add(`if`(
%p A269021       l[k]>=j, 1, 0), k=i+1..n), j=1..l[i]), i=1..n))(nops(l))
%p A269021     end:
%p A269021 g:= (n, i, l)-> `if`(n=0 or i=1, h([l[], 1$n])^2, `if`(i<1, 0,
%p A269021                  add(g(n-i*j, i-1, [l[], i$j]), j=0..n/i))):
%p A269021 a:= n-> `if`(n=0, 1, (2*n)!-g(2*n, n-1, [])):
%p A269021 seq(a(n), n=0..16);
%t A269021 h[l_] := Function[n, Total[l]!/Product[Product[1+l[[i]]-j+Sum[If[l[[k]] >= j, 1, 0], {k, i+1, n}], {j, 1, l[[i]]}], {i, 1, n}]][Length[l]];
%t A269021 g[n_, i_, l_] := If[n == 0 || i == 1, h[Join[l, Table[1, {n}]]]^2, If[i < 1, 0, Sum[g[n - i*j, i - 1, Join[l, Table[i, {j}]]], {j, 0, n/i}]]];
%t A269021 a[n_] := If[n == 0, 1, (2n)! - g[2n, n-1, {}]];
%t A269021 Table[a[n], {n, 0, 16}] (* _Jean-FranÃ§ois Alcover_, Apr 01 2017, translated from Maple *)
