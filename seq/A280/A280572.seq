%N A280572 E.g.f. satisfies: A(x - Integral A(x) dx) = x + Integral 4*A(x) dx.
%F A280572 E.g.f. A(x) satisfies:
%F A280572 (1) A(x - Integral A(x) dx) = x + Integral 4*A(x) dx.
%F A280572 (2) A(x) = x + 5 * G( (A(x) + 4*x)/5 ), where G(x) = Integral A(x) dx.
%F A280572 (3) A(x) = -4*x + 5 * Series_Reversion(x - Integral A(x) dx).
%F A280572 (4) A( (A(x) + 4*x)/5 ) = (A'(x) - 1)/(A'(x) + 4).
%F A280572 (5) A'(x - Integral A(x) dx) = (1 + 4*A(x))/(1 - A(x)).
%F A280572 a(n) = Sum_{k=0..n-1} A277410(n,k) * 5^(n-k-1).
%e A280572 E.g.f.: A(x) = x + 5*x^2/2! + 40*x^3/3! + 525*x^4/4! + 10025*x^5/5! + 253475*x^6/6! + 8015725*x^7/7! + 305359050*x^8/8! + 13645726250*x^9/9! + 701304298375*x^10/10! + 40822454374125*x^11/11! + 2658840618527250*x^12/12! +...
%e A280572 Let G(x) = Integral A(x) dx, then A(x - G(x)) = x + 4*G(x) where
%e A280572 G(x) = x^2/2! + 5*x^3/3! + 40*x^4/4! + 525*x^5/5! + 10025*x^6/6! + 253475*x^7/7! + 8015725*x^8/8! + 305359050*x^9/9! + 13645726250*x^10/10! + 701304298375*x^11/11! + 40822454374125*x^12/12! +...
%e A280572 Also, A(x) = x + 5 * G( (A(x) + 4*x)/5 ).
%e A280572 RELATED SERIES.
%e A280572 We have (A(x) + 4*x)/5 = Series_Reversion( x - Integral A(x) dx ), where
%e A280572 (A(x) + 4*x)/5 = x + x^2/2! + 8*x^3/3! + 105*x^4/4! + 2005*x^5/5! + 50695*x^6/6! + 1603145*x^7/7! + 61071810*x^8/8! + 2729145250*x^9/9! + 140260859675*x^10/10! + 8164490874825*x^11/11! + 531768123705450*x^12/12! +...
%e A280572 Further, A( (A(x) + 4*x)/5 ) = (A'(x) - 1)/(A'(x) + 4), which begins
%e A280572 A( (A(x) + 4*x)/5 ) = x + 6*x^2/2! + 63*x^3/3! + 1045*x^4/4! + 24105*x^5/5! + 716195*x^6/6! + 26137820*x^7/7! + 1134457060*x^8/8! + 57203895725*x^9/9! + 3292221321425*x^10/10! + 213282348138700*x^11/11! + 15380885339509825*x^12/12! +...
%t A280572 m = 23; A[_] = 0;
%t A280572 Do[A[x_] = -4 x + 5 InverseSeries[x - Integrate[A[x], x] + O[x]^m], {m}];
%t A280572 CoefficientList[A[x], x] * Range[0, m-1]! // Rest (* _Jean-Fran√ßois Alcover_, Sep 30 2019 *)
%o A280572 (PARI) /* A(x) = x + (p+q)*G((p*A(x) + q*x)/(p+q)) ; G(x) = Integral A(x) dx: */
%o A280572 {a(n, p=1, q=4) = my(A=x, G); for(i=1, n, G = intformal(A +x*O(x^n)); A = x + (p+q)*subst(G, x, (p*A + q*x)/(p+q)) +x*O(x^n)); n!*polcoeff(A, n)}
%o A280572 for(n=1, 30, print1(a(n, 1, 4), ", "))
%o A280572 (PARI) /* A(x - Integral p*A(x) dx) = x + Integral q*A(x) dx: */
%o A280572 {a(n, p=1, q=4) = my(A=[1], F=x); for(i=1, n, A=concat(A, 0); F=x*Ser(A); G=intformal(F); A[#A] = -polcoeff(subst(F, x, x - p*G) - q*G, #A) ); n!*A[n]}
%o A280572 for(n=1, 30, print1(a(n, 1, 4), ", "))
%o A280572 (PARI) /* Informal code to generate the first N terms: */
%o A280572 {N=20; p=1; q=4; A=x; for(i=1, N, G=intformal(A +x*O(x^N)); A = x + (p+q)*subst(G, x, (p*A + q*x)/(p+q))); Vec(serlaplace(A))}
