%S A280337 1,0,7,2,29,8,119,42,261,496,575,962,5629,2632,24951,32170,35589,
%T A280337 51184,387647,152514,1611261,2067016,2220407,3825578,24617221,
%U A280337 11090416,114122303,98478018,44374525,975435336,389819767,3419085738,3164075269,5627457008
%N A280337 Decimal representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 249", based on the 5-celled von Neumann neighborhood.
%C A280337 Initialized with a single black (ON) cell at stage zero.
%D A280337 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A280337 Robert Price, <a href="/A280337/b280337.txt">Table of n, a(n) for n = 0..126</a>
%H A280337 Robert Price, <a href="/A280337/a280337.tmp.txt">Diagrams of first 20 stages</a>
%H A280337 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A280337 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A280337 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A280337 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A280337 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A280337 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A280337 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A280337 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A280337 code = 249; stages = 128;
%t A280337 rule = IntegerDigits[code, 2, 10];
%t A280337 g = 2 * stages + 1; (* Maximum size of grid *)
%t A280337 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A280337 ca = a;
%t A280337 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A280337 PrependTo[ca, a];
%t A280337 (* Trim full grid to reflect growth by one cell at each stage *)
%t A280337 k = (Length[ca[[1]]] + 1)/2;
%t A280337 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A280337 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 2], {i ,1, stages - 1}]
