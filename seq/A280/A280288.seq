%e A280288 10 + 4 + 1 = 15
%e A280288 10 + 3 + 2 = 15
%e A280288 ...
%t A280288 Select[Range[4500], Mod[PartitionsP[#1], 2] == 0 && Mod[PartitionsQ[#1], 2] == 1 & ]
