%o A377102 (PARI) \\ using formula (1)
%o A377102 {a(n) = my(V=[0,1]); for(i=1,n, V=concat(V,0); A=Ser(V);
%o A377102 V[#V] = polcoef(x - sum(m=1,#V, subst(A,x, x^m*(1 - 2*x) +x*O(x^#V))),#V-1) ); polcoef(A,n)}
%o A377102 for(n=1,30,print1(a(n),", "))
%o A377102 (PARI) \\ using formula (2)
%o A377102 {a(n) = my(V=[0,1]); for(i=1,n, V=concat(V,0); A=Ser(V);
%o A377102 V[#V] = polcoef(x - sum(m=1,#V-1, V[m+1]*x^m*(1-2*x)^m/(1-x^m +O(x^#V))),#V-1) ); polcoef(A,n)}
%o A377102 for(n=1,30,print1(a(n),", "))
