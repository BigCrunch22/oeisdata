%o A377292 (Python)
%o A377292 A048726 = lambda n: (n << 2) ^ (n << 1)
%o A377292 def a(n):
%o A377292     if n == 0: return 0
%o A377292     f, s = 1, n
%o A377292     while s > 1:
%o A377292         f = A048726(f) | (s & 1)
%o A377292         s >>= 1
%o A377292     return f
%o A377292 print([a(n) for n in range(0, 52)])
