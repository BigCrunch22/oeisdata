%t A377020 expQ[n_] := expQ[n] = Module[{m = n, k = 2}, While[Divisible[m, k], m /= k; k++]; m < k]; q[n_] := AllTrue[FactorInteger[n][[;;, 2]], expQ]; Select[Range[100], q]
