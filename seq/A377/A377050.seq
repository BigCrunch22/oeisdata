%t A377050 nn=10000;
%t A377050 u=Table[Differences[Select[Range[nn],!SquareFreeQ[#]&],k],{k,2,16}];
%t A377050 mnrm[s_]:=If[Min@@s==1,mnrm[DeleteCases[s-1,0]]+1,0];
%t A377050 m=Table[Position[u[[k]],0][[1,1]],{k,mnrm[Union[First/@Position[u,0]]]}]
