%t A327380 b[n_, i_, k_] := b[n, i, k] = If[n == 0, 1, If[i < 1, 0, Sum[Function[t, b[t, Min[t, i - 1], k]*Binomial[k, j]][n - i*j], {j, 0, Min[k, n/i]}]]];
%t A327380 a[n_] := With[{k = 2}, Sum[b[n, n, k-i]*(-1)^i*Binomial[k, i], {i, 0, k}]];
