%t A327870 b[n_, i_, k_] := b[n, i, k] = If[i*(i + 1)/2 < n, 0,
%t A327870      If[n == 0, 1, If[i < 2, 0, b[n, i - 1, If[i == k, 0, k]]] +
%t A327870      If[i == k, 0, b[n - i, Min[n - i, i - 1], k]*Binomial[n, i]]]];
%t A327870 a[n_] := b[n, n, 0]*(n + 1) - Sum[b[n, n, k], {k, 1, n}];
