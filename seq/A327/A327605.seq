%t A327605 b[n_, i_, k_] := b[n, i, k] = With[{}, If[n == 0, Return@{1, 0}]; If[k == 0, Return@{1, 1}]; If[i (i + 1)/2 < n, Return@{0, 0}]; b[n, i - 1, k] + Function[h, Function[f, f + {0, f[[1]] h[[2]]/h[[1]]}][h[[1]] b[n - i, Min[n - i, i - 1], k]]][b[i, i, k - 1]]];
%t A327605 a[n_] := b[n, n, 2][[2]];
