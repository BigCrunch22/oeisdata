%N A248053 G.f.: Sum_{n>=0} x^n / (1-4*x)^(2*n+1) * [Sum_{k=0..n} C(n,k)^2 * 2^k * x^k] * [Sum_{k=0..n} C(n,k)^2 * 4^k * x^k].
%C A248053  Compare this sequence to its dual, A249921.
%F A248053 G.f.: Sum_{n>=0} x^n / (1-x)^(2*n+1) * [Sum_{k=0..n} C(n,k)^2 * x^k] * [Sum_{k=0..n} C(n,k)^2 * 4^(n-k) * 2^k * x^k].
%F A248053 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k)^2 * Sum_{j=0..k} C(k,j)^2 * 4^(k-j) * 2^j * x^j.
%F A248053 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k)^2 * 4^(n-k) * Sum_{j=0..k} C(k,j)^2 * 2^j * x^j.
%F A248053 a(n) = Sum_{k=0..[n/2]} 2^k * Sum_{j=0..n-2*k} C(n-k, k+j)^2 * C(k+j, j)^2 * 4^j.
%F A248053 a(n) ~ sqrt(24+17*sqrt(2)) * (6+4*sqrt(2))^n / (4*Pi*n). - _Vaclav Kotesovec_, Oct 04 2014
%e A248053 G.f.: A(x) = 1 + 5*x + 35*x^2 + 285*x^3 + 2519*x^4 + 23545*x^5 +...
%e A248053 where the g.f. is given by the binomial series identity:
%e A248053 A(x) = 1/(1-4*x) + x/(1-4*x)^3 * (1 + 2*x) * (1 + 4*x)
%e A248053 + x^2/(1-4*x)^5 * (1 + 2^2*2*x + 4*x^2) * (1 + 2^2*4*x + 16*x^2)
%e A248053 + x^3/(1-4*x)^7 * (1 + 3^2*2*x + 3^2*4*x^2 + 8*x^3) * (1 + 3^2*4*x + 3^2*16*x^2 + 64*x^3)
%e A248053 + x^4/(1-4*x)^9 * (1 + 4^2*2*x + 6^2*4*x^2 + 4^2*8*x^3 + 16*x^4) * (1 + 4^2*4*x + 6^2*16*x^2 + 4^2*64*x^3 + 2561*x^4)
%e A248053 + x^5/(1-4*x)^11 * (1 + 5^2*2*x + 10^2*4*x^2 + 10^2*8*x^3 + 5^2*16*x^4 + 32*x^5) * (1 + 5^2*4*x + 10^2*16*x^2 + 10^2*64*x^3 + 5^2*256*x^4 + 1024*x^5) +...
%e A248053 equals the series
%e A248053 A(x) = 1/(1-x) + x/(1-x)^3 * (1 + x) * (4 + 2*x)
%e A248053 + x^2/(1-x)^5 * (1 + 2^2*x + x^2) * (16 + 2^2*4*2*x + 4*x^2)
%e A248053 + x^3/(1-x)^7 * (1 + 3^2*x + 3^2*x^2 + x^3) * (64 + 3^2*16*2*x + 3^2*4*4*x^2 + 8*x^3)
%e A248053 + x^4/(1-x)^9 * (1 + 4^2*x + 6^2*x^2 + 4^2*x^3 + x^4) * (256 + 4^2*64*2*x + 6^2*16*4*x^2 + 4^2*4*8*x^3 + 16*x^4)
%e A248053 + x^5/(1-x)^11 * (1 + 5^2*x + 10^2*x^2 + 10^2*x^3 + 5^2*x^4 + x^5) * (1024 + 5^2*256*2*x + 10^2*64*4*x^2 + 10^2*16*8*x^3 + 5^2*4*16*x^4 + 32*x^5) +...
%e A248053 We can also express the g.f. by another binomial series identity:
%e A248053 A(x) = 1 + x*(4 + (1+2*x)) + x^2*(16 + 2^2*4*(1+2*x) + (1+2^2*2*x+4*x^2))
%e A248053 + x^3*(64 + 3^2*16*(1+2*x) + 3^2*4*(1+2^2*2*x+4*x^2) + (1+3^2*2*x+3^2*4*x^2+8*x^3))
%e A248053 + x^4*(256 + 4^2*64*(1+2*x) + 6^2*16*(1+2^2*2*x+4*x^2) + 4^2*4*(1+3^2*2*x+3^2*4*x^2+8*x^3) + (1+4^2*2*x+6^2*4*x^2+4^2*8*x^3+16*x^4))
%e A248053 + x^5*(1024 + 5^2*256*(1+2*x) + 10^2*64*(1+2^2*2*x+4*x^2) + 10^2*16*(1+3^2*2*x+3^2*4*x^2+8*x^3) + 5^2*4*(1+4^2*2*x+6^2*4*x^2+4^2*8*x^3+16*x^4) + (1+5^2*2*x+10^2*4*x^2+10^2*8*x^3+5^2*16*x^4+32*x^5)) +...
%e A248053 equals the series
%e A248053 A(x) = 1 + x*(1 + (4+2*x)) + x^2*(1 + 2^2*(4+2*x) + (16+2^2*4*2*x+4*x^2))
%e A248053 + x^3*(1 + 3^2*(4+2*x) + 3^2*(16+2^2*4*2*x+4*x^2) + (64+3^2*16*2*x+3^2*4*4*x^2+8*x^3))
%e A248053 + x^4*(1 + 4^2*(4+2*x) + 6^2*(16+2^2*4*2*x+4*x^2) + 4^2*(64+3^2*16*2*x+3^2*4*4*x^2+8*x^3) + (256+4^2*64*2*x+6^2*16*4*x^2+4^2*8*4*x^3+16*x^4))
%e A248053 + x^5*(1 + 5^2*(4+2*x) + 10^2*(16+2^2*4*2*x+4*x^2) + 10^2*(64+3^2*16*2*x+3^2*4*4*x^2+8*x^3) + 5^2*(256+4^2*64*2*x+6^2*16*4*x^2+4^2*8*4*x^3+16*x^4) + (1024+5^2*256*2*x+10^2*64*4*x^2+10^2*16*8*x^3+5^2*4*16*x^4+32*x^5)) +...
%t A248053 Table[Sum[2^k * Sum[Binomial[n-k, k+j]^2 * Binomial[k+j, j]^2 * 4^j,{j,0,n-2*k}],{k,0,Floor[n/2]}],{n,0,20}] (* _Vaclav Kotesovec_, Oct 04 2014 *)
%o A248053 (PARI) /* By definition: */
%o A248053 {a(n,p=4,q=2)=local(A=1); A=sum(m=0, n, x^m/(1-p*x)^(2*m+1) * sum(k=0, m, binomial(m, k)^2 * q^k * x^k) * sum(k=0, m, binomial(m,k)^2 * p^k * x^k) +x*O(x^n)); polcoeff(A, n)}
%o A248053 for(n=0, 25, print1(a(n,4,2), ", "))
%o A248053 (PARI) /* By a binomial identity: */
%o A248053 {a(n,p=4,q=2)=local(A=1); A=sum(m=0, n, x^m/(1-x)^(2*m+1) * sum(k=0, m, binomial(m, k)^2 * p^(m-k) * q^k * x^k) * sum(k=0, m, binomial(m, k)^2 * x^k) +x*O(x^n)); polcoeff(A, n)}
%o A248053 for(n=0, 25, print1(a(n,4,2), ", "))
%o A248053 (PARI) /* By a binomial identity: */
%o A248053 {a(n,p=4,q=2)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2 * sum(j=0, k, binomial(k, j)^2 * p^(k-j) * q^j * x^j)+x*O(x^n))), n)}
%o A248053 for(n=0, 25, print1(a(n,4,2), ", "))
%o A248053 (PARI) /* By a binomial identity: */
%o A248053 {a(n,p=4,q=2)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2 * p^(m-k) * sum(j=0, k, binomial(k, j)^2 * q^j * x^j)+x*O(x^n))), n)}
%o A248053 for(n=0, 25, print1(a(n,4,2), ", "))
%o A248053 (PARI) /* Formula for a(n): */
%o A248053 {a(n,p=4,q=2)=sum(k=0, n\2, sum(j=0, n-2*k, q^k * binomial(n-k, k+j)^2 * binomial(k+j, j)^2 * p^j))}
%o A248053 for(n=0, 25, print1(a(n,4,2), ", "))
