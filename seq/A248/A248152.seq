%N A248152 a(n) = 48 * 4^n * (2*n-1)!! * (2*n+3)!! / ((n+2)! * (n+4)!).
%C A248152 a(-1) = -2, a(-2) = 1/2 makes some formulas true for all n in Z.
%H A248152 G. C. Greubel, <a href="/A248152/b248152.txt">Table of n, a(n) for n = 0..200</a>
%F A248152 a(n) = C(n)*C(n+3) - C(n+1)*C(n+2) for n>=0 where C() is the Catalan numbers A000108.
%F A248152 a(n)*a(n+1) = 4*A005700(n)*A005700(n+2) for all n in Z.
%F A248152 a(n) = 24 * (2*n)! * (2*n+3)! / ( n! * (n+1)! * (n+2)! * (n+4)! ) for n>=0.
%F A248152 (n+4)*(n+2)*a(n) -4*(2*n+3)*(2*n-1)*a(n-1)=0.
%F A248152 0 = a(n)*(+3840*a(n+3) - 660*a(n+4)) + a(n+1)*(+256*a(n+2) + 144*a(n+3) + 15*a(n+4)) + a(n+2)*(+4*a(n+2) + a(n+3)) for all n in Z.
%F A248152 0 = a(n)^2*(+196608*a(n+1)^2 - 14336*a(n+1)*a(n+2) - 25872*a(n+2)^2) + a(n)*a(n+1)*(+67584*a(n+1)^2 + 16672*a(n+1)*a(n+2) - 56*a(n+2)^2) + a(n+1)^2*(+1008*a(n+1)^2 + 264*a(n+1)*a(n+2) + 3*a(n+2)^2) for all n in Z.
%e A248152 G.f. = 3 + 4*x + 14*x^2 + 72*x^3 + 462*x^4 + 3432*x^5 + 28314*x^6 + ...
%t A248152 a[ n_] := 48 4^n (2 n - 1)!! * (2 n + 3)!! / ((n + 2)! (n + 4)!);
%o A248152 (PARI) {a(n) = if( n<0, if( n<-2, 0, [-2, 1/2][-n]), 24 * (2*n)! * (2*n+3)! / ( n! * (n+1)! * (n+2)! * (n+4)! ))};
%o A248152 (Magma) [24*Factorial(2*n)*Factorial(2*n+3)/(Factorial(n)*Factorial(n+1)* Factorial(n+2)*Factorial(n+4)): n in [0..30]]; // _G. C. Greubel_, Aug 04 2018
