%e A277410 APPLICATION.
%e A277410 Given F(x) such that
%e A277410 F(x - Integral p*F(x) dx) = x + Integral q*F(x) dx
%e A277410 then
%e A277410 F(x) = Sum_{n>=1} a(n)*x^n/n!
%e A277410 where
%e A277410 a(n) = Sum_{k=0..n-1} A277410(n,k) * p^k * (p+q)^(n-k-1) for n>=1.
%e A277410 EXAMPLES.
%e A277410 A210949(n) = Sum_{k=0..n-1} A277410(n,k).
%e A277410 A277403(n) = Sum_{k=0..n-1} A277410(n,k) * 2^(n-k-1).
%e A277410 A279843(n) = Sum_{k=0..n-1} A277410(n,k) * 3^(n-k-1).
%e A277410 A279844(n) = Sum_{k=0..n-1} A277410(n,k) * 2^k * 3^(n-k-1).
%e A277410 A279845(n) = Sum_{k=0..n-1} A277410(n,k) * 2^k.
%e A277410 A280570(n) = Sum_{k=0..n-1} A277410(n,k) * 4^(n-k-1).
%e A277410 A280571(n) = Sum_{k=0..n-1} A277410(n,k) * 3^k * 4^(n-k-1).
%e A277410 A280572(n) = Sum_{k=0..n-1} A277410(n,k) * 5^(n-k-1).
