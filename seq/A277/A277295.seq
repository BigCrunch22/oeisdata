%t A277295 c[n_] := c[n] = Module[{A}, A[x_] = x; Do[A[x_] = x + A[y A[x] + (1-y) x + x O[x]^j]^2, {j, n}] // Normal; SeriesCoefficient[A[x], {x, 0, n}] // Expand];
%t A277295 T[n_, k_] := SeriesCoefficient[c[n], {y, 0, k}];
