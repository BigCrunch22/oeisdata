%p A277359 a:= proc(n) option remember; `if`(n<3, [1, 2, 7][n+1],
%p A277359       ((n^3+10*n^2-10*n+1)*a(n-1)-(2*(4*n^3+2*n^2-29*n+28))
%p A277359         *a(n-2)+(4*(n-2))*(2*n-3)^2*a(n-3))/(n*(n+1)))
%p A277359     end:
%p A277359 seq(a(n), n=0..25);
