%C A144178 (..., 9*9=81, 9*10=90, 10*2=20, 10*3=30, 10*4=40, 10*5=50, 10*6=60, 10*7=70, 10*8=80, 10*9=90, 10*10=100, 11*2=22, ...) becomes
%C A144178 (abs(..., 9*9, 81*9, 10*90, 10*2, 20*10, 3*30, 10*4, 40*10, 5*50, 10*6, 60*10, 7*70, 10*8, 80*10, 9*90, 10*10, 100*11, 2*22, ...)).
%e A144178 2*2  =  4 = a(1),
%e A144178 4*2  =  8 = a(2),
%e A144178 3*6  = 18 = a(3),
%e A144178 2*4  =  8 = a(4),
