%p A144067 with(numtheory): etr:= proc(p) local b; b:=proc(n) option remember; `if`(n=0, 1, add(add(d*p(d), d=divisors(j)) *b(n-j), j=1..n)/n) end end: a:=n-> etr(j->3^j)(n): seq(a(n), n=0..40);
%t A144067 etr[p_] := Module[{b}, b[n_] := b[n] = If[n == 0, 1, Sum[Sum[d*p[d], {d, Divisors[j]}]*b[n - j], {j, 1, n}]/n]; b]; a[n_] := etr[Function[3^#]][n]; Table[a[n], {n, 0, 40}] (* _Jean-Fran√ßois Alcover_, Mar 09 2015, after _Alois P. Heinz_ *)
