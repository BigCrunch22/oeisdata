%p A144451 A144451 := proc(n,k)
%p A144451     option remember;
%p A144451     if k < 1 or k > n then
%p A144451         0 ;
%p A144451     elif n = 1 or n=k then
%p A144451         1;
%p A144451     else
%p A144451         procname(n-2,k-1)-2*procname(n,k-1)+1 ;
%p A144451     end if;
