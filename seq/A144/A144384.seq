%t A144384 f[t_, n_] = If[n == 1, 1/(1 - t), (1 - t)/(1 - t^n)];
%t A144384 a = Table[Table[SeriesCoefficient[Series[f[t, m], {t, 0, 30}], n], {n, 0, 30}], {m, 1, 31}];
%t A144384 Flatten[Table[Table[a[[n - m + 1]][[m]], {m, 1, n }], {n, 1, 15}]]
%o A144384 (Maxima)(nn : 15, kk : 50)$
%o A144384 gf(n) := taylor(if n = 1 then 1/(1 - x) else (1 - x)/(1 - x^n), x, 0, kk)$
%o A144384 T(n,k) := ratcoef(gf(n), x, k)$
%o A144384 create_list(T(n - k, k), n, 1, nn, k, 0, n - 1);
