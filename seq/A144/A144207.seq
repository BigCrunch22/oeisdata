%p A144207 T:= proc(n,k) option remember; if k=0 then 1 elif k<0 or n<k then 0 elif k=n then binomial(n-1,2) *n^(n-3) else T(n-1,k) +add(binomial(n-1,j) * T(j+1,j+1) *T(n-1-j,k-j-1), j=2..k-1) fi end: seq(seq(T(n, k), k=0..n), n=0..11);
%t A144207 t[n_, k_] := t[n, k] = Which[k == 0, 1, k < 0 || n < k, 0, k == n, Binomial[n-1, 2]*n^(n-3), True, t[n-1, k] + Sum[Binomial[n-1, j]*t[j+1, j+1]*t[n-1-j, k-j-1], {j, 2, k-1}]]; Table[Table[t[n, k], {k, 0, n}], {n, 0, 11}] // Flatten (* _Jean-FranÃ§ois Alcover_, Dec 13 2013, translated from Maple *)
