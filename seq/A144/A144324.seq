%p A144324 with(numtheory): dc:= proc(b,c) proc(n) option remember; add(b(d) *c(n/d), d=`if`(n<0,{},divisors(n))) end end: A:= proc(n, k) local a, b, t; b[1]:= dc(a,a); for t from 2 to k do b[t]:= dc(b[t-1],b[t-1]) od: a:= n-> `if`(n=1, 1, b[k](n-1)); a(n) end: seq(seq(A(n, 1+d-n), n=1..d), d=1..11);
%t A144324 dc[b_, c_] := Module[{proc}, proc[n_] := proc[n] = Sum [b[d] *c[n/d], {d, If[n < 0, {}, Divisors[n]]}]; proc]; A [n_, k_] := Module[{a, b, t}, b[1] = dc[a, a]; For[t = 2, t <= k, t++, b[t] = dc[b[t-1], b[t-1]]]; a = Function[m, If[m == 1, 1, b[k][m-1]]]; a[n]]; Table[Table[A[n, 1+d-n], {n, 1, d}], {d, 1, 11}] // Flatten (* _Jean-FranÃ§ois Alcover_, Dec 20 2013, translated from Maple *)
