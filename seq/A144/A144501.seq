%F A144501 a(n) = ( 3*(3*n-4)*(3*n-8)*(9*n^2 - 21*n + 11)*a(n-1) + 3*(3*n-2)*a(n-2) - (3*n-5)*(3*n-2)*a(n-3) )/(2*(3*n-5)*(3*n-8)), with a(0) = 1, a(1) = 2, a(3) = 30. - _G. C. Greubel_, Oct 09 2023
%t A144501 a[n_]:= a[n]= If[n==0, 1, If[n<3, 2*(15)^(n-1), (3*(3*n-4)*(3*n-8)*(9*n^2- 21*n+11)*a[n-1] +3*(3*n-2)*a[n-2] -(3*n-5)*(3*n-2)*a[n-3])/(2*(3*n-5)*(3*n-8))]];
%t A144501 Table[a[n], {n,0,30}] (* _G. C. Greubel_, Oct 09 2023 *)
%o A144501 (Magma) I:=[1,2,30]; [n le 3 select I[n] else ( 3*(3*n-7)*(3*n-11)*(9*n^2 - 39*n + 41)*Self(n-1) + 3*(3*n-5)*Self(n-2) - (3*n-8)*(3*n-5)*Self(n-3) )/(2*(3*n-8)*(3*n-11)): n in [1..30]]; // _G. C. Greubel_, Oct 09 2023
