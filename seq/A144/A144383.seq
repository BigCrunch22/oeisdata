%t A144383 f[t_, n_] = 1/(t^n + t + 1);
%t A144383 a = Table[Table[SeriesCoefficient[Series[f[t, m], {t, 0, 30}], n], {n, 0, 30}], {m, 1, 31}];
%t A144383 Flatten[Table[Table[a[[n - m + 1]][[m]], {m, 1, n }], {n, 1, 15}]]
%o A144383 (Maxima) (nn : 12, kk : 50)$
%o A144383 gf(n) := taylor(1/(x^n + x + 1), x, 0, kk)$
%o A144383 T(n, k) := ratcoef(gf(n), x, k)$
%o A144383 create_list(T(n - k, k), n, 1, nn, k, 0, n - 1);
