%p A144647 A001515 := proc(n) simplify(hypergeom([n+1,-n],[],-1/2)) ; end: A144647 := proc(n) if n =0 then A001515(n) ; else A001515(n+1)-2*A001515(n)+A001515(n-1) ; fi; end: seq(A144647(n),n=0..30) ; # _R. J. Mathar_, Feb 01 2009
