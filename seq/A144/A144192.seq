%p A144192 pflat := proc(nmax) local a, ifs, n, p, c ; a := [0,1] ; for n from 2 to nmax do if not isprime(n) then ifs := ifactors(n)[2] ; for p in ifs do a := [op(a),op(1,p)] ; if op(2,p) > 1 then a := [op(a),op(2,p)] ; fi; od: fi; od: a ; end: L := pflat(300) ; for n from 1 to nops(L)-2 by 2 do printf("%d,", op(n,L) + op(n+1,L) ) ; end do: # _R. J. Mathar_, Apr 29 2010
