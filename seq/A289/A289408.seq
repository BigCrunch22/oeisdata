%S A289408 1,2,7,14,29,63,127,255,499,1023,2039,4091,8191,16371,32767,65531,
%T A289408 130823,262139,524087,1048523,2097143,4194107,8388519,16777147,
%U A289408 33554367,67108795,134217663,268435131,536870847,1073741755,2147483327,4294967227,8589871039
%N A289408 Decimal representation of the diagonal from the corner to the origin of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 569", based on the 5-celled von Neumann neighborhood.
%C A289408 Initialized with a single black (ON) cell at stage zero.
%D A289408 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A289408 Robert Price, <a href="/A289408/b289408.txt">Table of n, a(n) for n = 0..126</a>
%H A289408 Robert Price, <a href="/A289408/a289408.tmp.txt">Diagrams of first 20 stages</a>
%H A289408 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A289408 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A289408 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A289408 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A289408 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A289408 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A289408 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A289408 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A289408 code = 569; stages = 128;
%t A289408 rule = IntegerDigits[code, 2, 10];
%t A289408 g = 2 * stages + 1; (* Maximum size of grid *)
%t A289408 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A289408 ca = a;
%t A289408 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A289408 PrependTo[ca, a];
%t A289408 (* Trim full grid to reflect growth by one cell at each stage *)
%t A289408 k = (Length[ca[[1]]] + 1)/2;
%t A289408 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A289408 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
