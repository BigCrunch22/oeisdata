%o A122941 (PARI) /* Get T(n,k) from H(n,), the n-th self-composition of x+x^2: */
%o A122941 {H(n,p)=local(F=x+x^2, G=x+x*O(x^p));if(n==0,G=x,for(i=1,n,G=subst(F,x,G));G)}
%o A122941 {T(n,k)=round(polcoeff( sum(i=0,6*n+100,H(i,k+n-1)^n/2^(i+1)),k+n-1))}
