%N A122838 Triangular array read by rows: row n gives coefficients of p(n)(x), where p(0)=1, p(1)=x-1, p(n) = (x-fibonacci(n))*p(n-1)-(n-1)*p(n-2).
%e A122838 {1},
%e A122838 {-1, 1},
%e A122838 {0, -2, 1},
%e A122838 {2, 2, -4, 1},
%e A122838 {-6, 2, 11, -7, 1},
%e A122838 {22, -24, -37, 42, -12, 1},
%e A122838 {-146, 204, 217, -338, 133, -20,1},
%e A122838 {1766, -2654, -2395, 4359, -1995, 387, -33, 1},
%e A122838 {-36064, 56072, 46122, -91568, 45323, -9982, 1073, -54, 1},
%e A122838 {1212048, -1921280, -1492916, 3124562, -1616590, 381615, -46200, 2901, -88, 1},
%p A122838 p:=proc(n) option remember; if n = 0 then RETURN(1); fi; if n = 1 then RETURN(x-1); fi; sort(expand((x-fibonacci(n))*p(n-1)-(n-1)*p(n-2))); end; (_N. J. A. Sloane_)
