%e A122385 10^2 mod 7 = 2 <> 7^2 mod 10 = 9,
%e A122385 10^2 mod 8 = 4 = 8^2 mod 10, therefore a(10) = 8.
%t A122385 s={};Do[m=0;Until[Mod[n^2,m]==Mod[m^2,n],m++];AppendTo[s,m] ,{n,79}];s (* _James C. McMahon_, Oct 27 2024 *)
