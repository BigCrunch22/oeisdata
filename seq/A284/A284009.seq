%t A284009 a[p_?PrimeQ] := a[p] = Prime[ PrimePi[p] + 1]; a[1] = 1; a[n_] := a[n] = Times @@ ( a[First[#]] ^ Last[#] & ) /@ FactorInteger[n]; (* after _Jean-Fran√ßois Alcover_, in A003961 *) A[n_]:= If[n<2, n + 1, If[EvenQ[n], a[A[n/2]], A[(n - 1)/2] A[(n + 1)/2]]] ;Table[A[n], {n, 0, 51}]  (* sequence A260443 *) Table[PrimeOmega[LCM[A[n], A[n + 1]]], {n, 0, 101}] (* _Indranil Ghosh_, Mar 22 2017 *)
%o A284009 (PARI)
%o A284009 A003961(n) = my(f = factor(n)); for (i=1, #f~, f[i, 1] = nextprime(f[i, 1]+1)); factorback(f); \\ From _Michel Marcus_
%o A284009 A260443(n) = if(n<2, n+1, if(n%2, A260443(n\2)*A260443(n\2+1), A003961(A260443(n\2))));
%o A284009 A284008(n) = lcm(A260443(n),A260443(1+n));
%o A284009 A284009(n) = bigomega(A284008(n));
