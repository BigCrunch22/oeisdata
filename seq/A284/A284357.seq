%S A284357 1,3,5,13,21,53,81,219,321,867,1349,3397,5445,13765,20613,55749,83685,
%T A284357 223325,350405,877741,1390013,3491401,5344191,14291369,21109439,
%U A284357 57027793,88607355,223353093,360197903,899704459,1353758813,3654849763,5469747127,14630081865
%N A284357 Decimal representation of the x-axis, from the left edge to the origin, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 902", based on the 5-celled von Neumann neighborhood.
%C A284357 Initialized with a single black (ON) cell at stage zero.
%D A284357 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A284357 Robert Price, <a href="/A284357/b284357.txt">Table of n, a(n) for n = 0..126</a>
%H A284357 Robert Price, <a href="/A284357/a284357.tmp.txt">Diagrams of first 20 stages</a>
%H A284357 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A284357 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A284357 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A284357 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A284357 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A284357 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A284357 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A284357 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A284357 code = 902; stages = 128;
%t A284357 rule = IntegerDigits[code, 2, 10];
%t A284357 g = 2 * stages + 1; (* Maximum size of grid *)
%t A284357 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A284357 ca = a;
%t A284357 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A284357 PrependTo[ca, a];
%t A284357 (* Trim full grid to reflect growth by one cell at each stage *)
%t A284357 k = (Length[ca[[1]]] + 1)/2;
%t A284357 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A284357 Table[FromDigits[Part[ca[[i]] [[i]], Range[1, i]], 2], {i, 1, stages - 1}]
