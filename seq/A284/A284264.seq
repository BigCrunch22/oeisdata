%t A284264 A003961[p_?PrimeQ] := A003961[p] = Prime[ PrimePi[p] + 1]; A003961[1] = 1; A003961[n_] := A003961[n] = Times @@ ( A003961[First[#]] ^ Last[#] & ) /@ FactorInteger[n]  (* after _Jean-FranÃ§ois Alcover_, Dec 01 2011 *); A260443[n_]:= If[n<2, n + 1, If[EvenQ[n], A003961[A260443[n/2]], A260443[(n - 1)/2] * A260443[(n + 1)/2]]]; A000188[n_]:= Sum[Boole[Mod[i^2, n] == 0], {i, n}]; Table[PrimeOmega[A000188[A260443[n]]], {n, 0, 120}] (* _Indranil Ghosh_, Mar 28 2017 *)
