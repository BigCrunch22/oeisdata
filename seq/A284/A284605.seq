%p A284605     print(n,A064986(n!)) ;
%p A284605 end do:
%t A284605 A064986[n_] := Module[{x, d, g = 1}, For[i = 1, True, i++, g = g/(1 - x^(i!)); g = Series[g, {x, 0, n+1}]; If[i! > n, Break[]]]; Return[ SeriesCoefficient[g, {x, 0, n}]]];
