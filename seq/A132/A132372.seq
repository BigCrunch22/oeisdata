%p A132372         C[j] := C[j-1] + C[j+1] * A[j-1] od;
%p A132372     T[m] := [seq(coeff(C[2], x, j), j = 0..k)];
%p A132372     m := m + 1
%p A132372 od; ListTools:-Flatten(T) end:
