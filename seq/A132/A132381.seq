%p A132381 b:= proc(n, i) option remember; local j; if n=0 then [1, 0] elif i<1
%p A132381       then [0$2] else b(n, i-1); for j to n/i do zip((x, y)->x+y, %,
%p A132381       [`if`(isprime(i), 0, NULL), b(n-i*j, i-1)[]], 0) od; %[1..2] fi
