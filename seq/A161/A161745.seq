%p A161745     end:
%p A161745 a:= n-> b((<<0|1>, <1|1>>^n)[1,2]):
%p A161745 seq(a(n), n=0..10);  # _Alois P. Heinz_, May 08 2020
%t A161745 lst = {};
%t A161745 Do[AppendTo[lst, Subfactorial[Fibonacci[n]]], {n, 0, 2*3!}];
