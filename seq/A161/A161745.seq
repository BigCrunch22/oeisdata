%t A161745 (* Second program: *)
%t A161745 b[n_] := b[n] = If[n == 0, 1, n*b[n-1] + (-1)^n];
%t A161745 a[n_] := b[MatrixPower[{{0, 1}, {1, 1}}, n][[1, 2]]];
