%t A161674 tm[0] = 0; tm[n_?EvenQ] := tm[n] = tm[n/2]; tm[n_] := tm[n] = 1 - tm[(n - 1)/2]; Reap[For[n = 0, n <= 6000, n++, If[tm[n] + tm[n + 2] == 1, Sow[n]]]][[2, 1]] (* _G. C. Greubel_, Jan 05 2018 *)
