%N A326098 E.g.f.: Sum_{n>=0} (n+1) * (1 + x^n)^n * x^n/n!
%F A326098 E.g.f.: Sum_{n>=0} (n+1) * (1 + x^n)^n * x^n/n!.
%F A326098 E.g.f.: Sum_{n>=1} (n + x^n) * exp(x^n) * x^(n*(n-1))/(n-1)!.
%F A326098 a(n) = Sum_{d|n} (d + 1) * binomial(d, n/d - 1) * n!/d! for n>0, with a(0) = 1.
%e A326098 E.g.f.: A(x) = 1 + 2*x + 7*x^2 + 4*x^3 + 77*x^4 + 6*x^5 + 2527*x^6 + 8*x^7 + 33609*x^8 + 725770*x^9 + 907211*x^10 + 12*x^11 + 946028173*x^12 + 14*x^13 + ...
%e A326098 such that
%e A326098 A(x) = 1 + 2*(1+x)*x + 3*(1+x^2)^2*x^2/2! + 4*(1+x^3)^3*x^3/3! + 5*(1+x^4)^4*x^4/4! + 6*(1+x^5)^5*x^5/5! + 7*(1+x^6)^6*x^6/6! + 8*(1+x^7)^7*x^7/7! + ...
%e A326098 also
%e A326098 A(x) = (1 + x)*exp(x) + (2 + x^2)*exp(x^2)*x^2 + (3 + x^3)*exp(x^3)*x^6/2! + (4 + x^4)*exp(x^4)*x^12/3! + (5 + x^5)*exp(x^5)*x^20/4! + (6 + x^6)*exp(x^6)*x^30/5! + (7 + x^7)*exp(x^7)*x^42/6! + ...
%o A326098 (PARI) {a(n) = if(n==0,1, sumdiv(n,d, (d + 1) * binomial(d, n/d - 1) * n!/d! ) )}
%o A326098 for(n=0,30, print1(a(n),", "))
%o A326098 (PARI) /* E.g.f.: Sum_{n>=0} (n+1) * (1 + x^n)^n * x^n/n! */
%o A326098 {a(n) = my(A = sum(m=0,n, (m+1) * (x^m + 1 +x*O(x^n))^m * x^m/m! )); n!*polcoeff(A,n)}
%o A326098 for(n=0,30, print1(a(n),", "))
%o A326098 (PARI) /* E.g.f.: Sum_{n>=1} (n + x^n) * exp(x^n) * x^(n*(n-1))/(n-1)! */
%o A326098 {a(n) = my(A = sum(m=1,sqrtint(2*n+1), (m + x^m) * exp(x^m +x*O(x^n)) * x^(m*(m-1))/(m-1)! )); n!*polcoeff(A,n)}
%o A326098 for(n=0,30, print1(a(n),", "))
