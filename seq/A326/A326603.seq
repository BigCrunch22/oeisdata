%N A326603 E.g.f.: Sum_{n>=0} (x^(2*n-1) + 1)^n * x^n * exp(-x^(2*n+1)) / n!, an even function.
%H A326603 Paul D. Hanna, <a href="/A326603/b326603.txt">Table of n, a(n) for n = 0..260</a>
%F A326603 G.f. A(x) = Sum_{n>=0} a(n)*x^(2*n)/(2*n)!, where
%F A326603 (1) A(x) = Sum_{n>=0} (x^(2*n-1) + 1)^n * x^n * exp(-x^(2*n+1)) / n!,
%F A326603 (2) A(x) = Sum_{n>=0} (x^(2*n-1) - 1)^n * x^n * exp(+x^(2*n+1)) / n!.
%e A326603 E.g.f.: A(x) = 1 + 4*x^2/2! - 22*x^4/4! + 2*x^6/6! + 60482*x^8/8! - 4838398*x^10/10! + 119750402*x^12/12! + 7264857602*x^14/14! - 348713164798*x^16/16! + 4268249137152002*x^18/18! + ...
%e A326603 such that
%e A326603 A(x) = exp(-x) + (1 + x)*x*exp(-x^3) + (1 + x^3)^2*x^2*exp(-x^5)/2! + (1 + x^5)^3*x^3*exp(-x^7)/3! + (1 + x^7)^4*x^4*exp(-x^9)/4! + (1 + x^9)^5*x^5*exp(-x^11)/5! + (1 + x^11)^6*x^6*exp(-x^13)/6! + ...
%e A326603 also
%e A326603 A(x) = exp(+x) - (1 - x)*x*exp(+x^3) + (1 - x^3)^2*x^2*exp(+x^5)/2! - (1 - x^5)^3*x^3*exp(+x^7)/3! + (1 - x^7)^4*x^4*exp(+x^9)/4! - (1 - x^9)^5*x^5*exp(+x^11)/5! + (1 - x^11)^6*x^6*exp(+x^13)/6! + ...
%o A326603 (PARI) {a(n) = my(A = sum(m=0,2*n+1, (x^(2*m-1) + 1 +O(x^(2*n+2)) )^m * x^m * exp(-x^(2*m+1) +O(x^(2*n+2))) / m! ));
%o A326603 (2*n)!*polcoeff(A,2*n)}
%o A326603 for(n=0,30,print1(a(n),", "))
%o A326603 (PARI) {a(n) = my(A = sum(m=0,2*n+1, (x^(2*m-1) - 1 +O(x^(2*n+2)) )^m * x^m * exp(+x^(2*m+1) +O(x^(2*n+2))) / m! ));
%o A326603 (2*n)!*polcoeff(A,2*n)}
%o A326603 for(n=0,30,print1(a(n),", "))
