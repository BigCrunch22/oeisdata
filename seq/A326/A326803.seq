%o A326803 Cy = sqrt(1/2) - intformal( Sy*Dx + Sx*Dy, y);
%o A326803 Dy = sqrt(1/2) - intformal( Sy*Cx - Sx*Cy, y);
%o A326803 );
%o A326803 round( (2*n+1)!*(2*n)! * polcoeff( polcoeff(Sx, 2*n+1,x),2*n,y) )}
