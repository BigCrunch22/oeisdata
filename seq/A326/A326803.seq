%o A326803 (PARI)
%o A326803 {a(n) = my(Cx=1,Sx=x,Dx=1,Cy=1,Sy=y,Dy=1);
%o A326803 for(i=0,4*n+2,
%o A326803 Sx =             intformal( Cx*Dy + Cy*Dx, x) + O(x^(2*n+2));
%o A326803 Cx = sqrt(1/2) - intformal( Sx*Dy + Sy*Dx, x);
%o A326803 Dx = sqrt(1/2) - intformal( Sx*Cy - Sy*Cx, x);
