%t A326173 fasmax[y_]:=Complement[y,Union@@(Most[Subsets[#]]&/@y)];
%t A326173 Table[Length[fasmax[Select[Subsets[Range[n]],Plus@@Complement[Range[n],#]>=Plus@@#&]]],{n,0,10}]
