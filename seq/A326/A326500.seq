%t A326500 b[n_, i_, k_] := b[n, i, k] = If[n == 0, 1, If[i < 1, 0, Sum[Function[t, b[n-t, Min[n-t, i-1], k] Binomial[k + t - 1, t]][i j], {j, 0, n/i}]]];
%t A326500 T[n_, k_] := Sum[b[n, n, k - i] (-1)^i*Binomial[k, i], {i, 0, k}];
