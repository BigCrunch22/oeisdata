%I A271009 #11 Feb 16 2025 08:33:32
%S A271009 1,5,24,108,344,1432,5732,21764,86908,349544,1391832,5524304,22125204,
%T A271009 88465688,353818732,1415067460
%N A271009 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 246", based on the 5-celled von Neumann neighborhood.
%C A271009 Initialized with a single black (ON) cell at stage zero.
%D A271009 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271009 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271009 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271009 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271009 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271009 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271009 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271009 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271009 code=246; stages=128;
%t A271009 rule=IntegerDigits[code,2,10];
%t A271009 g=2*stages+1; (* Maximum size of grid *)
%t A271009 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271009 ca=a;
%t A271009 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271009 PrependTo[ca,a];
%t A271009 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271009 k=(Length[ca[[1]]]+1)/2;
%t A271009 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271009 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271009 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A271009 Cf. A271008.
%K A271009 nonn,more,changed
%O A271009 0,2
