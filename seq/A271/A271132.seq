%S A271132 4,4,23,-23,92,-60,123,-131,283,-255,372,-380,547,-459,684,-749,1021,
%T A271132 -937,1125,-1193,1537,-1361,1541,-1536,2052,-2073,2337,-2325,2625,
%U A271132 -2468,2928,-2953,3413,-3369,3769,-3816,4400,-4128,4459,-4571,5439,-5343,5672,-5692
%N A271132 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 291", based on the 5-celled von Neumann neighborhood.
%C A271132 Initialized with a single black (ON) cell at stage zero.
%D A271132 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271132 Robert Price, <a href="/A271132/b271132.txt">Table of n, a(n) for n = 0..127</a>
%H A271132 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271132 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271132 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271132 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271132 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271132 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271132 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271132 code=291; stages=128;
%t A271132 rule=IntegerDigits[code,2,10];
%t A271132 g=2*stages+1; (* Maximum size of grid *)
%t A271132 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271132 ca=a;
%t A271132 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271132 PrependTo[ca,a];
%t A271132 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271132 k=(Length[ca[[1]]]+1)/2;
%t A271132 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271132 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271132 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
