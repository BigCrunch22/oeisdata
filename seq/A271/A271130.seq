%I A271130 #9 Jul 26 2024 21:16:36
%S A271130 1,5,32,164,825,3349,13344,54101,217768,873501,3494984,13983029,
%T A271130 55952168,223819565,895289440,3581106817
%N A271130 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 291", based on the 5-celled von Neumann neighborhood.
%C A271130 Initialized with a single black (ON) cell at stage zero.
%D A271130 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271130 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271130 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271130 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271130 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271130 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271130 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271130 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271130 code=291; stages=128;
%t A271130 rule=IntegerDigits[code,2,10];
%t A271130 g=2*stages+1; (* Maximum size of grid *)
%t A271130 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271130 ca=a;
%t A271130 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271130 PrependTo[ca,a];
%t A271130 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271130 k=(Length[ca[[1]]]+1)/2;
%t A271130 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271130 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271130 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A271130 Cf. A271129.
%K A271130 nonn,more,changed
%O A271130 0,2
