%o A271985 (Python)
%o A271985 from sympy.ntheory.factor_ import digits
%o A271985 def bump(n,b):
%o A271985   s=digits(n,b)[1:]
%o A271985   l=len(s)
%o A271985   return sum(s[i]*(b+1)**bump(l-i-1,b) for i in range(l) if s[i])
%o A271985 def A271985(n):
%o A271985   if n==3: return 0
%o A271985   for i in range(2,11):
%o A271985     n=bump(n,i)-1
%o A271985   return n # _Pontus von Brömssen_, Sep 25 2020
