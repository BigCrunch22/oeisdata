%S A271135 4,-1,16,-11,24,-16,56,-48,48,-40,64,-40,96,-88,160,-148,144,-140,184,
%T A271135 -188,208,-188,328,-320,328,-224,360,-360,392,-284,504,-412,504,-556,
%U A271135 464,-396,480,-404,608,-528,664,-636,728,-692,648,-472,936,-864,920,-1044
%N A271135 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 294", based on the 5-celled von Neumann neighborhood.
%C A271135 Initialized with a single black (ON) cell at stage zero.
%D A271135 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271135 Robert Price, <a href="/A271135/b271135.txt">Table of n, a(n) for n = 0..127</a>
%H A271135 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271135 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271135 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271135 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271135 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271135 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271135 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271135 code=294; stages=128;
%t A271135 rule=IntegerDigits[code,2,10];
%t A271135 g=2*stages+1; (* Maximum size of grid *)
%t A271135 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271135 ca=a;
%t A271135 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271135 PrependTo[ca,a];
%t A271135 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271135 k=(Length[ca[[1]]]+1)/2;
%t A271135 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271135 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271135 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
%Y A271135 Cf. A271133.
