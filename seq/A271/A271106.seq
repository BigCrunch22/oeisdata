%e A271106 a(9) = 1 since 9 = 0^6 + 3*0^6 + 2^3 + 1*2/2.
%e A271106 a(24) = 1 since 24 = 1^6 + 3*0^6 + 2^3 + 5*6/2.
%e A271106 a(1501) = 1 since 1501 = 2^6 + 3*5^3 + 3^3 + 45*46/2.
%e A271106 a(1639) = 1 since 1639 = 0^6 + 3*6^3 + 1^3 + 44*45/2.
%e A271106 a(3013) = 1 since 3013 = 3^6 + 3*3^3 + 13^3 + 3*4/2.
%t A271106 TQ[n_]:=TQ[n]=n>0&&IntegerQ[Sqrt[8n+1]]
%t A271106 Do[r=0;Do[If[TQ[n-x^6-3*y^3-z^3],r=r+1],{x,0,n^(1/6)},{y,0,((n-x^6)/3)^(1/3)},{z,1,(n-x^6-3y^3)^(1/3)}];Print[n," ",r];Continue,{n,0,70}]
