%S A271545 1,6,15,39,56,140,181,305,362,670,735,1111,1252,1756,1881,2513,2662,
%T A271545 3582,3867,4919,5176,6508,6789,8353,8686,10614,11103,13143,13776,
%U A271545 16292,16909,19533,20238,23578,24279,27859,28876,32748,33777,38413,39258,44390,45555
%N A271545 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 387", based on the 5-celled von Neumann neighborhood.
%C A271545 Initialized with a single black (ON) cell at stage zero.
%D A271545 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A271545 Robert Price, <a href="/A271545/b271545.txt">Table of n, a(n) for n = 0..128</a>
%H A271545 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A271545 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A271545 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A271545 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A271545 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A271545 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A271545 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A271545 code=387; stages=128;
%t A271545 rule=IntegerDigits[code,2,10];
%t A271545 g=2*stages+1; (* Maximum size of grid *)
%t A271545 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A271545 ca=a;
%t A271545 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A271545 PrependTo[ca,a];
%t A271545 (* Trim full grid to reflect growth by one cell at each stage *)
%t A271545 k=(Length[ca[[1]]]+1)/2;
%t A271545 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A271545 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A271545 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
