%t A271423 multinomial[n_, k_List] := n!/Times @@ (k!); b[n_, i_, k_] := b[n, i, k] = If[n==0, 1, If[i<1, 0, Sum[multinomial[n, Join[{n-i*j}, Array[i&, j]]] * b[n - i*j, i - 1, k]/j!, {j, 0, Min[k, n/i]}]]]; T[n_, k_] := b[n, n, k] - If[k == 0, 0, b[n, n, k - 1]]; Table[T[n, k], {n, 0, 12}, {k, 0, n}] // Flatten (* _Jean-FranÃ§ois Alcover_, Jan 06 2017, after _Alois P. Heinz_ *)
