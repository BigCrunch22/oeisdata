%N A245464 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k) * ((1+x)^k - 1)^k.
%H A245464 Paul D. Hanna, <a href="/A245464/b245464.txt">Table of n, a(n) for n = 0..200</a>
%F A245464 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k) * (1 - (1+x)^k)^(n-k) * (1+x)^(k^2).
%F A245464 G.f.: Sum_{n>=0} (1+x)^(n^2) * x^n / (1-x + x*(1+x)^n)^(n+1). - _Paul D. Hanna_, Jan 20 2015
%e A245464 G.f.: A(x) = 1 + x + 2*x^2 + 3*x^3 + 8*x^4 + 21*x^5 + 70*x^6 + 263*x^7 +...
%e A245464 where we have the identity:
%e A245464 A(x) = 1 + x*(1 + ((1+x)-1))
%e A245464 + x^2*(1 + 2*((1+x)-1) + ((1+x)^2-1)^2)
%e A245464 + x^3*(1 + 3*((1+x)-1) + 3*((1+x)^2-1)^2 + ((1+x)^3-1)^3)
%e A245464 + x^4*(1 + 4*((1+x)-1) + 6*((1+x)^2-1)^2 + 4*((1+x)^3-1)^3 + ((1+x)^4-1)^4)
%e A245464 + x^5*(1 + 5*((1+x)-1) + 10*((1+x)^2-1)^2 + 10*((1+x)^3-1)^3 + 5*((1+x)^4-1)^4 + ((1+x)^5-1)^5) +...
%e A245464 is equal to
%e A245464 A(x) = 1 + x*(0 + (1+x))
%e A245464 + x^2*(0 + 2*(1-(1+x))*(1+x) + (1+x)^4)
%e A245464 + x^3*(0 + 3*(1-(1+x))^2*(1+x) + 3*(1-(1+x)^2)*(1+x)^4 + (1+x)^9)
%e A245464 + x^4*(0 + 4*(1-(1+x))^3*(1+x) + 6*(1-(1+x)^2)^2*(1+x)^4 + 4*(1-(1+x)^3)*(1+x)^9 + (1+x)^16)
%e A245464 + x^5*(0 + 5*(1-(1+x))^4*(1+x) + 6*(1-(1+x)^2)^3*(1+x)^4 + 4*(1-(1+x)^3)^2*(1+x)^9 + 5*(1-(1+x)^4)*(1+x)^16 + (1+x)^25) +...
%e A245464 Also,
%e A245464 A(x) = 1 + (1+x)*x/(1-x + x*(1+x))^2 + (1+x)^4*x^2/(1-x + x*(1+x)^2)^3 + (1+x)^9*x^3/(1-x + x*(1+x)^3)^4 + (1+x)^16*x^4/(1-x + x*(1+x)^4)^5 + (1+x)^25*x^5/(1-x + x*(1+x)^5)^6 + (1+x)^36*x^6/(1-x + x*(1+x)^6)^7 +...
%o A245464 (PARI) {a(n) = polcoeff(sum(m=0,n, x^m*sum(k=0,m, binomial(m,k) * ((1+x)^k - 1 +x*O(x^n))^k )) ,n)}
%o A245464 for(n=0,30,print1(a(n),", "))
%o A245464 (PARI) {a(n) = polcoeff(sum(m=0,n, x^m*sum(k=0,m, binomial(m,k) * (1 - (1+x)^k +x*O(x^n))^(m-k) * (1+x)^(k^2) )) ,n)}
%o A245464 for(n=0,30,print1(a(n),", "))
%o A245464 (PARI) {a(n)=local(A=1); A=sum(m=0, n, (1+x)^(m^2) * x^m / (1-x + x*(1+x)^m +x*O(x^n))^(m+1) ); polcoeff(A, n)}
%o A245464 for(n=0,30,print1(a(n),", "))
