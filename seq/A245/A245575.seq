%e A245575 a(14) = #{6+1, 1+6} = 2;
%e A245575 a(15) = #{9+6, 6+9} = 2;
%e A245575 a(16) = #{16+0, 12+4, 4+12, 0+16} = 4;
%e A245575 a(17) = #{16+1, 1+16} = 2;
%e A245575 a(18) = #{16+2, 12+6, 9+9, 6+12, 2+16} = 5;
%e A245575 a(19) = #{} = 0;
