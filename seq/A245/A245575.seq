%t A245575 qsQ[n_] := qsQ[n] = With[{s = Sqrt[n]}, Which[IntegerQ[s], True, n == Floor[s] (Floor[s]+1), True, True, False]]; a[n_] := Count[Range[0, n], k_ /; qsQ[k] && qsQ[n-k]]; Array[a, 100, 0] (* _Jean-FranÃ§ois Alcover_, May 08 2017 *) (* or *)
%t A245575 u[{x_,y_}] := 2-Boole[x==y]; a[n_] := Total[u /@ IntegerPartitions[n, {2}, Floor[Range[1 + 2 Sqrt@ n]^2/4]]]; Array[a, 100, 0] (* _Giovanni Resta_, May 08 2017 *)
