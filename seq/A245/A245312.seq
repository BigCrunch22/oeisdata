%e A245312 From the main diagonal in the above table, we may derive A245313:
%e A245312 [1/1, 2/2, 6/3, 28/4, 155/5, 1056/6, 8106/7, 71352/8, 690759/9, ...]
%e A245312 = [1, 1, 2, 7, 31, 176, 1158, 8919, 76751, ...].
%o A245312 (PARI) /* From: [x^(n-1)] A(x)^n = n^2*([x^(n+1)] A(x)^n) */
%o A245312 {a(n)=local(A=[1,1]);for(i=1,n,A=concat(A,0);m=#A-2;A[#A]=-Vec(Ser(A)^m*(1-m^2*x^2))[#A]/m);A[n+1]}
%o A245312 for(n=0,30,print1(a(n),", "))
%o A245312 (PARI) /* From differential equation: */
%o A245312 {a(n)=local(A=1+x,D=1);for(i=1,n,D=(A - x*A'+x*O(x^n));
%o A245312 A=1+x+x^2 + 3*x^3*A'/D + 2*x^4*(A')^2/D^2 + x^4*A^2*A''/D^3);polcoeff(A,n)}
%o A245312 for(n=0,30,print1(a(n),", "))
