%t A245242 Table[Sum[Binomial[n^2-k^2,n*k-k^2],{k,0,n}],{n,0,20}] (* _Vaclav Kotesovec_, Jul 15 2014 *)
%o A245242 (PARI) {a(n)=sum(k=0,n,binomial(n^2 - k^2, n*k - k^2))}
%o A245242 for(n=0,20,print1(a(n),", "))
