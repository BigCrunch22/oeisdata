%p A245732 b:= proc(n, k) option remember; `if`(n=0, 1,
%p A245732       add(b(n-j, k)*binomial(n, j), j=k..n))
%p A245732     end:
%p A245732 T:= (n, k)-> `if`(k=0, n^n, `if`(n=0, 0, b(n, k))):
%p A245732 seq(seq(T(n, k), k=0..n), n=0..12);
%t A245732 b[n_, k_] := b[n, k] = If[n == 0, 1, Sum[b[n-j, k]*Binomial[n, j], {j, k, n}]]; T[n_, k_] := If[k == 0, n^n, If[n == 0, 0, b[n, k]]]; T[0, 0] = 1; Table[Table[T[n, k], {k, 0, n}], {n, 0, 12}] // Flatten (* _Jean-FranÃ§ois Alcover_, Jan 05 2015, after _Alois P. Heinz_ *)
