%t A295053 mex := First[Complement[Range[1, Max[#1] + 1], #1]] &;
%t A295053 a[0] = 1; a[1] = 2; b[0] = 3;
%t A295053 a[n_] := a[n] = a[n - 1] + a[n - 2] + Sum[b[k], {k, 0, n - 1}];
%t A295053 b[n_] := b[n] = mex[Flatten[Table[Join[{a[n]}, {a[i], b[i]}], {i, 0, n - 1}]]];
