%t A117684 f[n_]:= If[PrimeQ[n], 1, n];
%t A117684 cf[n_]:= cf[n]= If[n==0, 1, f[n]*cf[n-1]]; (* A049614 *)
%t A117684 T[n_, k_]:= T[n, k]= cf[n]/(cf[k]*cf[n-k]);
%t A117684 a[n_]:= a[n]= Sum[T[n,k], {k,n}];
%t A117684 Table[a[n], {n,40}]
