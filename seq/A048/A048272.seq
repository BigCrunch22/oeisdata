%p A048272 A048272 := proc(n)
%p A048272     local a;
%p A048272     a := 1 ;
%p A048272     for pfac in ifactors(n)[2] do
%p A048272         if pfac[1] = 2 then
%p A048272             a := a*(1-pfac[2]) ;
%p A048272         else
%p A048272             a := a*(pfac[2]+1) ;
%p A048272         end if;
%p A048272     end do:
%p A048272     a ;
%p A048272 end proc: # Schmitt, sign corrected _R. J. Mathar_, Jun 18 2016
