%p A261477 g:= proc(n, l, m, h) option remember;
%p A261477       `if`(n=0, `if`(1 in [l,m] or l=2, `if`(h<3, x^h, 0), x^h),
%p A261477        add(`if`(j in [l,m], 0, g(n-1, j, l, max(h, j))), j=1..h+1))
%p A261477     end:
%p A261477 T:= n-> (p-> seq(coeff(p, x, i), i=0..n))(g(n, 0$3)):
%p A261477 seq(T(n), n=0..14);
%t A261477 g[n_, l_, m_, h_] := g[n, l, m, h] = If[n == 0, If[l == 1 || m == 1 || l == 2, If[h < 3, x^h, 0], x^h], Sum[If[j == l || j == m, 0, g[n - 1, j, l, Max[h, j]]], {j, 1, h + 1}]]; T[n_] := Function[p, Table[Coefficient[p, x, i], {i, 0, n}]][g[n, 0, 0, 0]]; Table[T[n], {n, 0, 14}] // Flatten (* _Jean-FranÃ§ois Alcover_, Feb 22 2016, after _Alois P. Heinz_ *)
