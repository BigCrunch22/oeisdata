%N A259209 E.g.f.: Sum_{n>=1} x^(n^2) * exp(x^n) / n!.
%F A259209 E.g.f.: -exp(1) + Sum_{n>=0} (1 + x^n)^n / n!.
%F A259209 a(n) = Sum_{d|n} binomial(d, n/d) * n!/d! for n>=1.
%e A259209 E.g.f.: A(x) = x + 2*x^2/2! + 3*x^3/3! + 16*x^4/4! + 5*x^5/5! + 366*x^6/6! +...
%e A259209 where
%e A259209 A(x) = x*exp(x) + x^4*exp(x^2)/2! + x^9*exp(x^3)/3! + x^16*exp(x^4)/4! + x^25*exp(x^5)/5! + x^36*exp(x^6)/6! +...
%e A259209 also
%e A259209 A(x) = -exp(1) + 1 + (1+x) + (1+x^2)^2/2! + (1+x^3)^3/3! + (1+x^4)^4/4! + (1+x^5)^5/5! + (1+x^6)^6/6! +...
%e A259209 Particular values.
%e A259209 A(1) = exp(2) - exp(1).
%e A259209 A(-1) = cosh(2) - exp(1).
%e A259209 A(1/2) = 0.8648559700938957468696599588156983897723576531...
%e A259209 A(1/2) = exp(1/2)/2 + exp(1/2^2)/(2!*2^4) + exp(1/2^3)/(3!*2^9) + exp(1/2^4)/(4!*2^16) + exp(1/2^5)/(5!*2^25) +...
%e A259209 A(1/2) = -exp(1) + 1 + (1+1/2) + (1+1/2^2)^2/2! + (1+1/2^3)^3/3! + (1+1/2^4)^4/4! + (1+1/2^5)^5/5! + (1+1/2^6)^6/6! +...
%t A259209 Table[Sum[Binomial[d, n/d]*n!/d!, {d, Divisors[n]}], {n, 1, 30}] (* _Vaclav Kotesovec_, Oct 20 2020 *)
%o A259209 (PARI) {a(n) = my(A=1); A = sum(m=1, n, x^(m^2) * exp(x^m +x*O(x^n)) / m!); n!*polcoeff(A, n)}
%o A259209 for(n=1, 30, print1(a(n), ", "))
%o A259209 (PARI) {a(n) = my(A=1); A = -exp(1) + sum(m=0, n, (1 + x^m +x*O(x^n))^m/m!); if(n==0,0, n!*polcoeff(A, n))}
%o A259209 for(n=1, 30, print1(a(n), ", "))
%o A259209 (PARI) {a(n) = if(n<1,0, sumdiv(n,d, binomial(d, n/d) * n!/d! ) )}
%o A259209 for(n=1, 30, print1(a(n), ", "))
