%N A220231 G.f.: Sum_{n>=1} (2 - (1-x)^n) * (1 - (1-x)^n)^(n-1) / (1-x)^(n^2).
%C A220231 Compare the g.f. of this sequence to the identity (when G(x) = 1/(1-x)):
%C A220231 1 = Sum_{n>=1} (2*G(x)^n - 1) * (1 - G(x)^n)^(n-1) for all G(x) such that G(0)=1.
%F A220231 G.f.: 1 + Sum_{n>=1} 2*(2 - (1-x)^(2*n)) * (1 - (1-x)^(2*n))^(2*n-1) / (1-x)^(4*n^2).
%F A220231 G.f.: -1 + Sum_{n>=0} 2*(2 - (1-x)^(2*n+1))*(1 - (1-x)^(2*n+1))^(2*n)/(1-x)^((2*n+1)^2).
%e A220231 G.f.: A(x) = 1 + 4*x + 22*x^2 + 184*x^3 + 2094*x^4 + 30300*x^5 +...
%e A220231 where
%e A220231 A(x) = (1+x)/(1-x) + (1+2*x-x^2)*(2*x-x^2)/(1-x)^4 + (1+3*x-3*x^2+x^3)*(3*x-3*x^2+x^3)^2/(1-x)^9 + (1+4*x-6*x^2+4*x^3-x^4)*(4*x-6*x^2+4*x^3-x^4)^3/(1-x)^16 +...
%e A220231 Compare the g.f. to the identity:
%e A220231 1 = (1+x)/(1-x) - (1+2*x-x^2)*(2*x-x^2)/(1-x)^4 + (1+3*x-3*x^2+x^3)*(3*x-3*x^2+x^3)^2/(1-x)^9 - (1+4*x-6*x^2+4*x^3-x^4)*(4*x-6*x^2+4*x^3-x^4)^3/(1-x)^16 +-...
%o A220231 (PARI) {a(n)=polcoeff(sum(m=1, n+1, (2 - (1-x)^m) * (1 - (1-x)^m +x*O(x^n))^(m-1)/(1-x+x*O(x^n))^(m^2)), n)}
%o A220231 for(n=0, 20, print1(a(n), ", "))
%o A220231 (PARI) {a(n)=polcoeff(1+sum(m=1, n\2+1, 2*(2 - (1-x)^(2*m)) * (1 - (1-x)^(2*m) +x*O(x^n))^(2*m-1)/(1-x+x*O(x^n))^(4*m^2)), n)}
%o A220231 for(n=0, 20, print1(a(n), ", "))
%o A220231 (PARI) {a(n)=polcoeff(-1+sum(m=0, n\2, 2*(2 - (1-x)^(2*m+1)) * (1 - (1-x)^(2*m+1) +x*O(x^n))^(2*m)/(1-x+x*O(x^n))^((2*m+1)^2)), n)}
%o A220231 for(n=0, 20, print1(a(n), ", "))
