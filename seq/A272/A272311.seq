%S A272311 1,6,14,35,55,87,135,200,256,340,424,536,672,868,1084,1381,1625,1925,
%T A272311 2233,2501,2857,3253,3721,4293,4817,5361,5977,6721,7517,8417,9377,
%U A272311 10522,11534,12618,13670,14790,15978,17246,18722,20314,21982,23602,25386,27162
%N A272311 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 462", based on the 5-celled von Neumann neighborhood.
%C A272311 Initialized with a single black (ON) cell at stage zero.
%D A272311 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272311 Robert Price, <a href="/A272311/b272311.txt">Table of n, a(n) for n = 0..128</a>
%H A272311 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272311 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272311 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272311 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272311 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272311 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272311 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272311 code=462; stages=128;
%t A272311 rule=IntegerDigits[code,2,10];
%t A272311 g=2*stages+1; (* Maximum size of grid *)
%t A272311 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272311 ca=a;
%t A272311 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272311 PrependTo[ca,a];
%t A272311 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272311 k=(Length[ca[[1]]]+1)/2;
%t A272311 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272311 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272311 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
