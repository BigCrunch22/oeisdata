%S A272150 1,5,36,185,785,3329,13181,52776,213637,856221,3419753,13673841,
%T A272150 54742645,218988469,876026268,3504040152
%N A272150 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 435", based on the 5-celled von Neumann neighborhood.
%C A272150 Initialized with a single black (ON) cell at stage zero.
%D A272150 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272150 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272150 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272150 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272150 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272150 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272150 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272150 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272150 code=435; stages=128;
%t A272150 rule=IntegerDigits[code,2,10];
%t A272150 g=2*stages+1; (* Maximum size of grid *)
%t A272150 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272150 ca=a;
%t A272150 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272150 PrependTo[ca,a];
%t A272150 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272150 k=(Length[ca[[1]]]+1)/2;
%t A272150 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272150 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272150 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272150 Cf. A272149.
%K A272150 nonn,more
%O A272150 0,2
