%S A272447 1,5,9,32,29,84,61,172,85,284,141,388,193,576,281,704,353,1004,373,
%T A272447 1244,493,1444,645,1736,817,2048,941,2372,1013,2688,1257,3144,1461,
%U A272447 3676,1253,4108,1625,4548,1717,5068,2053,5508,2221,6348,2305,6908,2525,7480
%N A272447 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 475", based on the 5-celled von Neumann neighborhood.
%C A272447 Initialized with a single black (ON) cell at stage zero.
%D A272447 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272447 Robert Price, <a href="/A272447/b272447.txt">Table of n, a(n) for n = 0..128</a>
%H A272447 Robert Price, <a href="/A272447/a272447.tmp.txt">Diagrams of the first 20 stages</a>
%H A272447 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272447 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272447 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272447 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272447 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272447 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272447 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272447 code=475; stages=128;
%t A272447 rule=IntegerDigits[code,2,10];
%t A272447 g=2*stages+1; (* Maximum size of grid *)
%t A272447 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272447 ca=a;
%t A272447 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272447 PrependTo[ca,a];
%t A272447 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272447 k=(Length[ca[[1]]]+1)/2;
%t A272447 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272447 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A272447 nonn,easy,changed
