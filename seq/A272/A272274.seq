%S A272274 1,8,40,188,808,3348,13884,56092,225404,904436,3640068,14600860,
%T A272274 58510396,234316180,937995428,3753709232
%N A272274 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 453", based on the 5-celled von Neumann neighborhood.
%C A272274 Initialized with a single black (ON) cell at stage zero.
%D A272274 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272274 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272274 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272274 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272274 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272274 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272274 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272274 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272274 code=453; stages=128;
%t A272274 rule=IntegerDigits[code,2,10];
%t A272274 g=2*stages+1; (* Maximum size of grid *)
%t A272274 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272274 ca=a;
%t A272274 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272274 PrependTo[ca,a];
%t A272274 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272274 k=(Length[ca[[1]]]+1)/2;
%t A272274 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272274 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272274 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272274 Cf. A272273.
%K A272274 nonn,more
%O A272274 0,2
