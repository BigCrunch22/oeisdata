%I A272833 #22 Feb 16 2025 08:33:34
%S A272833 1,5,33,181,841,3533,14437,58601,235089,941637,3777085,15107033,
%T A272833 60420581,241699417,966790837,3867377125
%N A272833 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 603", based on the 5-celled von Neumann neighborhood.
%C A272833 Initialized with a single black (ON) cell at stage zero.
%D A272833 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272833 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272833 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272833 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272833 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272833 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272833 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272833 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272833 code=603; stages=128;
%t A272833 rule=IntegerDigits[code,2,10];
%t A272833 g=2*stages+1; (* Maximum size of grid *)
%t A272833 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272833 ca=a;
%t A272833 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272833 PrependTo[ca,a];
%t A272833 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272833 k=(Length[ca[[1]]]+1)/2;
%t A272833 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272833 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272833 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272833 Cf. A272828.
%K A272833 nonn,more,changed
%O A272833 0,2
