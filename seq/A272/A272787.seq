%I A272787 #11 Feb 16 2025 08:33:34
%S A272787 1,5,21,77,277,1069,4297,17449,69093,275589,1136301,4681945,19032833,
%T A272787 76522001,307063977,1233009465
%N A272787 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 534", based on the 5-celled von Neumann neighborhood.
%C A272787 Initialized with a single black (ON) cell at stage zero.
%D A272787 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272787 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272787 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272787 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272787 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272787 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272787 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272787 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272787 code=534; stages=128;
%t A272787 rule=IntegerDigits[code,2,10];
%t A272787 g=2*stages+1; (* Maximum size of grid *)
%t A272787 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272787 ca=a;
%t A272787 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272787 PrependTo[ca,a];
%t A272787 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272787 k=(Length[ca[[1]]]+1)/2;
%t A272787 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272787 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272787 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272787 Cf. A272786.
%K A272787 nonn,more,changed
%O A272787 0,2
