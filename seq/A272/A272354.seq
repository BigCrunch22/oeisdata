%t A272354 kappa = Exp[-Sqrt[3]/4]*Exp[-NSum[psi[4^(-j)]/j, {j, 2, Infinity}, NSumTerms -> 200, WorkingPrecision -> digits + 5]]; RealDigits[kappa, 10, digits][[1]]
