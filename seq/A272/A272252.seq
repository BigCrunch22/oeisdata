%S A272252 1,4,9,24,21,72,45,112,93,224,117,320,173,464,205,528,293,848,225,
%T A272252 1100,145,1488,361,1396,593,1744,561,2012,637,2384,669,2336,1053,2952,
%U A272252 861,3600,589,4208,1073,4140,1241,4876,1145,5176,1573,5556,1509,6008,1621
%N A272252 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 449", based on the 5-celled von Neumann neighborhood.
%C A272252 Initialized with a single black (ON) cell at stage zero.
%D A272252 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272252 Robert Price, <a href="/A272252/b272252.txt">Table of n, a(n) for n = 0..128</a>
%H A272252 Robert Price, <a href="/A272252/a272252.tmp.txt">Diagrams of the first 20 stages</a>
%H A272252 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272252 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272252 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272252 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272252 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272252 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272252 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272252 code=449; stages=128;
%t A272252 rule=IntegerDigits[code,2,10];
%t A272252 g=2*stages+1; (* Maximum size of grid *)
%t A272252 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272252 ca=a;
%t A272252 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272252 PrependTo[ca,a];
%t A272252 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272252 k=(Length[ca[[1]]]+1)/2;
%t A272252 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272252 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A272252 nonn,easy,changed
%O A272252 0,2
