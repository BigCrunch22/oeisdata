%I A272424 #10 Jul 26 2024 21:16:39
%S A272424 1,4,32,164,724,3216,13608,55092,222620,886928,3540896,14161580,
%T A272424 56627040,226325460,905090160,3619558696
%N A272424 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 473", based on the 5-celled von Neumann neighborhood.
%C A272424 Initialized with a single black (ON) cell at stage zero.
%D A272424 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272424 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272424 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272424 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272424 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272424 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272424 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272424 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272424 code=473; stages=128;
%t A272424 rule=IntegerDigits[code,2,10];
%t A272424 g=2*stages+1; (* Maximum size of grid *)
%t A272424 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272424 ca=a;
%t A272424 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272424 PrependTo[ca,a];
%t A272424 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272424 k=(Length[ca[[1]]]+1)/2;
%t A272424 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272424 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272424 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272424 Cf. A272423.
%K A272424 nonn,more
%O A272424 0,2
