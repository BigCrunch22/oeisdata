%S A272319 1,5,28,136,608,2720,11676,48328,195532,784156,3140672,12574272,
%T A272319 50310244,201283712,805242016,3221200260
%N A272319 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 467", based on the 5-celled von Neumann neighborhood.
%C A272319 Initialized with a single black (ON) cell at stage zero.
%D A272319 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272319 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272319 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272319 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272319 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272319 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272319 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272319 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272319 code=467; stages=128;
%t A272319 rule=IntegerDigits[code,2,10];
%t A272319 g=2*stages+1; (* Maximum size of grid *)
%t A272319 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272319 ca=a;
%t A272319 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272319 PrependTo[ca,a];
%t A272319 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272319 k=(Length[ca[[1]]]+1)/2;
%t A272319 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272319 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272319 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272319 Cf. A272315.
%K A272319 nonn,more,changed
%O A272319 0,2
