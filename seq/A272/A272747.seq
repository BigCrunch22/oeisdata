%I A272747 #10 Feb 16 2025 08:33:34
%S A272747 1,4,28,149,709,2861,11109,43661,174645,703089,2808797,11242321,
%T A272747 44965633,179830033,719326977,2877573801
%N A272747 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 529", based on the 5-celled von Neumann neighborhood.
%C A272747 Initialized with a single black (ON) cell at stage zero.
%D A272747 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272747 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272747 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272747 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272747 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272747 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272747 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272747 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272747 code=529; stages=128;
%t A272747 rule=IntegerDigits[code,2,10];
%t A272747 g=2*stages+1; (* Maximum size of grid *)
%t A272747 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272747 ca=a;
%t A272747 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272747 PrependTo[ca,a];
%t A272747 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272747 k=(Length[ca[[1]]]+1)/2;
%t A272747 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272747 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272747 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A272747 Cf. A272746.
%K A272747 nonn,more,changed
%O A272747 0,2
