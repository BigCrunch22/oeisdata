%U A272092 1923,-2475,3971,-3707,3987,-3979,4019,-3739,4067,-4331,5219,-4379,4211,-4747
%N A272092 First differences of number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 425", based on the 5-celled von Neumann neighborhood.
%C A272092 Initialized with a single black (ON) cell at stage zero.
%D A272092 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A272092 Robert Price, <a href="/A272092/b272092.txt">Table of n, a(n) for n = 0..127</a>
%H A272092 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A272092 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A272092 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A272092 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A272092 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A272092 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A272092 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A272092 code=425; stages=128;
%t A272092 rule=IntegerDigits[code,2,10];
%t A272092 g=2*stages+1; (* Maximum size of grid *)
%t A272092 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A272092 ca=a;
%t A272092 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A272092 PrependTo[ca,a];
%t A272092 (* Trim full grid to reflect growth by one cell at each stage *)
%t A272092 k=(Length[ca[[1]]]+1)/2;
%t A272092 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A272092 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A272092 Table[on[[i+1]]-on[[i]],{i,1,Length[on]-1}] (* Difference at each stage *)
