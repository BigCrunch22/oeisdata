%t A073424 Array[If[# == 1, {0}, PadRight[{1}, #]] &, 14] // Flatten (* or *)
%t A073424 Unprotect[Power]; Power[0, 0] = 1; Protect[Power]; Table[0^m + 0^n - 2 Boole[m == n == 0], {n, 0, 14}, {m, 0, n}] // Flatten (* _Michael De Vlieger_, Aug 22 2018 *)
