%t A073007 nmax=250; c = k /. FindRoot[EllipticK[k^2] == 2*EllipticE[k^2], {k, 9/10}, WorkingPrecision -> nmax]; Take[RealDigits[1/N[Exp[-Pi*(EllipticK[1 - c^2]/EllipticK[c^2])], nmax]][[1]], 200] (* _G. C. Greubel_, Mar 10 2018 *)
