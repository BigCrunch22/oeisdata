%t A073714 max = 75; a[0] = 1; se[n_] := se[n] = Series[ Sum[x^(j*(j + 1)/2)*(1 + x)^j, {j, 0, max - n}]^(n + 1), {x, 0, max - n}]; t[n_, k_] := t[n, k] = Coefficient[se[n], x, k]; ft = Flatten[ Table[t[n - j, j], {n, 0, max}, {j, 0, n}]][[1 ;; max + 1]]; sol = Thread[ft == Table[a[k], {k, 0, max}]] // Solve; sol /. Rule -> Set; Table[a[k], {k, 0, max}] (* _Jean-François Alcover_, Aug 05 2013 *)
