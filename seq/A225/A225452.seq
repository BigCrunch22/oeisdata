%o A225452 (PARI) n=0;for(i=1,100,m=(8*(10^i-1)/9)+1;for(x=0,9,if(((n+(x*10^(i-1)))^7)%(10^i)==m,n=n+(x*10^(i-1));print1(x", ");break)))
