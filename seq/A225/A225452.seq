%e A225452        9^7 == -1      (mod 10).
%e A225452       69^7 == -11     (mod 10^2).
%e A225452      969^7 == -111    (mod 10^3).
%e A225452     8969^7 == -1111   (mod 10^4).
%e A225452     8969^7 == -11111  (mod 10^5).
