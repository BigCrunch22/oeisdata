%e A106210 where [T^-1](n,k) = -k^2*T(n-2,k) when (n-2)>=k>=0.
%e A106210 G.f. for column 0: 1/(1-0x) = 1*(1-1x) + 1*x*(1-1x)(1-2x) +
%e A106210 3*x^2*(1-1x)(1-2x)(1-3x) + 16*x^3*(1-1x)(1-2x)(1-3x)(1-4x) + ...
%e A106210 + T(n,0)*x^n*(1-1x)(1-2x)*..*(1-(n+1)*x) + ...
%e A106210 G.f. for column 1: 1/(1-1x) = 1*(1-2x) + 3*x*(1-2x)(1-3x) +
%e A106210 16*x^2*(1-2x)(1-3x)(1-4x) + 127*x^3*(1-2x)(1-3x)(1-4x)(1-5x) + ...
%e A106210 + T(n+1,1)*x^n*(1-2x)(1-3x)*..*(1-(n+2)*x) + ...
%e A106210 G.f. for column 2: 1/(1-2x) = 1*(1-3x) + 5*x*(1-3x)(1-4x) +
%e A106210 39*x^2*(1-3x)(1-4x)(1-5x) + 416*x^3*(1-3x)(1-4x)(1-5x)(1-6x) + ...
%e A106210 + T(n+2,2)*x^n*(1-3x)(1-4x)*..*(1-(n+3)*x) + ...
%o A106210 (PARI) T(n,k)=if(n<k,0,if(n==k,1,polcoeff( 1/(1-k*x)-sum(i=0,n-k-1,T(i+k,k)*x^i*prod(j=1,i+1,1-(j+k)*x+x*O(x^(n-k)))),n-k)))
%o A106210 (PARI) T(n,k)=local(A=matrix(1,1),B);A[1,1]=1; for(m=2,n+2,B=matrix(m,m);for(i=1,m, for(j=1,i, if(j==i,B[i,j]=j,if(j==1,B[i,j]=(A^2)[i-1,1], B[i,j]=(A^2)[i-1,j]));));A=B); return(if(k==0,if(n==0,1,A[n+1,k+1]),A[n+1,k]/k^2))
