%N A246455 G.f.: Sum_{n>=0} x^n / (1-2*x)^(2*n+1) * [Sum_{k=0..n} C(n,k)^2 * x^k] * [Sum_{k=0..n} C(n,k)^2 * 2^k * x^k].
%F A246455 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k)^2 * Sum_{j=0..k} C(k,j)^2 * 2^(k-j) * x^j.
%F A246455 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k)^2 * 2^(n-k) * Sum_{j=0..k} C(k,j)^2 * x^j.
%F A246455 a(n) = Sum_{k=0..[n/2]} Sum_{j=0..n-2*k} C(n-k, k+j)^2 * C(k+j, k)^2 * 2^j.
%F A246455 Recurrence: (n-5)*(n-4)*(n-2)*n^2*a(n) = 3*(n-5)*(n-4)*(4*n^3 - 12*n^2 + 10*n - 3)*a(n-1) - (n-5)*(n-4)*(n-1)*(34*n^2 - 102*n + 77)*a(n-2) - 3*(n-5)*(n-2)*(2*n-5)*a(n-3) + (n-3)*(53*n^4 - 636*n^3 + 2617*n^2 - 4254*n + 2226)*a(n-4) - 3*(n-4)*(n-1)*(2*n-7)*a(n-5) - (n-5)*(n-2)*(n-1)*(34*n^2 - 306*n + 689)*a(n-6) + 3*(n-2)*(n-1)*(4*n^3 - 60*n^2 + 298*n - 489)*a(n-7) - (n-6)^2*(n-4)*(n-2)*(n-1)*a(n-8). - _Vaclav Kotesovec_, Aug 27 2014
%F A246455 a(n) ~ c * d^n / n, where d = 5/2 + sqrt(2) + sqrt(29+20*sqrt(2))/2 = 7.698532234454699125... is the root of the equation d^4 - 10*d^3 + 19*d^2 - 10*d + 1 = 0, and c = 0.51515814369432019442929480754730417101213524575391074352368... . - _Vaclav Kotesovec_, Aug 27 2014
%e A246455 G.f.: A(x) = 1 + 3*x + 14*x^2 + 75*x^3 + 439*x^4 + 2718*x^5 + 17483*x^6 +...
%e A246455 where the g.f. is given by the binomial series:
%e A246455 A(x) = 1/(1-2*x) + x/(1-2*x)^3 * (1+x) * (1+2*x)
%e A246455 + x^2/(1-2*x)^5 * (1 + 2^2*x + x^2) * (1 + 2^2*2*x + 4*x^2)
%e A246455 + x^3/(1-2*x)^7 * (1 + 3^2*x + 3^2*x^2 + x^3) * (1 + 3^2*2*x + 3^2*4*x^2 + 8*x^3)
%e A246455 + x^4/(1-2*x)^9 * (1 + 4^2*x + 6^2*x^2 + 4^2*x^3 + x^4) * (1 + 4^2*2*x + 6^2*4*x^2 + 4^2*8*x^3 + 16*x^4)
%e A246455 + x^5/(1-2*x)^11 * (1 + 5^2*x + 10^2*x^2 + 10^2*x^3 + 5^2*x^4 + x^5) * (1 + 5^2*2*x + 10^2*4*x^2 + 10^2*8*x^3 + 5^2*16*x^4 + 32*x^5) +...
%e A246455 We can also express the g.f. by the binomial series:
%e A246455 A(x) = 1 + x*(1 + (2+x)) + x^2*(1 + 2^2*(2+x) + (4+2^2*2*x+x^2))
%e A246455 + x^3*(1 + 3^2*(2+x) + 3^2*(4+2^2*2*x+x^2) + (8+3^2*4*x+3^2*2*x^2+x^3))
%e A246455 + x^4*(1 + 4^2*(2+x) + 6^2*(4+2^2*2*x+x^2) + 4^2*(8+3^2*4*x+3^2*2*x^2+x^3) + (16+4^2*8*x+6^2*4*x^2+4^2*2*x^3+x^4))
%e A246455 + x^5*(1 + 5^2*(2+x) + 10^2*(4+2^2*2*x+x^2) + 10^2*(8+3^2*4*x+3^2*2*x^2+x^3) + 5^2*(16+4^2*8*x+6^2*4*x^2+4^2*2*x^3+x^4) + (32+5^2*16*x+10^2*8*x^2+10^2*4*x^3+5^2*2*x^4+x^5)) +...
%t A246455 Table[Sum[Sum[Binomial[n-k, k+j]^2 * Binomial[k+j, k]^2 * 2^j,{j,0,n-2*k}],{k,0,Floor[n/2]}],{n,0,20}] (* _Vaclav Kotesovec_, Aug 27 2014 *)
%o A246455 (PARI) /* By definition: */
%o A246455 {a(n)=local(A=1); A=sum(m=0, n, x^m/(1-2*x)^(2*m+1) * sum(k=0, m, binomial(m, k)^2*x^k) * sum(k=0, m, binomial(m, k)^2*2^k*x^k) +x*O(x^n)); polcoeff(A, n)}
%o A246455 for(n=0, 30, print1(a(n), ", "))
%o A246455 (PARI) /* By a binomial identity: */
%o A246455 {a(n)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2*sum(j=0, k, binomial(k, j)^2*2^(k-j)*x^j)+x*O(x^n))), n)}
%o A246455 for(n=0, 30, print1(a(n), ", "))
%o A246455 (PARI) /* By a binomial identity: */
%o A246455 {a(n)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2 * 2^(m-k) * sum(j=0, k, binomial(k, j)^2*x^j)+x*O(x^n))), n)}
%o A246455 for(n=0, 30, print1(a(n), ", "))
%o A246455 (PARI) /* Formula for a(n): */
%o A246455 {a(n)=sum(k=0, n\2, sum(j=0, n-2*k, binomial(n-k, k+j)^2*binomial(k+j, k)^2*2^j))}
%o A246455 for(n=0, 30, print1(a(n), ", "))
