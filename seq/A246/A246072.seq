%p A246072 b:= proc(n, k, p) local l, g; l, g:= sort([divisors(p)[]]),
%p A246072       proc(k, m, i, t) option remember; local d, j; d:= l[i];
%p A246072         `if`(i=1, m!, add(M(k, k-(d-t)*j, (d-t)$j)/j!*
