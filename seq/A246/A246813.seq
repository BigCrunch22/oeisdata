%N A246813 G.f.: Sum_{n>=0} x^n / (1-3*x)^(2*n+1) * [Sum_{k=0..n} C(n,k)^2 * x^k] * [Sum_{k=0..n} C(n,k)^2 * 3^k * x^k].
%F A246813 G.f.: Sum_{n>=0} x^n * Sum_{k=0..n} C(n,k)^2 * 3^(n-k) * Sum_{j=0..k} C(k,j)^2 * x^j.
%F A246813 a(n) = Sum_{k=0..[n/2]} Sum_{j=0..n-2*k} C(n-k, k+j)^2 * C(k+j, j)^2 * 3^j.
%F A246813 a(n) ~ sqrt(12 + 23/sqrt(3) + 2*sqrt(80 + 46*sqrt(3))) * (3 + sqrt(3) + sqrt(11 + 6*sqrt(3)))^n / (4*Pi*n). - _Vaclav Kotesovec_, Oct 04 2014
%e A246813 G.f.: A(x) = 1 + 4*x + 23*x^2 + 152*x^3 + 1085*x^4 + 8156*x^5 +...
%e A246813 where the g.f. is given by the binomial series:
%e A246813 A(x) = 1/(1-3*x) + x/(1-3*x)^3 * (1+x) * (1+3*x)
%e A246813 + x^2/(1-3*x)^5 * (1 + 2^2*x + x^2) * (1 + 2^2*3*x + 9*x^2)
%e A246813 + x^3/(1-3*x)^7 * (1 + 3^2*x + 3^2*x^2 + x^3) * (1 + 3^2*3*x + 3^2*9*x^2 + 27*x^3)
%e A246813 + x^4/(1-3*x)^9 * (1 + 4^2*x + 6^2*x^2 + 4^2*x^3 + x^4) * (1 + 4^2*3*x + 6^2*9*x^2 + 4^2*27*x^3 + 81*x^4)
%e A246813 + x^5/(1-3*x)^11 * (1 + 5^2*x + 10^2*x^2 + 10^2*x^3 + 5^2*x^4 + x^5) * (1 + 5^2*3*x + 10^2*9*x^2 + 10^2*27*x^3 + 5^2*81*x^4 + 243*x^5) +...
%e A246813 We can also express the g.f. by the binomial series:
%e A246813 A(x) = 1 + x*(1 + (3+x)) + x^2*(1 + 2^2*(3+x) + (9+2^2*3*x+x^2))
%e A246813 + x^3*(1 + 3^2*(3+x) + 3^2*(9+2^2*3*x+x^2) + (27+3^2*9*x+3^2*3*x^2+x^3))
%e A246813 + x^4*(1 + 4^2*(3+x) + 6^2*(9+2^2*3*x+x^2) + 4^2*(27+3^2*9*x+3^2*3*x^2+x^3) + (81+4^2*27*x+6^2*9*x^2+4^2*3*x^3+x^4))
%e A246813 + x^5*(1 + 5^2*(3+x) + 10^2*(9+2^2*3*x+x^2) + 10^2*(27+3^2*9*x+3^2*3*x^2+x^3) + 5^2*(81+4^2*27*x+6^2*9*x^2+4^2*3*x^3+x^4) + (243+5^2*81*x+10^2*27*x^2+10^2*9*x^3+5^2*3*x^4+x^5)) +...
%t A246813 Table[Sum[Sum[Binomial[n-k, k+j]^2 * Binomial[k+j, j]^2 * 3^j,{j,0,n-2*k}],{k,0,Floor[n/2]}],{n,0,20}] (* _Vaclav Kotesovec_, Oct 04 2014 *)
%o A246813 (PARI) /* By definition: */
%o A246813 {a(n)=local(A=1); A=sum(m=0, n, x^m/(1-3*x)^(2*m+1) * sum(k=0, m, binomial(m, k)^2 * x^k) * sum(k=0, m, binomial(m, k)^2 * 3^k * x^k) +x*O(x^n)); polcoeff(A, n)}
%o A246813 for(n=0, 25, print1(a(n), ", "))
%o A246813 (PARI) /* By a binomial identity: */
%o A246813 {a(n)=polcoeff(sum(m=0, n, x^m*sum(k=0, m, binomial(m, k)^2 * 3^(m-k) * sum(j=0, k, binomial(k, j)^2 * x^j)+x*O(x^n))), n)}
%o A246813 for(n=0, 25, print1(a(n), ", "))
%o A246813 (PARI) /* Formula for a(n): */
%o A246813 {a(n)=sum(k=0, n\2, sum(j=0, n-2*k, binomial(n-k, k+j)^2 * binomial(k+j, j)^2 * 3^j))}
%o A246813 for(n=0, 25, print1(a(n), ", "))
