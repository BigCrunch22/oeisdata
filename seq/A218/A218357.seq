%t A218357 M[n_] := M[n] = Divisors[5^n - 1] ~Complement~ U[n-1];
%t A218357 U[n_] := U[n] = If[n == 0, {}, M[n] ~Union~ U[n-1]];
%t A218357 a[n_] := Min[M[n]];
