%p A218698 T:= (n, k)-> b(n, n, k):
%p A218698 seq(seq(T(n,k), k=0..n), n=0..12);
%t A218698 b[n_, i_, k_] :=  b[n, i, k] =  If[n == 0, 1, If[i < 1, 0,  b[n, i - 1, k] + Sum[b[n - i*j, i - k, k], {j, 1, n/i}]]]; T[n_, k_] := b[n, n, k]; Table[Table[T[n, k], {k, 0, n}], {n, 0, 12}] // Flatten (* _Jean-FranÃ§ois Alcover_, Dec 27 2013, translated from Maple *)
