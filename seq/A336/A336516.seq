%t A336516 b[n_, i_, p_] := b[n, i, p] = If[n == 0, {p!, 0},
%t A336516      If[i < 1, {0, 0}, Sum[Function[{0, If[j == 0, 0, #[[1]]*i]} + #][
%t A336516        b[n - i*j, i - 1, p + j]/j!], {j, 0, n/i}]]];
%t A336516 a[n_] := b[n, n, 0][[2]];
