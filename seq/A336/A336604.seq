%p A336604     end:
%p A336604 seq(a(n), n=0..100);  # _Alois P. Heinz_, Oct 25 2020
%t A336604 a[0] = 1; a[1] = 2; a[n_] := a[n] = a[n - 1] + If[(r = Mod[a[n - 1], a[n - 2]]) < n, a[r], 0]; Array[a, 51, 0] (* _Amiram Eldar_, Oct 10 2020 *)
%o A336604 (C++)
%o A336604 for (int i = 2; i < 10000; i++)
%o A336604     {
%o A336604         j = a[i - 1];
%o A336604         k = a[i - 2];
%o A336604         l = j % k;
%o A336604         if (int(l) <= int(i)) a[i] = j + a[l]; else a[i] = a[i - 1];
%o A336604     }
