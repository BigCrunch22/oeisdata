%e A278885 (-10*y^11 + 100*y^10 - 150*y^9 - 70*y^8 + 130*y^7 + 130*y^6 - 70*y^5 - 150*y^4 + 100*y^3 - 10*y^2)*x^6/6! +
%e A278885 (-y^14 + 43*y^13 - 142*y^12 + 466*y^11 - 2245*y^10 + 5423*y^9 - 7480*y^8 + 7480*y^7 - 5423*y^6 + 2245*y^5 - 466*y^4 + 142*y^3 - 43*y^2 + y)*x^7/7! +...
%e A278885 such that functions A = A(x,y), B = B(x,y), and C = C(x,y) satisfy:
%e A278885 (1) A^2 + B^2 + C^2 = 1 + y^2 and
%e A278885 (2) A^3 + B^3 + C^3 = 1 + y^3.
%e A278885 RELATED SERIES.
%e A278885 B(x,y) = 1 + (-y^4 + y^3)*x^2/2! + (2*y^5 - 4*y^4 + 2*y^3)*x^3/3! +
%e A278885 (y^8 - 3*y^7 + 2*y^6 - 8*y^5 + 15*y^4 - 7*y^3)*x^4/4! + (-10*y^9 + 40*y^8 - 30*y^7 - 10*y^6 - 20*y^5 + 50*y^4 - 20*y^3)*x^5/5! + (-y^12 + 11*y^11 + 34*y^9 - 304*y^8 + 594*y^7 - 634*y^6 + 520*y^5 - 281*y^4 + 61*y^3)*x^6/6! + (42*y^13 - 364*y^12 + 462*y^11 - 168*y^10 + 2296*y^9 - 4956*y^8 + 2436*y^7 + 952*y^6 - 378*y^5 - 504*y^4 + 182*y^3)*x^7/7! +...
%e A278885 C(x,y) = y + (y^2 - y)*x^2/2! + (-2*y^4 + 4*y^3 - 2*y^2)*x^3/3! + (-7*y^6 + 15*y^5 - 8*y^4 + 2*y^3 - 3*y^2 + y)*x^4/4! + (20*y^8 - 50*y^7 + 20*y^6 + 10*y^5 + 30*y^4 - 40*y^3 + 10*y^2)*x^5/5! + (61*y^10 - 281*y^9 + 520*y^8 - 634*y^7 + 594*y^6 - 304*y^5 + 34*y^4 + 11*y^2 - y)*x^6/6! + (-182*y^12 + 504*y^11 + 378*y^10 - 952*y^9 - 2436*y^8 + 4956*y^7 - 2296*y^6 + 168*y^5 - 462*y^4 + 364*y^3 - 42*y^2)*x^7/7! +...
%e A278885 The scalar triple product yields
%e A278885 [A',B',C'] * ([A,B,C] X [A^2,B^2,C^2]) = A'^2 + B'^2 + C'^2
%e A278885 where
%e A278885 A'^2 + B'^2 + C'^2 = (y^4 - 2*y^3 + y^2) + (4*y^7 - 8*y^6 + 8*y^5 - 8*y^4 + 4*y^3)*x^2/2! + (-8*y^9 + 16*y^8 - 8*y^7 + 8*y^5 - 16*y^4 + 8*y^3)*x^3/3! +
%e A278885 (-28*y^11 + 124*y^10 - 240*y^9 + 356*y^8 - 424*y^7 + 356*y^6 - 240*y^5 + 124*y^4 - 28*y^3)*x^4/4! + (80*y^13 - 200*y^12 - 120*y^11 + 480*y^10 - 200*y^9 + 200*y^7 - 480*y^6 + 120*y^5 + 200*y^4 - 80*y^3)*x^5/5! + (244*y^15 - 2148*y^14 + 7048*y^13 - 13684*y^12 + 20236*y^11 - 25128*y^10 + 26864*y^9 - 25128*y^8 + 20236*y^7 - 13684*y^6 + 7048*y^5 - 2148*y^4 + 244*y^3)*x^6/6! +...
%e A278885 Also, we have the relation
%e A278885 A'^2 + B'^2 + C'^2 = (1+y^2)*(A^4 + B^4 + C^4) - (1+y^3)^2
%e A278885 where
%e A278885 A^4 + B^4 + C^4 = (y^4 + 1) + (4*y^5 - 8*y^4 + 4*y^3)*x^2/2! + (-8*y^7 + 16*y^6 - 16*y^4 + 8*y^3)*x^3/3! + (-28*y^9 + 124*y^8 - 212*y^7 + 232*y^6 - 212*y^5 + 124*y^4 - 28*y^3)*x^4/4! + (80*y^11 - 200*y^10 - 200*y^9 + 680*y^8 - 680*y^6 + 200*y^5 + 200*y^4 - 80*y^3)*x^5/5! + (244*y^13 - 2148*y^12 + 6804*y^11 - 11536*y^10 + 13432*y^9 - 13592*y^8 + 13432*y^7 - 11536*y^6 + 6804*y^5 - 2148*y^4 + 244*y^3)*x^6/6! +...
