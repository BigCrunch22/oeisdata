%o A049691 def A049691(n):
%o A049691     if n == 0:
%o A049691         return 0
%o A049691     c, j = 1, 2
%o A049691     k1 = n//j
%o A049691     while k1 > 1:
%o A049691         j2 = n//k1 + 1
%o A049691         c += (j2-j)*(A049691(k1)-2)
%o A049691         j, k1 = j2, n//j2
%o A049691     return n*(n-1)-c+j+2 # _Chai Wah Wu_, Aug 04 2024
