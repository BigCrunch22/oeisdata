%p A049893 s := proc(n) option remember; `if`(n < 1, 0, a(n) + s(n - 1)) end proc:
%p A049893 a := proc(n) option remember;
%p A049893 `if`(n < 4, [1,1,3][n], s(n - 1) - a(2^ceil(log[2](n - 1)) + 2 - n)):
%p A049893 end proc:
