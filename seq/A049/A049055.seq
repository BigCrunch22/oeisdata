%e A049055   1,  50, 132,  24;
%e A049055   ...
%t A049055 t[n_, k_] := Sum[ (-1)^i*Binomial[k, i]*(2^k - 1 - i)^n, {i, 0, k}]; Flatten[ Join[{1}, Table[t[n, k], {n, 1, 9}, {k, 1, n}]]] (* _Jean-Fran√ßois Alcover_, Dec 12 2011, after _Michael Somos_ *)
