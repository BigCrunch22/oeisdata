%p A077425 A077425 := proc(n::integer) local resul,i ; resul := 5 ; i := 1 ; while i < n do resul := resul+4 ; while issqr(resul) do resul := resul+4 ; od ; i:= i+1 ; od ; RETURN(resul) ; end proc:
