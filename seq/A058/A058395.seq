%p A058395 gf := n -> (1 + x)^n / (1 - x^2)^3: ser := n -> series(gf(n), x, 20):
%p A058395 seq(lprint([n], seq(coeff(ser(n), x, k), k = 0..9)), n = 0..9); # _Peter Luschny_, Apr 12 2023
