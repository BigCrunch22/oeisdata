%t A058633 A006577[n_] := Length[NestWhileList[If[OddQ[#], 3*# + 1, #/2] &, n, # > 1 &]] - 1;
%t A058633 Accumulate[Array[A006577, 100]] (* _Paolo Xausa_, Oct 01 2024 *)
%o A058633 (PARI) A006577(n)=if(n<0, 0, s=n; c=0; while(s>1, s=if(s%2, 3*s+1, s/2); c++); c)
