%p A089627 R := (n, x) -> simplify(hypergeom([1/2 - n/2, -n/2], [1], 4*x)):
%p A089627 Trow := n -> seq(coeff(R(n,x), x, j), j=0..n):
%p A089627 seq(print(Trow(n)), n=0..9); # _Peter Luschny_, Mar 18 2018
