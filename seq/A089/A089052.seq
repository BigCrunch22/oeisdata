%p A089052 A089052 := proc(n, k)
%p A089052     option remember;
%p A089052     if k > n then
%p A089052         return(0);
%p A089052     end if;
%p A089052     if k= 0 then
%p A089052         if n=0 then
%p A089052             return(1)
%p A089052         else
%p A089052             return(0);
%p A089052         end if;
%p A089052     end if;
%p A089052     if n mod 2 = 1 then
%p A089052             return procname(n-1, k-1);
%p A089052     end if;
%p A089052     procname(n-1, k-1)+procname(n/2, k);
%p A089052 end proc:
%t A089052 t[n_, k_] := t[n, k] = Which[k > n, 0, k == 0, If[n == 0, 1, 0], Mod[n, 2] == 1, t[n-1, k-1], True, t[n-1, k-1] + t[n/2, k]]; Table[t[n, k], {n, 0, 13}, {k, 0, n}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 14 2014, after Maple *)
