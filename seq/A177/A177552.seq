%p A177552 a:= n-> b(n, 0, 1):
%p A177552 seq(a(n), n=0..25);  # _Alois P. Heinz_, Oct 30 2013
%t A177552 b[u_, o_, t_] := b[u, o, t] = If[t > 6, 0, If[u + o + t < 7, (u + o)!,
%t A177552      Sum[b[u - j, o + j - 1, {1, 1, 1, 5, 6, 7}[[t]]], {j, 1, u}] +
%t A177552      Sum[b[u + j - 1, o - j, {2, 3, 4, 4, 2, 2}[[t]]], {j, 1, o}]]];
%t A177552 a[n_] := b[n, 0, 1];
