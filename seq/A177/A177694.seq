%t A177694 Clear[t, n, m, c, q, w, f, a];
%t A177694 f[0, a_] := 0; f[1, a_] := 1;
%t A177694 f[n_, a_] := f[n, a] = -f[n - 1, a] - a*f[n - 2, a];
%t A177694 c[n_, a_] := If[n == 0, 1, Product[f[i, a], {i, 1, n}]];
