%t A177522 b[u_, o_, t_] := b[u, o, t] = If[u + o == 0, 1,
%t A177522      Sum[b[u+j-1, o-j, If[MemberQ[{0, 3}, t], 1, 2]], {j, 1, o}] + If[t<3,
%t A177522      Sum[b[u-j, o+j-1, If[t == 2, 3, 0]], {j, 1, u}], 0]];
%t A177522 a[n_] := b[n, 0, 0];
