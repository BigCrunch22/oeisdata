%t A177693 Clear[t, n, m, c, q, w, f, a];
%t A177693 f[0, a_] := 0; f[1, a_] := 1;
%t A177693 f[n_, a_] := f[n, a] = -f[n - 1, a] - a*f[n - 2, a];
%t A177693 c[n_, a_] := If[n == 0, 1, Product[f[i, a], {i, 1, n}]];
%t A177693 w[n_, m_, q_] := c[n, q]/(c[m, q]*c[n - m, q]);
