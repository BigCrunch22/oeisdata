%p A177535 b:= proc(u, o, t) option remember; `if`(t>6, 0, `if`(u+o+t<7, (u+o)!,
%p A177535       add(b(u-j, o+j-1, [1, 3, 4, 5, 6, 1][t]), j=1..u)+
%p A177535       add(b(u+j-1, o-j, [2, 2, 2, 2, 2, 7][t]), j=1..o)))
%p A177535     end:
%p A177535 a:= n-> b(n, 0, 1):
%p A177535 seq(a(n), n=0..25);  # _Alois P. Heinz_, Oct 22 2013
%t A177535 b[u_, o_, t_] := b[u, o, t] = If[t > 6, 0, If[u + o + t < 7, (u + o)!,
%t A177535      Sum[b[u - j, o + j - 1, {1, 3, 4, 5, 6, 1}[[t]]], {j, 1, u}] +
%t A177535      Sum[b[u + j - 1, o - j, {2, 2, 2, 2, 2, 7}[[t]]], {j, 1, o}]]];
%t A177535 a[n_] := b[n, 0, 1];
