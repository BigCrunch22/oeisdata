%p A204459 b:= proc(n, i, t) option remember;
%p A204459       `if`(i<t or n<t*(t+1)/2 or n>t*(2*i-t+1)/2, 0,
%p A204459       `if`(n=0, 1, b(n, i-1, t) +`if`(n<i, 0, b(n-i, i-1, t-1))))
%p A204459     end:
%p A204459 A:= proc(n, k) local s; s:= k*(k*n+1);
%p A204459       `if`(k=0, 1, `if`(n=0 or irem(s, 2)=1, 0, b(s/2, k*n, k)))
%p A204459     end:
%p A204459 seq(seq(A(n, d-n), n=0..d), d=0..15);
%t A204459 b[n_, i_, t_] /; i<t || n<t*((t+1)/2) || n>t*((2*i-t+1)/2) = 0; b[0, _, _] = 1; b[n_, i_, t_] := b[n, i, t] = b[n, i-1, t] + If[n<i, 0, b[n-i, i-1, t-1]]; a[_, 0] = 1; a[0, _] = 0; a[n_, k_] := With[{s = k*(k*n+1)}, If[Mod[s, 2] == 1, 0, b[s/2, k*n, k]]]; Flatten[ Table[ a[n, d-n], {d, 0, 15}, {n, 0, d}]] (* _Jean-FranÃ§ois Alcover_, Jun 15 2012, translated from Maple, after _Alois P. Heinz_ *)
