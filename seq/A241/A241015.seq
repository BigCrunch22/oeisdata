%t A241015 multinomial[n_, k_] := n!/Times @@ (k!); M = multinomial; b[n_, k0_] := Module[{l, g}, l = {1, 3}; g[k_, m_, i_, t_] := g[k, m, i, t] = Module[{d, j}, d = l[[i]]; If[i==1, n^m, Sum[M[k, Join[{k-(d-t)*j}, Array[d-t&, j]]]/j!*(d-1)!^j *M[m, Join[{m-t*j}, Array[t&, j]]]*g[k-(d-t)*j, m-t*j, Sequence @@ If[d-t==1, {i-1, 0}, {i, t+1}]], {j, 0, Min[k/(d-t), If[t==0, Infinity, m/t]]}]]]; g[k0, n-k0, Length[l], 0]]; a[0] = 1; a[n_] := Sum[b[n, j]*StirlingS2[n, j]*Binomial[n, j]*j!, {j, 0, n}]; Table[a[n], {n, 0, 20}] (* _Jean-Fran√ßois Alcover_, Mar 13 2017, translated from Maple *)
