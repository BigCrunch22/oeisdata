%t A297567 b[n_, i_] := If[n == 0, {3}, If[i<1, {}, Table[Map[Function[x, x*Binomial[ 2^i + j - 1, j]], b[n - i*j, i - 1]], {j, 0, n/i}]] // Flatten];
%t A297567 T[n_] :=  b[n, n];
