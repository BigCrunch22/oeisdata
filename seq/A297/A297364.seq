%t A297364 usigma[n_] := Block[{d = Divisors[n]}, Plus @@ Select[d, GCD[ #, n/# ] == 1 &]]; a = {}; k=1; rmax = 0; While[Length[a]<10, s = usigma[ k];s1=usigma[s]; s2 = usigma[ s1]; r = s2/s1;  If[r > rmax, a = AppendTo[a, k]; rmax = r]; k++]; a
