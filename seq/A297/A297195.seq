%p A297195 seq(A297195(n),n=1..30) ; # _R. J. Mathar_, Mar 04 2018
%t A297195 A272644[n_, m_] := Sum[StirlingS2[m+1, i+1] (-1)^(m-i) i^(n-m) i!, {i, 0, m}];
%t A297195 a[n_] := If[n == 1, 1, Sum[A272644[n, m], {m, 1, n-1}]];
