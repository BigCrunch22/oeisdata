%p A002207 with(combinat,stirling1):seq(denom(1/i!*sum(bernoulli(j)/(j)*stirling1(i,j-1), j=1..i+1)), i=1..24);
%t A002207 Table[Denominator[Det[Array[Sum[KroneckerDelta[#1, #2+q]*1/(q+2)^1, {q, -1, n+1}] &, {n+1, n+1}]]], {n, 0, 20}] (* _John M. Campbell_, Dec 01 2011 *)
%t A002207 a[n_] := Denominator[n!^-1*Sum[BernoulliB[j]/j*StirlingS1[n, j-1], {j, 1, n+1}]]; a[-1] = 1; Table[a[n], {n, -1, 18}] (* _Jean-François Alcover_, May 16 2012, after Maple *)
%t A002207 Denominator@Table[Integrate[x Pochhammer[x - n, n], {x, 0, 1}]/(n + 1)!, {n, -1, 20}] (* _Vladimir Reshetnikov_, Oct 22 2015 *)
%t A002207 Denominator@CoefficientList[x/Log[1+x] + O[x]^20, x] (* _Oliver Seipel_, Jul 06 2024 *)
