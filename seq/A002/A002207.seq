%o A002207 (Python)
%o A002207 from math import factorial
%o A002207 from fractions import Fraction
%o A002207 from sympy.functions.combinatorial.numbers import stirling
%o A002207 def A002207(n): return (sum(Fraction(stirling(n+1,k,kind=1,signed=True),k+1) for k in range(n+2))/factorial(n+1)).denominator # _Chai Wah Wu_, Feb 12 2023
%o A002207 (SageMath)
%o A002207 from functools import cache
%o A002207 @cache
%o A002207 def h(n):
%o A002207     return (-sum((-1)**k * h(n - k) / (k + 1) for k in range(1, n + 1))
%o A002207             + (-1)**n * n / (2*(n + 1)*(n + 2)))
%o A002207 def a(n): return h(n).denom() if n > 0 else n + 2
%o A002207 print([a(n) for n in range(-1, 19)]) # _Peter Luschny_, Dec 12 2023
