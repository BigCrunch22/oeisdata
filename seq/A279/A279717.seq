%S A279717 1,1,10,11,10100,111,1101000,1001111,10000,1110011111,100100000,
%T A279717 110000111111,101101000000,11001111111,110110010000000,
%U A279717 101100011111111,11010100000000,110110100111111111,111101001000000000,1101000001111111111,11001111010000000000
%N A279717 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 189", based on the 5-celled von Neumann neighborhood.
%C A279717 Initialized with a single black (ON) cell at stage zero.
%D A279717 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A279717 Robert Price, <a href="/A279717/b279717.txt">Table of n, a(n) for n = 0..126</a>
%H A279717 Robert Price, <a href="/A279717/a279717.tmp.txt">Diagrams of first 20 stages</a>
%H A279717 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A279717 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A279717 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A279717 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A279717 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A279717 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A279717 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A279717 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A279717 code = 189; stages = 128;
%t A279717 rule = IntegerDigits[code, 2, 10];
%t A279717 g = 2 * stages + 1; (* Maximum size of grid *)
%t A279717 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A279717 ca = a;
%t A279717 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A279717 PrependTo[ca, a];
%t A279717 (* Trim full grid to reflect growth by one cell at each stage *)
%t A279717 k = (Length[ca[[1]]] + 1)/2;
%t A279717 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A279717 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
