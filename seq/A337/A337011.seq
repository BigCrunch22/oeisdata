%N A337011 a(n) = 2^n * exp(-1/2) * Sum_{k>=0} (k + 2)^n / (2^k * k!).
%H A337011 Robert Israel, <a href="/A337011/b337011.txt">Table of n, a(n) for n = 0..513</a>
%F A337011 E.g.f.: exp(4*x + (exp(2*x) - 1) / 2).
%F A337011 a(0) = 1; a(n) = 5 * a(n-1) + Sum_{k=2..n} binomial(n-1,k-1) * 2^(k-1) * a(n-k).
%F A337011 a(n) = Sum_{k=0..n} binomial(n,k) * 4^(n-k) * A004211(k).
%p A337011 E:= exp(4*x+exp(2*x)/2-1/2):
%p A337011 S:= series(E,x,31):
%p A337011 seq(coeff(S,x,n)*n!,n=0..30); # _Robert Israel_, Aug 14 2020
%t A337011 nmax = 20; CoefficientList[Series[Exp[4 x + (Exp[2 x] - 1)/2], {x, 0, nmax}], x] Range[0, nmax]!
%t A337011 a[0] = 1; a[n_] := a[n] = 5 a[n - 1] + Sum[Binomial[n - 1, k - 1] 2^(k - 1) a[n - k], {k, 2, n}]; Table[a[n], {n, 0, 20}]
