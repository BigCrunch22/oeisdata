%p A337877 N:= 3000: # for terms <= N
%p A337877 P:= select(isprime, [2,seq(i,i=3..N/2,2)]): nP:= nops(P):
%p A337877 R:= NULL:
%p A337877 for i from 1 to nP do
%p A337877   p2:= P[i]^2;
%p A337877   for j from i to nP do
%p A337877     x:= p2*P[j];
