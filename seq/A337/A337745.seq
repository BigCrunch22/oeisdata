%F A337745 G.f.: Sum_{n>=0} (1 + x^n*(1+x)^n)^n * x^n.
%F A337745 G.f.: Sum_{n>=0} (1+x)^(n^2) * x^(n*(n+1)) / (1 - x^(n+1)*(1+x)^n)^(n+1).
%e A337745 G.f.: A(x) = 1 + x + 2*x^2 + 2*x^3 + 3*x^4 + 5*x^5 + 7*x^6 + 14*x^7 + 20*x^8 + 27*x^9 + 49*x^10 + 87*x^11 + 128*x^12 + 189*x^13 + 345*x^14 + 608*x^15 + ...
%e A337745 where
%e A337745 A(x) = 1 + (1 + x*(1+x))*x + (1 + x^2*(1+x)^2)^2*x^2 + (1 + x^3*(1+x)^3)^3*x^3 + (1 + x^4*(1+x)^4)^4*x^4 + ... + (1 + x^n*(1+x)^n)^n*x^n + ...
%e A337745 also
%e A337745 A(x) = 1/(1 - x) + (1+x)*x^2/(1 - x^2*(1+x))^2 + (1+x)^4*x^6/(1 - x^3*(1+x)^2)^3 + (1+x)^9*x^12/(1 - x^4*(1+x)^3)^4 + (1+x)^16*x^20/(1 - x^5*(1+x)^4)^5 + (1+x)^25*x^30/(1 - x^6*(1+x)^5)^6  + ... + (1+x)^(n^2)*x^(n*(n+1))/(1 - x^(n+1)*(1+x)^n)^(n+1) + ...
%e A337745 RELATED SERIES.
%e A337745 A(1/2) = Sum_{n>=0} (4^n + 3^n)^n / (2*4^n)^n = Sum_{n>=0} 2*4^n * 3^(n^2) / (2*4^n - 3^n)^(n+1) = 3.198779485217682672125325895125...
%t A337745 nmax = 50; CoefficientList[Series[Sum[(1 + x^k*(1+x)^k)^k * x^k, {k, 0, nmax}], {x, 0, nmax}], x] (* _Vaclav Kotesovec_, Sep 18 2020 *)
%o A337745 (PARI) {a(n) = my(A=1); A = sum(m=0, n, (1 + x^m*(1+x)^m + x*O(x^n))^m * x^m); polcoeff(A, n)}
%o A337745 for(n=0, 50, print1(a(n), ", "))
%o A337745 (PARI) {a(n) = my(A=1); A = sum(m=0, sqrtint(n+1), (1+x + x*O(x^n))^(m^2) * x^(m*(m+1)) / (1 - x^(m+1)*(1+x)^m + x*O(x^n))^(m+1)); polcoeff(A, n)}
%o A337745 for(n=0, 50, print1(a(n), ", "))
