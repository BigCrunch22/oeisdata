%N A337721 G.f.: Sum_{n>=0} (1 + x*(1+x)^n)^n * x^n.
%F A337721 G.f.: Sum_{n>=0} (1 + x*(1+x)^n)^n * x^n.
%F A337721 G.f.: Sum_{n>=0} (1+x)^(n^2) * x^(2*n) / (1 - x*(1+x)^n)^(n+1).
%e A337721 G.f.: A(x) = 1 + x + 2*x^2 + 4*x^3 + 9*x^4 + 23*x^5 + 62*x^6 + 179*x^7 + 549*x^8 + 1773*x^9 + 6003*x^10 + 21233*x^11 + 78187*x^12 + ...
%e A337721 where
%e A337721 A(x) = 1 + (1 + x*(1+x))*x + (1 + x*(1+x)^2)^2*x^2 + (1 + x*(1+x)^3)^3*x^3 + (1 + x*(1+x)^4)^4*x^4 + ... + (1 + x*(1+x)^n)^n*x^n + ...
%e A337721 also
%e A337721 A(x) = 1/(1 - x) + (1+x)*x^2/(1 - x*(1+x))^2 + (1+x)^4*x^4/(1 - x*(1+x)^2)^3 + (1+x)^9*x^6/(1 - x*(1+x)^3)^4 + (1+x)^16*x^8/(1 - x*(1+x)^4)^5 + (1+x)^25*x^10/(1 - x*(1+x)^5)^6  + ... + (1+x)^(n^2)*x^(2*n)/(1 - x*(1+x)^n)^(n+1) + ...
%t A337721 nmax = 30; CoefficientList[Series[Sum[(1 + x*(1+x)^k)^k * x^k, {k, 0, nmax}], {x, 0, nmax}], x] (* _Vaclav Kotesovec_, Sep 17 2020 *)
%o A337721 (PARI) {a(n) = my(A=1); A = sum(m=0,n, (1 + x*(1+x)^m + x*O(x^n))^m * x^m); polcoeff(A,n)}
%o A337721 for(n=0,30,print1(a(n),", "))
