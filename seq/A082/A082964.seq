%e A082964 For n = 11 a(11) = (arctan(tan(11))-11)/Pi = -4;
%e A082964 For n = 12 a(12) = (arctan(tan(12))-12)/Pi = -4;
%e A082964 For n = 13 a(13) = (arctan(tan(13))-13)/Pi = -4;
%e A082964 For n = 14 a(14) = (arctan(tan(14))-14)/Pi = -4.
%o A082964 (PARI) \\ Arctangent numbers at n
%o A082964 numbers(n) = { for(x=1,n, m=(atan(tan(x))-x)/Pi; print1(floor(abs(m)+.5), ", ") ) }
