%o A087797 from math import isqrt
%o A087797 from sympy import primepi, integer_nthroot
%o A087797 def A087797(n):
%o A087797     def f(x): return n+x-primepi(x)-primepi(isqrt(x))-primepi(integer_nthroot(x,3)[0])
%o A087797     m, k = n, f(n)
%o A087797     while m != k:
%o A087797         m, k = k, f(k)
