%t A172352 Clear[f, c, a, t];
%t A172352 f[0, a_] := 0; f[1, a_] := 1;
%t A172352 f[n_, a_] := f[n, a] = f[n - 1, a] + a*f[n - 2, a];
%t A172352 c[n_, a_] := If[n == 0, 1, Product[f[i, a], {i, 1, n}]];
%t A172352 t[n_, m_, a_] := c[n, a]/(c[m, a]*c[n - m, a]);
