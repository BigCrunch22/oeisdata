%N A300044 G.f. A(x) satisfies: 1 + x = Sum_{n>=0} x^n * (1 - x*A(x)^n)^n.
%F A300044 G.f. A(x) satisfies:
%F A300044 (1) 1 + x = Sum_{n>=0} x^n * (1 - x*A(x)^n)^n.
%F A300044 (2) 1 + x = Sum_{n>=0} (-1)^n * x^(2*n) * A(x)^(n^2) / (1 - x*A(x)^n)^(n+1).
%e A300044 G.f.: A(x) = 1 - x + 3*x^2 - 9*x^3 + 29*x^4 - 94*x^5 + 304*x^6 - 974*x^7 + 3095*x^8 - 9809*x^9 + 31295*x^10 - 101459*x^11 + 335712*x^12 + ...
%e A300044 such that
%e A300044 1 + x = 1 + x*(1-x*A(x)) + x^2*(1-x*A(x)^2)^2 + x^3*(1-x*A(x)^3)^3 + x^4*(1-x*A(x)^4)^4 + x^5*(1-x*A(x)^5)^5 + x^6*(1-x*A(x)^6)^6 + ...
%e A300044 The g.f. also satisfies the series identity:
%e A300044 1 + x = 1/(1-x) - x^2*A(x)/(1-x*A(x))^2 + x^4*A(x)^4/(1-x*A(x)^2)^3 - x^6*A(x)^9/(1-x*A(x)^3)^4 + x^8*A(x)^16/(1-x*A(x)^4)^5 - x^10*A(x)^25/(1-x*A(x)^5)^6 + ...
%o A300044 (PARI) {a(n) = my(A=[1]); for(i=1,n, A = concat(A,0); A[#A] = Vec( sum(n=0,#A+1, x^n*(1 - x*Ser(A)^n)^n) )[#A+2] ); A[n+1]}
%o A300044 for(n=0,30, print1(a(n),", "))
%o A300044 (PARI) {a(n) = my(A=[1]); for(i=1,n, A = concat(A,0); A[#A] = Vec( sum(n=0,#A+1, (-1)^n * x^(2*n) * Ser(A)^(n^2) / (1 - x*Ser(A)^n)^(n+1)) )[#A+2] ); A[n+1]}
%o A300044 for(n=0,30, print1(a(n),", "))
