%p A300269 b:= proc(n, i, m) option remember; `if`(i=0, `if`(n=0, 1, 0),
%p A300269       add(b(irem(n+j, m), i-1, m), j=[i^3, m-i^3]))
%p A300269     end:
%p A300269 a:= n-> b(0, n-1, n):
%p A300269 seq(a(n), n=1..60);  # _Alois P. Heinz_, Mar 01 2018
%t A300269 b[n_, i_, m_] := b[n, i, m] = If[i == 0, If[n == 0, 1, 0],
%t A300269      Sum[b[Mod[n + j, m], i - 1, m], {j, {i^3, m - i^3}}]];
%t A300269 a[n_] := b[0, n - 1, n];
%t A300269 Table[a[n], {n, 1, 60}] (* _Jean-François Alcover_, Mar 19 2022, after _Alois P. Heinz_ *)
%o A300269 (Ruby)
%o A300269 def A(n)
%o A300269   ary = [1] + Array.new(n - 1, 0)
%o A300269   (1..n).each{|i|
%o A300269     i3 = 2 * i * i * i
%o A300269     a = ary.clone
%o A300269     (0..n - 1).each{|j| a[(j + i3) % n] += ary[j]}
%o A300269     ary = a
%o A300269   }
%o A300269   ary[((n * (n + 1)) ** 2 / 4) % n] / 2
%o A300269 end
%o A300269 def A300269(n)
%o A300269   (1..n).map{|i| A(i)}
%o A300269 end
%o A300269 p A300269(100)
