%o A300280 (PARI) /* Must set N to a large value for accuracy: */ N=10000;
%o A300280 {T(n,k) = round( sum(j=0,N, binomial(j+k,k) * binomial((j+k)*k,n-k) / 2^(j+k+1)*1. ) )}
%o A300280 for(n=0,12, for(k=0,n, print1(T(n,k),", "));print(""))
%o A300280 (PARI) /* Faster, without precision errors: */
%o A300280 {T(n,k) = my(A = sum(m=0, n, x^m * y^m * (1+x + x*O(x^n))^(m^2) / (2 - (1+x + x*O(x^n))^m )^(m+1) )); polcoeff(polcoeff(A, n,x), k,y)}
%o A300280 for(n=0,12, for(k=0,n, print1(T(n,k),", "));print(""))
