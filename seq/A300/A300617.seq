%e A300617 in which the coefficient of x^n in row n forms a diagonal of zeros.
%o A300617 (PARI) {a(n) = my(A=[1]); for(i=1, n+1, A=concat(A, 0); V=Vec(Ser(A)^(#A-1)); A[#A] = ((#A-1)^2*V[#A-1] - V[#A])/(#A-1) ); polcoeff( log(Ser(A)), n)}
%o A300617 for(n=1, 20, print1(a(n), ", "))
%o A300617 (PARI) {a(n) = my(A=[1]); for(i=1, n, A=concat(A, 0); m=#A; A[m] = -Vec( exp(m^1*x*Ser(A))*(1-m^2*x +x^2*O(x^m))^(1))[m+1]/m ); A[n]}
%o A300617 for(n=1, 20, print1(a(n), ", "))
