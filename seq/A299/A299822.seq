%p A299822 A299822 := proc(n)
%p A299822     local a,p,e,pe;
%p A299822     a := 1;
%p A299822     for pe in ifactors(n)[2] do
%p A299822         p := pe[1] ; e:= pe[2] ;
%p A299822         a := a*p*(p-1)*p^(e-1) ;
%p A299822     end do:
%p A299822     a ;
%p A299822 end proc:
