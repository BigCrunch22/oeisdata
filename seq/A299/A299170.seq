%e A299170   1924,  663,  629,
%e A299170   ...
%t A299170 n = 1500; lst = {}; Do[Do[Do[If[GCD[b, c, d] == 1,
%t A299170 r = Sqrt[1/(1/b^2 + 1/c^2 + 1/d^2)];
%t A299170   If[IntegerQ[r], lst = AppendTo[lst, {b, c, d}]]], {d, c - 1}],
