%p A032032 # second Maple program:
%p A032032 b:= proc(n) b(n):= `if`(n=0, 1, add(b(n-j)/j!, j=2..n)) end:
%p A032032 a:= n-> n!*b(n):
%p A032032 seq(a(n), n=0..25);  # _Alois P. Heinz_, Jul 29 2014
%t A032032 a[n_] := n! * Sum[ Binomial[k, j] * StirlingS2[n-k+j, j]*j! / (n-k+j)! * (-1)^(k-j), {k, 1, n}, {j, 0, k}]; a[0] = 1; Table[a[n], {n, 0, 22}] (* _Jean-Fran√ßois Alcover_, Sep 05 2012, from given formula *)
