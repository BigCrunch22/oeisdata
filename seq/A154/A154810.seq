%o A154810 (Python)
%o A154810 def A154810(n):
%o A154810     def f(x): return n+(x>>(l:=x.bit_length())-(k:=l+1>>1))-(int(bin(x)[k+1:1:-1],2)>(x&(1<<k)-1))+(1<<k-1+(l&1^1))-1
%o A154810     m, k = n, f(n)
%o A154810     while m != k:
%o A154810         m, k = k, f(k)
%o A154810     return int(bin(m)[2:]) # _Chai Wah Wu_, Jul 24 2024
