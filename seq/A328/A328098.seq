%N A328098 If A327928(n) is zero, a(n) = A003415(n), otherwise a(n) = A327858(n) = gcd(A003415(n), A276086(n)).
%C A328098 After zero, sequence contains only terms of A048103.
%H A328098 Antti Karttunen, <a href="/A328098/b328098.txt">Table of n, a(n) for n = 1..65537</a>
%o A328098 (PARI)
%o A328098 A003415(n) = if(n<=1, 0, my(f=factor(n)); n*sum(i=1, #f~, f[i, 2]/f[i, 1]));
%o A328098 A129251(n) = { my(f = factor(n)); sum(k=1, #f~, (f[k, 2]>=f[k, 1])); };
%o A328098 A276086(n) = { my(i=0,m=1,pr=1,nextpr); while((n>0),i=i+1; nextpr = prime(i)*pr; if((n%nextpr),m*=(prime(i)^((n%nextpr)/pr));n-=(n%nextpr));pr=nextpr); m; };
%o A328098 A328098(n) = if(1==n,0,my(u=A003415(n)); if(!A129251(u),u,gcd(u,A276086(u))));
