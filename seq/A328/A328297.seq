%t A328297 b[l_] := b[l] = If[Last[l] == 0, 1, Sum[If[i + j + k == 1, Function[h, If[h[[1]] < 0, 0, b[h]]][Sort[l - {i, j, k}]], 0], {i, {-1, 0, 1}}, {j, {-1, 0, 1}}, {k, {-1, 0, 1}}]];
%t A328297 T[n_, k_] := Sum[b[Sort[{k, j, n - k - j}]], {j, 0, n - k}];
