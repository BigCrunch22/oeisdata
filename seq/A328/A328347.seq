%F A328347 T(n,k) = T(n,n-k).
%e A328347 Triangle T(n,k) begins:
%e A328347      1;
%e A328347      1,    1;
%e A328347      3,    4,     3;
%e A328347      7,   15,    15,     7;
%e A328347     19,   52,    72,    52,    19;
%e A328347     51,  175,   300,   300,   175,    51;
