%p A328270 b:= proc(l) option remember; `if`(l[-1]=0, 1, (r-> add(
%p A328270       add(add(`if`(i+j+k=1, (h-> `if`(h[1]<0, 0, b(h)))(
%p A328270       sort(l-[i, j, k])), 0), k=r), j=r), i=r))([$-1..1]))
%p A328270     end:
%p A328270 a:= n-> (2*n+1)*b([0, n$2]):
