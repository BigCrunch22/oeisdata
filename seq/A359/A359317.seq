%t A359317 seq[len_,nmax_] := Module[{s = Table[0,{len}], n = 0, c = 0, bw, t}, While[c < len && n < nmax, bw = DigitCount[t = n*(n+1)*(n+2)/6, 2, 1] + 1; If[bw <= len && s[[bw]] == 0, c++; s[[bw]] = t]; n++]; s]; seq[30, 10^6] (* _Amiram Eldar_, Dec 26 2022 *)
