%o A359794 (Python)
%o A359794 def A359794(n):
%o A359794     def f(x):
%o A359794         c, s = n+(x>>1), bin(x)[2:]
%o A359794         l = len(s)
%o A359794         for i in range(l&1,l,2):
%o A359794             c -= int(s[i])+int('0'+s[:i],2)
%o A359794         return c
%o A359794     m, k = n, f(n)
%o A359794     while m != k: m, k = k, f(k)
