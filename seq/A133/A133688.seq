%o A133688 isprab(v) = {my(sig = sigma(v)); if (sig < 2*v, return (0)); if (sig == 2*v, return (1)); fordiv (v, d, if ((d != v) && (sigma(d)>=2*d), return (0));); return (1);}
%o A133688 a(n) = {my(p = 3^n, k = 1); while (1, if (k % 3 != 0, v = p * k; if (isprab(v), return (v));); k += 2;);}
