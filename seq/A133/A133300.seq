%e A133300 |*| |
%e A133300 -----
%e A133300 | |*|
%e A133300 -----
%e A133300 |*| |
%e A133300 -----
%p A133300 S:= (n, m)-> `if`(irem(n*m, 2)=1, 0, `if`(irem(n, 2)=0, 1,
%p A133300               floor((n+1)/2)^(m/2))):
%p A133300 seq(seq(S(1+d-m, m), m=1..d), d=1..14);  # _Alois P. Heinz_, Nov 10 2013
%t A133300 S[n_, m_] := If[Mod[n*m, 2]==1, 0, If[Mod[n, 2]==0, 1, Floor[(n+1)/2]^(m/2) ]]; Table[S[1+d-m, m], {d, 1, 14}, {m, 1, d}] // Flatten (* _Jean-Fran√ßois Alcover_, Jan 30 2016, after _Alois P. Heinz_ *)
