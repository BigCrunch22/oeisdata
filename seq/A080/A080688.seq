%t A080688 facs[n_]:=If[n<=1,{{}},Join@@Table[(Prepend[#1,d]&)/@Select[facs[n/d],Min@@#1>=d&],{d,Rest[Divisors[n]]}]];
%t A080688 Table[Sort[Table[Times@@Prime/@(f-1),{f,facs[n]}]],{n,20}] (* _Gus Wiseman_, Sep 05 2018 *)
