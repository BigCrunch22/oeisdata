%p A306813 b:= proc(n, x, y) option remember; `if`(min(n, x, y, n-x-y)<0, 0,
%p A306813       `if`(n=0, 1, add(b(n-1, x-d[1], y-d[2]),
%p A306813        d=[[-1, 0], [0, 1], [0, -1], [1, -1]])))
%p A306813     end:
%p A306813 a:= n-> b(2*n, 0, n):
%p A306813 seq(a(n), n=0..30);
%t A306813 b[n_, x_, y_] := b[n, x, y] = If[Min[n, x, y, n - x - y] < 0, 0, If[n == 0, 1, Sum[b[n - 1, x - d[[1]], y - d[[2]]], {d, {{-1, 0}, {0, 1}, {0, -1}, {1, -1}}}]]];
%t A306813 a[n_] := b[2n, 0, n];
%t A306813 a /@ Range[0, 30] (* _Jean-FranÃ§ois Alcover_, May 13 2020, after Maple *)
