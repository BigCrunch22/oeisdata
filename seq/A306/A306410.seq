%N A306410 G.f.: Sum_{n>=0} 2^n * x^n * (3^n + 2^n)^n / (2^n + 3^n*x)^(n+1)  =  Sum_{n>=0} a(n) * x^n / 2^(n^2).
%H A306410 Paul D. Hanna, <a href="/A306410/b306410.txt">Table of n, a(n) for n = 0..50</a>
%F A306410 G.f. A(x) = Sum_{n>=0} a(n) * x^n / 2^(n^2) is defined by
%F A306410 (1) A(x) = Sum_{n>=0} 2^n * x^n * (3^n - 2^n)^n / (2^n - 3^n*x)^(n+1),
%F A306410 (2) A(x) = Sum_{n>=0} 2^n * x^n * (3^n + 2^n)^n / (2^n + 3^n*x)^(n+1),
%F A306410 (3) A(x) = Sum_{n>=0} x^n * ((3/2)^n - 1)^n / (1 - (3/2)^n*x)^(n+1),
%F A306410 (4) A(x) = Sum_{n>=0} x^n * ((3/2)^n + 1)^n / (1 + (3/2)^n*x)^(n+1).
%F A306410 FORMULAS FOR TERMS.
%F A306410 a(n) = Sum_{k=0..n} binomial(n, k) * (3^k - 2^k)^k * (2^n * 3^k)^(n-k).
%F A306410 a(n) = Sum_{k=0..n} binomial(n, k) * (3^k + 2^k)^k * (-2^n * 3^k)^(n-k).
%F A306410 a(n) = 2^(n^2) * Sum_{k=0..n} binomial(n, k) * ((3/2)^n - (3/2)^k)^(n-k).
%F A306410 a(n) = 2^(n^2) * Sum_{k=0..n} binomial(n, k) * ((3/2)^n + (3/2)^k)^(n-k) * (-1)^k.
%e A306410 G.f. A(x) = Sum_{n>=0} a(n) * x^n / 2^(n^2) begins:
%e A306410 A(x) = 1 + 3/2*x + 65/2^4*x^2 + 14499/2^9*x^3 + 33321281/2^16*x^4 + 707203658403/2^25*x^5 + 133454900550883025/2^36*x^6 + 222607516710114412650819/2^49*x^7 + 3290276040925391895704354602241/2^64*x^8 + 432761522266798203207806930832349560003/2^81*x^9 + ...
%e A306410 such that
%e A306410 A(x) = 1/(1 + x) + 2*x*(3 + 2)/(2 + 3*x)^2 + 2^2*x^2*(3^2 + 2^2)^2/(2^2 + 3^2*x)^3 + 2^3*x^3*(3^3 + 2^3)^3/(2^3 + 3^3*x)^4 + 2^4*x^4*(3^4 + 2^4)^4/(2^4 + 3^4*x)^5 + 2^5*x^5*(3^5 + 2^5)^5/(2^5 + 3^5*x)^6 + 2^6*x^6*(3^6 + 2^6)^6/(2^6 + 3^6*x)^7 + ...
%e A306410 also
%e A306410 A(x) = 1/(1 - x) + 2*x*(3 - 2)/(2 - 3*x)^2 + 2^2*x^2*(3^2 - 2^2)^2/(2^2 - 3^2*x)^3 + 2^3*x^3*(3^3 - 2^3)^3/(2^3 - 3^3*x)^4 + 2^4*x^4*(3^4 - 2^4)^4/(2^4 - 3^4*x)^5 + 2^5*x^5*(3^5 - 2^5)^5/(2^5 - 3^5*x)^6 + 2^6*x^6*(3^6 - 2^6)^6/(2^6 - 3^6*x)^7 + ...
%e A306410 More explicitly,
%e A306410 A(x) = 1/(1 + x) + 2*x*5/(2 + 3*x)^2 + 2^2*x^2*13^2/(2^2 + 3^2*x)^3 + 2^3*x^3*35^3/(2^3 + 3^3*x)^4 + 2^4*x^4*97^4/(2^4 + 3^4*x)^5 + 2^5*x^5*275^5/(2^5 + 3^5*x)^6 + 2^6*x^6*793^6/(2^6 + 3^6*x)^7 + ...
%e A306410 also
%e A306410 A(x) = 1/(1 - x) + 2*x/(2 - 3*x)^2 + 2^2*x^2*5^2/(2^2 - 3^2*x)^3 + 2^3*x^3*19^3/(2^3 - 3^3*x)^4 + 2^4*x^4*65^4/(2^4 - 3^4*x)^5 + 2^5*x^5*211^5/(2^5 - 3^5*x)^6 + 2^6*x^6*665^6/(2^6 - 3^6*x)^7 + ...
%o A306410 (PARI) {a(n) = my(A = sum(m=0, n+1, 2^m * (3^m - 2^m)^m*x^m/(2^m - 3^m*x +x*O(x^n) )^(m+1) )); 2^(n^2)*polcoeff(A, n)}
%o A306410 for(n=0, 15, print1(a(n), ", "))
%o A306410 (PARI) {a(n) = my(A = sum(m=0, n+1, 2^m * (3^m + 2^m)^m*x^m/(2^m + 3^m*x +x*O(x^n) )^(m+1) )); 2^(n^2)*polcoeff(A, n)}
%o A306410 for(n=0, 15, print1(a(n), ", "))
%o A306410 (PARI) /* Binomial formula for terms */
%o A306410 {a(n) = sum(k=0, n, binomial(n, k) * (3^k - 2^k)^k * (2^n * 3^k)^(n-k) )}
%o A306410 for(n=0, 15, print1(a(n), ", "))
%o A306410 (PARI) /* Binomial formula for terms */
%o A306410 {a(n) = sum(k=0, n, binomial(n, k) * (3^k + 2^k)^k * (-2^n * 3^k)^(n-k) )}
