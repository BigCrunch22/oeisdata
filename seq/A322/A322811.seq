%o A322811 A007947(n) = factorback(factorint(n)[, 1]); \\ From A007947
%o A322811 A048675(n) = my(f = factor(n)); sum(k=1, #f~, f[k, 2]*2^primepi(f[k, 1]))/2; \\ From A048675
