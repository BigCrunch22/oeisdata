%t A322793 nn = 2^24; k = 1; P = 2; Union@ Reap[While[j = 2; While[P^j < nn, Sow[P^j]; j++]; j > 2, k++; P *= Prime[k]]][[-1, 1]] (* _Michael De Vlieger_, Oct 04 2023 *)
