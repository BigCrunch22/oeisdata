%F A322318 a(n) = A001615(n) / A322319(n).
%t A322318 a[n_] := If[n == 1, 1, Module[{f=FactorInteger[n]}, GCD[ Times@@ (First[#] ^(Last[#]-1)& /@  f), Times@@((#+1)& @@@ f)]]]; Array[a, 120] (* _Amiram Eldar_, Dec 05 2018 *)
%o A322318 (PARI)
%o A322318 A003557(n) = { my(f=factor(n)); for(i=1, #f~, f[i, 2] = f[i, 2]-1); factorback(f); }; \\ From A003557
%o A322318 A048250(n) = factorback(apply(p -> p+1, factor(n)[, 1]));
%o A322318 A322318(n) = gcd(A048250(n), A003557(n));
