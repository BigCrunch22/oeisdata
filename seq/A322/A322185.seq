%o A322185 (PARI) /* [x^n*y^n/n] log( Product_{n>=1} 1/(1 - (x + y)^n) ) */
%o A322185 N=30
%o A322185 {L = sum(n=1, 2*N+1, -log(1 - (x + y)^n +x*O(x^(2*N)) +y*O(y^(2*N))) ); }
%o A322185 {a(n) = polcoeff( n*polcoeff( L, n, x), n, y)}
%o A322185 for(n=1, N, print1( a(n), ", ") )
