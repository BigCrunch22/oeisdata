%e A322623 A(x) = 1 + 2*sinh(x) + 2*sinh(x)^2 + 2*sinh(x)^3 + 2*sinh(x)^4 + ...
%p A322623 S:= series((1+sinh(x))/(1-sinh(x)),x,51):
%p A322623 seq(coeff(S,x,j)*j!,j=0..50);  #  _Robert Israel_, Dec 31 2018
%o A322623 (PARI) {a(n) = my(X = x +x*O(x^n)); n! * polcoeff( (1 + sinh(X)) / (1 - sinh(X)),n)}
%o A322623 for(n=0,30, print1(a(n),", "))
