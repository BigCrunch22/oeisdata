%F A322618 G.f.: Sum_{n>=0} (-x)^n * (1 - x^n)^n / (1 + x - x^(n+1))^(n+1).
%e A322618 G.f.: A(x) = 1 - x + 4*x^2 - 11*x^3 + 24*x^4 - 47*x^5 + 95*x^6 - 204*x^7 + 440*x^8 - 915*x^9 + 1829*x^10 - 3556*x^11 + 6819*x^12 - 13043*x^13 + 25039*x^14 + ...
%e A322618 such that
%e A322618 A(x) = 1/(1 + 2*x) + x*(1+x)/(1+x + x^2)^2 + x^2*(1+x^2)^2/(1+x + x^3)^3 + x^3*(1+x^3)^3/(1+x + x^4)^4 + x^4*(1+x^4)^4/(1+x + x^5)^5 + x^5*(1+x^5)^5/(1+x + x^6)^6 + ...
%e A322618 also,
%e A322618 A(x) = 1 - x*(1-x)/(1+x - x^2)^2 + x^2*(1-x^2)^2/(1+x - x^3)^3 - x^3*(1-x^3)^3/(1+x - x^4)^4 + x^4*(1-x^4)^4/(1+x - x^5)^5 - x^5*(1-x^5)^5/(1+x - x^6)^6 + ...
%o A322618 (PARI) {a(n) = my(A = sum(m=0,n, x^m*(1 + x^m)^m/(1 + x + x^(m+1) +x*O(x^n) )^(m+1) ) ); polcoeff(A,n)}
%o A322618 for(n=0,50, print1(a(n),", "))
