%o A322224 (PARI)  {A322221(n, k) = my(Sx=x, Sy=y, Cx=1, Cy=1); for(i=1, 2*n,
%o A322224 Sx = intformal( Cx*Cy +x*O(x^(2*n)), x);
%o A322224 Cx = 1 + intformal( Sx*Cy, x);
%o A322224 Sy = intformal( Cy*Cx +y*O(y^(2*k)), y);
%o A322224 Cy = 1 + intformal( Sy*Cx, y));
%o A322224 (2*n-2*k)!*(2*k)! *polcoeff(polcoeff(Cx, 2*n-2*k, x), 2*k, y)}
%o A322224 a(n) = sum(k=0, n, A322221(n, k))
%o A322224 for(n=0, 20, print1(a(n), ", "))
