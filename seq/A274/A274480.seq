%o A274480 L=sum(j=1, #M, -(M^0-M)^j/j); M=sum(j=0, #L, (L/3^(#L-1))^j/j!); R=matrix(#M+1, #M+1, r, c,
%o A274480 if(r>=c, if(r<=#M, M[r, c], 3^((c-1)*(#M+1-c))))); M=R^(3^(#M-1)) ); M[n+1, k+1]}
