%o A274477 {LOGT(n, k)=local(M=Mat(1), L, R);
%o A274477 for(i=1, n,
%o A274477 L=sum(j=1, #M, -(M^0 - M)^j/j);
%o A274477 M=sum(j=0, #L, (L/2^(#L-1))^j/j!);
