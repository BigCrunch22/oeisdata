%t A274222 fa = FactorInteger; test[n_] := Union@Table[Mod[n/fa[n][[i, 1]]^(fa[n][[i,2]]), fa[n][[ i, 1]]] == 1, {i, Length[fa[n]]}] == {True}; Select[Range[1000], test]
