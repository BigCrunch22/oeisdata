%t A274142 T[0] = {0}; T[n_] := T[n] = Complement[Join[T[n-1]+1, x*T[n-1]], T[n-1]]; Reap[For[n = 0, n <= 25, n++, cnt = Count[T[n] /. x -> 1/2, _Integer]; Print[n, " ", cnt]; Sow[cnt]]][[2, 1]] (* _Jean-François Alcover_, Jun 14 2016 *)
