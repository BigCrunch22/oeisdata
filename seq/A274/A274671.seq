%o A274671 diag(expr, N=22, var=variables(expr)) = {
%o A274671   my(a = vector(N));
%o A274671   for (k = 1, #var, expr = taylor(expr, var[#var - k + 1], N));
%o A274671   for (n = 1, N, a[n] = expr;
%o A274671     for (k = 1, #var, a[n] = polcoeff(a[n], n-1)));
%o A274671   return(a);
%o A274671 };
