%p A274835 b:= proc(n, k, m, t) option remember; `if`(n=0, 1,
%p A274835      add(`if`(irem(j-t, k)=0, b(n-1, k, max(m, j),
%p A274835               irem(t+1, k)), 0), j=1..m+1))
%p A274835     end:
%p A274835 A:= (n, k)-> `if`(k=0, 1, b(n, k, 0, 1)):
%p A274835 seq(seq(A(n, d-n), n=0..d), d=0..14);
%t A274835 b[n_, k_, m_, t_] := b[n, k, m, t] = If[n==0, 1, Sum[If[Mod[j-t, k]==0, b[n-1, k, Max[m, j], Mod[t+1, k]], 0], {j, 1, m+1}]]; A[n_, k_]:= If[k==0, 1, b[n, k, 0, 1]]; Table[A[n, d-n], {d, 0, 14}, {n, 0, d}] // Flatten (* _Jean-Fran√ßois Alcover_, Dec 18 2016, after _Alois P. Heinz_ *)
