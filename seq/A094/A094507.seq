%p A094507 b:= proc(x, y, t) option remember; `if`(y<0 or y>x, 0,
%p A094507      `if`(x=0, 1, expand(b(x-1, y+1, [2, 2, 4, 2][t])
%p A094507       +b(x-1, y-1, [1, 3, 1, 3][t])*`if`(t=4, z, 1))))
%p A094507     end:
%p A094507 T:= n-> (p-> seq(coeff(p, z, i), i=0..degree(p)))(b(2*n, 0, 1)):
%p A094507 seq(T(n), n=0..15);  # _Alois P. Heinz_, Jun 02 2014
%t A094507 b[x_, y_, t_] := b[x, y, t] = If[y<0 || y>x, 0, If[x == 0, 1, Expand[b[x-1, y+1, {2, 2, 4, 2}[[t]]] + b[x-1, y-1, {1, 3, 1, 3}[[t]]]*If[t == 4, z, 1]]]]; T[n_] := Function[{p}, Table[Coefficient[p, z, i], {i, 0, Exponent[p, z]}]][b[2*n, 0, 1] ]; Table[T[n], {n, 0, 15}] // Flatten (* _Jean-Fran√ßois Alcover_, Apr 29 2015, after _Alois P. Heinz_ *)
