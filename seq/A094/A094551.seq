%t A094551 lst={}; Do[i1=n-1; i2=n; s1=i1; s2=i2; While[i1>1 && s1!=s2, If[s1<s2, i1--; s1=s1+i1, i2++; s2=s2+i2]]; If[s1==s2, AppendTo[lst, n]], {n, 2, 140}]; lst
