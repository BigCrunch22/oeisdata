%t A029978 f[n_]:=FromDigits[RealDigits[n,11][[1]]]==FromDigits[Reverse[RealDigits[n,11][[1]]]]; lst={}; Do[p=Prime[n]; If[f[p],AppendTo[lst,p]],{n,8!}]; lst (* _Vladimir Joseph Stephan Orlovsky_, Aug 10 2009 *)
