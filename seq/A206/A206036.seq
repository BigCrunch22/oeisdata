%t A206036 max = 9000; sigmaList = Table[DivisorSigma[1, n], {n, Prime[PrimePi[max]]}]; Select[Range[Floor[Sqrt[max]]], Count[sigmaList, sigmaList[[#]]] > 1 &] (* _Alonso del Arte_, Feb 06 2012 *)
