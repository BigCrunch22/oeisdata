%t A233507 b[n_, l_] := b[n, l] = Module[{k, t}, t = Min[l]; Which[n == 0, 1, t > 0, b[n-t, l-t], True, For[k = 1, l[[k]] > 0, k++]; Sum[If[n >= j, b[n, ReplacePart[l, k -> j]], 0], {j, 2, 3}] + If[k <= 6 && l[[k + 2]] == 0, b[n, ReplacePart[l, {k -> 1, k+2 -> 1}]], 0] + If[k <= 4 && l[[k+2]] == 0 && l[[k+2*2]] == 0, b[n, ReplacePart[l, {k -> 1, k+2 -> 1, k+2*2 -> 1}]], 0] + If[Mod[k, 2] > 0 && l[[k+1]] == 0, b[n, ReplacePart[l, {k -> 1, k+1 -> 1}]], 0]]]; a[n_] := b[n, Array[0&, 8]]; Table[Print[an = a[n]]; an, {n, 0, 16}] (* _Jean-Fran√ßois Alcover_, Dec 30 2013, translated from Maple *)
