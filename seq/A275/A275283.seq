%t A275283 b[n_, s_] := b[n, s] = Expand[If[n>s, Binomial[n-1, n-s-1]*x, 1] + Sum[Binomial[n-1, j-1]*b[n-j, s+j]*Binomial[s+j-1, j-1], {j, 1, (n-s)/2}]*x^2];
%t A275283 T[n_] := T[n] = Function[p, Table[Coefficient[p, x, i], {i, 0, n}]][b[n, 0]];
