%p A275389 b:= proc(n, i, t) option remember; `if`(t=0 and n>i*(i-1)/2, 0,
%p A275389      `if`(n=0, 1, add(b(n-j, j, 0)*binomial(n-1, j-1), j=1..min(n, i-1))
%p A275389      +`if`(t=1, add(b(n-j, j, 1)*binomial(n-1, j-1), j=i+1..n), 0)))
%p A275389     end:
%p A275389 a:= n-> b(n, 0, 1):
%p A275389 seq(a(n), n=0..30);
%t A275389 b[n_, i_, t_] := b[n, i, t] = If[t==0 && n > i*(i-1)/2, 0, If[n==0, 1, Sum[b[n-j, j, 0]*Binomial[n-1, j-1], {j, 1, Min[n, i-1]}] + If[t==1, Sum[b[n-j, j, 1]*Binomial[n-1, j-1], {j, i+1, n}], 0]]]; a[n_] := b[n, 0, 1]; Table[a[n], {n, 0, 30}] (* _Jean-Fran√ßois Alcover_, Feb 07 2017, translated from Maple *)
