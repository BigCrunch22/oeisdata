%o A275670 (PARI) /* Print first N rows of this triangle: */ N=32;
%o A275670 {a(n) = my(A=1 +x*O(x^n)); for(k=0, n, A = A^2 + y*x^(n+1-k)); polcoeff(A, n)}
%o A275670 {for(n=0, N, for(k=0,n, if(k==0,print1(polcoeff(a(n)+y*O(y^n),k,y)", "), if(polcoeff(a(n)+y*O(y^n),k,y)==0,break,print1(polcoeff(a(n)+y*O(y^n),k,y),", "))));print(""))}
