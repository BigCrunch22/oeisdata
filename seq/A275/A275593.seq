%p A275593 mnltr:= proc(p) local g; g:= proc(n) option remember; `if` (n=0, 1, add(((n-1)!/(n-k)!)*p(k) *g(n-k), k=1..n)): end: end: d := mnltr(c): c := n->`if`(n<=2, 1, d(n-2)): A275593 := n -> c(n): seq(A275593(n), n=1..16);
