%t A275080 z[n_, m_] := Module[{kmax, s}, kmax = n+1; s = Sum[(-1)^k*(1-(1-x^k)^m)/ (1-x^k)^m, {k, 1, kmax}]; SeriesCoefficient[1/(1+s), {x, 0, n}]];
%t A275080 g[n_, m_] := Sum[Binomial[m, k]*(-1)^(m-k)*z[n, k], {k, 0, m}];
