%p A275594 mnltr:= proc(p) local g; g:= proc(n) option remember; `if` (n=0, 1, add(((n-1)!/(n-k)!)*p(k) *g(n-k), k=1..n)): end: end: d := mnltr(c): c := n->`if`(n<=3, 1, d(n-3)): A275594 := n-> c(n): seq(A275594(n), n=1..19);
%t A275594 mnltr[p_] := Module[{g}, g[n_] := g[n] = If [n == 0, 1, Sum[((n-1)!/(n-k)!) *p[k]*g[n-k], {k, 1 n}]]; g]; d = mnltr[c]; c [n_] := If[n <= 3, 1, d[n - 3]]; A275594[n_] := c[n]; Table[A275594[n], {n, 1, 19}] (* _Jean-FranÃ§ois Alcover_, Jul 22 2017, translated from Maple *)
