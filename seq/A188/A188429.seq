%t A188429 kr[n_] := {k, r} /. ToRules[Reduce[0 <= r <= k && n == k*((k+1)/2)+r, {k, r}, Integers]]; L[n_] := Which[{k0, r0} = kr[n]; r0 == 0, k0, 1 <= r0 <= k0-2, k0+1, k0-1 <= r0 <= k0, k0+2]; Join[{1, 0, 2, 0, 0, 3, 4, 0, 0, 4, 5, 5, 6, 7}, Table[L[n], {n, 15, 80}]] (* _Jean-FranÃ§ois Alcover_, Oct 10 2015 *)
