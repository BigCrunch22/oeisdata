%N A188009 a(n) = [nr] - [nr-kr] - [kr], where r=(1+sqrt(5))/2, k=2, [ ]=floor.
%C A188009 See A188014.
%F A188009 a(n) = [nr] - [nr-2r] - [2r], where r=(1+sqrt(5))/2.
%F A188009 a(n) = A123740(n-2), n >= 3. - _Wolfdieter Lang_, Jun 27 2011
%t A188009 r=(1+5^(1/2))/2; k=2;
%t A188009 t=Table[Floor[n*r]-Floor[(n-k)*r]-Floor[k*r],{n,1,220}]   (* A188009 *)
