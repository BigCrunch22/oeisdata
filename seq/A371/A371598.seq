%o A371598 (Python)
%o A371598 from sympy import fibonacci
%o A371598 def a(n):
%o A371598     a_n = 1
%o A371598     mod = fibonacci(n + 1)
%o A371598     for i in range(1, n + 1):
%o A371598         a_n = (a_n * fibonacci(i)) % mod
%o A371598     return a_n
