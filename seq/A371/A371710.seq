%o A371710 (PARI) /* Sum_{n>=0} exp(-x*A(x)^n) * A(x)^(n^2) / n! = 1 */
%o A371710 {a(n) = my(A=[0,1]); for(i=1,n, A = concat(A,0);
%o A371710 A[#A] = -polcoeff( sum(m=0,sqrtint(#A+1), exp(-x*Ser(A)^m +x*O(x^#A)) * Ser(A)^(m^2)/m! ), #A-1); ); n!*A[n+1]}
%o A371710 for(n=1,30,print1(a(n),", "))
%o A371710 (PARI) /* Sum_{n>=0} (A(x)^n - x)^n / n! = 1 */
%o A371710 {a(n) = my(A=[0,1]); for(i=1,n, A = concat(A,0);
%o A371710 A[#A] = -polcoeff( sum(m=0,#A+1, (Ser(A)^m - x)^m/m! ), #A-1) ); n!*A[n+1]}
