%t A371409 (* Uses Algorithm P from Knuth's TAOCP section 7.2.1.6 to generate A063171 *)
%t A371409 plist[n_] := Block[{a = PadLeft [{}, 2*n, {1, 0}], m = 2*n - 1, j, k},
%t A371409     Reap[
%t A371409     While[True,
%t A371409         Sow[a]; a[[m]] = 0;
%t A371409         If[a[[m - 1]] == 0,
%t A371409             a[[--m]] = 1,
%t A371409             j = m - 1; k = 2*n - 1;
%t A371409             While[j > 1 && a[[j]] == 1, a[[j--]] = 0; a[[k]] = 1; k -= 2];
%t A371409             If[j == 1, Break[]];
%t A371409             a[[j]] = 1; m = 2*n - 1]
%t A371409     ]][[2, 1]]];
%t A371409 Join[{{2}}, Array[Delete[Map[PositionIndex[#][0] &, plist[#]], 0] &, 4, 2]]
