%t A178346 EulerianNumber[n_, k_] := EulerianNumber[n, k] = Sum[(-1)^j*(k-j)^n*Binomial[n+ 1, j], {j,0,k}];
%t A178346 A178346[n_, k_, m_]:= Binomial[n, k] - m*Binomial[n, k]*Binomial[n+1, k]/(k+1) + m*EulerianNumber[n+1, k+1];
