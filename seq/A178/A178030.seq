%e A178030 7,  53, 187, 457, 911,1597,2563,3857,5527,7621,...
%p A178030 A178030 := proc(n,k)
%p A178030     if k = 0 then
%p A178030         A000032(n);
%p A178030     elif n = 0 then
%p A178030         2 ;
%p A178030     elif n = 1 then
%p A178030         1 ;
%p A178030     else
%p A178030         (procname(n-1,k)^2+(4*k+1)*(-1)^n)/procname(n-2,k) ;
%p A178030     end if;
%p A178030 end proc: # _R. J. Mathar_, May 15 2016
%t A178030 f[0, a_] := 2; f[1, a_] := 1;
%t A178030 f[n_, a_] := f[n, a] = (f[n - 1, a]^2 - (4*a + 1)*(-1)^(n - 1))/f[n - 2, a];
