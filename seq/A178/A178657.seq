%e A178657 1, 808, 56896, 1055320, 7329975, 22338816, 32152848, 22338816, 7329975, 1055320, 56896, 808, 1;
%p A178657 A001263 := proc(n,k) if n <=0 or k <=0 then 0 ; elif k > n then 0 ; else binomial(n-1,k-1)*binomial(n,k-1)/k ; end if; end proc:
%p A178657 A178657 := proc(n,k) (1-x)^(2*n-1)*add(A001263(n+2*l,2*l+1)*x^l,l=0..20) ; expand(%) ; coeftayl(%,x=0,k) ; end proc: # _R. J. Mathar_, Aug 30 2011
%t A178657 p[x_, n_] = (1 - x)^(2*n - 1)*Sum[(Binomial[2*k + n, 2*k] Binomial[ 2*k + n, 1 + 2*k]/(2*k + n))*x^k, {k, 0, Infinity}];
%t A178657 Table[CoefficientList[FullSimplify[ExpandAll[p[x, n]]], x], {n, 2, 10}];
%t A178657 Flatten[%]
