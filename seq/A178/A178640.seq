%p A178640 A178640 := proc(n,k) (-1)^(n+1)*(x-1)^(n+1)*add( (5+8*j)^n*x^j,j=0..k) ; coeftayl(%,x=0,k) ; end proc: # _R. J. Mathar_, Apr 05 2011
%t A178640 Clear[m, m0, t, n, k]
%t A178640 m0 = {{1, 1}, {1, 0}}
%t A178640 m[l_] := MatrixPower[m0, l]
%t A178640 t[l_, k_] = If[l == 0, 1, m[l][[1, 1]]*k + m[l][[1, 2]]]
%t A178640 p[x_, n_, l_] := (-1)^(n + 1)*(-1 + x)^(n + 1)*Sum[t[l, k]^ n*x^k, {k, 0, Infinity}]
%t A178640 Table[Flatten[Table[CoefficientList[FullSimplify[ExpandAll[p[x, n, l]]], x], {n, 0, 10}]], {l, 0, 10}]
