%t A232464 f[list_]:=Apply[And,Table[Count[list,i]<4,{i,1,Max[list]}]];
%t A232464 g[list_]:=Length[list]!/Apply[Times,Table[Count[list,i]!,{i,1,Max[list]}]];
%t A232464 a[n_] := If[n == 0, 1, Total[Map[g, Select[IntegerPartitions[n], f]]]];
