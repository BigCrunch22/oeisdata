%o A232642 (Haskell)
%o A232642 import Data.List.Ordered (member); import Data.List (sort)
%o A232642 a232642 n k = a232642_tabf !! (n-1) !! (k-1)
%o A232642 a232642_row n = a232642_tabf !! (n-1)
%o A232642 a232642_tabf = f a082560_tabf [] where
%o A232642    f (xs:xss) zs = ys : f xss (sort (ys ++ zs)) where
%o A232642      ys = [v | v <- xs, not $ member v zs]
%o A232642 a232642_list = concat a232642_tabf
