%t A232534 b[n_, k_, i_] := b[n, k, i] = Module[{m = i*(i + 1)/2}, If[k > n, b[k, n, i], If[i < 1, If[n == 0 && k == 0, {0}, {}], If[k >= 0 && n + k > m || k < 0 && n - 2*k > m, {}, b[n, k, i - 1] ~Union~ Map[# + x^i &, b[n + i, k + i, i - 1] ~Union~ b[n - i, k, i - 1] ~Union~ b[n, k - i, i - 1]]]]]];
