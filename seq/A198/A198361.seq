%t A198361 a = 4; b = 3; c = 1;
%t A198361 f[x_] := a*x^2 + b*x; g[x_] := c*Cos[x]
%t A198361 Plot[{f[x], g[x]}, {x, -1, 1}]
%t A198361 r1 = x /. FindRoot[f[x] == g[x], {x, -1, -.9}, WorkingPrecision -> 110]
