%t A198991 a = 4; b = -2; c = -1;
%t A198991 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c
%t A198991 Plot[{f[x], g[x]}, {x, -2, 2}, {AxesOrigin -> {0, 0}}]
