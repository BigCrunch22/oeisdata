%t A198828 a = 1; b = -3; c = 2;
%t A198828 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c
%t A198828 Plot[{f[x], g[x]}, {x, -3, 3}, {AxesOrigin -> {0, 0}}]
%t A198828 r = x /. FindRoot[f[x] == g[x], {x, 1.49, 1.5}, WorkingPrecision -> 110]
