%p A203616 isA203616:=proc(j) local a,b,c;   a:=0; b:=0;
%p A203616    for c from 2 to j-1 do
%p A203616      if abs((j mod c)-c/2)<1 then a:=a+A004086(c); b:=b+c; fi;
%p A203616    od;
