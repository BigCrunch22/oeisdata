%o A203513 (Python)
%o A203513 from operator import mul
%o A203513 from functools import reduce
%o A203513 def v(n): return 1 if n==1 else reduce(mul, [reduce(mul, [j**2 - j*k + k**2 for j in range(1, k)]) for k in range(2, n + 1)])
