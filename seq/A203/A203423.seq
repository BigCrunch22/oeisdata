%t A203423 (* First program *)
%t A203423 f[j_] := 1/(j + 1); z = 16;
%t A203423 v[n_] := Product[Product[f[k] - f[j], {j, 1, k - 1}], {k, 2, n}]
%t A203423 1/Table[v[n], {n, 1, z}]             (* A203422 *)
%t A203423 Table[v[n]/(2 v[n + 1]), {n, 1, z}]  (* this sequence *)
