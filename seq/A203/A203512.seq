%t A203512 f[j_] := j (j + 1)/2; z = 15;
%t A203512 v[n_] := Product[Product[f[k] + f[j], {j, 1, k - 1}], {k, 2, n}]
%t A203512 Table[v[n], {n, 1, z}]               (* A203511 *)
%t A203512 Table[v[n + 1]/v[n], {n, 1, z - 1}]  (* A203512 *)
