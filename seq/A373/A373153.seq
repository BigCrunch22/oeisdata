%o A373153 (PARI)
%o A373153 A002110(n) = prod(i=1,n,prime(i));
%o A373153 A373153(n) = { my(f = factor(n),u); u=sum(k=1, #f~, f[k, 2]*A002110(primepi(f[k, 1])-1))%3; if(2==u,-1,u); };
