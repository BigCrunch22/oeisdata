%o A373437 (Python)
%o A373437 from sympy import divisor_sigma as sigma
%o A373437 def P(q):
%o A373437     result = []
%o A373437     for n in range(1, q + 1):
%o A373437         if sigma(sigma(2 * n)) == 2 * sigma(sigma(n)):
%o A373437             result.append(n)
%o A373437     print(result)
%o A373437 P(10**3)
