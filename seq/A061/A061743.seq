%o A061743 (Python)
%o A061743 from sympy import primepi
%o A061743 def A061743(n):
%o A061743     def f(x): return int(n+2+primepi(x+1)+primepi(x+1>>1))
%o A061743     m, k = n, f(n)
%o A061743     while m != k: m, k = k, f(k)
%o A061743     return m # _Chai Wah Wu_, Oct 17 2024
