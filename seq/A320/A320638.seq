%p A320638 A320638 := proc(n,noprts)
%p A320638     local a, p, w, el ;
%p A320638     a := 0 ;
%p A320638     for p in combinat[partition](n) do
%p A320638         if nops(p) = noprts then
%p A320638             w := true ;
%p A320638             for el in p do
%p A320638                 if modp(n, el) <> 0 then
%p A320638                     w := false;
%p A320638                     break;
%p A320638                 end if;
%p A320638             end do:
%p A320638             if w then
%p A320638                 a := a+1 ;
%p A320638             end if;
%p A320638         end if ;
%p A320638     end do:
%p A320638     a ;
%p A320638 end proc:
