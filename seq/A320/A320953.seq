%e A320953 GENERATING METHOD.
%e A320953 Start with CF = [1] and repeat (PARI code):
%e A320953 t = (1/4)*contfracpnqn(5*CF)[1,1]/contfracpnqn(5*CF)[2,1]; CF = contfrac(t)
%e A320953 This method can be illustrated as follows.
%e A320953 t0 = [1] = 1;
%e A320953 t1 = (1/4)*[5] = [1; 4] = 5/4;
%e A320953 t2 = (1/4)*[5; 20] = [1; 3, 1, 4, 4] = 101/80;
%e A320953 t3 = (1/4)*[5; 15, 5, 20, 20] = [1; 3, 1, 3, 20, 5, 80] = 155905/123104;
%e A320953 t4 = (1/4)*[5; 15, 5, 15, 100, 25, 400] = [1; 3, 1, 3, 20, 3, 1, 3, 24, 1, 3, 6, 1600] = 5857775885/4625350304;
%e A320953 t5 = (1/4)*[5; 15, 5, 15, 100, 15, 5, 15, 120, 5, 15, 30, 8000] = [1; 3, 1, 3, 20, 3, 1, 3, 24, 1, 3, 3, 1, 1, 4, 1, 1, 3, 3, 1, 29, 3, 2, 1, 14, 1, 1, 7, 32000] = 1487626504292631625/1174642703809375904; ...
%e A320953 where this constant t equals the limit of the iterations of the above process.
%o A320953 (PARI) /* Generate over 5700 digits in the decimal expansion */
%o A320953 CF=[1];
%o A320953 {for(i=1, 12, t = (1/4)*contfracpnqn(5*CF)[1, 1]/contfracpnqn(5*CF)[2, 1];
%o A320953 CF = contfrac(t) ); }
%o A320953 for(n=1, 150, print1(floor(10^(n-1)*t)%10, ", "))
