%p A320591 seq(coeff(series(mul((1+x^k/(1+x)^k),k=1..n),x,n+1), x, n), n = 0 .. 37); # _Muniru A Asiru_, Oct 16 2018
%t A320591 nmax = 37; CoefficientList[Series[Product[(1 + x^k/(1 + x)^k), {k, 1, nmax}], {x, 0, nmax}], x]
