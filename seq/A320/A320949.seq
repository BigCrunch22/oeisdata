%o A320949 \p120
%o A320949 {a(n) = my(A=[1]); for(i=1,n, A=concat(A,0); A[#A] = round(polcoeff(sum(n=0,600, (1+x +x*O(x^#A))^(n^2) / (1 + Ser(A))^(n+1)*1. ),#A-1)));A[n+1]}
