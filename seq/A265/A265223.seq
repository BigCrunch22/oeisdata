%t A265223 lim = 51; a = {}; Do[AppendTo[a, Take[#, 2 (k - 1) + 1] &@ Take[#[[k]], -(lim + k)]], {k, Length@ #}] &@ CellularAutomaton[150, {{1}, 0}, lim]; Accumulate[Count[#, n_ /; n == 0] & /@ a] (* _Michael De Vlieger_, Dec 09 2015 *)
