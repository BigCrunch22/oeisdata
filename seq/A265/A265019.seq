%t A265019 p[l_] := With[{n = Length[l]}, n!*Det[Table[t = j-i+1; If[t<0, 0, l[[i]]^t/t!], {i, 1, n}, {j, 1, n}]]]; g[n_, i_, l_] := g[n, i, l] = If[i*(i+1)/2<n, 0, If[n==0, p[l]*x^If[l=={}, 0, l[[-1]]], g[n, i-1, l] + If[i>n, 0, g[n-i, i-1, Join[{i}, l]]]]]; b[n_] := b[n] = g[n, n, {}]; T[0] = {1}; T[k_] := Table[Coefficient[b[n], x, k], {n, k, k*(k+1)/2}]; Table[T[k], {k, 0, 8}] // Flatten (* _Jean-Fran√ßois Alcover_, Feb 11 2017, translated from Maple *)
