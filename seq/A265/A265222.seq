%o A265222 (PARI) N=100 \\ Calculate and Print N digits of the constant
%o A265222 \p500 \\ set precision
%o A265222 {z=1.0; for(i=1,5*N, z = z + 1/6 - 1/3*suminf(n=1, frac(z^n)/2^n ) ); z}
%o A265222 {a(n) = floor((10^n*z))%10}
%o A265222 {m=0;for(n=0,N, print1( floor((10^n*z))%10,","); if(m==50,m=0;print(""));m=m+1)}
