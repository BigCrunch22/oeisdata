%e A265942 in which the antidiagonal sums yield [1,0,0,0,0,0,0,0,...].
%t A265942 nmax = 20; sol = {a[0] -> 1}; Do[A[x_] = Sum[a[k] x^k, {k, 0, n}] /. sol; eq = CoefficientList[x - Sum[x^k A[-k x], {k, 1, n}] + O[x]^(n + 1), x] == 0 /. sol; sol = sol ~Join~ Solve[eq][[1]], {n, 1, nmax}];
%t A265942 sol /. Rule -> Set;
%t A265942 a /@ Range[0, nmax-1] (* _Jean-Fran√ßois Alcover_, Nov 03 2019 *)
%o A265942 (PARI) {a(n)=my(A=[1]);for(i=1,n,A=concat(A,0);A[#A]=(-1)^(#A)*Vec(sum(m=1,#A,subst(Ser(A),x,-m*x)*x^m))[#A]);A[n+1]}
%o A265942 for(n=0, 30, print1(a(n), ", "))
%o A265942 (PARI) {a(n) = if(n==0,1, sum(k=0,n-1, (-1)^(n-k+1)*a(k)*(n-k+1)^k ) )}
