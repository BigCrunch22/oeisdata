%t A265430 lim = 134; a = {}; Do[AppendTo[a, Take[#[[k]], 2 (k - 1) + 1]], {k, Floor[Length[#]/2]}] &@ CellularAutomaton[188, {{1}, 0}, lim]; Count[#, n_ /; n == 0] & /@ a (* _Michael De Vlieger_, Dec 09 2015 *)
