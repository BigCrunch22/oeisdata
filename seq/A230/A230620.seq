%p A230620 b:= proc(u, o, t) option remember;
%p A230620       `if`(t=5, (u+o)!, `if`(u+o+t<4, 0,
%p A230620       add(b(u-j, o+j-1, [1, 1, 4, 4][t]), j=1..u)+
%p A230620       add(b(u+j-1, o-j, [2, 3, 5, 3][t]), j=1..o)))
%p A230620     end:
%p A230620 a:= n-> b(n, 0, 1):
%p A230620 seq(a(n), n=0..25);
%t A230620 b[u_, o_, t_] := b[u, o, t] =
%t A230620     If[t == 5, (u + o)!, If[u + o + t < 4, 0,
%t A230620     Sum[b[u - j, o + j - 1, {1, 1, 4, 4}[[t]]], {j, 1, u}] +
%t A230620     Sum[b[u + j - 1, o - j, {2, 3, 5, 3}[[t]]], {j, 1, o}]]];
%t A230620 a[n_] := b[n, 0, 1];
%t A230620 a /@ Range[0, 25] (* _Jean-FranÃ§ois Alcover_, Dec 22 2020, after _Alois P. Heinz_ *)
