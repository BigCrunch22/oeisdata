%e A230439 For n=3 the a(3)=6 contractible tight meanders of width 3 correspond to the following pairs of subsets of {1,2}: ({},{1}), ({},{2}), ({1},{}), ({2},{}), ({1},{2}), ({2},{1}).
%p A230439 # The Maple program based on the C code by Martin Plechsmid
%p A230439 proc()
%p A230439 local n,a,b,d,r;
%p A230439 option remember;
%p A230439   if args[1]=1 then
%p A230439    1
%p A230439   elif nargs=1 then
%p A230439    2*`+`(''procname(args,[i],[j])'$'j'=1..i-1'$'i'=2..args)
%p A230439   else
%p A230439    n:=args[1]; a:=args[2]; b:=args[3];
%p A230439    if b=[] then
%p A230439     `+`('procname(n,a,[k])'$'k'=1..n)
%p A230439    elif a[1]=b[1] then
%p A230439     0
%p A230439    elif a[1]<b[1] then
%p A230439     procname(n,b,a)
%p A230439    else
%p A230439     d:=a[1]-b[1];
%p A230439     r:=irem(b[1],d);
%p A230439     if r>0 then
%p A230439      procname(n-b[1],[d-r,op(subsop(1=r,a))],subsop(1=NULL,b))
%p A230439     else
%p A230439      procname(n-b[1],subsop(1=d,a),subsop(1=NULL,b))
%p A230439     fi
%p A230439    fi
%p A230439   fi
%p A230439 end;
%t A230439 (* Mathematica program based on the C code by Martin Plechsmid *)
%t A230439 f[n_, a___, b___] := f[n, a, b] =
%t A230439   Which[
%t A230439    n == 1, 1,
%t A230439    a == Null, 2 Sum[f[n, {i}, {j}], {i, 2, n}, {j, i - 1}],
%t A230439    b == {}, Sum[f[n, a, {i}], {i, n}],
%t A230439    First[a] == First[b], 0,
%t A230439    First[a] < First[b], f[n, b, a],
%t A230439    True,
%t A230439    Block[{d = First[a] - First[b], r, s},
%t A230439     r = Mod[First[b], d];
%t A230439     s = If[r == 0, {d}, {d - r, r}];
%t A230439     f[n - First[b], Join[s, Rest[a]], Rest[b]]
%t A230439     ]
%t A230439    ]
