%F A108617 T(n,0) = T(n,n) = A000045(n);
%F A108617 T(n,1) = T(n,n-1) = A000045(n+1) for n>0;
%F A108617 T(n,2) = T(n,n-2) = A000045(n+2) - 2 = A001911(n-1) for n>1;
