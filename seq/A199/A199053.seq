%t A199053 a = 1; b = 3; c = -1;
%t A199053 f[x_] := a*x^2 + b*Sin[x]; g[x_] := c
%t A199053 Plot[{f[x], g[x]}, {x, -3, 2}, {AxesOrigin -> {0, 0}}]
%t A199053 r = x /. FindRoot[f[x] == g[x], {x, -1.4, -1.3}, WorkingPrecision -> 110]
