%t A199962 a = 1; b = 3; c = 4;
%t A199962 f[x_] := a*x^2 + b*Cos[x]; g[x_] := c*Sin[x]
%t A199962 Plot[{f[x], g[x]}, {x, -1, 3}, {AxesOrigin -> {0, 0}}]
