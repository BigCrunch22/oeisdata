%t A199072 a = 2; b = 3; c = 1;
%t A199072 f[x_] := a*x^2 + b*Sin[x]; g[x_] := c
%t A199072 Plot[{f[x], g[x]}, {x, -2, 2}, {AxesOrigin -> {0, 0}}]
