%o A364268 (PARI) f(n, m) = (subst(bernpol(m+1, x), x, n+1)-subst(bernpol(m+1, x), x, 0))/(m+1);
%o A364268 a(n, s=2, t=2) = sum(k=1, n, k^(s+t)*f(n\k, s));
