%o A364269 (PARI) f(n, m) = (subst(bernpol(m+1, x), x, n+1)-subst(bernpol(m+1, x), x, 0))/(m+1);
%o A364269 a(n, s=3, t=2) = sum(k=1, n, k^(s+t)*f(n\k, s));
