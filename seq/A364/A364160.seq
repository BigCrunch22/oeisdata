%p A364160 filter:= proc(n) local F,i;
%p A364160   F:= ifactors(n)[2];
%p A364160   if nops(F) = 1 then return true fi;
%p A364160   i:= min[index](F[..,1]);
%p A364160   andmap(t -> F[t,2] < F[i,2], {$1..nops(F)} minus {i})
%p A364160 end proc:
%p A364160 filter(1):= true:
