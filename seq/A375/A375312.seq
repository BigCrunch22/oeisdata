%t A375312 groupofunits3xn[e_] := Flatten[Prepenf[Table[Table[3^(i + 1), {Ceiling[e/3^i] - 2 Ceiling[e/3^(i + 1)] + Ceiling[e/3^(i + 2)]}], {i, 0, 10}], 2]];
