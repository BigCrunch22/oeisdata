%e A375408   57: (1,1,3,1)
%e A375408   59: (1,1,2,1,1)
%t A375408 stc[n_]:=Differences[Prepend[Join @@ Position[Reverse[IntegerDigits[n,2]],1],0]]//Reverse;
%t A375408 Select[Range[0,100],!LessEqual@@stc[#]&&!GreaterEqual@@stc[#]&]
