%e A375499   7 3 [2, 2*x - 8, (2/3)*x + 1/3]
%e A375499   8 3 [2, 4, (2/3)*x + 1/3]
%e A375499   9 4 [2, 3, 4, x]
%e A375499   10 4 [2, 3, 4, x]
%e A375499   11 4 [2, 3, 4, x]
%e A375499   12 4 [2, 3, 4, (5/11)*x + 6/11]
%e A375499   13 4 [2, 3, 4, (5/11)*x + 6/11]
%e A375499   14 4 [2, 3, 4, (5/11)*x + 6/11]
