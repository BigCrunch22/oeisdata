%o A375706 def A375706(n):
%o A375706     def f(x): return int(n+1-sum(mobius(k)*(integer_nthroot(x, k)[0]-1) for k in range(2, x.bit_length())))
%o A375706     m, k = n, f(n)
%o A375706     while m != k: m, k = k, f(k)
