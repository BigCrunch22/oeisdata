%p A285103 a:= n-> convert(Bits[Split](b(n)), `+`):
%p A285103 seq(a(n), n=0..71);  # _Alois P. Heinz_, Mar 07 2024
%t A285103 b[n_] := b[n] = If[n == 0, 1, With[{t = b[n-1]}, BitXor[2^n*t, t]]];
