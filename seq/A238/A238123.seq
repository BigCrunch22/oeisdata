%t A238123 b[n_, l_List] :=  b[n, l] = If[n<1, x^l[[-1]], b[n-1, Append[l, 1]] +  Sum[If[i == 1 || l[[i-1]] > l[[i]], b[n-1, ReplacePart[l, i -> l[[i]] + 1]], 0], {i, 1, Length[l]}]]; T[n_] := If[n == 0, 1, Function[{p}, Table[Coefficient[p, x, i], {i, 0, n}]][b[n-1, {1}]]]; Table[T[n], {n, 0, 12}] // Flatten (* _Jean-FranÃ§ois Alcover_, Jan 07 2015, translated from Maple *)
%o A238123 (PARI) (A238123(n,k)=if(k, vecsum(apply(p->n!/Hook(Vecrev(p)), select(p->p[1]==k,partitions(n,[k,n])))), !n)); Hook(P,h=vector(P[1]),L=P[#P])={prod(i=1, L, h[i]=L-i+1)*prod(i=1,#P-1, my(D=-L+L=P[#P-i]); prod(k=0,L-1,h[L-k]+=min(k,D)+1))} \\  _M. F. Hasler_, Jun 03 2018
