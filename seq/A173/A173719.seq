%o A173719 (PARI){a=2;b=3;for(n=1,12000,s=a+b;ev=eval(Vec(Str(s)));if(sum(k=1,#ev,isprime(ev[k]))==#ev,print1(s", "));a=b;b=nextprime(b+2))}
