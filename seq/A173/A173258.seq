%p A173258 b:= proc(n, i) option remember;
%p A173258       `if`(n<1 or i<1, 0, `if`(n=i, 1, add(b(n-i, i+j), j=[-1, 1])))
%p A173258     end:
%p A173258 a:= n-> `if`(n=0, 1, add(b(n, j), j=1..n)):
%p A173258 seq(a(n), n=0..70);
%t A173258 b[n_, i_] := b[n, i] = If[n < 1 || i < 1, 0, If[n == i, 1, Sum[b[n - i, i + j], {j, {-1, 1}}]]]; a[n_] := If[n == 0, 1, Sum[b[n, j], {j, 1, n}]]; Table[a[n], {n, 0, 70}] // Flatten (* _Jean-FranÃ§ois Alcover_, Dec 13 2013, translated from Maple *)
