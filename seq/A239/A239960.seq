%p A239960 seq(a(n), n=0..70);  # _Alois P. Heinz_, Apr 03 2014
%t A239960 z = 54; d[p_] := d[p] = Length[DeleteDuplicates[p]]; Table[Count[IntegerPartitions[n], p_ /; d[p] == Count[p, 1]], {n, 0, z}]
