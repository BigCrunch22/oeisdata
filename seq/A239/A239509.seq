%p A239509 b:= proc(n, i) option remember; `if`(i*(i+1)/2<n, 0,
%p A239509       `if`(n=0, 1, b(n, i-1) +`if`(i<=n and not
%p A239509       isprime(i) and issqrfree(i), b(n-i, i-1), 0)))
%p A239509     end:
%p A239509 a:= n-> b(n$2):
%p A239509 seq(a(n), n=0..100);  # _Alois P. Heinz_, Jun 02 2015
%t A239509 b[n_, i_] := b[n, i] = If[i*(i+1)/2<n, 0, If[n==0, 1, b[n, i-1] + If[i <= n && !PrimeQ[i] && SquareFreeQ[i], b[n-i, i-1], 0]]]; a[n_] := b[n, n]; Table[a[n], {n, 0, 100}] (* _Jean-FranÃ§ois Alcover_, Jan 15 2016, after _Alois P. Heinz_ *)
