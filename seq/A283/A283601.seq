%S A283601 1,11,101,1011,10101,111011,1000101,10011011,100000101,1100011011,
%T A283601 10100000101,101001011011,1010000000101,10110100011011,
%U A283601 101000000000101,1111010001011011,10100000000000101,111101010100011011,1010001010000000101,11110011100001011011
%N A283601 Binary representation of the x-axis, from the origin to the right edge, of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 662", based on the 5-celled von Neumann neighborhood.
%C A283601 Initialized with a single black (ON) cell at stage zero.
%D A283601 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A283601 Robert Price, <a href="/A283601/b283601.txt">Table of n, a(n) for n = 0..126</a>
%H A283601 Robert Price, <a href="/A283601/a283601.tmp.txt">Diagrams of first 20 stages</a>
%H A283601 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A283601 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A283601 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A283601 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A283601 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A283601 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A283601 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A283601 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A283601 code = 662; stages = 128;
%t A283601 rule = IntegerDigits[code, 2, 10];
%t A283601 g = 2 * stages + 1; (* Maximum size of grid *)
%t A283601 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A283601 ca = a;
%t A283601 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A283601 PrependTo[ca, a];
%t A283601 (* Trim full grid to reflect growth by one cell at each stage *)
%t A283601 k = (Length[ca[[1]]] + 1)/2;
%t A283601 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A283601 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
