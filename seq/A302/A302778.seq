%t A302778 s[n_] := Boole[n > 1 && Length[(f = FactorInteger[n])] == 1 && (e = f[[;; , 2]]) == 2^IntegerExponent[e, 2]]; Accumulate @ Array[s, 100] (* _Amiram Eldar_, Nov 27 2020 *)
%o A302778 (PARI)
%o A302778 A209229(n) = (n && !bitand(n,n-1));
