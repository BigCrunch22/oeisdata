%N A302046 A filter sequence analogous to A101296 for nonstandard factorization based on the sieve of Eratosthenes (A083221).
%C A302046 Restricted growth sequence transform of A278524.
%C A302046 See A302042 for the description of the nonstandard factorization employed here.
%C A302046 For all i, j:
%C A302046   a(i) = a(j) => A253557(i) = A253557(j).
%C A302046   a(i) = a(j) => A302041(i) = A302041(j).
%C A302046   a(i) = a(j) => A302050(i) = A302050(j).
%C A302046   a(i) = a(j) => A302051(i) = A302051(j) => A302052(i) = A302052(j).
%H A302046 Antti Karttunen, <a href="/A302046/b302046.txt">Table of n, a(n) for n = 1..65537</a>
%H A302046 <a href="/index/Si#sieve">Index entries for sequences generated by sieves</a>
%o A302046 (PARI)
%o A302046 up_to = 32769;
%o A302046 rgs_transform(invec) = { my(om = Map(), outvec = vector(length(invec)), u=1); for(i=1, length(invec), if(mapisdefined(om,invec[i]), my(pp = mapget(om, invec[i])); outvec[i] = outvec[pp] , mapput(om,invec[i],i); outvec[i] = u; u++ )); outvec; };
%o A302046 write_to_bfile(start_offset,vec,bfilename) = { for(n=1, length(vec), write(bfilename, (n+start_offset)-1, " ", vec[n])); }
%o A302046 A020639(n) = { if(1==n,n,vecmin(factor(n)[, 1])); };
%o A302046 A078898(n) = { if(n<=1,n, my(spf=A020639(n),k=1,m=n/spf); while(m>1,if(A020639(m)>=spf,k++); m--); (k)); };
%o A302046 A001511(n) = 1+valuation(n,2);
%o A302046 A302045(n) = A001511(A078898(n));
%o A302046 A302044(n) = if(1==n,n,my(k=0); while((n%2), n = A268674(n); k++); n = (n/2^valuation(n, 2)); while(k>0, n = A250469(n); k--); (n));
%o A302046 A302041(n) = if(1==n, 0,1+A302041(A302044(n)));
%o A302046 Aux302046(n) = if(1==n,n, my(k=A302041(n), v = vector(k),i=1); while(n>1,v[i] = A302045(n); n = A302044(n); i++); vecsort(v));
%o A302046 write_to_bfile(1,rgs_transform(vector(up_to,n,Aux302046(n))),"b302046.txt");
