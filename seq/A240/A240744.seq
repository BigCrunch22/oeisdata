%p A240744 b:= proc(n, i) option remember; `if`(n=0, 1, series(
%p A240744       add(b(n-j, i+1)*`if`(i=j, x, 1), j=1..n), x, 10))
%p A240744     end:
%p A240744 a:= n-> coeff(b(n, 1), x, 9):
%p A240744 seq(a(n), n=45..80);
%t A240744 b[n_, i_] := b[n, i] = If[n == 0, 1, Series[Sum[b[n - j, i + 1]*If[i == j, x, 1], {j, 1, n}], {x, 0, 10}]]; a[n_] := SeriesCoefficient[b[n, 1], {x, 0, 9}]; Table[a[n], {n, 45, 80}] (* _Jean-Fran√ßois Alcover_, Nov 06 2014, after Maple *)
