%p A240605 b:= proc(x, y, m, v, d) option remember; `if`(y>x or y<0 or
%p A240605        y>m or v and y=m and d=1 or y=0 and irem(x, 2)=1, 0,
%p A240605       `if`(x=0, 1, `if`(v and y=m or y=0, 0, b(x-1, y, m, v,
%p A240605       `if`(d=2, 2, 1-d)))+ `if`(y=0 or y=1 and irem(x, 2)=0, 0,
%p A240605        b(x-1, y-1, m, v, `if`(d=2, `if`(v and y=m, 1, 2), 1-d)))+
%p A240605        b(x-1, y+1, m, v, `if`(d=2, 2, 1-d))))
%p A240605     end:
%p A240605 a:= n-> b(2*n-2, 0, iquo(n, 2, 'r'), r=0, 2):
%p A240605 seq(a(n), n=2..30);  # _Alois P. Heinz_, May 09 2014
%t A240605 b[x_, y_, m_, v_, d_] := b[x, y, m, v, d] = If[y>x || y<0 || y>m || v && y == m && d==1 || y==0 && Mod[x, 2]==1, 0, If[x==0, 1, If[v && y==m || y==0, 0, b[x-1, y, m, v, If[d==2, 2, 1-d]]] + If[y==0 || y==1 && Mod[x, 2]==0, 0, b[x-1, y-1, m, v, If[d==2, If[v && y==m, 1, 2], 1-d]]] + b[x-1, y+1, m, v, If[d==2, 2, 1-d]]]]; a[n_] := b[2*n-2, 0, Quotient[n, 2], Mod[ n, 2]==0, 2]; Table[a[n], {n, 2, 30}] (* _Jean-Fran√ßois Alcover_, Feb 24 2016, after _Alois P. Heinz_ *)
