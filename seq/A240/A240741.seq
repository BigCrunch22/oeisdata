%p A240741 b:= proc(n, i) option remember; `if`(n=0, 1, series(
%p A240741       add(b(n-j, i+1)*`if`(i=j, x, 1), j=1..n), x, 7))
%p A240741     end:
%p A240741 a:= n-> coeff(b(n, 1), x, 6):
%p A240741 seq(a(n), n=21..60);
%t A240741 b[n_, i_] := b[n, i] = If[n == 0, 1, Series[Sum[b[n-j, i+1]*If[i == j, x, 1], {j, 1, n}], {x, 0, 7}]]; a[n_] := SeriesCoefficient[b[n, 1], {x, 0, 6}]; Table[a[n], {n, 21, 60}] (* _Jean-Fran√ßois Alcover_, Nov 07 2014, after Maple *)
