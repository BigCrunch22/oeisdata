%p A240743 seq(a(n), n=36..70);
%t A240743 b[n_, i_] := b[n, i] = If[n == 0, 1, Series[Sum[b[n - j, i + 1]*If[i == j, x, 1], {j, 1, n}], {x, 0, 9}]]; a[n_] := SeriesCoefficient[b[n, 1], {x, 0, 8}]; Table[a[n], {n, 36, 70}] (* _Jean-Fran√ßois Alcover_, Nov 06 2014, after Maple *)
