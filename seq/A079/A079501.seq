%p A079501 b:= proc(n, s) option remember; `if`(n=0, 1, add(
%p A079501       `if`(n-j>0 and n-j<=s, 0, b(n-j, s)), j=s+1..n))
%p A079501     end:
%p A079501 a:= n-> 1 +add(b(n-j, j), j=1..n/2):
%p A079501 seq(a(n), n=1..60);  # _Alois P. Heinz_, Apr 29 2014
%t A079501 b[n_, s_] := b[n, s] = If[n == 0, 1, Sum[ If[n - j > 0 && n - j <= s, 0, b[n - j, s]], {j, s + 1, n}]]; a[n_] := 1 + Sum[b[n - j, j], {j, 1, n/2}]; Table[a[n], {n, 1, 60}] (* _Jean-FranÃ§ois Alcover_, Jan 16 2015, after _Alois P. Heinz_ *)
