%t A079400 a[n_] := Module[{p = Prime[n]}, p^11 * RamanujanTau[p]]; Array[a, 12] (* _Amiram Eldar_, Jan 10 2025 *)
%o A079400 (PARI) T(n)=n*(n+1)/2;
%o A079400 rtau3(n)=local(y, j); y=0; j=1; while (T(j-1)<n, j++); j--; for (i=1, j, y=y-(-1)^i*(2*i-1)*x^(T(i-1))); y=y^8; polcoeff(y, n-1);
%o A079400 forprime (p=2, 40, print1(p^11*rtau3(p), ", "))
