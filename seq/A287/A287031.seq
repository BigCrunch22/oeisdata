%o A287031 (PARI)
%o A287031 A286781_ser(N, t='t) = {
%o A287031   my(x='x+O('x^N), y0=1+O('x^N), y1=0, n=1);
%o A287031   while(n++,
%o A287031     y1 = (1 + x*y0 + 2*x^2*y0')*(1 - x*y0*(1-t))/(1-x*y0)^2;
%o A287031     if (y1 == y0, break()); y0 = y1; );
%o A287031   y0;
%o A287031 };
%o A287031 A286782_ser(N, t='t) = my(s=A286781_ser(N, t)); 1 + x*s + 2*x^2 * deriv(s,'x);
%o A287031 Kol(K, N=20) = {
%o A287031   my(v = A286782_ser(N+K+1, 't+O('t^(K+1))));
%o A287031   vector(N, n, polcoeff(polcoeff(v, K+n), K));
%o A287031 };
