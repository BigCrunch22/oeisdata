%t A287963 b[n_, j_] := b[n, j] = If[n == j, 1, Sum[b[n - j, i]*i*(Binomial[j - 1, i - 2] + (i - 1)/2*Binomial[j - 1, i - 3]), {i, 2, Min[j + 3, n - j]}]];
%t A287963 a[n_] := If[n == 0, 1, b[n, 1] + b[n, 2]];
