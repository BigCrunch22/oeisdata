%S A287710 1,1,0,0,11,11,0,0,1,1,0,0,11001,11001,0,0,10000000,10000000,1111000,
%T A287710 1111000,10000111,10000111,1111000,1111000,10010000111,10010000111,
%U A287710 1111000,1111000,111,111,111000,111000,10011010111,10011010111,1100001001000,1100001001000
%N A287710 Binary representation of the diagonal from the corner to the origin of the n-th stage of growth of the two-dimensional cellular automaton defined by "Rule 326", based on the 5-celled von Neumann neighborhood.
%C A287710 Initialized with a single black (ON) cell at stage zero.
%D A287710 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A287710 Robert Price, <a href="/A287710/b287710.txt">Table of n, a(n) for n = 0..126</a>
%H A287710 Robert Price, <a href="/A287710/a287710.tmp.txt">Diagrams of first 20 stages</a>
%H A287710 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A287710 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A287710 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A287710 Wolfram Research, <a href="http://atlas.wolfram.com/">Wolfram Atlas of Simple Programs</a>
%H A287710 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A287710 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A287710 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A287710 CAStep[rule_, a_] := Map[rule[[10 - #]] &, ListConvolve[{{0, 2, 0},{2, 1, 2}, {0, 2, 0}}, a, 2],{2}];
%t A287710 code = 326; stages = 128;
%t A287710 rule = IntegerDigits[code, 2, 10];
%t A287710 g = 2 * stages + 1; (* Maximum size of grid *)
%t A287710 a = PadLeft[{{1}}, {g, g}, 0,Floor[{g, g}/2]]; (* Initial ON cell on grid *)
%t A287710 ca = a;
%t A287710 ca = Table[ca = CAStep[rule, ca], {n, 1, stages + 1}];
%t A287710 PrependTo[ca, a];
%t A287710 (* Trim full grid to reflect growth by one cell at each stage *)
%t A287710 k = (Length[ca[[1]]] + 1)/2;
%t A287710 ca = Table[Table[Part[ca[[n]] [[j]],Range[k + 1 - n, k - 1 + n]], {j, k + 1 - n, k - 1 + n}], {n, 1, k}];
%t A287710 Table[FromDigits[Part[ca[[i]] [[i]], Range[i, 2 * i - 1]], 10], {i, 1, stages - 1}]
%Y A287710 Cf. A287711, A287712, A287713.
