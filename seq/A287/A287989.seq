%t A287989 b[n_, k_, j_] := b[n, k, j] = If[n == j, 1, Sum[Sum[b[n - j, t, i]* Binomial[i, t]*Binomial[j - 1, i - 1 - t], {t, Range[Max[1, i - j], Min[n - j, i - 1]] ~Complement~ {k}}], {i, 1, n - j}]];
%t A287989 a[n_] := If[n == 0, 1, Sum[b[n, k, k], {k, 1, n}]];
