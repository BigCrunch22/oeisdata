%o A350348 def A350348_list(nmax):
%o A350348     a=[]
%o A350348     for n in range(nmax):
%o A350348         a.append(next(k for k in count(1) if k not in a and all(Matrix((n-r)//2+1,(n-r)//2+1,lambda i,j:(a[r:]+[k])[i+j]).det()!=0 for r in range(n-2,-1,-2))))
