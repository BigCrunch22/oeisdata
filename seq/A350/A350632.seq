%e A350632          . . . . . . . . . . .
%e A350632 - so a(0) = 0,
%e A350632      a(1) = 0 + 1 = 1,
%e A350632      a(2) = 1 + 8 = 9,
%e A350632      a(3) = 9 + 12 = 21,
%e A350632      a(4) = 21 + 24 = 45,
%e A350632      a(5) = 45 + 12 = 57.
