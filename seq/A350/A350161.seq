%t A350161 T[n_, k_] := Sum[(-1)^(j + 1) * Floor[n/(2*j - 1)]^k, {j, 1, n}]; Table[T[k, n - k + 1], {n, 1, 11}, {k, 1, n}] // Flatten (* _Amiram Eldar_, Dec 18 2021 *)
%o A350161 (PARI) T(n, k) = sum(j=1, n, (-1)^(j+1)*(n\(2*j-1))^k);
%o A350161 (PARI) T(n, k) = sum(j=1, n, sumdiv(j, d, kronecker(-4, j/d)*(d^k-(d-1)^k)));
