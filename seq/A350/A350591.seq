%o A350591 from sympy import isprime, primerange
%o A350591 def ok320102(p):
%o A350591     onelocs = (i for i, bi in enumerate(bin(p)[2:][::-1]) if bi == '1')
%o A350591     return not any(isprime(p-2**k) for k in onelocs)
%o A350591 def aupto(limit):
%o A350591     alst = []
%o A350591     A350591 = set(p for p in primerange(1, limit+1) if ok320102(p))
%o A350591     for p in primerange(1, limit+1):
%o A350591         onelocs = (i for i, bi in enumerate(bin(p)[2:][::-1]) if bi == '1')
%o A350591         if any(p-2**k in A350591 for k in onelocs):
%o A350591             alst.append(p)
%o A350591     return alst
