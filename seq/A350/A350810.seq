%o A350810 (Python)
%o A350810 def R(n): return int(str(n)[::-1])
%o A350810 def a(n):
%o A350810     Rn2 = R(n**2)
%o A350810     q, r = divmod((n-Rn2)**2, n+Rn2)
%o A350810     return q if r == 0 else q + 1
%o A350810 print([a(n) for n in range(1, 67)]) # _Michael S. Branicky_, Jan 17 2022
