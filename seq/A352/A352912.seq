%o A352912 (Python)
%o A352912 from sympy import isprime, prime
%o A352912 from itertools import count, islice
%o A352912 def agen(): # generator of terms
%o A352912     for n in count(1):
%o A352912         pn, fk = prime(n), 1
%o A352912         for k in range(1, pn+1):
%o A352912             if isprime(pn + fk): yield pn + fk
%o A352912             fk *= k
%o A352912 print(list(islice(agen(), 51))) # _Michael S. Branicky_, Apr 16 2022
