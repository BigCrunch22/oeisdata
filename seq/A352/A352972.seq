%N A352972 a(n) = Sum_{j=0..2*n} Sum_{k=0..j} A026536(j, k).
%H A352972 G. C. Greubel, <a href="/A352972/b352972.txt">Table of n, a(n) for n = 0..500</a>
%F A352972 a(n) = Sum_{j=0..2*n} Sum_{k=0..j} A026536(j, k).
%t A352972 T[n_, k_]:= T[n, k]= If[k==0 || k==2*n, 1, If[k==1 || k==2*n-1, Floor[n/2], If[EvenQ[n], T[n-1, k-2] +T[n-1, k-1] +T[n-1, k], T[n-1, k-2] +T[n-1, k]] ]];
%t A352972 A352972[n_]:= A352972[n]= Sum[T[j,k], {j,0,2*n}, {k,0,j}];
%t A352972 Table[A352972[n], {n,0,40}]
%o A352972 (SageMath)
%o A352972 @CachedFunction
%o A352972 def T(n, k): # A026536
%o A352972     if k == 0 or k == 2*n: return 1
%o A352972     elif k == 1 or k == 2*n-1: return n//2
%o A352972     elif n % 2 == 1: return T(n-1, k-2) + T(n-1, k)
%o A352972     return T(n-1, k-2) + T(n-1, k-1) + T(n-1, k)
%o A352972 def A352972(n): return sum(sum(T(j,k) for k in (0..j)) for j in (0..2*n))
