%p A086956 seq(A[i],i=1..N); # _Robert Israel_, Aug 01 2019
%t A086956 nn = 100;
%t A086956 V = Table[0, {nn}];
%t A086956 For[n = 1, n <= nn, n++,
%t A086956      Dn = Select[Divisors[n], V[[#]] <= 2&];
%t A086956      v = Min[Dn];
%t A086956      a[n] = v; V[[v]] = V[[v]]+1];
