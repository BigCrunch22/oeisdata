%t A086191 b[n_, i_, s_] := b[n, i, s] = Module[{f}, If[n == 0 || i == 1, 1, If[i<2, 0, f = FactorInteger[i][[All, 1]]; b[n, i-1, Select[s, #<i&]] + If[i <= n && f~Intersection~s == {}, b[n-i, i-1, Select[s~Union~f, #<i&]], 0]]]];
%t A086191 c[n_] := b[n, n, {}] - b[n-1, n-1, {}];
%t A086191 a[n_] := c[Prime[n]];
