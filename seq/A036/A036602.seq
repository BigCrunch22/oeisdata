%t A036602 b[n_, h_] := b[n, h] = If[n < 2, n, If[h < 1, 0, If[OddQ[n], 0, Function[t, t*(1-t)/2][b[n/2, h-1]]] + Sum[b[i, h-1]*b[n-i, h-1], {i, 1, n/2}]]];
%t A036602 A[n_, k_] := b[k+1, n];
