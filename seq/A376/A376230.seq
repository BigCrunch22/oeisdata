%o A376230 (PARI) /* Series_Reversion(x / Product_{n>=0} (1 + x^(2^n) + x^(2^(n+1))) ) */
%o A376230 {a(n) = my(A = serreverse(x / prod(k=0, #binary(n), (1 + x^(2^k) + x^(2^(k+1))) +x*O(x^n))));  polcoeff(A, n)}
%o A376230 for(n=1, 30, print1(a(n), ", "))
%o A376230 (PARI) /* A(x)^2 = A( x*A(x) + x*A(x)^2 + x*A(x)^3 ) */
%o A376230 {a(n) = my(A=[1], F); for(i=1, n, A=concat(A, 0); F=x*Ser(A);
%o A376230 A[#A] = -polcoeff( F^2 - subst(F, x, x*F + x*F^2 + x*F^3), #A+1) ); A[n]}
%o A376230 for(n=1, 30, print1(a(n), ", "))
