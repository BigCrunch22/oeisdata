%e A376335   ...
%t A376335 b[n_,k_]:=Sum[Binomial[n,j],{j,0,k}]; T[n_,k_]:=If[0<=k<=n-2,b[n-1,k],b[n-1,2n-4-k]]; Table[T[n,k],{n,2,10},{k,0,2n-4}]//Flatten
