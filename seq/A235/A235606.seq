%t A235606 amax = nmax = 10; km0 = 10; Clear[dd]; L[a_, s_, km_] := Sum[JacobiSymbol[ -a, 2k+1]/(2k+1)^s, {k, 0, km}]; d[1, n_, km_] := 2(2n-1)! L[-1, 2n, km] (2/Pi)^(2n) // Round; d[a_ /; a>1, n_, km_] := (2n-1)! L[-a, 2n, km] (2a/ Pi)^(2n)/Sqrt[a] // Round; dd[km_] := dd[km] = Table[d[a, n, km], {a, 1, amax}, {n, 1, nmax}]; dd[km0]; dd[km = 2km0]; While[dd[km] != dd[km/2, km = 2km]]; A235606 = dd[km]; Table[A235606[[ a-n+1, n]], {a, 1, amax}, {n, 1, a}] // Flatten (* _Jean-FranÃ§ois Alcover_, Feb 05 2016 *)
%t A235606 dds[b_, nm_] := With[{ns = Range[nm]}, (-1)^(ns - 1) If[Mod[b, 4] == 1, Sum[JacobiSymbol[k, b] (b - 4 k)^(2 ns - 1), {k, 1, (b - 1)/2}], Sum[JacobiSymbol[b, 2 k + 1] (b - (2 k + 1))^(2 ns - 1), {k, 0, (b - 2)/2}]]];
%t A235606 dsfs[1, nm_] := dsfs[1, nm] = (2 Range[nm] - 1)! CoefficientList[Series[Tan[x], {x, 0, 2 nm - 1}]/x, x^2];
%t A235606 dsfs[b_, nm_] := dsfs[b, nm] = Fold[Function[{ds, dd}, Append[ds, dd - Sum[ds[[-i]] (-b^2)^i Binomial[2 Length[ds] + 1, 2 i], {i, Length[ds]}]]], {}, dds[b, nm]];
%t A235606 rowA235606[a_, nm_] := With[{facs = FactorInteger[a], ns = Range[nm]}, With[{b = Times @@ (#^Mod[#2, 2] &) @@@ facs}, If[a == b, dsfs[b, nm], If[b == 1, 1/2, 1] dsfs[b, nm] Sqrt[a/b]^(4 ns - 1) Times @@ Cases[facs, {p_, e_} /; p > 2 && e > 1 :> 1 - JacobiSymbol[b, p]/p^(2 ns)]]]];
%t A235606 arr = Table[rowA235606[a, 10], {a, 10}];
%t A235606 Flatten[Table[arr[[r - n + 1, n]], {r, Length[arr]}, {n, r}]] (* _Matthew House_, Oct 30 2024 *)
