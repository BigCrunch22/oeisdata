%o A342632 (Python)
%o A342632 import math
%o A342632 for n in range (0, 21):
%o A342632      counter = 0
%o A342632      for x in range (1, pow(2, n)+1):
%o A342632         for y in range(1, pow(2, n)+1):
%o A342632             if math.gcd(y, x) ==  1:
%o A342632                 counter += 1
%o A342632      print(n, counter)
