%p A167373 A167373 := proc(n)
%p A167373     option remember;
%p A167373     if n < 4 then
%p A167373         op(n+1,[1,3,-1,-2]) ;
%p A167373     else
%p A167373         procname(n-3) ;
%p A167373     end if;
%p A167373 end proc:
%p A167373 seq(A167373(n),n=0..20) ; # _R. J. Mathar_, Feb 06 2020
