%t A262146 a[ n_] := SeriesCoefficient[ - x^(-5/8) EllipticTheta[ 2, 0, x^3] / EllipticTheta[ 2, 0, x^(1/2)], {x, 0, 2 n + 1}];
%o A262146 (PARI) {a(n) = my(A); if( n<0, 0, n = 2*n + 1; A = x * O(x^n); polcoeff( - eta(x + A) * eta(x^12 + A)^2 / (eta(x^2 + A)^2 * eta(x^6 + A)), n))};
