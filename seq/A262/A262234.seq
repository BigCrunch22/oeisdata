%t A262234 b[u_, o_, h_] := b[u, o, h] = If[u + o == 0, 1, Sum[Sum[b[u - j, o + j - 1, h + i - 1], {i, 1, u + o - h}], {j, 1, u}] + Sum[Sum[b[u + j - 1, o - j, h - i], {i, 1, h}], {j, 1, o}]];
%t A262234 a[n_] := If[n == 0, 1, Sum[b[j - 1, n - j, n - j], {j, 1, n}]];
