%p A267632 A267632 := proc(n,k)
%p A267632     local a,msel,p;
%p A267632     a := 0 ;
%p A267632     for msel in combinat[choose](n,k) do
%p A267632         if modp(add(p,p=msel),n) = 0 then
%p A267632             a := a+1 ;
%p A267632         end if;
%p A267632     end do:
%p A267632     a;
%p A267632 end proc: # _R. J. Mathar_, May 15 2016
