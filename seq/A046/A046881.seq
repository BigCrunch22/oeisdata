%e A046881 65 = 1^2 + 8^2 = 4^2 + 7^2;
%e A046881 1729 = 1^3 + 12^3 = 9^3 + 10^3; etc.
%t A046881 (* This naive program is not convenient for n > 3 *) r[n_, k_] := Reduce[0 < x < y && x^n + y^n == k, {x, y}, Integers]; a[n_] := Catch[ For[ k = 1, True, k++, rk = r[n, k]; If[rk =!= False, If[ Head[rk] == Or && Length[rk] == 2, Print["n = ", n, ", k = ", k]; Throw[k]]]]]; Table[a[n], {n, 1, 3}] (* _Jean-FranÃ§ois Alcover_, Jul 30 2013 *)
