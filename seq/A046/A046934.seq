%p A046934 alias(PS = ListTools:-PartialSums):
%p A046934 A046934Triangle := proc(len) local a, k, P, T; a := 0; P := [1]; T := [];
%p A046934 for k from 1 to len  do T := [op(T), P]; P := PS([a, op(P)]); a := P[-1] od;
