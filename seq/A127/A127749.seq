%p A127749 A127749 := proc(n,k)
%p A127749     option remember ;
%p A127749     if k > n then
%p A127749         0 ;
%p A127749     elif k = n then
%p A127749         2*n+1 ;
%p A127749     else
%p A127749         -(2*k+1)*add( procname(n,i)/(2*i+1),i=k+1..min(n,2*k)) ;
