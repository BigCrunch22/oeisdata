%p A127749 seq(seq( A127749(n,k),k=0..n),n=0..20) ; # _R. J. Mathar_, Feb 09 2021
%t A127749 nmax = 10;
%t A127749 A[n_, k_] := If[k <= n <= 2k, 1/(2n+1), 0];
%t A127749 invA = Inverse[Table[A[n, k], {n, 0, nmax}, {k, 0, nmax}]];
%t A127749 T[n_, k_] := invA[[n+1, k+1]];
