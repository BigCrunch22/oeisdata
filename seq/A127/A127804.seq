%t A127804 rows = 30;
%t A127804 A[n_, k_] := If[k <= n, If[n <= 2 k, 1/(2*2^n - 1), 0], 0];
%t A127804 T = Table[A[n, k], {n, 0, rows-1}, {k, 0, rows-1}] // Inverse;
%t A127804 a[n_] := T[[n+1]] // Total;
