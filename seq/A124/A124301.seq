%o A124301 (Python)
%o A124301 from itertools import count
%o A124301 from sympy import mobius
%o A124301 def A124301(n):
%o A124301     c, m = 1, 1
%o A124301     for i in count(2):
%o A124301         k = mobius(i)
%o A124301         if m!=k:
%o A124301             if c>n:
%o A124301                 return i-c
%o A124301             c = 0
%o A124301         c += 1
%o A124301         m = k # _Chai Wah Wu_, Oct 08 2024
