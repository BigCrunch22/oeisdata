%p A124094 b:= proc(n, i, m) option remember;
%p A124094       if n<0 then 0
%p A124094     elif n=0 then 1
%p A124094     elif i<1 then 0
%p A124094     else b(n, i-1, m) +`if`(igcd(m, i)=1, b(n-i, i, m), 0)
%p A124094       fi
%p A124094     end:
%p A124094 T:= (n, m)-> b(n, n, m):
%p A124094 seq (seq (T(n, 1+d-n), n=0..d), d=0..13);  # _Alois P. Heinz_, Sep 28 2011
%t A124094 b[n_, i_, m_] := b[n, i, m] = Which[n < 0, 0, n == 0, 1, i < 1, 0, True, b[n, i-1, m] + If[GCD[m, i] == 1, b[n-i, i, m], 0]]; t[n_, m_] := b[n, n, m]; Table[Table[t[n, 1+d-n], {n, 0, d}], {d, 0, 13}] // Flatten (* _Jean-FranÃ§ois Alcover_, Jan 10 2014, translated from _Alois P. Heinz_'s Maple code *)
%o A124094 (PARI) sigmastar(n,m)= { local(d,res=0) ; d=divisors(n) ; for(i=1,matsize(d)[2], if( gcd(d[i],m)==1, res += d[i] ; ) ; ) ; return(res) ; } f(n,m)= { local(qvec=vector(n+1,i,gcd(1,m))) ; for(i=1,n, qvec[i+1]=sum(k=1,i,sigmastar(k,m)*qvec[i-k+1])/i ; ) ; return(qvec[n+1]) ; } { for(d=1,18, for(c=0,d-1, r=d-c ; print1(f(c,r),",") ; ) ; ) ; }
