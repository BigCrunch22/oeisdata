%p A343799 b:= proc(n, t) option remember; `if`(n=0, [1, 0],
%p A343799      `if`(t=0, 0, (p-> p+[0, p[2]])(b(n, iquo(t, 2)))+
%p A343799                   (p-> p+[0, p[1]])(b(n-1, t+1))))
%p A343799     end:
%p A343799 a:= n-> b(n, 1)[2]:
%p A343799 seq(a(n), n=0..30);
%t A343799 b[n_, t_] := b[n, t] = If[n == 0, {1, 0}, If[t == 0, {0, 0},
%t A343799      With[{p = b[n, Quotient[t, 2]]}, p + {0, p[[2]]}] +
%t A343799      With[{p = b[n - 1, t + 1]}, p + {0, p[[1]]}]]];
%t A343799 a[n_] := b[n, 1][[2]];
%t A343799 Table[a[n], {n, 0, 30}] (* _Jean-Fran√ßois Alcover_, May 02 2022, after _Alois P. Heinz_ *)
