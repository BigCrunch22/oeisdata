%t A343489 T[n_, k_] := Sum[If[k == (g = GCD[j, n] - 1) == 0, 1, k^g], {j, 1, n}]; Table[T[k, n - k], {n, 0, 11}, {k, 0, n}] // Flatten (* _Amiram Eldar_, Apr 17 2021 *)
%o A343489 (PARI) T(n, k) = sum(j=1, n, k^(gcd(j, n)-1));
%o A343489 (PARI) T(n, k) = if(n==0, 0, sumdiv(n, d, eulerphi(n/d)*k^(d-1)));
