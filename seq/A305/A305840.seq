%t A305840 nn = 39; f[x_] := Product[(1 + x^n)^a[n], {n, 1, nn}]; sol = SolveAlways[0 == Series[f[x] - 1/(1 + ContinuedFractionK[-x^k, 1, {k, 1, nn}]), {x, 0, nn}], x]; Table[a[n], {n, 1, nn}] /. sol // Flatten
