%o A129135 A129135_list, m, x = [], 1, 0
%o A129135 for n in range(5,21):
%o A129135     x, m = x*n + m*binomial(n,5), -m
%o A129135     A129135_list.append(x) # _Chai Wah Wu_, Nov 01 2014
