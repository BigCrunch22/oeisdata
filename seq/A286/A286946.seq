%N A286946 a(1) = 1; a(n+1) = Sum_{k=1..n} a(n)/gcd(a(k),a(n)).
%H A286946 Robert Israel, <a href="/A286946/b286946.txt">Table of n, a(n) for n = 1..481</a>
%H A286946 <a href="/index/Lc#lcm">Index entries for sequences related to lcm's</a>
%F A286946 a(1) = 1; a(n+1) = Sum_{k=1..n} lcm(a(k),a(n))/a(k).
%e A286946 a(1) = 1;
%e A286946 a(2) = a(1)/gcd(a(1),a(1)) = 1/gcd(1,1) = 1;
%e A286946 a(3) = a(2)/gcd(a(1),a(2)) + a(2)/gcd(a(2),a(2)) = 1/gcd(1,1) + 1/gcd(1,1) = 2;
%e A286946 a(4) = a(3)/gcd(a(1),a(3)) + a(3)/gcd(a(2),a(3)) + a(3)/gcd(a(3),a(3)) = 2/gcd(1,2) + 2/gcd(1,2) + 2/gcd(2,2) = 5, etc.
%p A286946 A[1]:= 1:
%p A286946 for n from 1 to 50 do
%p A286946   A[n+1]:= add(A[n]/igcd(A[k],A[n]),k=1..n)
%p A286946 od:
%p A286946 seq(A[i],i=1..50); # _Robert Israel_, Sep 01 2017
%t A286946 a[1] = 1; a[n_] := a[n] = Sum[a[n - 1]/GCD[a[k - 1], a[n - 1]], {k, 2, n}]; Table[a[n], {n, 26}]
%t A286946 a[1] = 1; a[n_] := a[n] = Sum[LCM[a[k - 1], a[n - 1]]/a[k - 1], {k, 2, n}]; Table[a[n], {n, 26}]
