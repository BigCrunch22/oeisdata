%e A098424 a(15) = #{(5,7,11),(7,11,13),(11,13,17),(13,17,19)} = 4.
%t A098424 With[{pts=Select[Partition[Prime[Range[1200]],3,1],Last[#]-First[#] == 6&]}, Table[Count[pts,_?(First[#]<=n&)],{n,110}]] (* _Harvey P. Dale_, Nov 09 2011 *)
