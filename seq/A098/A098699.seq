%o A098699 from sympy import factorint
%o A098699 def A098699(n):
%o A098699     if n < 2:
%o A098699         return n+1
%o A098699     for m in range(1,(n**2>>2)+1):
%o A098699         if sum((m*e//p for p,e in factorint(m).items())) == n:
%o A098699             return m
