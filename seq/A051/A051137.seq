%t A051137 b[n_, k_] := DivisorSum[n, EulerPhi[#]*k^(n/#) &] / n;
%t A051137 c[n_, k_] := If[EvenQ[n], (k^(n/2) + k^(n/2+1))/2, k^((n+1)/2)];
%t A051137 T[0, _] = 1; T[n_, k_] := (b[n, k] + c[n, k])/2;
