%o A051046    (\x -> fromIntegral (a000720 x) > hs !! (x - 1)) [1..]
%o A051046    where hs = zipWith (/)
%o A051046               [1..] $ map (subtract 1.5) $ scanl1 (+) $ map (1 /) [1..]
