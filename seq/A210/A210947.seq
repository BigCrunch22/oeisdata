%t A210947 p[f_, g_] := With[{m = Max[Length[f], Length[g]]}, PadRight[f, m, 0] + PadRight[g, m, 0]]; b[n_, i_] := b[n, i] = Module[{f, g}, If[n == 0, {1}, If[i == 1, {1, n}, f = b[n, i-1]; g = If[i>n, {0}, b[n-i, i]]; p[p[f, g], Append[Array[0&, i], g[[1]] ]]]]]; T[n_, k_] := T[n, k] = b[n, n][[k+1]] + If[k<2, 0, T[n, k-1]]; Table [Table [T[n, k], {k, 1, n}], {n, 1, 11}] // Flatten (* _Jean-FranÃ§ois Alcover_, Mar 11 2015, after _Alois P. Heinz_ *)
