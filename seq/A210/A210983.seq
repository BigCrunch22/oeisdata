%e A210983 3,     4;
%e A210983 7,     8,  10;
%e A210983 14;
%e A210983 16,   17,  20,  25;
%e A210983 28,   29,  31,  35,  41;
%e A210983 45,   47,  48,  51,  56,  63;
%e A210983 68,   71,  72,  74,  78,  84,  92;
%e A210983 98,  102, 104, 105, 108, 113, 120, 129;
%e A210983 136, 141, 144, 145, 147, 151, 157, 165, 175;
%e A210983 183, 189, 193, 195, 196, 199, 204, 211, 220, 231;
%e A210983 ...
%Y A210983 Partial sums of A130517 (when that sequence is regarded as a flattened triangle). Other versions are A212013, A212123, A213363, A213373.
