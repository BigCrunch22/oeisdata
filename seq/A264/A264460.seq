%t A264460 b[u_, o_] := b[u, o] = If[u+o == 0, 1, Sum[b[u-j, o+j-1], {j, 1, u}] + Sum[Series[b[u+j-1, o-j] x^u, {x, 0, 2}] // Normal, {j, 1, o}]];
%t A264460 a[n_] := Coefficient[b[n, 0], x, 1];
