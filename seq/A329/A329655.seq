%p A329655 T:= (n,k)-> value(Sum(binomial(n,j)*binomial(k, j)*j!, j=1..k)):
%p A329655 seq(seq(T(n, 1+d-n), n=1..d), d=1..12);
%t A329655 T[n_, k_] := Sum[Binomial[n, j] * Binomial[k, j] * j!, {j, 1, k}]; Table[T[n - k + 1, k], {n, 1, 11}, {k, 1, n}] // Flatten (* _Amiram Eldar_, Nov 25 2019 *)
