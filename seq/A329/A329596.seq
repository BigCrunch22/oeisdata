%o A329596       sequence(i1+1)=(sum(factor(sequence(i1)))-sum(factor((i1))));
%o A329596    else
%o A329596        sequence(i1+1)=(sum(factor(sequence(i1)))+sum(factor((i1))));
%o A329596 end
