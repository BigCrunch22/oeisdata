%t A141604 f[n_]:= f[n]= If[n<4, 1, (f[n-1]*f[n-3] +f[n-2]^2)/f[n-4]]; (* A006720 *)
%t A141604 A141604[n_, k_]:= Round[f[n]/(f[k]*f[n-k])];
