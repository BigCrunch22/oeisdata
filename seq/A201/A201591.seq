%t A201591 a = 6; c = 0;
%t A201591 f[x_] := a*x^2 + c; g[x_] := Csc[x]
%t A201591 Plot[{f[x], g[x]}, {x, 0, Pi}, {AxesOrigin -> {0, 0}}]
%t A201591 r = x /. FindRoot[f[x] == g[x], {x, .5, .6}, WorkingPrecision -> 110]
