%t A201567 a = 1; c = 3;
%t A201567 f[x_] := a*x^2 + c; g[x_] := Csc[x]
%t A201567 Plot[{f[x], g[x]}, {x, 0, Pi}, {AxesOrigin -> {0, 0}}]
%t A201567 r = x /. FindRoot[f[x] == g[x], {x, .3, .4}, WorkingPrecision -> 110]
