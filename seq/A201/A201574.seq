%t A201574 a = 1; c = 7;
%t A201574 f[x_] := a*x^2 + c; g[x_] := Csc[x]
%t A201574 Plot[{f[x], g[x]}, {x, 0, Pi}, {AxesOrigin -> {0, 0}}]
%t A201574 r = x /. FindRoot[f[x] == g[x], {x, .1, .2}, WorkingPrecision -> 110]
