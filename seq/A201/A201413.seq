%t A201413 a = 5; c = 0;
%t A201413 f[x_] := a*x^2 + c; g[x_] := Sec[x]
%t A201413 Plot[{f[x], g[x]}, {x, 0, Pi/2}, {AxesOrigin -> {0, 0}}]
