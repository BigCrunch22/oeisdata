%t A201581 a = 1; c = 10;
%t A201581 f[x_] := a*x^2 + c; g[x_] := Csc[x]
%t A201581 Plot[{f[x], g[x]}, {x, 0, Pi}, {AxesOrigin -> {0, 0}}]
%t A201581 r = x /. FindRoot[f[x] == g[x], {x, .1, .2}, WorkingPrecision -> 110]
%t A201581 RealDigits[r]   (* A201578 *)
