%t A201763 a = -1; b = 0; c = 8;
%t A201763 f[x_] := a*x^2 + b*x + c; g[x_] := E^x
%t A201763 Plot[{f[x], g[x]}, {x, -3, 3}, {AxesOrigin -> {0, 0}}]
%t A201763 r = x /. FindRoot[f[x] == g[x], {x, -2.8, -2.9}, WorkingPrecision -> 110]
