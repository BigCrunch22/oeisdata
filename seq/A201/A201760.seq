%t A201760 a = -1; b = 0; c = 6;
%t A201760 f[x_] := a*x^2 + b*x + c; g[x_] := E^x
%t A201760 Plot[{f[x], g[x]}, {x, -3, 3}, {AxesOrigin -> {0, 0}}]
%t A201760 r = x /. FindRoot[f[x] == g[x], {x, -2.5, -2.4}, WorkingPrecision -> 110]
