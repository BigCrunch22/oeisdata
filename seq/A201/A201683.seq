%t A201683 a = 1; c = -2;
%t A201683 f[x_] := a*x^2 + c; g[x_] := Csc[x]
%t A201683 Plot[{f[x], g[x]}, {x, 0, Pi}, {AxesOrigin -> {0, 0}}]
