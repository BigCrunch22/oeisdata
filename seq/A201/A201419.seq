%t A201419 a = 8; c = 0;
%t A201419 f[x_] := a*x^2 + c; g[x_] := Sec[x]
%t A201419 Plot[{f[x], g[x]}, {x, 0, Pi/2}, {AxesOrigin -> {0, 0}}]
%t A201419 r = x /. FindRoot[f[x] == g[x], {x, .3, .4}, WorkingPrecision -> 110]
