%p A331331 ser := n -> series(KummerU(-n, 1/3, x), x, n+1):
%p A331331 seq(seq((-3)^(n-k)*coeff(ser(n), x, k), k=0..n), n=0..8);
%p A331331 # Alternative:
%p A331331 gf := exp(t*x/(1-3*x))/(1-3*x)^(1/3): ser := n -> series(gf, x, n+1):
%p A331331 c := n -> coeff(ser(n), x, n): seq(seq(n!*coeff(c(n), t, k), k=0..n), n=0..8);
%t A331331 (* rows[n], n[0..oo] *)
%t A331331 n=12;r={};For[k=0,k<n+1,k++,AppendTo[r,Binomial[n,n-k]/Product[3*j+1,{j,0,k-1}]*Product[3*j+1,{j,0,n-1}]]];r
%t A331331 (* columns[k], k[0..oo] *)
