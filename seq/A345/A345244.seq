%N A345244 G.f. A(x) satisfies: A(x) = x + x^2 * exp(3 * Sum_{k>=1} (-1)^(k+1) * A(x^k) / k).
%F A345244 G.f.: x + x^2 * Product_{n>=1} (1 + x^n)^(3*a(n)).
%F A345244 a(n+2) = (3/n) * Sum_{k=1..n} ( Sum_{d|k} (-1)^(k/d+1) * d * a(d) ) * a(n-k+2).
%t A345244 nmax = 30; A[_] = 0; Do[A[x_] = x + x^2 Exp[3 Sum[(-1)^(k + 1) A[x^k]/k, {k, 1, nmax}]] + O[x]^(nmax + 1) // Normal, nmax + 1]; CoefficientList[A[x], x] // Rest
%t A345244 a[1] = a[2] = 1; a[n_] := a[n] = (3/(n - 2)) Sum[Sum[(-1)^(k/d + 1) d a[d], {d, Divisors[k]}] a[n - k], {k, 1, n - 2}]; Table[a[n], {n, 1, 30}]
