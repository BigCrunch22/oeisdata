%t A193845 z = 10;
%t A193845 p[n_, x_] := (x + 1)^n;
%t A193845 q[n_, x_] := (x + 1)^n
%t A193845 p1[n_, k_] := Coefficient[p[n, x], x^k];
%t A193845 p1[n_, 0] := p[n, x] /. x -> 0;
%t A193845 d[n_, x_] := Sum[p1[n, k]*q[n - 1 - k, x], {k, 0, n - 1}]
%t A193845 h[n_] := CoefficientList[d[n, x], {x}]
