%t A219875 A219875[n_, m_] := n*m + Ceiling[n / GoldenRatio] * Ceiling[m / GoldenRatio];
%t A219875 Table[A219875[n-m+1, m], {n, 15}, {m, n}] (* _Paolo Xausa_, Mar 20 2024 *)
%o A219875 (PARI) prod(m,n) = {phi = (1+sqrt(5))/2; return (m*n + ceil(m/phi)*ceil(n/phi));}
