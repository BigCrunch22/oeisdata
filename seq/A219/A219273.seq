%p A219273 h:= proc(l) local n; n:=nops(l); add(i, i=l)!/mul(mul(1+l[i]-j+
%p A219273       add(`if`(l[k]>=j, 1, 0), k=i+1..n), j=1..l[i]), i=1..n)
%p A219273     end:
%p A219273 b:= (n, l)-> `if`(n<1, h(l), b(n-1, l) +b(n-1, [l[], n])):
%p A219273 a:= n-> b(n, []):
