%t A104936 egp[len_]:=Module[{egterms=FromDigits/@Partition[RealDigits[EulerGamma, 10, 1000][[1]],len,1]},Select[egterms,IntegerLength[#]==len&&PrimeQ[#]&]];egp[2] (* _Harvey P. Dale_, Oct 30 2011 *)
