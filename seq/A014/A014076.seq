%o A014076 (Python)
%o A014076 from sympy import primepi
%o A014076 def A014076(n):
%o A014076     if n == 1: return 1
%o A014076     m, k = n-1, primepi(n) + n - 1 + (n>>1)
%o A014076     while m != k:
%o A014076         m, k = k, primepi(k) + n - 1 + (k>>1)
%o A014076     return m # _Chai Wah Wu_, Jul 31 2024
