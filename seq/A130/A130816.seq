%t A130816 RecurrenceTable[{a[n] == a[n-3] + Floor[a[n-2]/2], a[1] == 2, a[2] == 2, a[3] == 1}, a, {n,1,70}] (* _Georg Fischer_, May 05 2021 *)
%o A130816 (C) int f1 = 2; int f2 = 2; int f3 = 1; int next_seq() { int f = f1 + f2/2; f1 = f2; f2 = f3; f3 = f; return f; }
