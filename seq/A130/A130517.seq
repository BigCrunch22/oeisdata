%p A130517 A130517 := proc(n,k)
%p A130517      if k <= (n+1)/2 then
%p A130517         n-2*(k-1) ;
%p A130517     else
%p A130517         1-n+2*(k-1) ;
%p A130517     end if;
%p A130517 end proc: # _R. J. Mathar_, Jul 21 2012
