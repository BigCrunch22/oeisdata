%F A363173 a(n) = binomial(n,6) = A000579(n) if n is odd, A000579(n) - A260417(n/2) if n is even.
%t A363173 A363173[nmax_]:=Module[{d,aon},d[m_,n_]:=Boole[Divisible[n,m]];
%t A363173 aon[3,n_]:=(5n^2-48n+76)/48d[2,n]+3/4d[4,n]+(7n-38)/6d[6,n]-8d[12,n]-20d[18,n]-16d[24,n]-19d[30,n]+8d[42,n]+68d[60,n]+60d[84,n]+48d[90,n]+60d[120,n]+48d[210,n];
%t A363173 aon[4,n_]:=(7n-42)/12d[6,n]-5/2d[12,n]-4d[18,n]+3d[24,n]+6d[42,n]+34d[60,n]-6d[84,n]-6d[120,n];
%t A363173 aon[5,n_]:=(n-6)/4d[6,n]-3/2d[12,n]-2d[24,n]+4d[42,n]+6d[84,n]+6d[120,n];
%t A363173 aon[6,n_]:=4d[30,n]-4d[60,n];
%t A363173 aon[7,n_]:=d[30,n]+4d[60,n];
%t A363173 Table[Binomial[n,6]-If[EvenQ[n],Sum[aon[i,n]Binomial[i,3],{i,3,7}]n+d[2,n]Binomial[n/2,3],0],{n,3,nmax}]];
