%t A363519 b[l_, i_, t_] := b[l, i, t] = Expand[If[l == {}, 1, Sum[Function[f, b[ReplacePart[l, j -> Nothing], j, If[f, 1 - t, t]]*If[f, x, 1]][l[[j]] == t], {j, Join[{1}, Range[i, Length@l]]}]]];
%t A363519 T[n_] := CoefficientList[b[ Table[Mod[i, 2], {i, 2, n}], 1, 0], x];
