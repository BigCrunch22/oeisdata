%e A111146 At y=2: Sum_{k=0..n} 2^k*T(n,k) = A113327(n) where (1 + 2*x + 8*x^2 + 36*x^3 +...+ A113327(n)*x^n +..) = 1/(1 - 2/1!*x*(1! + 2!*x + 3!*x^2 + 4!*x^3 +..) ).
%e A111146 At y=3: Sum_{k=0..n} 3^k*T(n,k) = A113328(n) where (1 + 3*x + 18*x^2 + 117*x^3 +...+ A113328(n)*x^n +..) = 1/(1 - 3/2!*x*(2! + 3!*x + 4!*x^2 + 5!*x^3 +..) ).
%e A111146 At y=4: Sum_{k=0..n} 4^k*T(n,k) = A113329(n) where (1 + 4*x + 32*x^2 + 272*x^3 +...+ A113329(n)*x^n +..) = 1/(1 - 4/3!*x*(3! + 4!*x + 5!*x^2 + 6!*x^3 +..) ).
%t A111146 T[n_, k_] := Module[{x = X + X*O[X]^n, y = Y + Y*O[Y]^k}, A = 1/(1 - x*y*Sum[x^j*Product[y + i, {i, 0, j - 1}], {j, 0, n}]); Coefficient[ Coefficient[A, X, n], Y, k]];
%t A111146 Table[T[n, k], {n, 0, 10}, {k, 0, n}] // Flatten (* _Jean-François Alcover_, May 26 2019, from PARI *)
%o A111146 (PARI) {T(n,k)=local(x=X+X*O(X^n),y=Y+Y*O(Y^k)); A=1/(1-x*y*sum(j=0,n,x^j*prod(i=0,j-1,y+i))); return(polcoeff(polcoeff(A,n,X),k,Y))} (Hanna)
