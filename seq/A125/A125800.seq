%p A125800 f[0]:= 1/(1-z):
%p A125800 S[0]:= series(f[0],z,21):
%p A125800 for n from 1 to 20 do
%p A125800   ff:= unapply(f[n-1],z);
%p A125800   f[n]:= simplify(1/3*sum(ff(w*z^(1/3)),w=RootOf(Z^3-1,Z)))/(1-z);
