%o A033048 import Data.List (unfoldr)
%o A033048 a033048 n = a033048_list !! (n-1)
%o A033048 a033048_list = filter (all (< 2) . unfoldr (\x ->
%o A033048    if x == 0 then Nothing else Just $ swap $ divMod x 12)) [1..]
