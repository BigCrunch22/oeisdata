%N A346271 Sum_{n>=0} a(n) * x^n / (n!)^2 = exp( BesselI(0,2*sqrt(x)) - 1 - x^2 / 4 ).
%F A346271 Sum_{n>=0} a(n) * x^n / (n!)^2 = exp( x + Sum_{n>=3} x^n / (n!)^2 ).
%F A346271 a(0) = 1; a(n) = n * a(n-1) + (1/n) * Sum_{k=3..n} binomial(n,k)^2 * k * a(n-k).
%t A346271 nmax = 20; CoefficientList[Series[Exp[BesselI[0, 2 Sqrt[x]] - 1 - x^2/4], {x, 0, nmax}], x] Range[0, nmax]!^2
%t A346271 a[0] = 1; a[n_] := a[n] = n a[n - 1] + (1/n) Sum[Binomial[n, k]^2 k a[n - k], {k, 3, n}]; Table[a[n], {n, 0, 20}]
