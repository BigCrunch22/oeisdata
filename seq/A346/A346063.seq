%o A346063 from sympy import prime, isprime, primepi
%o A346063 def a(n):
%o A346063     p = prime(n); m = p*p - 1
%o A346063     while not isprime(m): m = m//2
%o A346063     return primepi(m)
%o A346063 for n in range(1, 71): print(a(n))
