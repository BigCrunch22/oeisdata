%T A242296 85184,97336,117649,185193,300763,474552,551368,658503,729000,778688,
%U A242296 804357,970299,1000000,1092727,1295029,1481544,1520875,1860867,1953125,2197000,2299968,2352637
%N A242296 Greedy-summable cubes.
%C A242296 Greedy summability is defined at A242293.
%H A242296 Clark Kimberling, <a href="/A242296/b242296.txt">Table of n, a(n) for n = 1..400</a>
%e A242296 Let s(n) = n^3 = A000578(n).  Then
%e A242296 a(1) =  216 = 125 + 64 + 27;
%e A242296 a(2) = 729 = 512 + 216 + 1;
%e A242296 a(3) = 2197 = 1728 + 343 + 125 + 1;
%e A242296 a(4) = 2744 = 2197 + 512 + 27 + 8.
%t A242296 z = 200;  s = Table[n^3, {n, 1, z}]; t = Table[{s[[n]], #, Total[#] == s[[n]]} &[DeleteCases[-Differences[FoldList[If[#1 - #2 >= 0, #1 - #2, #1] &, s[[n]], Reverse[Select[s, # < s[[n]] &]]]], 0]], {n, z}]
%t A242296 r[n_] := s[[n]] - Total[t[[n]][[2]]];
%t A242296 tr = Table[r[n], {n, 2, z}]  (* A242293 *)
%t A242296 c = Table[Length[t[[n]][[2]]], {n, 2, z}] (* A242294 *)
%t A242296 f = 1 + Flatten[Position[tr, 0]]  (* A242295*)
%t A242296 f^3  (* A242296 *) (* _Peter J. C. Moses_, May 06 2014 *)
