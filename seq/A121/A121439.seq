%e A121439 -143, -22, 11, -11, 1;...
%o A121439 (PARI) /* Matrix Inverse of A121334 */ {T(n,k)=local(M=matrix(n+1,n+1,r,c,if(r>=c,binomial(r*(r-1)/2+r-c,r-c)))); return((M^-1)[n+1,k+1])}
