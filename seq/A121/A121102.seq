%F A121102 G.f. -4*x^4 / ( (x-1)*(5*x-1)*(5*x^2-1) ). - _R. J. Mathar_, Jul 31 2019
%F A121102 4*a(n) = 5^(n-2) +1 - 10*A056487(n-4). - _R. J. Mathar_, Jul 31 2019
%p A121102 A121102 := proc(n)
%p A121102     local mr,ar,cr,dr ,ir,p5;
%p A121102     if n = 1 then
%p A121102         ar := 1 ;
%p A121102     else
%p A121102         ar := 0 ;
%p A121102     end if;
%p A121102     dr := 1-ar ;
%p A121102     p5 := 5^(floor(n/2)-1) ;
%p A121102     if n = 1 then
%p A121102         cr :=0 ;
%p A121102     else
%p A121102         cr := (p5-1)/2+2*ar/5 ;
%p A121102     end if;
%p A121102     mr := (3-2*(-1)^n)*p5/2-1/2 ;
%p A121102     if n = 1 then
%p A121102         ir := 1;
%p A121102     else
%p A121102         ir := (5^(n-2)+1)/4  +(2-(-1)^n)*p5/2 -3*ar/5 ;
%p A121102     end if;
%p A121102     ir-ar-dr-cr-mr ;
%p A121102 end proc:
