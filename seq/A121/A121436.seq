%o A121436 (PARI) /* Matrix Inverse of A122176 */
%o A121436 {T(n,k)=local(M=matrix(n+1,n+1,r,c,if(r>=c,binomial((c-1)*(c-2)/2+r,r-c)))); return((M^-1)[n+1,k+1])}
%o A121436 for(n=0,10,for(k=0,n,print1(T(n,k),", "));print(""))
%o A121436 (PARI) /* Obtain by G.F. */
%o A121436 {T(n,k)=polcoeff(1-sum(j=0, n-k-1, T(j+k,k)*x^j/(1-x+x*O(x^n))^(j*(j+1)/2+j*k+k*(k+1)/2+2)), n-k)}
%o A121436 for(n=0,10,for(k=0,n,print1(T(n,k),", "));print(""))
