%t A121230 Clear[t, M, a, v, a0] t[n_, m_] := {{0, 1, 0}, {0, 0, 1}, {1, 1, 0}}[[n, m]] a0 = Table[t[n, m]*t[i, j], {n, 1, 3}, {m, 1, 3}, {i, 1, 3}, {j, 1, 3}]; M = Flatten[Table[{Flatten[Table[a0[[ n, m]][[1, i]], {n, 1, 3}, {i, 1, 3}]], Flatten[Table[a0[[n, m]][[2, i]], {n, 1, 3}, {i, 1, 3}]], Flatten[Table[a0[[n, m]][[3, i]], {n, 1, 3}, {i, 1, 3}]]}, {m, 1, 3}], 1] v[1] = Table[Fibonacci[n], {n, 0, 8}] v[n_] := v[n] = M.v[n - 1] a = Table[Floor[v[n][[1]]], {n, 1, 50}] Det[M - x*IdentityMatrix[9]] Factor[%] aaa = Table[x /. NSolve[Det[M - x*IdentityMatrix[9]] == 0, x][[n]], {n, 1, 9}] Abs[aaa] a1 = Table[N[a[[n]]/a[[n - 1]]], {n, 7, 50}] ListDensityPlot[M, Mesh -> False]
