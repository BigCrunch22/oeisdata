%t A121742 Do[f=Mod[DivisorSigma[11,n],691];g=Mod[DivisorSigma[11,n+1],691];h=Mod[DivisorSigma[11,n+2],691];If[f==g&&g==h,Print[{n,f}]],{n,1,1500000}]
