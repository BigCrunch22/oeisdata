%t A213493 (Do[If[Length[Union[{w, x, y, Abs[w - x], Abs[x - y], Abs[y - w]}]] ==
%t A213493 6, s = s + 1], {w, 0, n}, {x, 0, n}, {y, 0, n}]; s)]];
