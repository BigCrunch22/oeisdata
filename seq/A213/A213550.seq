%t A213550 b[n_] := n (n + 1)/2; c[n_] := n
%t A213550 t[n_, k_] := Sum[b[k - i] c[n + i], {i, 0, k - 1}]
%t A213550 TableForm[Table[t[n, k], {n, 1, 10}, {k, 1, 10}]]
%t A213550 Flatten[Table[t[n - k + 1, k], {n, 12}, {k, n, 1, -1}]]
