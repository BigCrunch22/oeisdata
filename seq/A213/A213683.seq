%t A213683 b[n_, i_] := b[n, i] = If[n == 0, 1, If[i < 1, 0, Sum[Binomial[A213674[i] + j - 1, j]*b[n - i*j, i - 1], {j, 0, n/i}]] // FullSimplify];
%t A213683 A213674[n_] := b[n - 1, n - 1] + If[n > 2, 1, 0];
%t A213683 A81[n_] := A81[n] = If[n <= 1, n, Sum[Sum[d*A81[d], {d, Divisors[j]}]*A81[n - j], {j, 1, n - 1}]/(n - 1)];
%t A213683 a[n_] := A213674[n] - A81[n];
