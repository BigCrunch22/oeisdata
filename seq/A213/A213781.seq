%t A213781 b[n_] := Floor[(n + 2)/2]; c[n_] := n;
%t A213781 t[n_, k_] := Sum[b[k - i] c[n + i], {i, 0, k - 1}]
%t A213781 TableForm[Table[t[n, k], {n, 1, 10}, {k, 1, 10}]]
%t A213781 Flatten[Table[t[n - k + 1, k], {n, 12}, {k, n, 1, -1}]]
%t A213781 r[n_] := Table[t[n, k], {k, 1, 60}]  (* A213781 *)
%t A213781 s[n_] := Sum[t[i, n + 1 - i], {i, 1, n}]
