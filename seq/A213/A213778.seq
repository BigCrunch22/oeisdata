%t A213778 b[n_] := n; c[n_] := 1 + Floor[n/2];
%t A213778 t[n_, k_] := Sum[b[k - i] c[n + i], {i, 0, k - 1}]
%t A213778 TableForm[Table[t[n, k], {n, 1, 10}, {k, 1, 10}]]
%t A213778 Flatten[Table[t[n - k + 1, k], {n, 12}, {k, n, 1, -1}]]
