%e A116685 T(6,2)=3 because we have [4,1,1],[3,2,1] and [2,2,1,1].
%p A116685 g:=sum(x^i/(1-x^i)/product(1-t*x^j,j=1..i-1),i=1..50): gser:=simplify(series(g,x=0,18)): for n from 1 to 15 do P[n]:=coeff(gser,x^n) od: 1; for n from 2 to 15 do seq(coeff(P[n],t,j),j=0..n-2) od;
%p A116685 # yields sequence in triangular form
%t A116685 rows = 15; max = rows + 2; col[k0_ /; k0 > 0] := col[k0] = Sum[x^(2*k + k0)/Product[ (1 - x^(k + j)), {j, 0, k0}], {k, 1, Ceiling[max/2]}] + O[x]^max // CoefficientList[#, x] &; col[0] := Table[Switch[n, 1, 0, 2, 1, _, n - 1 - col[1][[n]]], {n, 1, Length[col[1]]}]; Join[{1}, Table[ col[k][[n+2]], {n, 0, rows-1}, {k, 0, n-1}] // Flatten] (* _Jean-Fran√ßois Alcover_, Sep 11 2017, after _Alois P. Heinz_ *)
