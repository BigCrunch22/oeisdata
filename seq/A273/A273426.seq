%S A273426 1,5,17,69,285,1061,5385,26097,112665,469785,1911585,7716713,31014865,
%T A273426 124339097,497903529,1992781329
%N A273426 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 718", based on the 5-celled von Neumann neighborhood.
%C A273426 Initialized with a single black (ON) cell at stage zero.
%D A273426 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273426 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273426 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273426 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273426 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273426 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273426 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273426 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273426 code=718; stages=128;
%t A273426 rule=IntegerDigits[code,2,10];
%t A273426 g=2*stages+1; (* Maximum size of grid *)
%t A273426 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273426 ca=a;
%t A273426 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273426 PrependTo[ca,a];
%t A273426 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273426 k=(Length[ca[[1]]]+1)/2;
%t A273426 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273426 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273426 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A273426 Cf. A273425.
%K A273426 nonn,more,changed
%O A273426 0,2
