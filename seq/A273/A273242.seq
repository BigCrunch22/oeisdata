%I A273242 #9 Jul 26 2024 21:16:41
%S A273242 1,8,45,201,857,3556,14121,57293,231128,925033,3708108,14822072,
%T A273242 59327809,237306937,949193761,3796909261
%N A273242 Number of active (ON, black) cells at stage 2^n-1 of the two-dimensional cellular automaton defined by "Rule 613", based on the 5-celled von Neumann neighborhood.
%C A273242 Initialized with a single black (ON) cell at stage zero.
%D A273242 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273242 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273242 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273242 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273242 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273242 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273242 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273242 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273242 code=613; stages=128;
%t A273242 rule=IntegerDigits[code,2,10];
%t A273242 g=2*stages+1; (* Maximum size of grid *)
%t A273242 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273242 ca=a;
%t A273242 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273242 PrependTo[ca,a];
%t A273242 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273242 k=(Length[ca[[1]]]+1)/2;
%t A273242 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273242 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273242 Part[on,2^Range[0,Log[2,stages]]] (* Extract relevant terms *)
%Y A273242 Cf. A273241.
%K A273242 nonn,more
%O A273242 0,2
