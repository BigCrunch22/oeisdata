%I A273536 #10 Feb 16 2025 08:33:35
%S A273536 1,6,15,32,49,82,135,200,265,338,419,532,653,802,1015,1256,1513,1754,
%T A273536 1995,2276,2597,2930,3275,3680,4133,4658,5203,5796,6421,7122,7947,
%U A273536 8864,9773,10634,11491,12404,13377,14346,15351,16416,17521,18826,20187,21596,23025
%N A273536 Partial sums of the number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 782", based on the 5-celled von Neumann neighborhood.
%C A273536 Initialized with a single black (ON) cell at stage zero.
%D A273536 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273536 Robert Price, <a href="/A273536/b273536.txt">Table of n, a(n) for n = 0..128</a>
%H A273536 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273536 Eric Weisstein's World of Mathematics, <a href="https://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273536 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273536 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273536 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273536 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273536 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273536 code=782; stages=128;
%t A273536 rule=IntegerDigits[code,2,10];
%t A273536 g=2*stages+1; (* Maximum size of grid *)
%t A273536 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273536 ca=a;
%t A273536 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273536 PrependTo[ca,a];
%t A273536 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273536 k=(Length[ca[[1]]]+1)/2;
%t A273536 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273536 on=Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%t A273536 Table[Total[Part[on,Range[1,i]]],{i,1,Length[on]}] (* Sum at each stage *)
%Y A273536 Cf. A273534.
