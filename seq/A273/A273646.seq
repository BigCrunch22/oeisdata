%S A273646 1,4,17,28,61,89,121,160,233,285,357,401,509,529,621,636,849,1001,
%T A273646 1109,1257,1377,1449,1601,1669,1813,2097,2121,2297,2545,2621,2789,
%U A273646 2828,3209,3465,3709,4005,4261,4377,4669,4645,5033,5373,5529,5621,6021,6221,6457
%N A273646 Number of active (ON, black) cells in n-th stage of growth of two-dimensional cellular automaton defined by "Rule 817", based on the 5-celled von Neumann neighborhood.
%C A273646 Initialized with a single black (ON) cell at stage zero.
%D A273646 S. Wolfram, A New Kind of Science, Wolfram Media, 2002; p. 170.
%H A273646 Robert Price, <a href="/A273646/b273646.txt">Table of n, a(n) for n = 0..128</a>
%H A273646 Robert Price, <a href="/A273646/a273646.tmp.txt">Diagrams of the first 20 stages</a>
%H A273646 N. J. A. Sloane, <a href="http://arxiv.org/abs/1503.01168">On the Number of ON Cells in Cellular Automata</a>, arXiv:1503.01168 [math.CO], 2015
%H A273646 Eric Weisstein's World of Mathematics, <a href="http://mathworld.wolfram.com/ElementaryCellularAutomaton.html">Elementary Cellular Automaton</a>
%H A273646 S. Wolfram, <a href="http://wolframscience.com/">A New Kind of Science</a>
%H A273646 <a href="/index/Ce#cell">Index entries for sequences related to cellular automata</a>
%H A273646 <a href="https://oeis.org/wiki/Index_to_2D_5-Neighbor_Cellular_Automata">Index to 2D 5-Neighbor Cellular Automata</a>
%H A273646 <a href="https://oeis.org/wiki/Index_to_Elementary_Cellular_Automata">Index to Elementary Cellular Automata</a>
%t A273646 CAStep[rule_,a_]:=Map[rule[[10-#]]&,ListConvolve[{{0,2,0},{2,1,2},{0,2,0}},a,2],{2}];
%t A273646 code=817; stages=128;
%t A273646 rule=IntegerDigits[code,2,10];
%t A273646 g=2*stages+1; (* Maximum size of grid *)
%t A273646 a=PadLeft[{{1}},{g,g},0,Floor[{g,g}/2]]; (* Initial ON cell on grid *)
%t A273646 ca=a;
%t A273646 ca=Table[ca=CAStep[rule,ca],{n,1,stages+1}];
%t A273646 PrependTo[ca,a];
%t A273646 (* Trim full grid to reflect growth by one cell at each stage *)
%t A273646 k=(Length[ca[[1]]]+1)/2;
%t A273646 ca=Table[Table[Part[ca[[n]][[j]],Range[k+1-n,k-1+n]],{j,k+1-n,k-1+n}],{n,1,k}];
%t A273646 Map[Function[Apply[Plus,Flatten[#1]]],ca] (* Count ON cells at each stage *)
%K A273646 nonn,easy
%O A273646 0,2
