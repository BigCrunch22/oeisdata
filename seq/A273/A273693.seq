%p A273693 with(combinat):
%p A273693 A:= proc(n, k) option remember; local h, i, x, y, z;
%p A273693       if n<2 then 1 elif k<2 then k
%p A273693     else h:= ilog[k]((k-1)*n+1);
%p A273693          if k^h=(k-1)*n+1 then A((n-1)/k, k)^k*
%p A273693             multinomial(n-1, ((n-1)/k)$k)
%p A273693        else x, y:=(k^h-1)/(k-1), (k^(h-1)-1)/(k-1);
%p A273693             for i from 0 do z:= (n-1)-(k-1-i)*y-i*x;
%p A273693               if y<=z and z<=x then A(y, k)^(k-1-i)*
%p A273693                  multinomial(n-1, y$(k-1-i), x$i, z)*
%p A273693                  A(x, k)^i*A(z, k); break fi
%p A273693             od
%p A273693       fi fi
%p A273693     end:
%p A273693 seq(seq(A(n, d-n), n=0..d), d=0..14);
%t A273693 multinomial[n_, k_] := n!/Times @@ (k!); A[n_, k_] := A[n, k] = Module[{h, i, x, y, z}, Which[n<2, 1, k<2, k, True, h = Floor @ Log[k, (k-1)*n+1]; If[k^h == (k-1)*n+1, A[(n-1)/k, k]^k*multinomial[n-1, Array[((n-1)/k)&, k]], {x, y} = {(k^h-1)/(k-1), (k^(h-1)-1)/(k-1)}; For[i = 0, True, i++, z = (n-1)-(k-1-i)*y-i*x; If[y<=z && z<=x, A[y, k]^(k-1-i)*multinomial[n-1, Join[Array[y&, k-1-i], Array[x&, i], {z}]]*A[x, k]^i*A[z, k] // Return]] ]]]; Table[A[n, d-n], {d, 0, 14}, {n, 0, d}] // Flatten (* _Jean-FranÃ§ois Alcover_, Mar 13 2017, translated from Maple *)
