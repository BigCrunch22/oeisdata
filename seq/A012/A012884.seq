%t A012884 prQ[n_] := n == 1 || PrimeQ[n];
%t A012884 okQ[n_] := Module[{dd, nd}, dd = IntegerDigits[n]; nd = Length[dd]; AllTrue[Range[nd], prQ@ FromDigits@ Take[dd, #]&] && AllTrue[Range[nd-1], prQ@ FromDigits@ Drop[dd, #]&]];
