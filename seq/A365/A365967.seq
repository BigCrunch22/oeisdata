%t A365967 a[0] = 1; a[1] = 1; a[2] = 2; a[3] = 3;
%t A365967 a[n_] := a[n] = If[EvenQ[n], a[n-1] + a[n-2] + a[n-3] + a[n-4], a[n-1] + a[n-2]];
