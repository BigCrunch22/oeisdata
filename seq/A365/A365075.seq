%e A365075   .8000000000000000000...
%e A365075   .1111111111111111111...
%e A365075   .4285714285714285714...
%e A365075   .1000000000000000000...
%e A365075   ...
%t A365075 t1={}; For[n=2, n <= 24, n++, AppendTo[t1, 1/(n-1)]; For[i=2, i <= Floor[(n-1)/2], i++, If[GCD[i, n-i] == 1, AppendTo[t1, i/(n-i)]]]]; (* A182972/A182973 *)
%t A365075 a={}; For[i=1, i<Length[t1], i++, AppendTo[a, Mod[Floor[10^i*Part[Rest[t1], i]], 10]]]; a
%o A365075 (Python)
%o A365075 from itertools import count, islice
%o A365075 from math import gcd
%o A365075 def A365075_gen(): # generator of terms
%o A365075     c = 1
%o A365075     for n in count(2):
%o A365075         for i in range(1,1+(n-1>>1)):
%o A365075             if gcd(i,n-i)==1:
%o A365075                 c *= 10
%o A365075                 yield (i*c//(n-i))%10
%o A365075 A365075_list = list(islice(A365075_gen(),30)) # _Chai Wah Wu_, Aug 28 2023
