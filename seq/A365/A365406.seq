%o A365406 def A365406_gen(startvalue=1): # generator of terms >= startvalue
%o A365406     return filter(lambda i:(a:=(d:=divisors(i))[len(d)-1>>1])==1<<a.bit_length()-1,count(max(startvalue,1)))
