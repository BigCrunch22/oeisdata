%t A356250 T[n_, k_] := Sum[(j * Floor[n/j])^k, {j, 1, n}]; Table[T[k, n - k], {n, 1, 11}, {k, 1, n}] // Flatten (* _Amiram Eldar_, Jul 31 2022 *)
%o A356250 (PARI) T(n, k) = sum(j=1, n, (j*(n\j))^k);
%o A356250 (PARI) T(n, k) = if(k==0, n, sum(j=1, n, j^k*sumdiv(j, d, 1-(1-1/d)^k)));
