%o A298564 (Python)
%o A298564 def A298564list(n):
%o A298564     def generator():
%o A298564         a, b, c = 5, 3, 0
%o A298564         while True:
%o A298564             yield c
%o A298564             a *= 2
%o A298564             b *= 3
%o A298564             c += 2 - a + b
%o A298564     a = generator()
%o A298564     return [next(a) for _ in range(n)]
