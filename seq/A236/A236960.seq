%o A236960 (PARI) /* From Root Series G, Calculate T(n,k) of Triangle: */
%o A236960 {T(n, k)=local(F=x, M, N, P, m=max(n, k)); M=matrix(m+2, m+2, r, c, F=x;
%o A236960 for(i=1, r+c-2, F=subst(F, x, G +x*O(x^(m+2)))); polcoeff(F, c));
%o A236960 N=matrix(m+1, m+1, r, c, M[r, c]);
%o A236960 P=matrix(m+1, m+1, r, c, M[r+1, c]); (P~*N~^-1)[n+1, k+1]}
%o A236960 /* Calculates Root Series G and then Prints ROWS of Triangle: */
%o A236960 {ROWS=12;V=[1,1];print("");print1("Root Sequence: [1, 1, ");
%o A236960 for(i=2,ROWS,V=concat(V,0);G=x*truncate(Ser(V));
%o A236960 for(n=0,#V-1,if(n==#V-1,V[#V]=n^n-T(n,0));for(k=0,n, T(n,k)));print1(V[#V]", "););
%o A236960 print1("...]");print("");print("");print("Triangle begins:");
%o A236960 for(n=0,#V-2,for(k=0,n,print1(T(n,k),", "));print(""))}
