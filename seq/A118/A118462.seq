%p A118462 b:= proc(n, i) option remember; `if`(n=0, [0], `if`(i<1, [], [seq(
%p A118462       map(p->p+2^(i-1)*j, b(n-i*j, i-1))[], j=0..min(1, n/i))]))
%p A118462     end:
%p A118462 T:= n-> sort(b(n$2))[]:
%p A118462 seq(T(n), n=0..14);  # _Alois P. Heinz_, Sep 06 2014
%t A118462 b[n_, i_] := b[n, i] = If[n==0, {0}, If[i<1, {}, Flatten[Table[b[n-i*j, i-1 ] + 2^(i-1)*j, {j, 0, Min[1, n/i]}]]]]; T[n_] := Sort[b[n, n]]; Table[ T[n], {n, 0, 14}] // Flatten (* _Jean-FranÃ§ois Alcover_, Dec 27 2015, after _Alois P. Heinz_ *)
