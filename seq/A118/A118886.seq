%t A118886 amax = 2000; xmax = Sqrt[amax] // Ceiling; Clear[f]; f[_] = 0; Do[q = x^2 + x y + y^2; f[q] = f[q] + 1, {x, 0, xmax}, {y, x, xmax}];
%t A118886 A118886 = Select[Range[0, 3 xmax^2], # <= amax && f[#] > 1&] (* _Jean-FranÃ§ois Alcover_, Jun 21 2018 *)
