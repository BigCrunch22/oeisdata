%N A229257 O.g.f.: Sum_{n>=0} x^n / Product_{k=1..n} (1 - n^2*k*x).
%H A229257 Seiichi Manyama, <a href="/A229257/b229257.txt">Table of n, a(n) for n = 0..242</a>
%F A229257 a(n) = Sum_{k=0..n} (k^2)^(n-k) * Stirling2(n, k).
%F A229257 E.g.f.: Sum_{n>=0} (exp(n^2*x) - 1)^n / (n! * n^(2*n)).
%e A229257 O.g.f.: A(x) = 1 + x + 2*x^2 + 14*x^3 + 168*x^4 + 3147*x^5 + 90563*x^6 +...
%e A229257 where
%e A229257 A(x) = 1 + x/(1-x) + x^2/((1-2^2*1*x)*(1-2^2*2*x)) + x^3/((1-3^2*1*x)*(1-3^2*2*x)*(1-3^2*3*x)) + x^4/((1-4^2*1*x)*(1-4^2*2*x)*(1-4^2*3*x)*(1-4^2*4*x)) +...
%e A229257 Exponential Generating Function.
%e A229257 E.g.f.: E(x) = 1 + x + 2*x^2/2! + 14*x^3/3! + 168*x^4/4! + 3147*x^5/5! +...
%e A229257 where
%e A229257 E(x) = 1 + (exp(x)-1) + (exp(4*x)-1)^2/(2!*4^2) + (exp(9*x)-1)^3/(3!*9^3) + (exp(16*x)-1)^4/(4!*16^4) + (exp(25*x)-1)^5/(5!*25^5) +...
%t A229257 Flatten[{1,Table[Sum[(k^2)^(n-k) * StirlingS2[n, k],{k,0,n}],{n,1,20}]}] (* _Vaclav Kotesovec_, May 08 2014 *)
%o A229257 (PARI) {a(n)=polcoeff(sum(m=0,n,x^m/prod(k=1,m,1-m^2*k*x +x*O(x^n))),n)}
%o A229257 for(n=0,30,print1(a(n),", "))
%o A229257 (PARI) {a(n)=n!*polcoeff(sum(m=0,n,(exp(m^2*x+x*O(x^n))-1)^m/(m!*m^(2*m))),n)}
%o A229257 for(n=0,30,print1(a(n),", "))
%o A229257 (PARI) {a(n)=sum(k=0, n, (k^2)^(n-k) * stirling(n, k, 2))}
%o A229257 for(n=0,30,print1(a(n),", "))
