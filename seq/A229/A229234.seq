%N A229234 O.g.f.: Sum_{n>=0} n! * x^n / Product_{k=1..n} (1 - n*k*x).
%H A229234 Seiichi Manyama, <a href="/A229234/b229234.txt">Table of n, a(n) for n = 0..300</a>
%F A229234 a(n) = Sum_{k=0..n} k^(n-k) * k! * Stirling2(n, k).
%F A229234 E.g.f.: Sum_{n>=0} (exp(n*x) - 1)^n / n^n.
%e A229234 O.g.f.: A(x) = 1 + x + 3*x^2 + 19*x^3 + 189*x^4 + 2671*x^5 + 50253*x^6 +...
%e A229234 where
%e A229234 A(x) = 1 + x/(1-x) + 2!*x^2/((1-2*1*x)*(1-2*2*x)) + 3!*x^3/((1-3*1*x)*(1-3*2*x)*(1-3*3*x)) + 4!*x^4/((1-4*1*x)*(1-4*2*x)*(1-4*3*x)*(1-4*4*x)) +...
%e A229234 Exponential Generating Function.
%e A229234 E.g.f.: E(x) = 1 + x + 3*x^2/2! + 19*x^3/3! + 189*x^4/4! + 2671*x^5/5! +...
%e A229234 where
%e A229234 E(x) = 1 + (exp(x)-1) + (exp(2*x)-1)^2/2^2 + (exp(3*x)-1)^3/3^3 + (exp(4*x)-1)^4/4^4 + (exp(5*x)-1)^5/5^5 +...
%t A229234 Flatten[{1,Table[Sum[k^(n-k) * k! * StirlingS2[n, k],{k,0,n}],{n,1,20}]}] (* _Vaclav Kotesovec_, May 08 2014 *)
%o A229234 (PARI) {a(n)=polcoeff(sum(m=0,n,m!*x^m/prod(k=1,m,1-m*k*x +x*O(x^n))),n)}
%o A229234 for(n=0,30,print1(a(n),", "))
%o A229234 (PARI) {a(n)=n!*polcoeff(sum(m=0,n,(exp(m*x+x*O(x^n))-1)^m/m^m),n)}
%o A229234 for(n=0,30,print1(a(n),", "))
%o A229234 (PARI) {a(n)=sum(k=0, n, k^(n-k) * k! * stirling(n, k, 2))}
%o A229234 for(n=0,30,print1(a(n),", "))
