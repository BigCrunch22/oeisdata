%p A229243 G:= proc(n, k) option remember; local j; if k>n then G(n, n)
%p A229243       elif n=0 then 1 elif k<1 then 0 else G(n-k, k);
%p A229243       for j from k-1 to 1 by -1 do %*(n-j)/j +G(n-j, k) od; % fi
%p A229243     end:
%p A229243 A:= (n, k)-> G(n*k, n):
%p A229243 seq(seq(A(n, d-n), n=0..d), d=0..10);
%t A229243 G[n_, k_] := G[n, k] = Module[{j, g}, Which[k > n, G[n, n], n == 0, 1, k < 1, 0, True, g = G[n-k, k]; For[j = k-1, j >= 1, j--, g = g*(n-j)/j + G[n-j, k] ]; g ] ]; A[n_, k_] := G[n*k, n]; Table[Table[A[n, d-n], {n, 0, d}], {d, 0, 10}] // Flatten (* _Jean-Fran√ßois Alcover_, Dec 23 2013, translated from Maple *)
