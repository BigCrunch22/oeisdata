%N A222524  O.g.f.: Sum_{n>=0} n^n*(2*n+1)^n * exp(-n*(2*n+1)*x) * x^n / n!.
%F A222524  a(n) = 1/n! * [x^n] Sum_{k>=0} k^k*(2*k+1)^k * x^k / (1 + k*(2*k+1)*x)^(k+1).
%F A222524 a(n) = 1/n! * Sum_{k=0..n} (-1)^(n-k)*binomial(n,k) * k^n*(2*k+1)^n.
%e A222524  O.g.f.: A(x) = 1 + 3*x + 41*x^2 + 1057*x^3 + 40057*x^4 + 2006631*x^5 +...
%e A222524 where
%e A222524 A(x) = 1 + 3*x*exp(-3*x) + 10^2*exp(-10*x)*x^2/2! + 21^3*exp(-21*x)*x^3/3! + 36^4*exp(-36*x)*x^4/4! + 55^5*exp(-55*x)*x^5/5! +...
%e A222524 is a power series in x with integer coefficients.
%o A222524  (PARI) {a(n)=polcoeff(sum(k=0, n, k^k*(2*k+1)^k*exp(-k*(2*k+1)*x +x*O(x^n))*x^k/k!), n)}
%o A222524 for(n=0, 25, print1(a(n), ", "))
%o A222524 (PARI) {a(n)=(1/n!)*polcoeff(sum(k=0, n, k^k*(2*k+1)^k*x^k/(1+k*(2*k+1)*x +x*O(x^n))^(k+1)), n)}
%o A222524 for(n=0, 20, print1(a(n), ", "))
%o A222524 (PARI) {a(n)=1/n!*sum(k=0, n, (-1)^(n-k)*binomial(n, k)*k^n*(2*k+1)^n)}
%o A222524 for(n=0, 20, print1(a(n), ", "))
