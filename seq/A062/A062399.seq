%t A062399 dtn[ L_ ] := Fold[ 10#1+#2&, 0, L ] a=Map[ dtn, Flatten[ Table[ Table[ Mod[ Range[ i, i-n+1, -1 ], 10 ], {i, 1, 9} ], {n, 1, 300} ], 1 ] ]; f[ n_ ] := Module[ {}, b=Select[ a, IntegerQ[ #/n ]& ]; If[ b=={}, 0, First[ b ] ] ] Table[ f[ n ], {n, 0, 63} ]
