%p A062196 T := (n, k) -> binomial(n, k)*binomial(n + 2, k);
%p A062196 seq(seq(T(n, k), k=0..n), n=0..9); # _Peter Luschny_, Sep 30 2021
%t A062196 A062196[n_, k_]:= Binomial[n, k]*Binomial[n+2, k];
