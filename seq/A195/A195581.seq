%p A195581 b:= proc(n, k) option remember; `if`(n<2, `if`(k<n, 0, 1),
%p A195581       add(binomial(n-1, r)*b(r, k-1)*b(n-1-r, k-1), r=0..n-1))
%p A195581     end:
%p A195581 T:= (n, k)-> b(n, k)-b(n, k-1):
%p A195581 seq(seq(T(n, k), k=0..n), n=0..10);
%t A195581 b[n_, k_] := b[n, k] = If[n == 0, 1, If[n == 1, If[k > 0, 1, 0], Sum[Binomial[n-1, r-1]*b[r-1, k-1]*b[n-r, k-1], {r, 1, n}] ] ]; t [n_, k_] := b[n, k] - If[k > 0, b[n, k-1], 0]; Table[Table[t[n, k], {k, 0, n}], {n, 0, 10}] // Flatten (* _Jean-Fran√ßois Alcover_, Dec 17 2013, translated from Maple *)
