%t A195304 a = 3; b = 4; h = 2 a/3; k = b/3;
%t A195304 f[t_] := (t - a)^2 + ((t - a)^2) ((a*k - b*t)/(a*h - a*t))^2
%t A195304 s = NSolve[D[f[t], t] == 0, t, 150]
%t A195304 f1 = (f[t])^(1/2) /. Part[s, 4]
