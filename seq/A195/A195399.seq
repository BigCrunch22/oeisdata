%t A195399 RealDigits[%]    (* (B) A195400 *)
%t A195399 N[x3, 100]
%t A195399 RealDigits[%]    (* (C) A195401 *)
%t A195399 N[(x1 + x2 + x3)/(a + b + c), 100]
