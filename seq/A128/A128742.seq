%p A128742 a:= n-> b(n, false, 0):
%p A128742 seq(a(n), n=0..40);  # _Alois P. Heinz_, Oct 24 2017
%t A128742 b[n_, t_, l_] := b[n, t, l] = If[n == 0, 1, Sum[b[n - j, j == l, j], {j, 1, Min[n, If[t, l, n]]}]];
%t A128742 a[n_] := b[n, False, 0];
