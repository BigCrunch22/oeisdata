%o A348042 up_to = 105;
%o A348042 A348042sq(row,col) = A348041sq(row*col,A348041sq(row,col));
%o A348042 A348042list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A348042sq(col,(a-(col-1))))); (v); };
