%p A348201 s:= proc(n) option remember;
%p A348201      `if`(n=0, [[]], map(x-> seq([x[], i], i=[-1, 1]), s(n-1)))
%p A348201     end:
%p A348201 b:= proc(l) option remember; (n-> `if`(l=[0$n], 1, add((h-> `if`(
%p A348201       add(i^2, i=h)<add(i^2, i=l), b(sort(h)), 0))(l+x), x=s(n))))(nops(l))
%p A348201     end:
%p A348201 a:= n-> b([n$3]):
%p A348201 seq(a(n), n=0..20);
%t A348201 s[n_] := s[n] = If[n == 0, {{}}, Sequence @@ Table[Append[#, i], {i, {-1, 1}}] & /@ s[n - 1]];
%t A348201 b[l_List] := b[l] = With[{n = Length[l]}, If[l == Table[0, {n}], 1, Sum[With[{h = l + x}, If[h.h < l.l, b[Sort[h]], 0]], {x, s[n]}]]];
%t A348201 a[n_] := b[{n, n, n}];
