%o A348292 function isprime(x)
%o A348292 {
%o A348292 for(i=2;i<(x-1);i++)
%o A348292 {
%o A348292   if(x%i==0)
%o A348292   {
%o A348292   return false;
%o A348292   }
%o A348292 }
%o A348292 return true;
%o A348292 }
