%e A368239     32, 33, 35, 36;
%e A368239     34, 37;
%e A368239     38, 39;
%e A368239     40;
%e A368239     ...
%o A368239 (PARI) row(n) = { my (r = [0], b = binary(n)); for (k = 1, #b, r = [3*v+b[k]|v<-r]; if (b[k]==0, r = concat(r, [v-1|v<-r]););); Set(r); }
