%o A368626 (PARI) {a(n) = my(A=1+x, A_); for(i=1, n, A=truncate(A) + x*O(x^i); B=subst(A,x,-x); A = 1 + x*(A^2 - B^2)/2 + x*(A^3 + B^3)/2 ; ); polcoeff(A,n)}
%o A368626 for(n=0, 30, print1(a(n), ", "))
