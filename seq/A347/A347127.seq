%t A347127 f[p_, e_] := (p + 1)*e + p; a[1] = 1; a[n_] := Times @@ f @@@ FactorInteger[n]; Array[a, 100] (* _Amiram Eldar_, Aug 24 2021 *)
%o A347127 (PARI) A347127(n) = { my(f=factor(n)); prod(i=1, #f~, ((f[i, 1]+1)*f[i, 2] + f[i, 1])); };
%o A347127 (PARI)
%o A347127 A001615(n) = if(1==n,n, my(f=factor(n)); prod(i=1, #f~, f[i, 1]^f[i, 2] + f[i, 1]^(f[i, 2]-1))); \\ After code in A001615
%o A347127 A003557(n) = (n/factorback(factorint(n)[, 1]));
%o A347127 A327251(n) = sumdiv(n, d, A001615(n/d)*d);
