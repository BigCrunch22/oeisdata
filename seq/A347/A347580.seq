%t A347580 t[n_, k_] := If[k == 1, n, Sum[2(n-(k-1) r), {r, 1, Quotient[n-1, k-1]}]];
%t A347580 f[n_, k_] := If[k == 1, n, t[n, k]/2];
%t A347580 T[n_, k_] := T[n, k] = If[k == 1, 1, Sum[f[n, i] T[i, k-1], {i, 1, n-1}]];
