%o A381348 (Python)
%o A381348 def rows(n):
%o A381348     l = set(range((n >> 1) + 1))
%o A381348     while True:
%o A381348         newl = {x**2 % n for x in l}
%o A381348         if newl == l: break
%o A381348         l = newl
%o A381348     return l
