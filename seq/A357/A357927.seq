%t A357927 f[n_] := Module[{j}, For[j = Floor@Log[GoldenRatio, n], Fibonacci[j+1] <= n, j++]; j-1];
%t A357927 a[n_] := If[n == 0, 1, Binomial[n, f[n]]];
