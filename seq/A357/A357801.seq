%e A357801 C(x,r) = 1 + 6*x^4/4! + (2268 + 6048*r^4)*x^8/8! + (7434504 + 56282688*r^4 + 35126784*r^8)*x^12/12! + (95227613712 + 1409371197696*r^4 + 2514356038656*r^8 + 679185948672*r^12)*x^16/16! + (3354162536029536 + 81696140755536384*r^4 + 284770675495950336*r^8 + 220415417637617664*r^12 + 33022883487154176*r^16)*x^20/20! + (264444869673131894208 + 9583398717725834749440*r^4 + 54913653475645427527680*r^8 + 83079959422282198548480*r^12 + 35701050229143616880640*r^16 + 3393656235362623684608*r^20)*x^24/24! + ...
%e A357801 where S(x,r) = 1 + Integral S(x,r)^3 * D(x,r)^3 dx.
