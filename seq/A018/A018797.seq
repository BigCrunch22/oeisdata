%t A018797 a[n_] := For[idn = IntegerDigits[n]; k = 1, True, k++, idk = IntegerDigits[k^3]; If[Length[idn] <= Length[idk], If[idk[[1 ;; Length[idn]]] == idn, Return[k^3]]]]; Table[an = a[n]; Print["a(", n, ") = ", an]; an, {n, 1, 1000}] (* _Jean-FranÃ§ois Alcover_, Mar 13 2016 *)
%t A018797 id[n_]:=With[{idn=IntegerDigits[n]},idn==Take[IntegerDigits[#], Length[ idn]]&]; With[{c=Range[0,250]^3},Table[SelectFirst[c,id[k]],{k,0,40}]]//Quiet (* Requires Mathematica version 10 or later *) (* _Harvey P. Dale_, Dec 08 2018 *)
