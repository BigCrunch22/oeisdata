%o A362826 EulerT(v)={Vec(exp(x*Ser(dirmul(v, vector(#v, n, 1/n))))-1, -#v)}
%o A362826 M(n, m=n)={my(v=vector(m), u=vector(n, n, n==1)); for(j=1, #v, v[j]=concat([1], EulerT(u))~; u=dirmul(u, vector(n, n, n^(j-1)))); Mat(v)}
