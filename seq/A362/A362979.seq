%t A362979 t[n_] := Count[IntegerDigits[Prime[n], 2], 1]  (* A014499 *)
%t A362979 u = Table[t[n], {n, 1, 200}];
%t A362979 p[n_] := Flatten[Position[u, n]]
%t A362979 w = TableForm[Table[Prime[p[n]], {n, 3, 16}]]
