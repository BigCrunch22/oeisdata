%o A362648 (PARI)
%o A362648 B(c,k)=sum(j=0, c\2, if(k%2, 1, 2^(c-2*j))*k^j*binomial(c, 2*j)*(2*j)!/(2^j*j!))
%o A362648 K(v)=my(S=Set(v)); prod(i=1, #S, my(k=S[i], c=#select(t->t==k, v)); B(c, k))
%o A362648 R(v, m)=concat(vector(#v, i, my(t=v[i], g=gcd(t, m)); vector(g, i, t/g)))
%o A362648 permcount(v) = {my(m=1, s=0, k=0, t); for(i=1, #v, t=v[i]; k=if(i>1&&t==v[i-1], k+1, 1); m*=t*k; s+=t); s!/m}
%o A362648 T(n,k) = {if(n==0, 1, my(s=0); forpart(q=n, s+=permcount(q)*polcoef(exp(sum(m=1, k, K(R(q,m))*x^m/m, O(x*x^k))), k)); s/n!)}
