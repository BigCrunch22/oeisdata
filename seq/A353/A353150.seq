%o A353150 def agen(): # generator of terms
%o A353150     R1 = {(0, 0)}
%o A353150     while True:
%o A353150         yield len(R1)
%o A353150         R = R1
%o A353150         R1 = set().union(*({(x|(1<<y), 0), (x, y+1)} for x, y in R))
