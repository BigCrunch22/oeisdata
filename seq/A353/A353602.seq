%o A353602 (Python)
%o A353602 def T(n, k):
%o A353602     j, n2, c = 2, n*n, 0
%o A353602     while c != k:
%o A353602         if pow(j, n-1, n2) == 1: c += 1
%o A353602         j += 1
%o A353602     return j-1
%o A353602 def auptodiag(maxd):
%o A353602     return [T(d+2-j, j) for d in range(1, maxd+1) for j in range(d, 0, -1)]
