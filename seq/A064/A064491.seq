%o A064491 def A064491gen(): # generator of terms
%o A064491     n = 1
%o A064491     yield n
%o A064491     while True:
%o A064491         n += divisor_count(n)
%o A064491         yield n
%o A064491 A064491_list = list(islice(A064491gen(),20)) # _Chai Wah Wu_, Dec 13 2021
