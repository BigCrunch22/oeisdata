%e A096744 {1,133,138,55,12,3,1,1},
%e A096744 {1,429,498,219,55,12,3,1,1},
%e A096744 {1,1696,2103,969,273,55,12,3,1,1},
%e A096744 {1,6583,8734,4215,1266,273,55,12,3,1,1},
