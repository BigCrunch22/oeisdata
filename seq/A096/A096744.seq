%e A096744 {1,128401,185364,94755,30249,7266,1428,273,55,12,3,1,1},...
%e A096744 T^3 begins:
%e A096744 {1},
%e A096744 {3,1},
%e A096744 {6,3,1},
%e A096744 {16,12,3,1},
%e A096744 {40,37,12,3,1},
%e A096744 {133,138,55,12,3,1},
%e A096744 {429,498,219,55,12,3,1},...
%e A096744 which equals T shift one place diagonally left and upward.
%t A096744 T[n_, 0] := 1; T[n_, n_] := 1; T[n_, k_] := Sum[ Sum[ T[n - 2, j]*T[j, i]*T[i, k - 1], {j, 0, n - 2}], {i, 0, n - 1}] ; Table[T[n, k], {n,0,10}, {k,0,n}] (* _G. C. Greubel_, Nov 28 2016 *)
%o A096744 (PARI) {T(n, k) = if(n<k||k<0, 0 , if(n==k||k==0, 1, sum(i=0, n-1, sum(j=0, n-2, T(n-2, j)*T(j, i))*T(i, k-1) )))}
