%t A176492 (*A060187*)
%t A176492 p[x_, n_] = (1 - x)^(n + 1)*Sum[(2*k + 1)^n*x^k, {k, 0, Infinity}];
%t A176492 f[n_, m_] := CoefficientList[FullSimplify[ExpandAll[p[x, n]]], x][[m + 1]];
%t A176492 << DiscreteMath`Combinatorica`;
%t A176492 t[n_, m_, 0] := Binomial[n, m];
%t A176492 t[n_, m_, 1] := Eulerian[1 + n, m];
%t A176492 t[n_, m_, 2] := f[n, m];
%t A176492 t[n_, m_, q_] := t[n, m, q] = t[n, m, q - 2] + t[n, m, q - 3] - 1;
