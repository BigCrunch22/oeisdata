%t A176491 (*A060187*)
%t A176491 p[x_, n_] = (1 - x)^(n + 1)*Sum[(2*k + 1)^n*x^k, {k, 0, Infinity}];
%t A176491 f[n_, m_] := CoefficientList[FullSimplify[ExpandAll[p[x, n]]], x][[m + 1]];
%t A176491 << DiscreteMath`Combinatorica`;
%t A176491 t[n_, m_, 0] := Binomial[n, m];
%t A176491 t[n_, m_, 1] := Eulerian[1 + n, m];
%t A176491 t[n_, m_, 2] := f[n, m];
%t A176491 t[n_, m_, q_] := t[n, m, q] = t[n, m, q - 2] + t[n, m, q - 3] - 1;
%t A176491 Table[Flatten[Table[Table[t[n, m, q], {m, 0, n}], {n, 0, 10}]], {q, 0, 10}]
