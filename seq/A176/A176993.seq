%e A176993 12, 210; # n=3, m=0,3
%e A176993 40, 360, -3024; # n=4,  m=0,1,4
%e A176993 180, 840, 55440; # n=5, m=0,1,5
%e A176993 1008, 3360, 60480, -1235520; # n=6, m=0,1,2,6
%e A176993 6720, 18144, 151200, 32432400; # n=7, m=0,1,2,7
%e A176993 51840, 120960, 665280, 19958400, -980179200; # n=8, m=0,1,2,3,8
%e A176993 453600, 950400, 3991680, 51891840, 33522128640; # n=9, m=0,1,2,3,9
%e A176993 4435200, 8553600, 29652480, 242161920, 10897286400, -1279935820800; # n=10, m=0,1,2,3,4,10
%t A176993 t[n_, m_] = Binomial[n + (m + 1), (m + 1)] - x*Binomial[n - (m + 1), (m + 1)];
%t A176993 (* if the solution exists it is made part of the array: if not, it is deleted*)
%t A176993 Table[Flatten[Table[If[Solve[t[n, m] == 0, x] == {}, {}, n!
%t A176993         x /. Solve[t[n, m] == 0, x]], {m, 0, n}], 1], {n, 0, 10}];
%t A176993 Flatten[%]
