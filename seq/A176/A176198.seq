%t A176198 m:=13;
%t A176198 p[x_,n_,q_]:= (1-x)^(n+1)*Sum[((q*j+1)^n+(q*(j+1)-1)^n)*x^j, {j,0,m+ 2}];
%t A176198 f[n_,k_,q_]:= Coefficient[Series[p[x,n,q], {x,0,m+2}], x, k];
