%e A090277 1 2 4 3
%p A090277 ring:= proc(k) option remember; local l, a, b, c, swap, h; l:= [1,2,3,4]; swap:= proc(i,j) h:=l[i]; l[i]:=l[j]; l[j]:=h end; a:= proc() swap(1,2); swap(3,4); l[k] end; b:= proc() swap(2,3); l[k] end; c:= proc() swap(3,4); l[k] end; [l[k], seq ([seq ([a(), b()][], j=1..3), a(), c()][], i=1..3)] end: a:= n-> ring(1)[modp(n-1, 24)+1]: seq (a(n), n=1..99); # _Alois P. Heinz_, Aug 19 2008
%t A090277 ring[k_] := ring[k] = Module[{l = Range[4], a, b, c, swap, h}, swap[i_, j_] := (h = l[[i]]; l[[i]] = l[[j]]; l[[j]] = h); a := (swap[1, 2]; swap[3, 4]; l[[k]]); b := (swap[2, 3]; l[[k]]); c := (swap[3, 4]; l[[k]]); Join[{l[[k]]}, Table[{Table[{a, b}, {j, 1, 3}], a, c}, {i, 1, 3}]] // Flatten]; a[n_] := ring[1][[Mod[n-1, 24]+1]]; Table[a[n], {n, 1, 99}] (* _Jean-Fran√ßois Alcover_, Mar 19 2014, after _Alois P. Heinz_ *)
