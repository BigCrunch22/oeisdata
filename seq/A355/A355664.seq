%o A355664 (PARI) toruns(n) = { my (r=[]); while (n, my (v=valuation(n+n%2, 2)); n\=2^v; r=concat(v, r)); r }
%o A355664 fromruns(r) = { my (v=0); for (k=1, #r, v=(v+k%2)*2^r[k]-k%2); v }
%o A355664 A(n,k) = { fromruns(Vec(Pol(toruns(n)) * Pol(toruns(k)))) }
