%o A355419 (Python)
%o A355419 from sympy import prime
%o A355419 def f(n):
%o A355419   S = 0
%o A355419   for x in range(1, n + 1):
%o A355419     for y in range(x + 1 , n + 1):
%o A355419       if ((pow(x, y, n) == pow(y, x, n))):
%o A355419         S += 2
%o A355419   return S + n
%o A355419 def a(n): return f(prime(n))
