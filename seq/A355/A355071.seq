%N A355071 G.f.: Sum_{n>=0} a(n)*x^n/(n!*4^(n*(n-1)/2)) = log( Sum_{n>=0} x^n/(n!*4^(n*(n-1)/2)) ).
%F A355071 a(0) = 0; a(n) = 1 - Sum_{k=1..n-1} 4^(k*(n-k)) * binomial(n-1,k) * a(n-k).
%o A355071 (PARI) a(n) = n!*4^(n*(n-1)/2)*polcoef(log(sum(k=0, n, x^k/(k!*4^(k*(k-1)/2)))+x*O(x^n)), n);
%o A355071 (PARI) a_vector(n) = my(v=vector(n+1)); v[1]=0; for(i=1, n, v[i+1]=1-sum(j=1, i-1, 4^(j*(i-j))*binomial(i-1, j)*v[i-j+1])); v;
