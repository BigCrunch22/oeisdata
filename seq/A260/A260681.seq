%U A260681 805993247839619614799176726719363512,2572332284084802308827712032135882716710570503279953274299454873
%N A260681 a(1) = a(2) = 1; a(n) = a(n-1) + gpf(1 + Product_{k = 1..n - 2} a(k)), where gpf means "greatest prime factor" (A006530).
%e A260681 a(3) = a(2) + gpf(1 + a(1)) = 1 + gpf(1 + 1) = 1 + 2 = 3.
%e A260681 a(4) = a(3) + gpf(1 + a(1) * a(2)) = 3 + gpf(1 + 1 * 1) = 3 + 2 = 5.
%e A260681 a(5) = a(4) + gpf(1 + a(1) * a(2) * a(3)) = 5 + gpf(1 + 1 * 1 * 3) = 5 + 2 = 7.
%o A260681 (PARI) gpf(n)=my(f=factor(n)[, 1]); f[#f];
%o A260681 a(n)=if(n>2, a(n-1)+gpf(1+prod(i=1, n-2, a(i))), 1)
%o A260681 first(m)=my(v=vector(m)); v[1]=1; v[2]=1; for(i=3, m, v[i]=v[i-1]+gpf(1+prod(k=1, i-2, v[k]))); v
%Y A260681 Cf. A006530, A078695.
