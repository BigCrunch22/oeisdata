%o A260717 (Scheme)
%o A260717 (define (A260717 n) (A260717bi (A002260 n) (A004736 n)))
%o A260717 (define (A260717bi row col) ((rowfun_n_for_A003309sieve row) col))
%o A260717 (define (add1 n) (1+ n))
%o A260717 ;; Uses definec-macro which can memoize also function-closures:
%o A260717 (definec (rowfun_n_for_A003309sieve n) (if (= 1 n) add1 (let* ((prevrowfun (rowfun_n_for_A003309sieve (- n 1))) (everynth (prevrowfun 1))) (compose-funs prevrowfun (nonzero-pos 1 1 (lambda (i) (modulo (- i 1) everynth)))))))
