%p A260338 phi:= proc(m, n) option remember;
%p A260338         `if`(n+m<2, 1, `if`(n+m=2, 2, m*phi(m-1, n)-
%p A260338         `if`(n=0, (m-1)*(m-2)/2*phi(m-3, 0), -n*phi(m, n-1))))
%p A260338       end:
%p A260338 seq(seq(phi(m-k, k), k=0..m), m=0..10);  # _Alois P. Heinz_, Jul 30 2015
%t A260338 phi[m_, n_] := phi[m, n] = If[n+m < 2, 1, If[n+m == 2, 2, m*phi[m-1, n] - If[n == 0, (m-1)*(m-2)/2*phi[m-3, 0], -n*phi[m, n-1]]]]; Table[Table[phi[ m-k, k], {k, 0, m}], {m, 0, 10}] // Flatten (* _Jean-FranÃ§ois Alcover_, Feb 17 2016, after _Alois P. Heinz_ *)
