%t A102375 digits = 103; m0 = 5; dm = 2; Clear[f, g]; f[x_, m_] := Sum[((-1)^(j + 1)*x^( 2^(j + 1) - 2 - j))/Product[1 - x^(2^k - 1), {k, 1, j}] , {j, 1, m}] // N[#, digits]&; g[m_] := g[m] = (1/x /. FindRoot[f[x, m] == 1, {x, 5/9, 4/9, 6/9}, WorkingPrecision -> digits ]); g[m0]; g[m = m0 + dm]; While[RealDigits[g[m], 10, digits] != RealDigits[g[m - dm], 10, digits], Print["m = ", m]; m = m + dm]; RealDigits[g[m], 10, digits] // First (* _Jean-Fran√ßois Alcover_, Jun 19 2014 *)
