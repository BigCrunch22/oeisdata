%p A102625 A102625:=proc(n,k) if k<=n+1 then k*(2*n-k+1)!/2^(n-k+1)/(n-k+1)! else 0 fi end proc:
%p A102625 for n from 0 to 8 do seq(A102625(n,k),k=1..n+1) od; # yields sequence in triangular form
%t A102625 t[n_, k_] := k*(2n-k+1)!/(2^(n-k+1)*(n-k+1)!); Table[t[n, k], {n, 0, 8}, {k, 1, n+1}] // Flatten (* _Jean-François Alcover_, Jan 21 2013 *)
%o A102625 (PARI) {T(n, k) = my(m = n-k+1); if( k<1 || k>n+1, 0, k * (n+m)! / (2^m * m!))}; /* _Michael Somos_, Aug 16 2016 */
