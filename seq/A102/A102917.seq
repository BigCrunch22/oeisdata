%e A102917 + A102921(n)*x^(2n+1)*(1-x)(1-2x)*..*(1-(n+1)x) + ...
%o A102917 (PARI) {a(n)=if(n==0,1,polcoeff(1-sum(k=0,n-1,a(k)*x^k*prod(j=1,k\2+1,1-j*x+x*O(x^n))),n))}
